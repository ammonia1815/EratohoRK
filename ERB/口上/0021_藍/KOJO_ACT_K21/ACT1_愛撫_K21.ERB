	IF 구상기입체크(0)
		;전부터애무
		IF TCVAR:MASTER:위치전후 == 1
;OriginalString : 
			PRINTFORMW 
		;뒤로부터애무
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF
