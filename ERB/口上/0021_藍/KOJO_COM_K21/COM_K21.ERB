	;=================================================
	;ACT분류, 애무
	;=================================================
	IF 구상기입체크(0) && MENUMATCH(TFLAG:ACT, "애무")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
