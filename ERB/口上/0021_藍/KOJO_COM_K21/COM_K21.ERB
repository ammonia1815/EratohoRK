	;=================================================
	IF 구상기입체크(0) && TRUECHECK(MENUMATCH(TFLAG:ACT, "성봉"), TEQUIP:성교봉사중) > 0
		;Ｖ성교봉사중
		IF V_SEX(MASTER)
;OriginalString : 
			PRINTFORMW 
		;Ａ성교봉사중
		ELSE
;OriginalString : 
