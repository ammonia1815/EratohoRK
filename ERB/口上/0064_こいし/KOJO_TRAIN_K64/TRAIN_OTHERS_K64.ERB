;────────────────────────────────────
;EV21, 대만족구상
;────────────────────────────────────
;·죠교자의대만족보너스를취득했다경우에 표시
;-------------------------------------------------
;=================================================
@KOJO_EVENT_K64_21(ARG)
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━━
;코이시님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크()
	;=================================================
	;상냥하게 응답한다
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("상냥하게 응답한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을 받아들인다/기분 좋다!
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을 받아들인다/기분 좋다!")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;안에 내!
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("안에 내!")
		;분기가필요예요… 우선 다음에
		;어느 쪽으로 냈으면 좋은 것인지, 사정했다의 것인가, 양은 어떤가
		CALL HTMLPRINTW(@"「후우…%K64_SYMBOL("BHeartW")%」")
		PRINTFORMW 「%K64_CNAME()%의 보지, 대단히 기분 좋았어요♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;비명지른다
	;=================================================
	IF 구상기입체크() && IS_NOWCOMNAME("비명지른다")
		PRINTFORMW 「지금의 울음 소리, 대단히 좋았어요」
		PRINTFORMW 「 나, 코훈 해 버렸다♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;애정을 담아 한다
	;=================================================
	IF 구상기입체크() && IS_NOWCOMNAME("애정을 담아 한다")
		CALL HTMLPRINTW(@"「와-이♪ 아래층-응%K64_SYMBOL("BHeartW")%」")
		RETURN 1
	ENDIF
	
	;SELECTCOM == 15는 상층에서 처리
	;=================================================
	;REACT분류가적극적으로 한다(적극적으로 한다, 이루는대로등)
	;=================================================
	IF 구상기입체크(0) && IS_COMGRONAME("적극적으로 한다/받아들인다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;그 외를 처리
	;---------------------------------------------
	SELECTCASE RANDEVENT(2, 0)
		CASE 0
			PRINTFORMW 「이건이나 보고 다해가 되어 버리는군」
		CASEELSE
			PRINTFORMW 「응, 좋은 느낌이군요♪」
			PRINTFORMW 「후후,%K64_CNAME()%도 나의 기쁘게 하는 방법을 알아 왔는지 해들♪」
	ENDSELECT
	RETURN 1
ENDIF


