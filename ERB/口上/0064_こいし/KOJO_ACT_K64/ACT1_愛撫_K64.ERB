IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자가자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 1)
		IF TEQUIP:TARGET:죠교자페니스밴드	;죠교자가페니스밴드 몸에 걸치고 있다
			PRINTFORMW
		ELSE								;그 외
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;안면승마중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:TARGET:안면승마, TEQUIP:TARGET:안면승마항문) > 0
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;;하반신복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(0) && PANTSCHECK(1, 1)
		IF LOWERJACKET(1) == 1	;하반신겉옷를 걷고
			PRINTFORMW 
		ELSE					;속옷 너머로
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

