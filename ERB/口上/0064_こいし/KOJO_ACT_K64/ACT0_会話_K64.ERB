IF 구상기입체크() && !TFLAG:ACT파생
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크() && FIRSTACT(0)
		PRINTFORMW 「그렇게 말하면」
		PRINTFORMW 「신입의 애완동물의 주제에 옷을 입고 있다는 나마이키예요」
		PRINTFORMW 「…탈의하고 싶지 않았으면, 자신으로부터 벗는 것」
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상에페니스 있어
	;=================================================
	IF 구상기입체크(0) && PENIS(MASTER)
		IF TCVAR:MASTER:발기도 >= 1000	;발기 하고 있다
			PRINTFORMW 
		ELSE							;그 외
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상에페니스없음
	;=================================================
	IF 구상기입체크(0) && !PENIS(MASTER)
		IF PALAM:MASTER:윤활 > 500	;젖고 있다
			PRINTFORMW 
		ELSE						;그 외
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크()
		PRINTFORMW 「우선 그것, 방해이니까 벗어」
		PRINTFORMW 「중요한 나들이옷을 찢어진 구는 없지요?」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT5_1, 옷을 입도록(듯이) 명령
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크() && TFLAG:ACT파생 == 1
	;좀 더 세세하게 하고 싶지만 뒷전으로…
	PRINTFORMW 「아, 이제 옷 입어도 좋아」
	RETURN 1
ENDIF

;────────────────────────────────────
;ACT6, 합의를 요구한다
;────────────────────────────────────
;·처음을 받아도 괜찮을까 얘기한다
;·처음=처녀or동정(양쪽 모두의 경우도 있다)
;·이미합의를 얻고 있는 경우는 발생하지 않는다
;·죠교대상가처녀에서도동정도 아닌 경우는 발생하지 않는다
;·TFLAG:징계·플래그가 서 있으면 발생하지 않는다
;·턴방침가"이상"라면 발생하지 않는다
;·화제 『합의』가 기출이 되어, 이후, 동일조교중에서는 억제된다
;-------------------------------------------------
;·코이시의 경우, 이것도 기본 나오지 않는다
;-------------------------------------------------
@KOJO_ACT_K64_6
;-------------------------------------------------
;함수@TARGET_DESCRIPTION로 TEQUIP(성교중/죠교대상Ｃ사용/성교봉사중/자위중 & 2)로 지문은변화 한다
;후는 기호로 분기를 증감해 주세요
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT6, 합의를 요구한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0)
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상이남자의 경우
	;=================================================
	IF 구상기입체크(0) && TALENT:MASTER:남자
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가후타나리의 경우
	;=================================================
	IF 구상기입체크(0) && TALENT:MASTER:후타나리
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가여자의 경우
	;=================================================
	IF 구상기입체크(0) && !PENIS(MASTER)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;성별 마다의 나누어 쓰고를 하지 않으면, 여기만 써 둡시다
	;남자, 후타나리, 여자분기를 모두 묻었을 경우는 불필요합니다. 삭제합시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT7, 징계한다고 위협한다
;────────────────────────────────────
;·지금부터징계·한다고 말하기 시작한다
;·이 ACT가 발생하고 있는 시점에서 TFLAG:징계·플래그는 서 있다
;·턴방침가"소프트", "이상"라면 발생하지 않는다
;·TEQUIP:밀어넘어지는중시에는 발생하지 않는다
;-------------------------------------------------
;·코이시가 일부러 선언하는 것은 희
;-------------------------------------------------
@KOJO_ACT_K64_7
;-------------------------------------------------
;여기에서는 지문의변화는 없다. 분기의 증감은 기호로
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT7, 징계한다고 위협한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크()
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다」의 언젠가
	;=================================================
	IF 구상기입체크(0) && IS_PREVCOMNAME("무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「기분좋게 해」（이 철면피! ）
	;=================================================
	IF 구상기입체크() && IS_PREVCOMNAME("기분좋게 해")
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「아무리 애완동물에게는 수치는 없기 때문에는 」
				PRINTFORMW 「조금 철면피가 지나지 않는다?」
				PRINTFORMW 「이래서야 아내가 부끄러워서 끝 끝 산책도 할 수 없고.…조금 예의 범절 고쳐 준다」
			CASEELSE
				PRINTFORMW 「그러면 아기분이 좋은 사형을 해 주기 때문에」
				PRINTFORMW 「기분 좋아지고 싶다면 마음대로 아무쪼록」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「더 하드하게 해」（이 돼지째! ）
	;=================================================
	IF 구상기입체크() && IS_PREVCOMNAME("더 하드하게 해")
		SELECTCASE RANDACT(2, 1)
			CASE 0
				PRINTFORMW 「과연, 상냥하게 하면 기어오른다 라고」
				PRINTFORMW 「전에 언니가 말했었던 것은 이런 것일까?」
				PRINTFORMW 「…조금 예의 범절 고쳐 주어요」
			CASEELSE
				PRINTFORMW 「하드하게 해라, 래? -다 자신의 입장을 잘 알지 않구나…」
				PRINTFORMW 「사형이야! 사형!」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「용서를 빌다」（허락해 받을 수 없어했다）
	;=================================================
	IF 구상기입체크() && IS_PREVCOMNAME("용서를 빌다")
		SELECTCASE RANDACT(2, 2)
			CASE 0
				PRINTFORMW 「좋다? 애완동물은 남편님의 모두를 받아들여」
				PRINTFORMW 「모른다면, 알 때까지 예의범절을 가르쳐 주어요」
			CASEELSE
				PRINTFORMW 「그렇게 허락했으면 좋으면, 허락해 주고 없다♪」
				PRINTFORMW 「…사형이군요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「자위를 시작한다」（인의 이야기를 들어라! ）
	;=================================================
	IF 구상기입체크() && IS_PREVCOMNAME("자위를 시작한다")
		SELECTCASE RANDACT(2, 3)
			CASE 0
				PRINTFORMW 「뭐? 자제 할 수 없게 되었어?」
				PRINTFORMW 「인내력이 없는 아이는, 사형이야」
			CASEELSE
				PRINTFORMW 「뭐 해? 나, 그런 일 흰 응이라고 말하지 않다」
				PRINTFORMW 「그렇게 제멋대로인 일 한다면, 여기에도 생각이 있어요」
				PRINTFORMW 「그런데, 어느 사형으로 할까…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크()
		SELECTCASE RANDACT(2, 4)
			CASE 0
				PRINTFORMW 「아무래도%K64_CNAME()%에는, 귤나무～있고 사형이 필요같구나♪」
				CALL HTMLPRINTW(@"「자, 어느 벌로 할까나…%K64_SYMBOL("BHeartW")%」")
			CASEELSE
				PRINTFORMW 「, 징계나무 될 각오는 좋다?」
				PRINTFORMW 「죽음의 구가 있다면, (들)물어 두어 주어요」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT8, 고함치다
;────────────────────────────────────
;·큰 소리로고함치다
;·턴방침가"소프트", "노멀"에서는 발생하지 않는다
;·TFLAG:징계·플래그에 관련되지 않고, 발생할 가능성이 있다（징계·안에 한층 더고함치다가능성등 ）
;-------------------------------------------------
;·코이시가 고함치고 있다. 코이시는 고함칩니까? 이상하다고 생각하지 않습니까? 당신
;·저빈도면서 확실히 나오는 것이지만…
;·코이시의 성질상, 격앙 해 큰 소리를 지르는 모양은 완전히 있을 수 없다고 까지는 말하지 않지만, 그 광경은 상상 하기 어렵다
;· 나와 같은 천학 비재의 몸에는 어려운 개소의 하나. 도망치는 것 같고 싫지만… 봉인했다 (분)편이 오히려 자연?
;·왜일까 거의 전용의 함수를 할 수 있었다. 함수의 재료는 어디에서 나오는지 모른다
;·이지만 상손 먹어는 있다고는 말하기 어려울지도… 실제 미묘
;-------------------------------------------------
@KOJO_ACT_K64_8
;-------------------------------------------------
;지문은"%CALLNAME:TARGET%는 %CALLNAME:MASTER%를 큰 소리로 고함쳤다…"만으로 파생은 없다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT8, 고함치다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크()
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전COM가 「무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다」의 언젠가
	;=================================================
	IF 구상기입체크() && IS_PREVCOMNAME("무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다")
		CALL BIGPRINTW("「완전히, 애완동물의 주제에 건방지구나!」")
		CALL BIGPRINTW(@"「\@ RAND:2 ? 그렇게 쳐 뿌릴 수 있는들 # 파 다랑어에 \@싶은거야!?」")
		PRINTFORMDW 진하고는 웃으면서 화나 있는 것 같다. 어쩐지 기색이 나쁘다…
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크()
		SELECTCASE TENSION(TARGET, 7)	;죠교자의초조함
			CASE "높다"
				CALL BIGPRINTW("「아앗, 이제(벌써)! 안절부절 한다!」")
				CALL BIGPRINTW("「어째서 분명하게 애완동물답게 할 수 없는거야!?」")
				PRINTFORMDW 진하고는 웃으면서 화나 있는 것 같다. 어쩐지 기색이 나쁘다…
			CASEELSE
				CALL BIGPRINTW("「아″!!!」")
				PRINTFORMW 「, 시원해졌다」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT9, 기분나쁘게 웃다
;────────────────────────────────────
;·기분 나쁜 미소를 띄운다
;·이상계로부터 발생하는 경우도 있다
;·금일의방침가"노닥노닥"라면 발생하지 않는다
;·화제 『비웃는다』가 기출이 되어, 이후, 동일조교중에서는 억제된다
;-------------------------------------------------
;·코이시비웃으면서 여러 가지 한다. 무섭다!
;-------------------------------------------------
@KOJO_ACT_K64_9
;-------------------------------------------------
;함수@TARGET_DESCRIPTION에 의해 TEQUIP(성교중/죠교대상Ｃ사용/성교봉사중/자위중 & 2)로 지문은변화 한다
;기타·조건없음를 최초로 묻은 다음, 뒤는 기호로 분기를 증감해 주세요
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT9, 기분나쁘게 웃다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크()
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;PENIS(TARGET)/! PENIS(TARGET)/TEQUIP:죠교자페니스밴드등으로 분기 해도 괜찮다
	;=================================================
	;성교중
	;=================================================
	IF 구상기입체크(0) && TEQUIP:TARGET:성교중
		SELECTCASE TEQUIP:성교중	;체위에 의한 파생
			CASE 1	;정상위				체중을 실어 %CALLNAME:MASTER%의 질내에 가득
				PRINTFORMW 
			CASE 2	;후배위				뒤에서 %CALLNAME:MASTER%의 질내에 가득
				PRINTFORMW 
			CASE 3	;기승위				%CALLNAME:MASTER%의 암컷 구멍에 끝까지 밀어넣어
				PRINTFORMW 
			CASE 4	;대면좌위			밀착한 %CALLNAME:MASTER%의 질을 애무하며
				PRINTFORMW 
			CASE 5	;배면좌위			%CALLNAME:MASTER%에게 삽입하여 껴안고
				PRINTFORMW 
			CASE 6	;항문섹스		%CALLNAME:MASTER%의의 항문에 넣어
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;죠교대상의Ｃ에의애무를 하고 있는 상황. TEQUIP관리인 것으로 가능성은 비교적 있다
	;=================================================
	;죠교대상Ｃ사용
	;=================================================
	IF 구상기입체크(0) && GROUPMATCH(TEQUIP:TARGET:죠교대상Ｃ사용, 1, 2)
		IF TEQUIP:TARGET:죠교대상Ｃ사용 == 1	;수음·애무계속중
			PRINTFORMW 
		ELSE									;펠라·쿤닐링구스 계속중
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;PENIS(MASTER)/! PENIS(MASTER)/TEQUIP:페니스밴드등으로 분기 해도 괜찮다
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상기입체크(0) && TEQUIP:성교봉사중
		SELECTCASE TEQUIP:성교봉사중	;체위에 의한 파생
			CASE 1	;정상위시킨다			크게 벌어진 가랑이의 사이에 맞아들이고
				PRINTFORMW 
			CASE 2	;후배위시킨다			배후로부터질내에삽입 되고
				PRINTFORMW 
			CASE 3	;역강간				미끈미끈의 질로 큐우큐우 짜내고
				PRINTFORMW 
			CASE 4	;대면좌위시킨다			체를 밀착시켜 질 안쪽으로 환영해
				PRINTFORMW 
			CASE 5	;배면좌위시킨다			등을 맡겨 만끽해
				PRINTFORMW 
			CASE 6	;항문섹스시킨다	장액에 미끌거리는 항문으로 억압
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자가자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:TARGET:자위중, 1)
		IF TEQUIP:TARGET:죠교자페니스밴드	;죠교자가페니스밴드 몸에 걸치고 있다
			PRINTFORMW
		ELSE								;그 외
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크()
		;실제 사랑스럽다. (이)지만행동와 부조화인 것으로 무섭다!
		SELECTCASE RANDACT(5, 0)
			CASE 0		;사랑스럽다!
				PRINTFORMW （%RAND_SPLIT("싱글벙글/히죽히죽/에헤헤-/반짝☆/나")%）
			CASE 1		;조소?
				PRINTFORMW 「%RAND_SPLIT("아하하/아하핫/우후후/우후훗/에헤헤/에헤헤-/후후후후/후후…/킥킥/킥킥…")%」
			CASE 2		;조금 이상해?
				PRINTFORMW 「%RAND_SPLIT("우후후/우후후…/후후후/후후후…/킥킥/킥킥…")%」
			CASE 3		;위험하다! 무섭다!
				CALL HTMLPRINTW(@"「<font color='#FF4000'>아%"꺄"*(120+RAND:120)%</font>」")
			CASEELSE	;는 대대적인 못된 장난
				;조교중는오토모드도 있고 스킵 다발인 것으로 어렵다. 이렇게 말하면서 한계에 도전
				IF FLAG:오토모드 || MESSKIP()	;오토모드와 스킵상태는 무거운 처리를 회피
					;여기서 뭐 할까는 생각중
				ELSE
					;그다지 상손 먹어는 있다고는 생각되지 않는다. 조금 생각하자
					;도중에 스킵 되어도 귀찮음. 조금 보류
					;차라리 전화면 컷 인에했다 (분)편이손를 빠지는지도 모르겠지만…
					;어쨌든 때문했다 있고 일을 하기 위해서는, 대대적인 장치가필요가 되므로 일단 놓아두자
					;정직, 함수 사양은 커녕 완성형조차 어슴푸레하지만…
					;시시한 함수의 너무 써, 함수 쓰는 것 자체가 약간 식상 기색… 뭐 함수 쓰는 것 싫지 않지만
					LOCAL:0 = CURRENTREDRAW()
					LOCAL:1 = 5 + RAND:6
					PRINTL 
					REDRAW 0
					FOR LOCAL:2, 0, LOCAL:1
						{
						LOCALS:0 = 　%"　" * RAND:20%%RAND_SPLIT("아하하/아하핫/후후/우후훗/에헤헤/우헤헤/후후후후/후후…/" + 
							"킥킥/킥킥…/우후후/우후후…/후후후/후후후…/킥킥/킥킥…")%%" " * RAND:20% 
						}
						CALL ALIGNMENTS(RAND_SPLIT("LEFT/CENTER/RIGHT"))
						CLEARLINE 1
						REDRAW 1
						;OriginalString : 쿠쿡/쿠쿡…/ウフフ/ウフフ…/フフフ/フフフ…/クスクス/クスクス…")%%"　" * RAND:20%　
						;IN-OUT까지 일련으로 실시하는 것 같은 함수 가 있어도 된다고 생각하지 않습니까?
						;라고 하는 것보다, 그것을 만들지 않는 곳의 부분… 아마 안정되지 않는 예감이 해요
						CALL FADEIN(4, LOCALS:0)
						REDRAW 0
						CLEARLINE 1
						REDRAW 1
						CALL FADEOUT(4, LOCALS:0)
						REDRAW 0
					NEXT
					REDRAW LOCAL:0
					ALIGNMENT LEFT
				ENDIF
				PRINTFORMDW 방안이 도달하는 곳(중)으로부터 진하고의 치 있고 목소리가 들려 온다…
				PRINTFORMDW 정체의 모르는 공포에 %CALLNAME:MASTER%의 제정신이 서서히 깎아져 간다…
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
