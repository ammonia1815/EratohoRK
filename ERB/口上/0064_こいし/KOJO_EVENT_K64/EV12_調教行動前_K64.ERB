;조건 없음
;구상기입 체크(확률％, 입력했다숫자의 확률로구상를 표시)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0)
	;=================================================
	;만족 50％이상
	;=================================================
	IF 구상기입체크(0) && TENSION(TARGET, 8) == "높다"
		PRINTFORMW 「후후, 즐겁다♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;이성 25％이하
	;=================================================
	IF 구상기입체크(0) && TENSION(TARGET, 5) == "낮다"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;흥미 25％이하
	;=================================================
	IF 구상기입체크(0) && TENSION(TARGET, 6) == "낮다"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;초조함 50％이상
	;=================================================
	IF 구상기입체크(0) && TENSION(TARGET, 7) == "높다"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
