;※상기 조건에 들어맞는 경우는 가능
;-------------------------------------------------
;·TFLAG:REACT파생(0=들은체 만체 한다/1=가게해줘 준다/2=가게해줘 주지 않는다）
;-------------------------------------------------
;ACT에 있어서 TCVAR:MASTER:애태움도 == 1(가게 한다)에서 만나도 COM70의선택로 TCVAR:MASTER:애태움도 > 1(초조하게 한다)(이)가 되는 경우 있어
;현재의 사양에서는 이손의변화를 참조할 방법은 없다. 본체측의 대응이 바람직한다!
;-------------------------------------------------
@KOJO_COM_K64_70
;-------------------------------------------------
;·TFLAG:REACT파생(0=들은체 만체 한다/1=가게해줘 준다/2=가게해줘 주지 않는다）
;-------------------------------------------------

;%CALLNAME:TARGET%는 심술궂은 미소를 띄우며 %CALLNAME:MASTER%를 애태우기 시작했다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT파생 2, 가게해줘 주지 않는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;우선 최소한의 분기를 놓아둡니다만
;PENIS(MASTER) 등으로페니스의 유무를 참조했다 (분)편이 무난할지도 모릅니다
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:REACT파생 == 2
	;=================================================
	;ACT분류, 애무
	;=================================================
	IF 구상기입체크(0) && MENUMATCH(TFLAG:ACT, "애무")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(MENUMATCH(TFLAG:ACT, "성봉"), TEQUIP:TARGET:성교봉사중) > 0
		IF V_SEX(MASTER)	;Ｖ성교봉사중
			PRINTFORMW 
		ELSE				;Ａ성교봉사중
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;%CALLNAME:TARGET%는 상냥하게 미소지으면서애무를 계속했다
;━━━━━━━━━━━━━━━━━━━━━━━━
;파생 1, 가게해줘 준다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && TFLAG:REACT파생 == 1
	;=================================================
	;ACT분류, 애무
	;=================================================
	IF 구상기입체크(0) && MENUMATCH(TFLAG:ACT, "애무")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(MENUMATCH(TFLAG:ACT, "성봉"), TEQUIP:TARGET:성교봉사중) > 0
		IF V_SEX(MASTER)	;Ｖ성교봉사중
			PRINTFORMW 
		ELSE				;Ａ성교봉사중
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;%CALLNAME%는 못된 장난 같은 미소를 띄우면서, %CALLNAME:MASTER%의 요구를 (들)물어 류했다…
;━━━━━━━━━━━━━━━━━━━━━━━━
;파생 0, 요구를 들은체 만체 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크() && TFLAG:REACT파생 == 0
	SELECTCASE RANDCOM(3, 0)
		CASE 0
			PRINTFORMW 「? 어디에 가고 싶은 것인지?」
			PRINTFORMW 「산책? 그렇지 않으면 화장실?」
		CASE 1
			PRINTFORMW 「에?  %RAND_SPLIT("산책/화장실")%에 가고 싶은거야?」
			PRINTFORMW 「킥킥」
		CASEELSE
			PRINTFORMW 「응? 자주(잘) 들리지 않았어요」
			PRINTFORMW 「좀 더 큰 소리로 말해 봐? 후후」
	ENDSELECT
	RETURN 1
ENDIF
