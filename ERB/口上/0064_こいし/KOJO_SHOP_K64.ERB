;*************************************************
;숍이벤트구상
;*************************************************

;=================================================
;EV101, 의상변경
;=================================================
;·숍으로부터의상변경을 실시했을 경우에 표시
;-------------------------------------------------
@KOJO_EVENT_K64_101(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=
;  1의 위…(0. 이제(벌써) 입고 있다/1. 갈아입었다/2. 부끄러워서 입을 수 없다/3. 싫어해 입지 않는다/9. 가지고 있지 않다)
;  10의 위…(0. 통상/10. 반라/20. 전라/30. 속옷없음)
;  백의 위… 코스프레를 위해서(때문에) 보류
;  천의 위…(0. 죠교자/1000. 죠교대상)
;  예(죠교대상가 부끄러워해전라로 할 수 없었다=1022)
;-------------------------------------------------

;-------------------------------------------------
;죠교자가 갈아입었다
;-------------------------------------------------
IF 구상기입체크() && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		CASE 1		;통상되었다
			PRINTFORMW 「역시 주인은 분명하게 옷을 입는 것」
			PRINTFORMW 「애완동물에게 옷을 입히는 것은 어떨까라고 생각하는데」
			RETURN 2
		CASE 11		;반라되었다
			PRINTFORMW 「이런 것이, 섹시하다고 말하는거죠?」
			;빈유로 각크시라고 하는 자그마한 저항(그렇지만 사실은 로리콘이었다거나 하는거야)
			;여기는 만지작거려 보람이 있을 듯 하는 생각이 들어요… 조금 바리에이션이라든지 분기라든지 생각해 두자
			IF TALENT:TARGET:빈유
				PRINTFORMDW %CALLNAME:MASTER%(은)는, 진하고의 가슴 팍을 시선을 돌림 하면 비꼬는 듯한 깊은 한숨을 붙었다
				PRINTFORMW 「네…뭐야 그것.%INNER_T(TARGET, 1)%하지만 안된다고 말해? 그렇지 않으면…」
			ENDIF
			RETURN 2
		CASE 21		;전라되었다
			PRINTFORMW 「…? 별로 부끄럽지는 않아요」
			PRINTFORMW 「애완동물에게 알몸을 볼 수 있어도, 뭐라고 생각하지 않는 것」
			RETURN 2
		;CASE 31	;…… 스으스으 하는 ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교자가 부끄러워해 갈아입을 수 없다
;-------------------------------------------------
IF 구상기입체크() && ARG < 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		CASE 12		;반라라도 부끄럽다
			PRINTFORMW 「응―…어쩐지%K64_CNAME()%에는 과분한 생각이 들어요」
			PRINTFORMW 「또 다음으로 합시다」
			RETURN 2
		CASE 22		;평상시부터전라는 조금……
			PRINTFORMW 「응―…어쩐지%K64_CNAME()%에는 과분한 생각이 들어요」
			PRINTFORMW 「또 다음으로 합시다」
			RETURN 2
		;CASE 32	;팬츠가 없기 때문에 부끄러운 걸! ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 「응─… 뭔가%K64_CNAME()%에는 과분한 생각이 들어요」
		;	PRINTFORMW 「또 다음으로 합시다」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교자가 싫어해 갈아입을 수 없다
;-------------------------------------------------
IF 구상기입체크() && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		CASE 13		;반라는 싫어
			PRINTFORMW 「… 어쩐지 춥지 않다?」
			PRINTFORMW 「역시 입지 않았다고 안되구나. 신체가 차가워져버린다」
			RETURN 2
		CASE 23		;평상시부터전라는 싫어
			PRINTFORMW 「… 어쩐지 춥지 않다?」
			PRINTFORMW 「역시 입지 않았다고 안되구나. 신체가 차가워져버린다」
			RETURN 2
		;CASE 33	;스으스으 하는 것은 혐 ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 「… 뭔가 춥지 않아?」
		;	PRINTFORMW 「역시 입지 않았다고 안되구나. 신체가 차가워져 버린다」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교자가 갈아입는 것을 그만두었다
;-------------------------------------------------
IF 구상기입체크(0) && ARG < 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		CASE 0		;통상인 채
			PRINTFORMW 
			RETURN 2
		CASE 10		;반라인 채
			PRINTFORMW 
			RETURN 2
		CASE 20		;전라인 채
			PRINTFORMW 
			RETURN 2
		;CASE 30	;스으스으 하는 대로 ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교대상를 갈아입음시켰다
;-------------------------------------------------
IF 구상기입체크() && ARG >= 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		CASE 1001	;통상되었다
			PRINTFORMW 「애완동물의 버릇에 옷을 입혀 줄 수 있다니」
			PRINTFORMW 「%K64_CNAME()%(은)는 행운아」
			RETURN 2
		CASE 1011	;반라되었다
			;이 쓰는 법이라고 첫회와 2회째 이후의 분기를 갖고 싶은 분위기
			PRINTFORMW 「반 정도 애완동물다움이 나왔기 때문에, 옷도 반」
			PRINTFORMW 「무엇? 불평 있어?」
			RETURN 2
		CASE 1021	;전라되었다
			;이 쓰는 법이라면노출증에 의한 분기를 갖고 싶은 분위기
			PRINTFORMW 「그래요, 애완동물은 그것이 자연스러워」
			PRINTFORMW 「부끄럽다? 무슨 말해. 애완동물에게 수치는 없어요」
			RETURN 2
		;CASE 1031	;…… 스으스으 하는 ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교대상가 부끄러워해 갈아입을 수 없다
;-------------------------------------------------
IF 구상기입체크() && ARG >= 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		CASE 1012	;반라라도 부끄럽다
			PRINTFORMW 「조금%K64_CNAME()%, 애완동물로서의 자각이 부족하지 않아?」
			PRINTFORMW 「 좀 더 엄격하게 예의범절을 가르치지 않으면 안 되네요」
			RETURN 2
		CASE 1022	;평상시부터전라는 부끄럽다
			PRINTFORMW 「조금%K64_CNAME()%, 애완동물로서의 자각이 부족하지 않아?」
			PRINTFORMW 「 좀 더 엄격하게 예의범절을 가르치지 않으면 안 되네요」
			RETURN 2
		;CASE 1032	;팬츠가 없기 때문에 부끄러운 걸! ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 「조금%K64_CNAME()%, 애완동물로서의 자각이발리나 있고 것이 아니야?」
		;	PRINTFORMW 「좀 더 엄격하게 예의범절을 가르치지 않으면 안 되네요」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교대상가 싫어해 갈아입을 수 없다
;-------------------------------------------------
IF 구상기입체크() && ARG >= 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		CASE 1013	;반라는 싫어
			PRINTFORMW 「아무튼, 건방지구나! 애완동물의 버릇에」
			PRINTFORMW 「조금 자신의 입장을 깨닫게 해 줄 필요가 있을 듯 하구나」
			RETURN 2
		CASE 1023	;평상시부터전라는 싫어
			PRINTFORMW 「아무튼, 건방지구나! 애완동물의 버릇에」
			PRINTFORMW 「조금 자신의 입장을 깨닫게 해 줄 필요가 있을 듯 하구나」
			RETURN 2
		;CASE 1033	;스으스으 하는 것은 혐 ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 「뭐, 건방짐군요! 애완동물의 주제에」
		;	PRINTFORMW 「조금 자신의 입장을 깨닫게 해 주는필요가 있을 듯 하구나」
		;	RETURN 2
	ENDSELECT
ENDIF

;-------------------------------------------------
;죠교대상를 갈아입음시킨다의 것을 그만두었다
;-------------------------------------------------
IF 구상기입체크(0) && ARG >= 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		CASE 1000	;통상인 채
			PRINTFORMW 
			RETURN 2
		CASE 1010	;반라인 채
			PRINTFORMW 
			RETURN 2
		CASE 1020	;전라인 채
			PRINTFORMW 
			RETURN 2
		;CASE 1030	;스으스으 하는 대로 ※현재의 Rev에서는 실장되어 있지 않은 개소
		;	PRINTFORMW 
		;	RETURN 2
	ENDSELECT
ENDIF


;=================================================
;EV102, 죠교자변경 FIRSTEVENT()
;=================================================
;·죠교자를 교대시켰다때에 표시
;-------------------------------------------------
@KOJO_EVENT_K64_102(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 구상주가 다른 캐릭터에게 맡긴, 1, 구상주가죠교자되었다)
;구죠교자는 「%CALLNAME:SHOP_OLDTARGET%」, 신죠교자는 「%CALLNAME:SHOP_NEWTARGET%」로 참조할 수 있다
;구상주는 TARGET로 참조
;-------------------------------------------------

;-------------------------------------------------
;다른죠교자에 교대
;-------------------------------------------------
IF 구상기입체크() && ARG == 0
	SELECTCASE NO:SHOP_NEWTARGET	;교대처의 캐릭터로 분기
		CASE 61		;사토리
			IF TALENT:TARGET:주인님	;코이시가주인님
				PRINTFORMW 「언니, 조금 부탁이지만…」
				PRINTFORMW 「%K64_CNAME(MASTER, 2)%의 예의 범절, 조금의 사이만 대신해 주지 않는다?」
				IF CFLAG:TARGET:죠교레벨 < 3					;코이시의죠교Lv가 3 미만
					PRINTFORMW 「어떻게도 아직 익숙해지지 않아서…」
					PRINTFORMW 「언니의 방식을 보고 싶은거야」
				ELSEIF MARK:TARGET:반항각인 >= 5 && !FAVOR(500)	;코이시의죠교대상로부터의반항각인Lv5 이상 또한 죠교대상로부터의호의 500 미만
					PRINTFORMW 「%K64_CNAME(MASTER, 2)%라고, 매우 반항적인의」
					PRINTFORMW 「조금 언니로부터도 확실히 해 주면 살아나요」
				ELSEIF !FAVOR(-100)								;코이시의죠교대상로부터의호의가-100 미만
					PRINTFORMW 「어쩐지, 나 미움받고 있는 생각이 들어」
					PRINTFORMW 「어떻게 하면 좋은가 몰라서…」
				ELSEIF CFLAG:TARGET:소모 > 5					;코이시의 피로가 축적하고 있다
					PRINTFORMW 「조금 지쳐 버렸기 때문에…」
					PRINTFORMW 「온천에라도 잠겨 유곽」
				ELSE											;상기 이외
					PRINTFORMW 「아니,%K64_CNAME(MASTER, 2)%에 질린 것이 아니야?」
					PRINTFORMW 「조금 휴게야 휴게♪」
				ENDIF
			ELSEIF TALENT:SHOP_NEWTARGET:주인님	;사토리가주인님
				IF ASSI:1 == SHOP_NEWTARGET || ASSI:2 == SHOP_NEWTARGET	;사토리가조수였다
					PRINTFORMW 「저것, 이제 된거야? 조금 재미있었는데…」
				ELSE							;상기 이외
					PRINTFORMW 「아, 언니돌아가세요」
				ENDIF
			ELSE						;어느쪽이나주인님가 아니다
				PRINTFORMW 「언니가 예의 범절을 해? 그렇다면 안심이구나♪」
			ENDIF
		CASE 62, 63	;린/하늘
			IF TALENT:TARGET:주인님	;코이시가주인님
				PRINTFORMW 「%CALLNAME:SHOP_NEWTARGET%, 조금 나가 오기 때문에,%K64_CNAME(MASTER, 2)%의 돌보기라고 두어 주지 않는다?」
			ELSEIF TALENT:SHOP_NEWTARGET:주인님	;린/하늘이주인님
				PRINTFORMW 「네, 벌써 끝나? 모처럼 재미있어졌는데」
			ELSE						;어느쪽이나주인님가 아니다
				PRINTFORMW 「네, %CALLNAME:SHOP_NEWTARGET%하지만 예의 범절을 해? 괜찮은가…」
			ENDIF
		CASEELSE	;상기 이외
			IF TALENT:TARGET:주인님	;코이시가주인님
				IF MARK:TARGET:반항각인 >= 5	;코이시의죠교대상로부터의반항각인Lv5 이상
					PRINTFORMW 
				ELSE							;상기 이외
					PRINTFORMW 
				ENDIF
			ELSEIF TALENT:SHOP_NEWTARGET:주인님	;교대상손가주인님
				IF MARK:TARGET:반항각인 >= 5	;코이시의죠교대상로부터의반항각인Lv5 이상
					PRINTFORMW 「이렇게 귀찮은 아이이라니 (듣)묻지 않았어요」
				ELSE							;상기 이외
					PRINTFORMW 「저것, 이제 된거야?」
				ENDIF
			ELSE						;어느쪽이나주인님가 아니다
				IF MARK:TARGET:반항각인 >= 5	;코이시의죠교대상로부터의반항각인Lv5 이상
					PRINTFORMW 「%K64_CNAME(MASTER, 2)%는 말하는 것 (듣)묻지 않는 아이이니까 큰 일이어요?」
				ELSE							;상기 이외
					PRINTFORMW 「네. 그리고는 잘 부탁해」
				ENDIF
			ENDIF
	ENDSELECT
	RETURN 2
ENDIF

;-------------------------------------------------
;코이시님이죠교자되었다
;-------------------------------------------------
IF 구상기입체크() && ARG == 1
	IF SHOP_OLDTARGET < 0	;교대원이 없다
		;이 케이스가 여전히 일어날 수 있는인가 다시 검증하지 않으면 안 되지만
		SELECTCASE NO:(FLAG:2)	;누구가주인님인가로 분기
			CASE 61		;사토리
				PRINTFORMW 「언니,%K64_CNAME(MASTER, 2)%그리고 놀다 오는군♪」
			CASE 62, 63	;린/하늘
				PRINTFORMW 「%CALLNAME:(FLAG:2)%, 조금%K64_CNAME(MASTER, 2)%빌리는군♪」
			CASE 64		;코이시
				PRINTFORMW 「그런데,%K64_CNAME(MASTER, 2)%(을)를 귀여워해 올까」
			CASEELSE	;상기 이외
				PRINTFORMW 「자내가%K64_CNAME(MASTER, 2)%의 예의 범절 해 오는군」
		ENDSELECT
	ELSE		;교대원이 있는… 보통은 여기
		SELECTCASE NO:SHOP_OLDTARGET	;누가 교대원인가로 분기
			CASE 61		;사토리
				IF TALENT:TARGET:주인님	;코이시가주인님
					PRINTFORMW 「가와 언니♪」
					IF CFLAG:TARGET:죠교레벨 < 3					;코이시의죠교Lv가 3 미만
						PRINTFORMW 「 나도 언니같이 할 수 있을까…」
					ELSEIF MARK:TARGET:반항각인 >= 5 && !FAVOR(500)	;코이시의죠교대상로부터의반항각인Lv5 이상 또한 죠교대상로부터의호의 500 미만
						PRINTFORMW 「어때? 고집이 있었던이겠지?」
						IF MARK:SHOP_OLDTARGET:반항각인 == 0 && FAVOR(100, 0, SHOP_OLDTARGET)	;사토리의죠교대상로부터의반발각인Lv0 또한 죠교대상로부터의호의 100 이상
							PRINTFORMW 「네? 전혀 그렇지 않았다?」
							PRINTFORMW 「그런…무슨 일이야…」
						ENDIF
					ELSEIF !FAVOR(-100)								;코이시의죠교대상로부터의호의가-100 미만
						PRINTFORMW 「이것으로 조금은 따라 줄까」
					ELSEIF CFLAG:TARGET:소모 > 5					;코이시의 피로가 축적하고 있다
						PRINTFORMW 「사실은 좀 더 쉬고 싶었지만…」
					ELSE											;상기 이외
						PRINTFORMW 「나머지는 스스로 해요」
					ENDIF
				ELSEIF TALENT:SHOP_OLDTARGET:주인님	;사토리가주인님
					IF CFLAG:TARGET:죠교레벨 < 3					;코이시의죠교Lv가 3 미만
						PRINTFORMW 「응, 나가 할 수 있을까」
					ELSEIF !FAVOR(-100)								;코이시의죠교대상로부터의호의-100 미만
						PRINTFORMW 「네─, 나는 안된다고 생각해? 어쩐지 미움받고 있고」
