;-------------------------------------------------
;함수명:KOJO_RAND_K39
;개  요점:구상를 랜덤으로 생성한다
;인  수:ARGS… 행위의 분류
;인  수:ARG…1으로 TARGET  2로 MASTER의상태변화
;비  고:식 안의 함수
;기본적으로@FADE, 혹은@PRINTCOLOR로 사용하는 일을 상정. 문자색지정은 그 쪽으로 통째로 맡김
;SQN 쿠즈노하구상에 인스파이어 되어 만들었다. 작자의 mao의 사람에게는 감사를
;-------------------------------------------------
@KOJO_RAND_K39(ARGS, ARG)
#FUNCTIONS
LOCALS = 
FOR LOCAL, 0, ARG
	SIF LOCAL
		LOCALS += ""
IF ARG == 1 ;죠교자
	SELECTCASE ARGS
		CASE "키스", "유두 들이마셔"
			;죠교자의상태변화파생
			SELECTCASE GET_CONDITION(TARGET)
				CASE "정욕", "울굴"
					SELECTCASE RAND:25
						CASE 0
							LOCALS = %LOCALS%%RANDTEXT_K39(",", "! 　")%%WARD_C_K39()%%ELLIPSIS2_K39()%는,%WARD_A_K39()%
						CASE 1
							LOCALS = %LOCALS%%WARD_A_K39()%%ELLIPSIS2_K39()%%WARD_C_K39()%…%RANDTEXT_K39("구", "", "")%, 츄
						CASE 2
							LOCALS = %LOCALS%%RANDTEXT_K39("할짝", "")%, 피츄%ELLIPSIS2_K39()%%RANDTEXT_K39("구", "구", "")%, 츄웃
						CASE 3
							LOCALS = %LOCALS%%RANDTEXT_K39("엣취", "", "구")%, 쥬루%RANDTEXT_K39("", "", "＿")%%ELLIPSIS2_K39()%응, 응쿠
						CASE 4
							LOCALS = %LOCALS%! 푸하%RANDTEXT_K39("", "＿")%, 응%ELLIPSIS2_K39()%응구
						CASE 5
							LOCALS = %LOCALS%%WARD_A_K39()%,%RANDTEXT_K39("엣취", "", "구")%…%WARD_C_K39()%, 츄푸
						CASE 6
							LOCALS = %LOCALS%응구%ELLIPSIS2_K39()%…%WARD_A_K39()%, 응츄우
						CASE 7
							LOCALS = %LOCALS%%RANDTEXT_K39("응", "＿")%응%RANDTEXT_K39("", "")%…… 후%ELLIPSIS2_K39()%%RANDTEXT_K39("후~", "하")%、%WARD_A_K39()%… 응쿠
						CASE 8
							LOCALS = %LOCALS%%RANDTEXT_K39("", "응")%%ELLIPSIS2_K39()%%RANDTEXT_K39("츗", "")%, 츄쿠…,%WARD_A_K39()%
						CASE 9
							LOCALS = %LOCALS%%WARD_A_K39()%, 하아%ELLIPSIS2_K39()%응구, 츄우%RANDTEXT_K39("", "＿")%… 츄쿠
						CASE 10
							LOCALS = %LOCALS%%RANDTEXT_K39("후~, 하아", "하, 하")%%ELLIPSIS2_K39()%, 응응%RANDTEXT_K39("", "＿")%… 응쿠
						CASE 11
							LOCALS = %LOCALS%, 응%RANDTEXT_K39("응", "구")%%ELLIPSIS2_K39()%%RANDTEXT_K39("구", "")%, 피츄
						CASE 12
							LOCALS = %LOCALS%응%ELLIPSIS2_K39()%…,%WARD_A_K39()%, 응응%RANDTEXT_K39("", "＿")%… 응쿠
						CASE 13
							LOCALS = %LOCALS%는%RANDTEXT_K39("", "＿")%, 하아%ELLIPSIS2_K39()%%WARD_A_K39()%,%WARD_C_K39()%! 응
						CASE 14
							LOCALS = %LOCALS%… 하아%ELLIPSIS2_K39()%응구, 응츄%RANDTEXT_K39("", "", "＿")%
						CASE 15
							LOCALS = %LOCALS%%RANDTEXT_K39("엣취", "", "구", "구")%、%WARD_C_K39()%…,%RANDTEXT_K39("츗", "", "")%, 응, 응쿳
						CASE 16
							LOCALS = %LOCALS%츗, 츄%RANDTEXT_K39("", "", "")%%ELLIPSIS2_K39()%,%WARD_A_K39()%, 응후
						CASE 17
							LOCALS = %LOCALS%응, 응쿠%RANDTEXT_K39("", "＿")%…%RANDTEXT_K39("후~, 하아", "하, 하")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "", "")%
						CASE 18
							LOCALS = %LOCALS%%RANDTEXT_K39("", "구", "응")%, 츄%RANDTEXT_K39("구", "", "")%%ELLIPSIS2_K39()%응, 응츄%RANDTEXT_K39("", "", "＿")%… 응쿠, 응쿠
						CASE 19
							LOCALS = %LOCALS%, 읏… 응쿠,%RANDTEXT_K39("응구", "＿")%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%%WARD_C_K39()%, 츄쿠
						CASE 20
							LOCALS = %LOCALS%%WARD_A_K39()%, 응%ELLIPSIS2_K39()%응구, 응츄…, 츄쿠
						CASE 21
							LOCALS = %LOCALS%,%WARD_C_K39()%%ELLIPSIS2_K39()%%RANDTEXT_K39("구", "")%… 피츄
						CASE 22
							LOCALS = %LOCALS%엣취,%RANDTEXT_K39("", "", "구")%%ELLIPSIS2_K39()%…%WARD_A_K39()%, 응쿠
						CASE 23
							LOCALS = %LOCALS%%RANDTEXT_K39(",", "!")% 는,%RANDTEXT_K39("후~", "하")%……%WARD_A_K39()%, 응쿠
						CASE 24
							LOCALS = %LOCALS%%RANDTEXT_K39("＿", "구", "", "")%, 츄쿳…%RANDTEXT_K39("페로", "")%%RANDTEXT_K39("", "")%, 응%ELLIPSIS2_K39()%
					ENDSELECT
				CASEELSE ;그 외
					SELECTCASE RAND:25
						CASE 0
							LOCALS = %LOCALS%응, 응%RANDTEXT_K39("", "", "＿")%%ELLIPSIS2_K39()%응구
						CASE 1
							LOCALS = %LOCALS%%WARD_A_K39()%, 읏%ELLIPSIS2_K39()%%RANDTEXT_K39("", "구", "＿")%
						CASE 2
							LOCALS = %LOCALS%, 응츄%ELLIPSIS2_K39()%응%RANDTEXT_K39("응", "구", "＿")%
						CASE 3
							LOCALS = %LOCALS%%RANDTEXT_K39("응", "구", "＿")%, 응후%ELLIPSIS2_K39()%%RANDTEXT_K39("", "", "＿")%
						CASE 4
							LOCALS = %LOCALS%, 응%RANDTEXT_K39("", "", "＿")%%ELLIPSIS2_K39()%응구
						CASE 5
							LOCALS = %LOCALS%%RANDTEXT_K39("", "구", "")%… 쿠츗%ELLIPSIS2_K39()%
						CASE 6
							LOCALS = %LOCALS%%ELLIPSIS2_K39()%%WARD_A_K39()%… 응읏
						CASE 7
							LOCALS = %LOCALS%%RANDTEXT_K39("", "츄")%、%RANDTEXT_K39("", "구", "")%…… 응
						CASE 8
							LOCALS = %LOCALS%구,%RANDTEXT_K39("구", "", "엣취", "")%… 츄루
						CASE 9
							LOCALS = %LOCALS%%WARD_A_K39()%… 응%RANDTEXT_K39("、", ",", "", "……", "……、")%,%RANDTEXT_K39("", "구", "츄")%
						CASE 10
							LOCALS = %LOCALS%%RANDTEXT_K39("아", "")%…%RANDTEXT_K39("", "구", "")%, 츗
						CASE 11
							LOCALS = %LOCALS%%RANDTEXT_K39("는", "아", "", "응")%%RANDTEXT_K39(",", "……、")%…… 츄
						CASE 12
							LOCALS = %LOCALS%, 읏츄…%RANDTEXT_K39("", "", "")%
						CASE 13
							LOCALS = %LOCALS%, 츄%RANDTEXT_K39(",", "……", "……、")%%RANDTEXT_K39("", "", "＿")%
						CASE 14
							LOCALS = %LOCALS%%WARD_A_K39()%%RANDTEXT_K39("", "＿")%… 날름%RANDTEXT_K39("", "＿")%……%RANDTEXT_K39("응", "구", "＿")%
						CASE 15
							LOCALS = %LOCALS%%RANDTEXT_K39(",", ",", "……")%응%RANDTEXT_K39(",", "、")%응구
						CASE 16
							LOCALS = %LOCALS%응…… 응쿠%ELLIPSIS2_K39()%%RANDTEXT_K39("츄", "", "구", "")%
						CASE 17
							LOCALS = %LOCALS%응%RANDTEXT_K39("", "", "")%…… 후%RANDTEXT_K39("＿", "…", "……、")%응구
						CASE 18
							LOCALS = %LOCALS%%RANDTEXT_K39("츗", "츄", "", "")%、%RANDTEXT_K39("구", "")%%ELLIPSIS2_K39()%
						CASE 19
							LOCALS = %LOCALS%,%RANDTEXT_K39("", "구", "", "츄")%%ELLIPSIS2_K39()%응%RANDTEXT_K39("、", ",", ",")%응구
						CASE 20
							LOCALS = %LOCALS%는,%RANDTEXT_K39("할짝", "", "")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "", "", "")%
						CASE 21
							LOCALS = %LOCALS%%RANDTEXT_K39("응", "응", "응")%%RANDTEXT_K39("……", "…", "…,")%%RANDTEXT_K39("하, 하아", "하, 하")%…… 츄%RANDTEXT_K39("", "", "")%
						CASE 22
							LOCALS = %LOCALS%응구,%RANDTEXT_K39("응…", "응……")%%RANDTEXT_K39("츗,", "구", "")%
						CASE 23
							LOCALS = %LOCALS%구,%RANDTEXT_K39("", "", "", "응")%%RANDTEXT_K39("…", "……", ",", "…,")%
						CASE 24
							LOCALS = %LOCALS%%WARD_A_K39()%,%RANDTEXT_K39("응구", "", "")%%RANDTEXT_K39("…", "……", ",", "…,")%응
					ENDSELECT
			ENDSELECT
			IF AFFECTION("상사상애")
				LOCALS = %LOCALS%%ELLIPSIS_K39()%%RANDTEXT_K39("!", "", "＿", RAND_SYMBOL_K39())%
			ELSE
				LOCALS = %LOCALS%%ELLIPSIS_K39()%%RANDTEXT_K39("＿", "", EIGHTHNOTE_K39(), "♪")%
			ENDIF
		CASE "쿤닐링구스", "입으로 애무"
			;죠교자의상태변화파생
			SELECTCASE GET_CONDITION(TARGET)
				CASE "정욕", "울굴"
					SELECTCASE RAND:25
						CASE 0
							LOCALS = %LOCALS%%WARD_C_K39()%%RANDTEXT_K39("", "＿")%, 츄%ELLIPSIS2_K39()%는… 응, 엣취
						CASE 1
							LOCALS = %LOCALS%… 응%RANDTEXT_K39("", "", "＿")%, 누푹, 누프%ELLIPSIS2_K39()%
						CASE 2
							LOCALS = %LOCALS%츄프,%WARD_C_K39()%구%ELLIPSIS2_K39()%,%WARD_C_K39()%구
						CASE 3
							LOCALS = %LOCALS%%RANDTEXT_K39("후~, 하아", "하, 하")%%ELLIPSIS2_K39()%%WARD_C_K39()%%RANDTEXT_K39("", "＿")%, 츄
						CASE 4
							LOCALS = %LOCALS%%WARD_A_K39()%,%WARD_C_K39()%구%ELLIPSIS2_K39()%응,%RANDTEXT_K39("쿠츗", "")%,
						CASE 5
							LOCALS = %LOCALS%%WARD_C_K39()%,%WARD_C_K39()%구…… 는, 아%ELLIPSIS2_K39()%%WARD_A_K39()%
						CASE 6
							LOCALS = %LOCALS%날름날름…,%WARD_C_K39()%%RANDTEXT_K39("", "＿")%, 츄쿠%ELLIPSIS2_K39()%
						CASE 7
							LOCALS = %LOCALS%%RANDTEXT_K39("글자", "")%%RANDTEXT_K39("", "", "＿")%,%RANDTEXT_K39("", "", "글자")%구%ELLIPSIS2_K39()%응, 엣취
						CASE 8
							LOCALS = %LOCALS%%WARD_C_K39()%%RANDTEXT_K39("", "＿")%…%RANDTEXT_K39("", "")%, 츄%RANDTEXT_K39("", "", "＿")%%ELLIPSIS2_K39()%는, 아…%WARD_A_K39()%
						CASE 9
							LOCALS = %LOCALS%%WARD_C_K39()%%WARD_B_K39()%、%WARD_C_K39()%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%, 츄
						CASE 10
							LOCALS = %LOCALS%미끌, 츄%WARD_B_K39()%%ELLIPSIS2_K39()%, 누프
						CASE 11
							LOCALS = %LOCALS%%RANDTEXT_K39("", "")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "할짝")%, 엣취, 엣취
						CASE 12
							LOCALS = %LOCALS%%RANDTEXT_K39("", "할짝")%, 응%ELLIPSIS2_K39()%구…… 츄%WARD_B_K39()%
						CASE 13
							LOCALS = %LOCALS%%RANDTEXT_K39("", "＿")%, 누프…, 츄%RANDTEXT_K39("", "", "＿")%%ELLIPSIS2_K39()%구
						CASE 14
							LOCALS = %LOCALS%%RANDTEXT_K39("", "할짝")%,%RANDTEXT_K39("", "")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "", "＿")%, 엣취
						CASE 15
							LOCALS = %LOCALS%%RANDTEXT_K39("레로", "")%%RANDTEXT_K39("", "＿")%, 츄%RANDTEXT_K39("", "＿")%,%ELLIPSIS2_K39()%응, 쥬우
						CASE 16
							LOCALS = %LOCALS%%RANDTEXT_K39("할짝", "")%, 엣취%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%%WARD_B_K39()%, 츄우
						CASE 17
							LOCALS = %LOCALS%%RANDTEXT_K39("", "＿")%…,%RANDTEXT_K39("후~, 하아", "하, 하")%%ELLIPSIS2_K39()%, 엣취
						CASE 18
							LOCALS = %LOCALS%%RANDTEXT_K39("", "＿")%, 개%ELLIPSIS2_K39()%응,%WARD_C_K39()%구…
						CASE 19
							LOCALS = %LOCALS%%WARD_A_K39()%, 츄%ELLIPSIS2_K39()%엣취,%WARD_C_K39()%%RANDTEXT_K39("", "＿")%
						CASE 20
							LOCALS = %LOCALS%는…, 아%ELLIPSIS2_K39()%,%RANDTEXT_K39("", "글자")%… 엣취
						CASE 21
							LOCALS = %LOCALS%%RANDTEXT_K39("", "")%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "＿")%…, 엣취, 엣취
						CASE 22
							LOCALS = %LOCALS%%RANDTEXT_K39("", "", "＿")%,%ELLIPSIS2_K39()%%RANDTEXT_K39("쿠츗", "")%,
						CASE 23
							LOCALS = %LOCALS%%RANDTEXT_K39("", "글자")%%RANDTEXT_K39("、", ",", "……")%%RANDTEXT_K39("", "", "＿", "…")%, 쥬루루
						CASE 24
							LOCALS = %LOCALS%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "글자")%, 츄%WARD_B_K39()%…%RANDTEXT_K39("", "구츄")%
					ENDSELECT
				CASEELSE ;그 외
					SELECTCASE RAND:25
						CASE 0
							LOCALS = %LOCALS%는%ELLIPSIS2_K39()%%RANDTEXT_K39("엣취", "")%, 응%RANDTEXT_K39("", "", "＿")%…%RANDTEXT_K39("글자", "", "")%
						CASE 1
							LOCALS = %LOCALS%%RANDTEXT_K39("엣취", "")%, 츄%WARD_B_K39()%%ELLIPSIS2_K39()%페로
						CASE 2
							LOCALS = %LOCALS%%ELLIPSIS2_K39()%%WARD_B_K39()%,
						CASE 3
							LOCALS = %LOCALS%,%ELLIPSIS2_K39()%구, 츄%WARD_B_K39()%
						CASE 4
							LOCALS = %LOCALS%%RANDTEXT_K39("피츄", "")%, 츄%WARD_B_K39()%%ELLIPSIS2_K39()%응, 츄
						CASE 5
							LOCALS = %LOCALS%응%ELLIPSIS2_K39()%구, 츄%WARD_B_K39()%
						CASE 6
							LOCALS = %LOCALS%%RANDTEXT_K39("구", "")%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%는, 아…
						CASE 7
							LOCALS = %LOCALS%%RANDTEXT_K39("", "")%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "＿")%
						CASE 8
							LOCALS = %LOCALS%%RANDTEXT_K39("", "할짝")%… 후%ELLIPSIS2_K39()%%RANDTEXT_K39("", "구")%
						CASE 9
							LOCALS = %LOCALS%응%RANDTEXT_K39("", "", "＿")%%ELLIPSIS2_K39()%구, 츄푸
						CASE 10
							LOCALS = %LOCALS%%ELLIPSIS2_K39()%페로,%RANDTEXT_K39("할짝", "")%
						CASE 11
							LOCALS = %LOCALS%%RANDTEXT_K39("", "", "")%%ELLIPSIS2_K39()%
						CASE 12
							LOCALS = %LOCALS%%ELLIPSIS2_K39()%%RANDTEXT_K39("구", "", "")%
						CASE 13
							LOCALS = %LOCALS%%RANDTEXT_K39("글자", "")%%RANDTEXT_K39("", "", "＿")%, 츄루%ELLIPSIS2_K39()%구
						CASE 14
							LOCALS = %LOCALS%%RANDTEXT_K39("구", "")%%RANDTEXT_K39("", "＿")%, 미끌%ELLIPSIS2_K39()%
						CASE 15
							LOCALS = %LOCALS%%RANDTEXT_K39("", "", "", "＿")%, 피챳%ELLIPSIS2_K39()%%RANDTEXT_K39("", "페로")%
						CASE 16
							LOCALS = %LOCALS%%RANDTEXT_K39("", "＿")%,%ELLIPSIS2_K39()%%RANDTEXT_K39("레로", "")%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%
						CASE 17
							LOCALS = %LOCALS%%RANDTEXT_K39("페로", "")%, 츗%ELLIPSIS2_K39()%구
						CASE 18
							LOCALS = %LOCALS%%ELLIPSIS2_K39()%%RANDTEXT_K39("할짝", "")%、%RANDTEXT_K39("", "")%
						CASE 19
							LOCALS = %LOCALS%%RANDTEXT_K39("개", "")%%RANDTEXT_K39("", "＿")%%ELLIPSIS2_K39()%%RANDTEXT_K39("", "글자")%, 츄쿠
						CASE 20
