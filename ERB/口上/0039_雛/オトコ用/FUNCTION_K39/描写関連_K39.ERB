;==================================================
;
;	묘사 함수 두는 곳
;		각 표현은 개인적 취미
;
;==================================================
;본체 측에이했다의 것을 삭제
;-------------------------------------------------

;-------------------------------------------------
;함수명:TITTIES_DESCRIPTION_K39
;개  요점:미유인 것, 그것이 모두
;인  수:ARG… 캐릭터등록번호. 생략 하면 TARGET
;반환값:캐릭터 라인
;비  고:식 안의 함수
;사용예:%TITTIES_DESCRIPTION_K39(TARGET)%
;-------------------------------------------------
@TITTIES_DESCRIPTION_K39(ARG = -2)
#FUNCTIONS
#LOCALSSIZE 1
LOCALS:0 = 
ARG = ARG != -2 ? ARG # TARGET
;거유시
IF TALENT:ARG:거유
	STRDATA LOCALS:0
;OriginalString : 形の整った、たわわな
		DATA 형태가 갖추어진, 휘어있는
;OriginalString : 張りのある、豊かな
		DATA 붙은 있는, 풍부한
;OriginalString : 
	ENDDATA
;빈유시
ELSEIF TALENT:ARG:빈유
	STRDATA LOCALS:0
;OriginalString : 控えめながらも形の整った
		DATA 소극적이면서도 형태가 갖추어졌다
;OriginalString : ささやかながらも気品のある
		DATA 자그마하면서도 기품이 있다
;OriginalString : 
	ENDDATA
;그 외
ELSE
	STRDATA LOCALS:0
;OriginalString : 見事に均整のとれた美しい
		DATA 보기좋게 균정을 취할 수 있던 아름답다
;OriginalString : たまらない感触の美しい
		DATA 견딜 수 없는 감촉의 아름답다
;OriginalString : 
	ENDDATA
ENDIF
RETURNF LOCALS:0

;-------------------------------------------------
;함수명:MOIST
;개  요점:ARG의윤활상태에 해당하면 진을반환
;인  수:캐릭터번호. 생략 하면 MASTER
;비  고:식 안의 함수
;현재의 바리안트의 사양상, 기능하는 것은 MASTER의 경우에 대해서만
;-------------------------------------------------
@MOIST_K39(ARG = -2)
#FUNCTIONS
#LOCALSSIZE 1
LOCALS:0 = 
ARG = ARG != -2 ? ARG # MASTER
SELECTCASE PALAM:ARG:윤활
	CASE IS < 250
		LOCALS:0 = "낮다"
	CASE IS < 500
		LOCALS:0 = "일반"
	CASE IS > 499
		LOCALS:0 = "높다"
ENDSELECT
RETURNF LOCALS:0

;-------------------------------------------------
;MASTER용과 결론짓는 일로 한다
;-------------------------------------------------
@PENIS_DESCRIPTION2_K39(ARG)
#FUNCTIONS
#LOCALSSIZE 1
LOCALS:0 = 
SELECTCASE TCVAR:MASTER:발기도
	CASE IS > 1500
		LOCALS:0 = %RAND_SPLIT("한계까지/터질듯이/위험할 정도로")%%RAND_SPLIT("우뚝 솟았다/격분했다/흘립했다")%
	CASE IS > 1000
		LOCALS:0 = %RAND_SPLIT("우뚝 솟았다/격분했다/흘립했다")%
	CASEELSE
		LOCALS:0 = %RAND_SPLIT("아직도 부드럽다/딱딱함을 증하면서 있다/뭉실뭉실 머리를 쳐들고 있다")%
ENDSELECT
RETURNF LOCALS:0

