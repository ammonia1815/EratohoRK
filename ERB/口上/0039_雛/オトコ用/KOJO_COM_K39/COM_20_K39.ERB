			SELECTCASE RANDCOM(3, 10)
				CASE 0
					{
					CALL HTMLPRINTW(@"「읏후아%AHEART% 아%VOICEDDOT%%AHEART% 안됏%AHEART% " + 
					@"도, 이제(벌써)%AHEART% 가 버린닷! 가 버린다앗…%EHEART%」")
					}
