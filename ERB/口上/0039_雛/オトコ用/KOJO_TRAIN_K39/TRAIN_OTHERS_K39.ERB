;────────────────────────────────────
;EV21, 大만족구상
;────────────────────────────────────
;・죠교자の대만족보너스を취득했다場合に表示
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_21(ARG)
;-------------------------------------------------
#LOCALSIZE 1
VARSET LOCAL
LOCAL:0 = GET_KFLAG_K39("상사상애")

;━━━━━━━━━━━━━━━━━━━━━━━━
;히나さま大만족
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100)
	;=================================================
	;상냥하게 응답한다
	;=================================================
	IF IS_NOWCOMNAME("상냥하게 응답한다")
;OriginalString : 「\@ LOCAL:0 ? うふっ,何かご褒美を考えなくちゃね # 愛想がいいのが一番ですね \@
		PRINTFORM 「\@ LOCAL:0 ? 후후, 뭔가 포상을 줘야겠네 # 애교가 있는 게 제일이네요 \@
		CALL PRINT_SYMBOL_K39
;OriginalString : 히나は慈愛に満ちた表情で%CALLNAME:MASTER%を見つめている…
		PRINTFORMDW 히나는 자애로운 표정으로 %CALLNAME:MASTER%을 바라보고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을 받아들인다
	;=================================================
	IF IS_NOWCOMNAME("쾌감을 받아들인다")
;OriginalString : 「\@ LOCAL:0 ? うふっ,可愛い # 솔직な子は好きですよ \@
		PRINTFORM 「\@ LOCAL:0 ? 우후후, 귀여워라 # 솔직한 아이는 좋아해요 \@
		CALL PRINT_SYMBOL_K39
;OriginalString : 히나は微笑みながら%CALLNAME:MASTER%を見つめている…
		PRINTFORMDW 히나는 미소를 지으며 %CALLNAME:MASTER%을 바라보고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;비명지른다
	;=================================================
	IF IS_NOWCOMNAME("비명지른다")
;OriginalString : 「\@ LOCAL:0 ? ねぇ,もっと。もっと聞かせて # そんな甘い声で鳴かれると私…,ちょっと興奮してしまいます \@……」
		PRINTFORMW 「\@ LOCAL:0 ? 저기, 좀 더, 좀 더 들려줘 # 그렇게 달콤한 목소리를 내시면… 저, 조금 흥분해 버려요 \@……」
		RETURN 1
	ENDIF
	
	;=================================================
	;애정을 담아 한다
	;=================================================
	IF IS_NOWCOMNAME("애정을 담아 한다")
;OriginalString : 「\@ LOCAL:0 ? そんなに優しくされると蕩けちゃう # そんなに優しく당했다ら,蕩けてしまいます \@…」
		PRINTFORMW 「\@ LOCAL:0 ? 그렇게 상냥하게 하면 녹아 버려 # 그렇게 상냥하게 하시면, 녹아 버려요 \@…」
;OriginalString : 히나は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
		PRINTFORMDW 히나는 젖은 눈동자로 %CALLNAME:MASTER%을 바라보고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;적극적으로 한다(적극적으로 한다,이루는대로等)
	;=================================================
	IF 구상記入チェック(100) && IS_COMGRONAME("적극적으로 한다")
		;-------------------------------------------------
		;ED경과
		;-------------------------------------------------
		IF GET_KFLAG_K39("ED경과", TARGET)
;OriginalString : 「好き,大好き…」
			PRINTFORMW 「좋아해, 정말 좋아해…」
;OriginalString : 히나は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
			PRINTFORMDW 히나는 젖은 눈동자로 %CALLNAME:MASTER%을 바라보고 있다…
			RETURN 1
		ENDIF
		
		;---------------------
		;その他
		;---------------------
		;상사상애
		IF GET_KFLAG_K39("상사상애")
;OriginalString : 「こんな事,駄目なのは分かってる。でも…」
			PRINTFORMW 「이런 일, 안되는 건 알고 있어. 그래도…」
;OriginalString : 「\@ RAND:2 ? もう, # %CNAME(MASTER, TARGET)%と \@離れたくない……」
			PRINTFORMW 「\@ RAND:2 ? 이제, # %CNAME(MASTER, TARGET)%과 \@ 떨어지고 싶지 않아……」
;OriginalString : 히나は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
			PRINTFORMDW 히나는 젖은 눈동자로 %CALLNAME:MASTER%을 바라보고 있다…
		;その他
		ELSE
;OriginalString : 「적극적으로求めてくれると,私も嬉しいです…」
			PRINTFORMW 「적극적으로 요구해 주시면, 저도 기뻐요…」
;OriginalString : 히나は微笑みながら%CALLNAME:MASTER%を見つめている…
			PRINTFORMDW 히나는 미소를 지으며 %CALLNAME:MASTER%을 바라보고 있다…
		ENDIF 
		RETURN 2
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	CALL HTMLPRINTW(@"「\@ LOCAL:0 ? 쿡쿡, 좋은 느낌이네 # 후후, 나쁘지 않네요 \@…%HTML_HEARTB(, COLOR2_K39("PINK"))%」")
	RETURN 1
ENDIF

;────────────────────────────────────
;EV22, 상태변화구상
;────────────────────────────────────
;・상태이상が付いたときに表示
;・TCVAR:상태변화が죠교자,TCVAR:MASTER:상태변화が죠교대상
;・상태변화が起こると3ターン지속する
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_22(ARG)
;-------------------------------------------------
;・近い将来,죠교자と죠교대상で상태변화구상呼び出しを切り分けたい意向
;・そして상태변화以外の발기描写,윤활描写,소변描写あたりにも구상呼び出しの枠を追加했다い考え(2015/01/09)
;-------------------------------------------------
;*************************************************
;EV22_0, 죠교자상태변화
;*************************************************
IF 구상記入チェック(100) ;&& ARG == 0
	;죠교자の상태변화で派生
	SELECTCASE GET_CONDITION(TARGET)
		CASE "피폐"
			;상태변화지속ターンを2に
			TCVAR:상태변화지속 = 2
			;2/3의확률로표시無し RESULTに0을반환
			IF RAND:3
				RETURN 0
			;1/3의확률로표시
			ELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? ちょっと%RAND_SPLIT("やりすぎかしら/疲れたわね")% # ちょっと疲れま했다ね \@…」
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 조금 %RAND_SPLIT("지나쳤나/지쳤어")% # 조금 지쳤어요 \@…」
;OriginalString : 히나は肩で大きく息をしている…
				PRINTFORMDW 히나는 숨을 크게 몰아쉬고 있다…
				RETURN 1
			ENDIF
		CASE "쇠약"
			RETURN 0
		CASE "무기력"
			RETURN 0
		CASE "몽롱"
			RETURN 0
		CASE "정욕"
			IF RAND:3
				RETURN 0
			ELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? も,もう駄目……。참다,できないわ…… # ご免なさい,私,もう참다できませんっ! \@」
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 이, 이젠 안 돼……더는, 못 참겠어…… # 죄송해요, 저, 더는 참을 수 없어요! \@」
;OriginalString : 히나は정욕に濁った目で%CALLNAME:MASTER%を見据えながら近寄ってきた…
				PRINTFORMDW 히나는 정욕에 흐려진 눈으로 %CALLNAME:MASTER%을 바라보며 다가왔다…
				RETURN 1
			ENDIF
		CASE "분노"
			IF AFFECTION("상사상애")
				TCVAR:상태변화지속 = 1
			ELSEIF AFFECTION("익애")
				IF RAND:3
					TCVAR:상태변화지속 = 2
				ELSE
					TCVAR:상태변화지속 = 1
				ENDIF
			ELSEIF AFFECTION("권애")
				IF RAND:3
					TCVAR:상태변화지속 = 3
				ELSE
					TCVAR:상태변화지속 = 2
				ENDIF
			ELSE
				TCVAR:상태변화지속 = 3
			ENDIF
			IF RAND:3
				RETURN 0
			ELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? ねぇ,本気で怒ってもいいの # 私を怒らせて楽しいですか \@?」
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 저기, 진심으로 화내도 괜찮지 # 저를 화나게 해서 즐거우신가요 \@?」
				RETURN 1
			ENDIF
		CASE "싫증"
			;죠교の경과시간
			SELECTCASE TFLAG:경과시간
				;長い
				CASE IS >= 60
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? ねぇ,もう終わりにする # そろそろ止めましょうか \@…?」
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 있지, 이제 끝낼까 # 슬슬 그만둘까요 \@…?」
					TCVAR:상태변화지속 = 3
				;普通
				CASE IS >= 30
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? なんかいまひとつな感じ # どうもいまひとつですね \@…」
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 뭔가 부족한 느낌 # 어쩐지 뭔가 모자란 느낌이네요 \@…」
					TCVAR:상태변화지속 = 2
				;短い
				CASEELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? なんか気分が乗らない感じ # どうも気分が乗りませんね \@…」
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 왠지 기분이 나질 않네  # 이상하게 기분이 나질 않네요 \@…」
					TCVAR:상태변화지속 = 1
			ENDSELECT
			RETURN 1
		CASE "울굴"
			;//TARGETの場合,TALENT:광기がある場合にのみ1/5の確率で울굴化
			;//광기持ちで無い場合は死に구상になる
			RETURN 0
	ENDSELECT
ENDIF

