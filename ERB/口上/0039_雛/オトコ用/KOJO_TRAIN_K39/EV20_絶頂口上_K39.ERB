						CASE 1
							CALL HTMLPRINTW(@"「んっくっ、私、自分で慰めてっ! あっ%LOCALS:2% あぁっ! やっ、あぁぁっっ!」")
					ENDSELECT
					SELECTCASE  RANDEVENT(2, 284)
						CASE 0
							CALL HTMLPRINTL(@"「イっ…イクぅぅっっ%LOCALS:2% イっちゃうぅぅっっ!!」")
						CASE 1
							CALL HTMLPRINTL(@"「ひぅっ! あッ……イくっ%LOCALS:2% イくうぅっ…ッ…!!」")
					ENDSELECT
				ENDIF
				;改行、QUAKE、WAIT
				CALL QUAKE(4)
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;입による애무で절정(입で가슴애무를 강요等)
	;=================================================
	IF (IS_NOWACTNAME("가슴애무강제") && TFLAG:ACT파생 == 2) || TB_CHECK_K39("입")
		SELECTCASE RANDEVENT(2, 285)
			CASE 0
				CALL TB_EX_K39
			CASE 1
				;상사상애
				IF GET_KFLAG_K39("상사상애")
					SELECTCASE RANDEVENT(2, 286)
						CASE 0
							CALL HTMLPRINTW(@"「ひぁっ%LOCALS:2% だめっ、そんなに유두핥기ちゃダメぇぇっっ!!」")
						CASE 1
							CALL HTMLPRINTW(@"「%RAND_SPLIT("あぁ/んっ")%っ! んくっ…… だめぇっ%LOCALS:2% もうっ、もうぅっ!」")
					ENDSELECT
					SELECTCASE RANDEVENT(2, 287)
						CASE 0
							CALL HTMLPRINTL(@"「イっちゃうっ%LOCALS:2% イっちゃうぅっ!! あぁぁっ…!」")
						CASE 1
							CALL HTMLPRINTL(@"「ふぁぁっ%LOCALS:2% だめっ! イっ%LOCALS:2% くぅぅっっ!」")
					ENDSELECT
				;その他
				ELSE
					SELECTCASE RANDEVENT(2, 288)
						CASE 0
							CALL HTMLPRINTW(@"「んあぁっ%LOCALS:2% ダメっ%LOCALS:2% そんなに乳首를 핥아られたら…ッ…!」")
