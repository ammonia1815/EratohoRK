					ENDSELECT
					SELECTCASE  RANDEVENT(2, 289)
						CASE 0
							CALL HTMLPRINTL(@"「イきますっ! イっちゃいますっ…ッ…%LOCALS:2% ああぁぁぁっ!!」")
						CASE 1
							CALL HTMLPRINTL(@"「イっ…イクぅぅっっ%LOCALS:2% イっちゃうぅぅっっ!!」")
					ENDSELECT
				ENDIF
				CALL QUAKE(4)
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;페니스による애무で절정(파이즈리など)
	;=================================================
	IF (IS_NOWACTNAME("파이즈리한다") || TB_CHECK_K39("페니스"))
		SELECTCASE RANDEVENT(2, 290)
			CASE 0
				CALL TB_EX_K39
			CASE 1
				;상사상애
				IF GET_KFLAG_K39("상사상애")
					SELECTCASE RANDEVENT(2, 291)
						CASE 0
							CALL HTMLPRINTW(@"「ひぁっ%LOCALS:2% だめっ%LOCALS:2% そこばっかり擦り付けたらダメぇぇっっ!!」")
						CASE 1
							CALL HTMLPRINTW(@"「やぁぁっっ%LOCALS:2% そんなに擦ったらっ! イっちゃう%LOCALS:2% イっちゃうよぉっっ!!」")
					ENDSELECT
					SELECTCASE RANDEVENT(2, 292)
						CASE 0
							CALL HTMLPRINTL(@"「ふぁぁっ%LOCALS:2% だめっ%LOCALS:2% イっ、くぅぅっっ!」")
						CASE 1
							CALL HTMLPRINTL(@"「あっ、あぁっ%LOCALS:2% イクぅぅっっ%LOCALS:2% イっちゃうぅぅっっ!!」")
					ENDSELECT
				;その他
				ELSE
					SELECTCASE RANDEVENT(2, 293)
						CASE 0
							CALL HTMLPRINTW(@"「いやぁっ! だめっ%LOCALS:2% 駄目ですっ…! そこばっかり、擦らないでぇっ!」")
						CASE 1
							CALL HTMLPRINTW(@"「あぅっ、あぁっ%LOCALS:2% そんなに擦られたらッ! もうっ%LOCALS:2% もうぅっ、私…ッ…!」")
					ENDSELECT
					SELECTCASE  RANDEVENT(2, 294)
						CASE 0
							CALL HTMLPRINTL(@"「ひぅっ%LOCALS:2% あッ……イくっ、イくうぅっ…ッ…!!」")
						CASE 1
							CALL HTMLPRINTL(@"「イきますっ! イっちゃいますっ…ッ…%LOCALS:2% ああぁぁぁっ!!」")
					ENDSELECT
				ENDIF
				CALL QUAKE(4)
				WAIT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-----------------------------------
	;その他汎用処理
