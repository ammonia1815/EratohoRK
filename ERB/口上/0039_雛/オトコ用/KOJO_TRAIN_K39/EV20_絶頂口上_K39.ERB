							CALL HTMLPRINTL(@"「ひぅっ%LOCALS:2% あんっ%LOCALS:2% あッ……いっ、イクっ…!」")
					ENDSELECT
				;その他
				ELSE
					SELECTCASE RANDEVENT(2, 283)
						CASE 0
