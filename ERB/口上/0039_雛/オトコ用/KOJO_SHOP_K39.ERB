IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		;半裸は嫌
		CASE 13
			IF 상사상애
;OriginalString : 「そんなの嫌…」
				PRINTFORMW 「그런 건 싫어…」
			ELSE
;OriginalString : 「嫌です…」
				PRINTFORMW 「싫어요…」
			ENDIF
			RETURN 2
		;普段から전라なんて嫌
		CASE 23
			IF 상사상애
;OriginalString : 「嫌よ、そんな馬鹿みたいな格好…」
				PRINTFORMW 「싫어, 그런 바보같은 모습은…」
			ELSE
;OriginalString : 「私を辱めたいんですか?」
				PRINTFORMW 「제게 수치를 주실 생각인가요?」
			ENDIF
			RETURN 2
		[SKIPSTART]
		;すーすーするのは嫌
		CASE 33
			IF 상사상애
;OriginalString : 「駄目!　そんなの絶対にだめっ!」
				PRINTFORMW 「타목! 그런 것 절대로 안돼!」
			ELSE
;OriginalString : 「そんなの駄目です…」
				PRINTFORMW 「그런 것 안됩니다…」
			ENDIF
			RETURN 2
		[SKIPEND]
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

