		ELSE
;OriginalString : 
			PRINTFORMW
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM15, 쾌감을 받아들인다
	;기입 체크
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("쾌감을 받아들인다")
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM16, 기분 좋다!
	;기입 체크
