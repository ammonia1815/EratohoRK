	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:MASTER:사정 == 5
		IF KFLAG:152 == 0
			IF KFLAG:153 == 0
				PRINTFORMW 「꺄……」
				CALL QUAKE(2)
				PRINTFORML 
