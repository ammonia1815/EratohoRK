		;통상
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 
				;조루
				CASE 4
					PRINTFORMW 
				;그 외
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL
		ENDIF
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;동시절정(죠교자가 동시에Ａ절정)
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:Ａ절정
		SELECTCASE NOWEX:MASTER:사정
			;드라이사정
			CASE 1
				PRINTFORMW 
				CALL QUAKE(4)
				PRINTFORMW 
			;그 외
			CASEELSE
				PRINTFORMW 
				CALL QUAKE(4)
				PRINTFORMW 
		ENDSELECT
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;애태움로부터
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:MASTER:애태움도
		;애태움중
		IF TCVAR:MASTER:애태움도 > 1
			SELECTCASE NOWEX:MASTER:사정
				;Ａ책사정
				CASE 5
					PRINTFORMW 
				;그 외(조루)
				CASEELSE
					PRINTFORMW 
			ENDSELECT
		;결정타
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 
				;그 외
				CASEELSE
					PRINTFORMW 
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;사정의파생
	SELECTCASE NOWEX:MASTER:사정
		;드라이사정
		CASE 1
			PRINTFORMW 
		;대량사정
		CASE 3
			PRINTFORMW 
			RETURN LOCAL
		;조루
		CASE 4
			PRINTFORMW 
		;Ａ책사정
		CASE 5
			PRINTFORMW 
		;그 외(사정)
		CASEELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL
	ENDSELECT
	RETURN 2
ENDIF

