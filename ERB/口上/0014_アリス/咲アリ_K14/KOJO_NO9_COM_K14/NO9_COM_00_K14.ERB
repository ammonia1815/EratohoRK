;=====================================================================
;회화계 반응 COM0 접수대
;=====================================================================
;·주로 ACT분류:회화계/휴식계 시에 발생한다
;·발생은 트리거이벤트제. 특정의 ACT가 트리거가 된다
;·ACT분류:회화에 대해서는 REACT로 완결시킨다의 것이 바람직함
;-------------------------------------------------

;────────────────────────────────────
;NO9_COM0, 침묵한다
;────────────────────────────────────
;·따랐던 것에는 안 된다
;·ACT1, 지금 기분을 묻다/자신에게의 마음을 묻다의 경우는 REACT파생 1, "질문에 답하지 않는다"되어징계·포인트가산
;·REACT인상:나쁜인상
;·의존도변화(-1, -1)
;-------------------------------------------------
;·ACT가지금 기분을 묻다/자신에게의 마음을 묻다의 경우는 파생 1, 질문에 답하지 않는 취급
;·그 외의 경우는 파생 0, 단순한 침묵
;-------------------------------------------------
;트리거하는행동나 상황…
;ACT분류 0:회화
;	ACT:지금 기분을 묻다/자신에게의 마음을 묻다/성에 관한 이야기를 하다/상냥하게 위로한다
;		엄하게 위협한다/징계한다고 위협한다/고함치다/기분나쁘게 웃다
;ACT분류 2:도구
;	ACT:
;ACT분류 4:수치
;	ACT:수치플레이/야외플레이
;ACT분류 6:가학
;	ACT:밧줄/눈가리개/볼개그/매도/삼각목마
;ACT분류 7:이상
;	ACT:방치플레이/페니스밴드장착
;ACT분류 9:휴식
;	ACT:휴식시킨다/영양제를 먹인다/치료한다
;-------------------------------------------------
@KOJO_NO9_MS_COM_K14_0
;-------------------------------------------------
;·TFLAG:REACT파생(0=단순한 침묵/1=질문에 답하지 않는다)
;-------------------------------------------------

;REACT파생 마다 나누어 본다
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM0_0, 파생:단순한 침묵
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	;MENUMATCH의 범위내에서 REACT부하까지 보는 것은 힘들기 때문에, 분리해 상층에서 처리한다
	;라고 할까, 나중에 추가당했다요소를 기존 부분에 끼어들게 하는데 어쩔 수 없이라고 한 (분)편이 보다 올바르다
	;REACT부하에 관해서는, 본체의 갱신 선취해 실장했다의 것으로, 나중에 추가당했다REACTION_MESSAGE와 개소에 따라서는
	;어긋나는 점이 있을지도 모른다
	;=================================================
	;무리를했다행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를했다행동"
		;실은 그렇지만도 않다
		IF COMCOR_POSI()
			PRINTFORMW 「뭐야, 말을 했으면 끝까지 말해!」
			PRINTFORMW 「계속 신경 쓰이잖아…」
		;실은 싫어
		ELSE
			PRINTFORMW 「무리하게 침묵할 이유는 없다고 생각해?」
			PRINTFORMW 「말하고 싶은게 있으면, 그대로 말하는게 좋다고 생각한다고?」
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가가학
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "가학")
		SELECTCASE RANDCOM(3, 0)
			CASE 0
				PRINTFORMW 「이 정도로 입 다물다니 너 답지 않다고?」
			CASE 1
				PRINTFORMW 「뭐야, 침묵해 버려서는... 뭐라고 말 좀 해봐」
			CASE 2
				PRINTFORML 「언제부터 이 정도에 굴복하게 됐어?」
				PRINTFORMW 「……시시하네. 봐봐, 뭐라고 말 좀 해봐」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;REACT방치플레이로 처리해야 할 일단 테두리만 남긴다
	;================================================
	;ACT분류가이상
	;================================================
	;IF 구상기입 체크(0) && MENUMATCH(TFLAG:ACT, "이상")
	;ENDIF
	
	;================================================
	;ACT분류가휴식
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "휴식")
		SELECTCASE RANDCOM(2, 1)
			CASE 0
				PRINTFORML 「왜 침묵하고 있는지 모르겠네…」
				PRINTFORMW 「시시하네. 봐봐, 어떻게든 말하세요」
			CASE 1
				PRINTFORML 「입도 들을 수 없을 수록 지치고 있다고 일은 없다고 생각하는데?」
				PRINTFORMW 「그렇지 않으면 말하고 싶지 않은 이유라도 있을까」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;샌 만큼은 여기서 처리
	;-------------------------
	SELECTCASE RANDCOM(2, 10)
		CASE 0
			PRINTFORMW 「입다물지 말고 뭔가 말하면 어떻게야…?」
		CASE 1
			PRINTFORML 「그렇게……, 말하고 싶지 않으면 그것도 좋아요」
			PRINTFORMW 「마음대로 해 주세요」
	ENDSELECT
	RETURN 1
	;RESULT가 돌아가지 않는 경우, 시스템은 통괄 범용 리액션(@KOJO_REACT_KX)을 부르러 간다
	;범용 리액션의 테두리를 사용하지 않고, KOJO_REACT_KX내에서 SELECTCASE SELECTCOM로 똥 긴 네스트를 만드는 일도 가능한 것은 않거나
	;혹은 IS_COMGRONAME등으로 REACT분류로 정리해 처리해, 마음껏손를 뽑는 일도 또 가능
	;조건은구상측에서 임의로 설정할 수 있으므로, 뭐 뭐든지 있음이지만, 통상는 우선 사용하는필요는 없을 것
	;다만, 장래적으로는 KOJO_REACT_KX의 테두리를 이동시킨다든지, 다른 범용 처리를 REACT 이후의 처리 상층에 가지고 가고 싶은 감은 있다
	;보정치 처리나 ACT분류/RAECT 분류로 모아 처리하는데는, 그 쪽이 보다 형편이 좋기 때문에
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM0_1 파생:질문에 답하지 않는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 1
	;소비기력에 의한 파생
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운행동", "무리가 없는행동"
			;2/3
			IF RAND:3
				PRINTFORMW 「그래…, 대답하고 싶지 않구나?」
				PRINTFORMW 「하지만 당신을 위해서는 어쩔 수 없어, 그런 거야…」
			;1/3
			ELSE
				PRINTFORMW 「 나의 질문에 대답하지 않는다라…, 좋은 담력이네」
				PRINTFORMW 「 후회하지 않았으면, 하는군요…」
			ENDIF
		CASE "무리를했다행동"
			;실은 그렇지만도 않다
			IF COMCOR_POSI()
				PRINTFORMW 「……뭐야 그거. 말을 했으니, 분명하게 질문에 답하세요」
				PRINTFORMW 「뭐, 조금 부끄러운 내용이었으면, 몰라도」
			;실은 싫어
			ELSE
				PRINTFORMW 「이렇게까지 대답하지 않다니, 다른 이유가 있어?」
				PRINTFORMW 「마음에 들지 않네, 그런 태도……」
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF
;COM0, 침묵한다

;────────────────────────────────────
;COM1, 마음 약하게 응답한다
;────────────────────────────────────
;·REACT분류는 파생에 의해변화
;·ACT("엄하게 위협한다/징계한다고 위협한다/고함치다/밀어 넘어트린다")로 일정 조건을 채우면 REACT파생 1, 애원에 파생
;·REACT인상:약간좋은인상
;·의존도변화(0, +1)
;-------------------------------------------------
;트리거하는행동나 상황…
;ACT분류:0:회화
;	ACT:지금 기분을 묻다/자신에게의 마음을 묻다/성에 관한 이야기를 하다/상냥하게 위로한다
;		엄하게 위협한다/징계한다고 위협한다/고함치다/기분나쁘게 웃다/의상 변경을 요구한다/합의를 요구한다
;ACT분류:1:애무
;	ACT:밀어 넘어트린다
;ACT분류:2:도구
;	ACT:
;ACT분류:3:성교
;	ACT
;ACT분류:4:수치
;	ACT:수치플레이/야외플레이
;ACT분류:5:봉사
;	ACT:
;ACT분류:6:가학
;	ACT:매도
;ACT분류:7:이상
;	ACT:방치플레이/페니스밴드장착
;ACT분류:9:휴식
;	ACT:휴식시킨다/영양제를 먹인다/치료한다
;ACT분류:10:성교봉사
;	ACT:
;TEQUIP:볼개그는 튕겨진다
;-------------------------------------------------
@KOJO_NO9_MS_COM_K14_1
;-------------------------------------------------
;·TFLAG:REACT파생(0=마음 약하게 응답한다/1=애원 한다)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM1_0, 파생:마음 약하게 응답한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	;=================================================
	;무리를했다행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를했다행동"
		;실은 그렇지만도 않다
		IF COMCOR_POSI()
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORMW 「그 태도 뒤에는 숨은 의미가 있는거 같네」
					PRINTFORMW 「그것이 무엇인지는, 나도 모르겠지만……」
				CASE 1
					PRINTFORMW 「단순하면 마음의 기미……인가. 아, 신경쓰지 않아도 될거 같네」
					PRINTFORMW 「지금의%CNAME(MASTER, TARGET)%의 모습을 보고, 웬지 모르게 말에 나왔을 뿐이니까」
					CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("익애") ? 후후 # 후후 \@…」")
					SIF ANIME_CONFIG("FADE")
						CLEARLINE 1
			ENDSELECT
		;실은 싫어
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 0
					PRINTFORMW 「뭐야 그거, 일부로 지친 모습을 보여줘서 무슨 의미가 있다는 거야?」
				CASE 1
					PRINTFORMW 「가끔은 부드러운 모습을 보고 싶다는거야??」
					PRINTFORMW 「정말, %CNAME(MASTER, TARGET)%가 무슨 생각을 하는건지 모르겠어」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가회화
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "회화")
		SELECTCASE RANDCOM(2, 2)
			CASE 0
				PRINTFORML 「무슨 말인지 잘 들리지 않네요」
				PRINTFORMW 「그런 무기력인 태도, 너 답지 않다고 생각하는데?」
			CASE 1
				PRINTFORML 「희한하네.그거」
				PRINTFORMW 「상태가 좋지 않은거야?」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가가학
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "가학")
		SELECTCASE RANDCOM(2, 3)
			CASE 0
				PRINTFORMW 「상당히 무기력하네? 그런거, 너답지 않다고 생각하는데」
;OriginalString : (COLOR_K14("사쿠야"), "「私の勝손でしょう、そんなの…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「마음대로 해, 그런 거…」")
				PRINTFORMW 「그렇구나? 나는 별로 상관없어」
			CASE 1
				PRINTFORML 「사쿠야의 일이니까, 틀림없이 무엇인가 말을 한다고 생각했지만…」
				PRINTFORMW 「조금 기운 빠지는 것 같은데? 도대체 왜 그래?」
;OriginalString : (COLOR_K14("사쿠야"), "「どうでもいいでしょう…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「됐으니까 아무래도 좋아…」")
				PRINTFORMW 「뭐, 그렇구나……, 그래도, 무엇인가 걸려」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가이상
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "이상")
		PRINTFORML 「방치해지는 것이, 그렇게 불안한 것일까나?」
		PRINTFORMW 「사쿠야에도 이런 섬세한 면이 있군요…」
;OriginalString : (COLOR_K14("사쿠야"), "「何よそれ、馬鹿にしてる？」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야 그것, 바보취급을 하고 있는거야?」")
		PRINTFORMW 「그럴리가, 단순한 솔직한 감상이야. 후후」
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류가휴식
	;================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "휴식")
		SELECTCASE RANDCOM(2, 5)
			CASE 0
				PRINTFORMW 「뭐야, 상당히 기운이 없는거 같은데?」
				PRINTFORMW 「혹시, 당신…… 배가 고픈거야?」
;OriginalString : (COLOR_K14("사쿠야"), "「人の事、何だと思ってるのかしら…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「사람을 도대체 뭐라고 생각하는거야…」")
			CASE 1
				PRINTFORMW 「응, 그만두어 주지 않을래. 그런거」
;OriginalString : (COLOR_K14("사쿠야"), "「どういう意味よ」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「어떤 의미야」")
				PRINTFORML 「모르겠어?」
;OriginalString : (COLOR_K14("사쿠야"), "「모르겠어」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「알지 않네요」")
				PRINTFORMW 「그런 말들…, 그런 반항적인 태도는, 사쿠야에게는 맞지 않다고 생각한다고?」
;OriginalString : (COLOR_K14("사쿠야"), "「계속해도 돼?」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「계속해도 돼?」")
				PRINTFORM 「후후, 그런 부분이 사쿠야 다워...」
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;샌 만큼은 여기서 처리
	;-------------------------
	SELECTCASE RANDCOM(2, 10)
		CASE 0
			PRINTFORML 「무슨 말인지 잘 들리지 않네」
			PRINTFORMW 「그런 무기력인 태도, 너답지 않다고 생각하는데?」
		CASE 1
			PRINTFORML 「드문데, 그런 태도」
			PRINTFORMW 「어딘가 몸이라도 나쁜 것일까?」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM1_1 파생:애원 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 1
	CALL KOJO_NO9_MS_COM_K14_7
	RETURN RESULT
ENDIF
;COM1, 마음 약하게 응답한다

;────────────────────────────────────
;COM2, 무뚝뚝하게 응답한다
;────────────────────────────────────
;·REACT분류:없음
;·따랐던 것에는 안 된다
;·징계·포인트가 가산된다
;·REACT인상:나쁜인상
;·의존도변화(-2, -2)
;-------------------------------------------------
;트리거하는행동나 상황…
;ACT분류:0:회화
;	ACT:지금 기분을 묻다/자신에게의 마음을 묻다/성에 관한 이야기를 하다/상냥하게 위로한다
;		엄하게 위협한다/징계한다고 위협한다/고함치다/기분나쁘게 웃다/의상 변경을 요구한다/합의를 요구한다
