;ACT분류:수치
;	ACT:자위/비패개장
;ACT분류:봉사
;	ACT:파이즈리/스마타
;ACT분류:이상
;	ACT:피스트퍽/애널피스트/양쪽피스트
;ACT분류:성봉
;	ACT:정상위시킨다/후배위시킨다/대면좌위시킨다/배면좌위시킨다/항문섹스시킨다/역강간
;-------------------------------------------------
@KOJO_NO9_COM_K14_16
;-------------------------------------------------
;·REACT파생없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM16, 기분 좋다!
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	LOCALS = \@ AFFECTION("익애") ? 후후 # 후후 \@
	;여기에 최초로 떨어지는 것이 어느 ACT분류인가가 문제
	;처음라고는 말해도 REACT의 어디선가 이미 걸리고 있는 가능성은 아무튼 있다
	;=================================================
	;처음 FIRSTCOM(0)
	;=================================================
	IF 구상기입체크(100) && FIRSTCOM(0)
		PRINTFORMW 「조금 뜻밖의 반응이군요?  %CNAME(MASTER, TARGET)%에 그렇게 사랑스러운 곳이 있다고는 몰랐어요」
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「아무튼에서도…, 나쁘지 않아요. 솔직하게 하고 있으면 얼마든지 귀여워해 주어요」
				PRINTFORM 「단, 나나름의 방식으로…그런데
				CALL PRINT_SYMBOL_K14
			CASE "무리가 없는행동"
				PRINTFORMW 「단지 그렇게…, 감사받는 것은 기분은 나쁘지 않아요. 하물며, 이런 처지에 몰아넣은 상대에 대해서?」
				PRINTFORMW 「어차피라면, 지금의 그 기분을 잊지 않으면 좋은 것. 그렇지 않으면…」
				CALL FADEIN(13, @"「%CNAME(MASTER, TARGET)%에 있어, 너무 행복한 미래는 없다고 생각하기 때문에」")
				;페이드 아웃
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「우후후…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			CASE "무리를했다행동"
				;도시파는 미지근하지 않다는 일로 하나
				PRINTFORMW 「우도…, 나에게 아양 팔기 위해 무리하고 있는 것이 미에미에이지만」
				PRINTFORMW 「어느 의미, 불쾌합니다들 있다고도 말할 수 있는 것일까…응」
				;의존도변화			무리하고 있다면죠교대상의의존도도변화해야 한다고 생각한다
				CALL ADD_DEPEND(-4, +3)
		ENDSELECT
		RETURN 1
	ENDIF
	
	;PRINTFORML %CALLNAME:MASTER%는 아첨하도록(듯이) 기분 좋은 것 같은 소리로 허덕이고 있다…
	;부하 2는 상층에서 정리해 처리
	;=================================================
	;무리를했다행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를했다행동"
		;죠교자BASE:만족파생
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「일견\@ !IS_NOWACTNAME("키스") ? 기분 # \@좋을 것 같아도, 조금 비결과인것 같아…」
				PRINTFORMW 「거기까지 무리해, 나에게 아양을 팔고 싶은 것일까?」
				PRINTFORM 「아무튼에서도…, 그러한 일이라면 다양하게 수고는 줄일 수 있는 것일까. 그런데
				CALL PRINT_SYMBOL_K14
			CASE "일반"
				PRINTFORMW 「솔직하게 느끼고 있는 척 해도, 너무나 명백한 것은, 응?」
				PRINTFORMW 「이런 것…, 어느 의미 나에게로의 모욕은 일을 기억해 두는 것이 좋아요…」
			CASE "낮다"
				PRINTFORMW 「뭐야 그것, 느끼고 있는 체? 나에게 아양을 팔자는 (뜻)이유?」
				PRINTFORMW 「무리하고 있는 것이 보이고 보이고로, 너무 좋은 기분은 하지 않네요…」
		ENDSELECT
		;의존도변화
		CALL ADD_DEPEND(-3, +2)
		RETURN 1
	ENDIF
	
	;MENUMATCH로부터 분리
	;=================================================
	;ACT가자위 혹은죠교대상가자위중
	;=================================================
	IF 구상기입체크(100) && TRUECHECK(IS_NOWACTNAME("자위"), GETBIT(TEQUIP:자위중, 0)) > 0
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				;2/3의확률로표시
				IF RAND:3
					;죠교대상가작은체형
					IF TALENT:MASTER:작은체형
						PRINTFORM 「그렇게…, 그것은 최상이구나
						CALL PRINT_SYMBOL_K14
						PRINTFORMW 「작은\@ TALENT:MASTER:남자 ? 남자 # 여자 \@의 아이가 이런 일 해, 조금 제발이라고도 생각하는데」
					;그 외
					ELSE
						PRINTFORMW 「수치않다 기울 수 있는도 없고, 자주(잘) 그런 일을 말할 수 있어요\@ TENSION(TARGET, 8) == "높다" ? ?  # … \@」
						PRINTFORMW 「\@ TALENT:MASTER:남자 ? 여자의 앞에서 열중해 자위 하고 있는 남자라니, 볼품는 듯한 있어에도 정도가 있어요 # 그렇지만, 후후…, 지금의 %CNAME(MASTER, TARGET)%, 꽤 사랑스러워요 \@」
					ENDIF
					;의역 PRINTFORMW 「뭐, 나쁘지 않은 경치예요. 이봐요…, 좀 더 자주(잘), 나에게 보고%RANDTEXT_K14("같게", "키라고 줘")%……」
					PRINTFORMW 「뭐, 나쁘지 않은 경치예요. 이봐요…, 좀 더 나%RANDTEXT_K14("에게 보여줘", "를 보면서 해")%……」
					;1/3의 확률로 페이드 아웃을 부른다
					IF !RAND:3
						CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「우후후…」")
						SIF ANIME_CONFIG("FADE")
							CLEARLINE 1
					ENDIF
				;1/3
				ELSE
					PRINTFORMW 「어머나, 그것은 좋았어요? 이렇게 말해도…」
					PRINTFORM 「 나는 보고 있을 뿐이지만, 이군요
					CALL PRINT_SYMBOL_K14
				ENDIF
			CASE "무리가 없는행동"
				PRINTFORMW 「…과연.%CNAME(MASTER, TARGET)%(은)는 그러한 식으로 하는 것이 키모치 좋구나」
				PRINTFORMW 「이봐요, 좋으니까 계속해. 나에게 잘 보이도록…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가애무
	;=================================================
	IF 구상기입체크(85) && MENUMATCH(TFLAG:ACT, "애무")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				;2/3
				IF RAND:3
					;BASE:만족파생
					SELECTCASE TENSION(TARGET, 8)
						CASE "높다"
							PRINTFORMW 「뭐, 이 내가 하는 것이 당연하지만!  %LOCALS%…」
							PRINTFORM 「이봐요, 이런 것은 어떻게든 해들? 좀 더 좋은 소리로 울릴 수 있어 주어요
						CASE "일반"
							PRINTFORMW 「그것은 최상이군요?  %LOCALS%…」
							PRINTFORM 「뭐,%CNAME(MASTER, TARGET)%말하는 소리를 듣는 것은, 나쁘지 않은 기분이예요
						CASE "낮다"
							PRINTFORMW 「%LOCALS%…, 그렇게 좋았던 것일까 해들? 그렇지만 아무튼…」
							PRINTFORM 「솔직한 것은 좋은 일이야
					ENDSELECT
					;PS로 닫는다
					CALL PRINT_SYMBOL_K14
				;1/3
				ELSE
					PRINTFORMW 「후후, 사랑스러워요. 그렇지만, 이런 건은 시초야」
					PRINT 「 좀 더 좋은 소리로 울릴 수 있어 주기 때문에, 기대하고 있어
					CALL PRINT_SYMBOL_K14
				ENDIF
			CASE "무리가 없는행동"
				PRINTFORMW 「%LOCALS%…, 천만에요. 이렇게 말했다 곳일까」
				PRINTFORM 「솔직하게 하고 있으면, %RANDTEXT_K14("좀 더", "얼마든지")% 잘 해 주어요
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가도구
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "도구")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「이런 완구가 상당히 마음에 드는 같구나?」
				PRINTFORMW 「 그렇지만 뭐…,%CNAME(MASTER, TARGET)%가 즐길 수 있고 있다면 최상일까」
			CASE "무리가 없는행동"
				PRINTFORMW 「철없네요? 이런 것으로 좋아져버린다는」
				PRINTFORMW 「게다가, 감사까지 되어버리자(면)…, 조금 미묘한 기분이군요」
		ENDSELECT
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? 킥킥 # 우후후 \@…」")
		SIF ANIME_CONFIG("FADE")
			CLEARLINE 1
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가성교
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성교")
		;주도권은죠교자
		IF SEX_INITIATIVE(TARGET)
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORMW 「후후, 사랑스러운 일 말하잖아. 그렇지만 그런 일 말할 수 있다 라는 일은, 아직 여유가 있군요?」
					PRINTFORMW 「좋아요…, 숨도 쉴 수 없을 정도 , 마음껏 시라고 주기 때문에…」
				CASE "무리가 없는행동"
					PRINTFORMW 「후~, 후~…, 좋아요. 그렇게 기분 좋다면…」
					PRINTFORM 「 좀 더, 좀 더 시라고 주어요. 망가져버리는 위, 찔러 준다
					CALL PRINT_SYMBOL_K14
			ENDSELECT
			PRINTFORMDL %CALLNAME%(은)는 그렇게 고하면, %CALLNAME:MASTER%의 \@ TEQUIP:성교중 == 6 ? 항 # 질 \@안을 격렬하게 찔러, 떠,
			PRINTFORMDW 그 쾌락에 번민모습을, 마음껏 즐기고 있다…
		;주도권은죠교대상（기승위등 ）
		ELSE
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORM 「좋아요, 지금의 %CNAME(MASTER, TARGET)%…, 매우 사랑스러워요
					CALL PRINT_SYMBOL_K14
					PRINTFORMW 「이봐요, 좀 더 움직여.%CNAME(MASTER, TARGET)%의 이야라시이 모습, 나에게 보이게 해 주세요…」
				CASE "무리가 없는행동"
					PRINTFORMW 「어머나, 그것은 최상이구나. 나로서도, %CALLNAME:MASTER%(이)가 흐트러지는 모습을 볼 수 있는 것은 나쁘지 않아요」
					PRINTFORM 「 그렇지만, 좀 더 좋아지고 싶으면…,%CNAME(MASTER, TARGET)%가 스스로 노력하지 않으면
					CALL PRINT_SYMBOL_K14
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;자위계는 상층에 이전
	;=================================================
	;ACT분류가수치 혹은죠교대상가자위중
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "수치")
		;현상이라면비패개장에만 될 것 같은?
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「%CNAME(MASTER, TARGET)%에는 수치심이라고 해도의가 없는 것일까? 그렇지만 아무튼…」
				PRINTFORM 「솔직한 반응이고, %RANDTEXT_K14("조금", "약간")% 사랑스러웠는지 원이야
				CALL PRINT_SYMBOL_K14
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			CASE "무리가 없는행동"
				PRINTFORMW 「%LOCALS%…, 태들 그런 일 말하다니? 뭐, 기특한 배려여요」
				PRINTFORMW 「그렇게 솔직하게 하고 있는 한, 얼마든지 귀여워해 준다%RANDTEXT_K14("로부터", "원")%…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "봉사")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORM 「%RANDTEXT_K14("어머나,", "＿")% 사랑스러운 일 말하잖아
				;AHCB2
				CALL AUTOHEARTCB2_K14
				PRINTFORMW  그렇지만, 정말로 그렇게 생각한다면…」
				PRINTFORMW 「그것을 태도로 가리켜 받지 않으면%RANDTEXT_K14("?", "＿")%」
			CASE "무리가 없는행동"
				PRINTFORMW 「…응. 아무튼 그것은 좋은 일이지만」
				PRINTFORMW 「지금은%RAND_SPLIT(@"%CNAME(MASTER, TARGET)%/자신")%(이)가 해야 할 일에 집중해 주었으면 한 것%RANDTEXT_K14("?", "＿")%」
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;ACT분류가이상
	;=================================================
	;IF 구상기입 체크(0) && GET_ACTM(TFLAG:ACT) == 8
	;ENDIF
	
	;=================================================
	;ACT분류가성봉
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성봉")
		;주도권은죠교자（역강간등 ）
		IF SEX_INITIATIVE(TARGET)
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORMW 「하, 하…, 뭐, 당연해요」
					PRINTFORM 「이봐요, 아직도 지금부터. 마음껏 요는 주기 때문에, 각오 해…응, 아
					CALL PRINT_SYMBOL2_K14
				CASE "무리가 없는행동"
					PRINTFORM 「응응
					CALL AUTOHEARTCB_K14
					PRINTFORMW 　상당히, 솔직하지 않다」
					PRINTFORMW 「 그렇지만, 아직도 이런 것이 아니니까…」
			ENDSELECT
			PRINTFORMDL %CALLNAME%(은)는 그렇게 고하면, %CALLNAME:MASTER%(을)를 짜 깐 채로 과시하는것 같이 허리를 요염하게 벌름거려
			PRINTFORMDW 마음껏 쾌락을 탐냈다. 근처에는 추잡한 물소리와 두 명의 교성이 영향을 주어 건너고 있다…
		;주도권은죠교대상
		ELSE
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORMW 「그런 당연한 일 말해져도? 그것은 좋으니까…」
					IF PALAM:쾌Ｖ > 8500 || PALAM:쾌Ａ > 8500
						PRINTFORMW 「 나, 조금 더 하면 이 지우자야. 이봐요, 빨리 시라고 주세요…」
					ELSE
						PRINTFORMW 「빨리 계속을 시라고 주세요. 나, 아직 부족해요…」
					ENDIF
				CASE "무리가 없는행동"
					PRINTFORMW 「후~, 후~…, 아무튼, 그것은 알았기 때문에…」
					PRINTFORMW 「빨리 계속을 해 줄 수 있는 것일까? 속상해…」
			ENDSELECT
			PRINTFORMDL %CALLNAME%(은)는 허리를 구군요들 등무늬%CALLNAME:MASTER%의 찌르기 포함을 재촉했다…
			PRINTFORMDW 요염한 기대에, 그 신체를 뜨겁고 불 비출 수 있는이면서…
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;새어 오면 여기서 처리
	;-------------------------
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운행동"
			;BASE:만족라도 좋을지도 시련. 뭐 곤란했을 때, 헤매었을 때의 RAND라고 하는 일로
			SELECTCASE RANDCOM(2, 50)
				CASE 0
					PRINTFORMW 「%LOCALS%…, 아무튼 그렇게 말해져 기분은 나쁘지 않아요」
					PRINTFORM 「적어도, 그 말에 거짓말은 없는 것 같고, 군요
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「그렇게 말해져 기분은 나쁘지 않네요? 어쩐지, 능숙하게 실을 수 있었던 생각도 드는데…」
					PRINTFORMW 「…라고, 뚫은 견해를 하는 나 자신도 대개군요.%LOCALS%」
			ENDSELECT
		CASE "무리가 없는행동"
			SELECTCASE RANDCOM(2, 51)
				CASE 0
					PRINTFORMW 「기분 좋다! 당연한 일 말해져도, 응?」
					PRINTFORMW 「아무튼에서도…, 솔직한 것은 나쁜 일이 아니어요」
				CASE 1
					PRINTFORMW 「사랑스러운 일 말하잖아. 뭐, 그것은 좋은데…」
					PRINTFORMW 「그 예는, 머지않아 형태가 있는 것으로 했으면 좋네요?  %LOCALS%」
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF
