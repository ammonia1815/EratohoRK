IF 구상기입체크(100) && !TFLAG:REACT파생
	;죠교자상태변화파생
	SELECTCASE GET_CONDITION(TARGET)
		CASE "정욕", "울굴", "몽롱"
			PRINTFORMW 「모처럼 좋은 기분이었는데, 엉망으로 해 주어요…」
;OriginalString : (COLOR_K14("사쿠야"), "「私の知った事ではないわ。そんなの」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「내가 안 일은 아니어요. 그런 것」")
			PRINTFORMW 「지금의 말, 잘 기억해 두는 일이군요. 후회해도 모르니까…」
		CASE "분노"
			PRINTFORMW 「응…아무것도 내가 최악의 기분때에 말하지 않아도 괜찮다고 생각하는데」
			PRINTFORMW 「부탁이니까 더 이상 화나게 하지 마. 자신이 어떻게 하는지, 나에게도 알지 않으니까……」
		CASE "싫증"
			PRINTFORMW 「재미있는 일 말하잖아? 약간 싫증을 감추는 정도이지만」
			PRINTFORMW 「 그렇지만 아무튼……그러한 것은, 사쿠야에 있어 너무 좋은 결과는 되지 않는다고는 생각하지만, 이군요」
		CASEELSE
			IF RAND:3
				SELECTCASE TFLAG:징계·포인트
					CASE IS >= 80
						PRINTFORML 「말하고 싶은 일은 그 만큼 까? 뭔가 있다면 지금 토해내 두는 것이 좋아요」
						PRINTFORMW 「머지않아 그런 입은 들을 수 없게 해 주기 때문에…」
					CASE IS >= 40
						PRINTFORML 「입은 재앙의 원, 은 자주(잘) 말한 것」
;OriginalString : (COLOR_K14("사쿠야"), "「どういう事よ」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「어떻게 말하는 일이야」")
						PRINTFORMW 「지금 알 필요는 없어요. 그 중 싫어도 깨닫기 때문에, 군요…」
					CASEELSE
						PRINTFORML 「…재미있는 일 말해 주어요. 뭐, 좋은데」
						PRINTFORMW 「다만, 그만한 각오는 해 받아요…」
				ENDSELECT
			ELSE
				PRINTFORMW 「어머나, 말해 주어요」
;OriginalString : (COLOR_K14("사쿠야"), "「なによ、悪い？」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 나쁘다?」")
				PRINTFORMW 「별로? 다만, 상응하는 보답은 받는 일이 된다고 생각하지만, 이군요」
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM12_1, 파생:강한척 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:REACT파생 == 1
	IF RAND:3
		;죠교자상태변화파생
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울굴", "몽롱"
				PRINTFORML 「별로 좋아요, 지금은 무슨 말을 해져도 신경이 쓰이지 않으니까」
				PRINTFORMW 「오히려, 그런 귀부인을 다랑어 다랑어에 녹이는 것이 즐거움으로 방법 없는거야」
				PRINTFORMW 「후후후…」
			CASE "분노"
				PRINTFORMW 「거기에 비하면 상당히 사랑스러운 소리 냈지만?」
				PRINTFORMW 「보통이라면 웃어 끝내는 곳이지만……지금은 그러한 기분은 아닌거예요. 미안한데…」
			CASE "싫증"
				PRINTFORMW 「응, 어차피라면 좀 더 나은 거짓말하면 어떻게야?」
;OriginalString : (COLOR_K14("사쿠야"), "「う、嘘なんてついてないわ」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「거, 거짓말은 붙어 있지 않아요」")
				PRINTFORML 「거짓말에 거짓말을 거듭하면 자신이 괴로워져요. 거기에 변명도 범용 지나고」
				PRINTFORMW 「진짜, 싫증한 여자…」
			CASEELSE
				PRINTFORMW 「거짓말한다면, 좀 더 능숙하게 하는 것이 좋다고 생각하는데?」
;OriginalString : (COLOR_K14("사쿠야"), "「だ、誰が嘘なんて！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「누, 누가 거짓말이라니!」")
				PRINTFORMW 「후후, 필사적으로 되어 버려서…타애 없네요, 사쿠야」
		ENDSELECT
	ELSE
		SELECTCASE RANDCOM(2, 20)
			CASE 0
				PRINTFORMW 「실례해버리는 원이군요. 그렇지만…」
				PRINTFORMW 「몸은 거짓말할 수 없는 것 같지만…?」
			CASE 1
				PRINTFORMW 「강한척 해 보였더니…, 여유가 없는 것은 들키고 들키고이지만」
				PRINTFORMW 「거기응과 개, 어떤가 해들. 응?」
		ENDSELECT
	ENDIF
	RETURN 1
