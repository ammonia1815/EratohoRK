			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇겠지요？ 이봐요, 신경을 집중시켜. 좀 더 자주(잘) 해 주어요…♪」")
			PRINTFORMDW 사쿠야는 손바닥으로 앨리스의 유두를 굴리면서, 유방을 들어 올리도록(듯이), 아래에서 위에의 큰 움직임 나오고 유하기 만…
			PRINTL
			CALL QUAKE(1)
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「あっ")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　んんっ！ おっぱい気持ち良くって…」")
			CALL QUAKE(4)
;OriginalString : (COLOR_K14("엘리스"), "「んっ、くぅっ！ イクっ、イクぅっっ！！」")
			CALL PRINTCOLORW(COLOR_K14("엘리스"), "「읏, 쿠웃！ 간닷, 간다앗！！」")
