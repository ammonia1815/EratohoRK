;────────────────────────────────────
;NO9_EV22_1, 죠교대상상태변화
;────────────────────────────────────
;M_COND MASTER가 지정했다상태라면 진을반환
;------------------------------------------------
;캐릭터 한정구상로 여기서 MASTER에 말하게 하는 것은 좋음 기분인 감
;통상라면 TARGET가 MASTER에 대해서 한 마디로 끝날 것 같지만
;------------------------------------------------

;BASE로 파생해도 좋은 것 같다
;━━━━━━━━━━━━━━━━━━━━━━━━
;사쿠야씨[피폐]화
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && M_COND("피폐")
	TCVAR:MASTER:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		SELECTCASE RANDEVENT(3, 0)
			CASE 0
;OriginalString : (COLOR_K14("사쿠야"), "「ちょっと疲れたわね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「조금 지쳤군요…」")
			CASE 1
;OriginalString : (COLOR_K14("사쿠야"), "「ふぅ…疲れたわね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후우…지쳤군요…」")
			CASE 2
;OriginalString : (COLOR_K14("사쿠야"), "「なんか身体がだるいわね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭔가 신체가 나른하네요…」")
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

