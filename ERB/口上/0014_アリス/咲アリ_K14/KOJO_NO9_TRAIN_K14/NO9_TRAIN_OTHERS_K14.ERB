ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스님[정욕]화
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && T_COND("정욕")
	IF RAND:3
		RETURN 0
	ELSE
		;엘리스님만족도 파생 처리
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORML 「도, 이제 타목… 참을 수 없어요!」
				PRINTFORMW 「철저하게 귀여워해 주기 때문에 각오 해…」
			CASE "일반"
				PRINTFORML 「안돼, 이런 건…억제하지 않으면…」
				PRINTFORMW 「 그렇지만, 사쿠야를 엄청으로 하고 싶어서, 나…」
			CASE "낮다"
				PRINTFORMW 「 나…스스로도 무서울 정도  흥분하고 있다…」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스님[분노]화
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && T_COND("분노")
	IF AFFECTION("상사상애")
		TCVAR:상태변화지속 = 1
	ELSEIF AFFECTION("익애")
		IF RAND:3
			TCVAR:상태변화지속 = 2
		ELSE
			TCVAR:상태변화지속 = 1
		ENDIF
	ELSEIF AFFECTION("권애")
		IF RAND:3
			TCVAR:상태변화지속 = 3
		ELSE
			TCVAR:상태변화지속 = 2
		ENDIF
	ELSE
		TCVAR:상태변화지속 = 3
	ENDIF
	IF RAND:3
		RETURN 0
	ELSE
		;엘리스님만족도 파생 처리
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORML 「나쁘지 않은 기분이었지만… 완전히 엉망으로 해 준 원이군요?」
				PRINTFORMW 「그 대상은 비싸게 들기 때문에, 각오 해…」
			CASE "일반"
				PRINTFORML 「부탁이니까, 그다지 화나게 하지 말고 주세요」
				PRINTFORMW 「어디까지 자신을 누를 수 있는지, 그다지 자신이 없어요…」
			CASE "낮다"
				PRINTFORMW 「 나를 화나게 해도 좋은 일은 없어요…」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;100%
;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스님[싫증]화
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && T_COND("싫증")
	;죠교의경과시간
	SELECTCASE TFLAG:경과시간
		;길다
		CASE IS >= 60
			PRINTFORMW 「질려 온 원이군요…이제 끝낼까」
			TCVAR:상태변화지속 = 3
		;보통
		CASE IS >= 30
			PRINTFORMW 「시시해요…진짜 싫증한 여자」
			TCVAR:상태변화지속 = 2
		;짧다
		CASEELSE
			PRINTFORMW 「귀염성이 없어서 트마라나이원이군요…」
			TCVAR:상태변화지속 = 1
	ENDSELECT
	RETURN 1
ENDIF

