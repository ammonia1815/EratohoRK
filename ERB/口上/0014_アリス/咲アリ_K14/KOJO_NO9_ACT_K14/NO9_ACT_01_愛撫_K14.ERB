				CALL PRINTCOLOR(COLOR_K14("엘리스"), "「슬슬 결정타를 찔러 주어요…")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			CASEELSE ;애태움없음
				PRINTFORMW 「이제 때일까?」
				CALL PRINTCOLOR(COLOR_K14("엘리스"), "「그러면, 조금격렬하게 주어요…")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자삽입중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:성교중
		;추가행동계의 bit를 클리어 해 표시시키지 않다
		CLEARBIT TFLAG:추가행동구상, 0
		CLEARBIT TFLAG:추가행동지문스킵, 0
		
		;연속실행
		IF TFLAG:ACT == TFLAG:전ACT
			PRINTFORMDL 앨리스는 사쿠야를 격렬하게 범하면서%ANUS_DESCRIPTION(MASTER)%(을)를 계속 애무하고 있다…
			;일행 비운다
			PRINTL
			;진동 효과
			CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「あんっ、んんっ！ や、おしりそんなに弄ったらぁッ！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아읏, 응읏! 야, 엉덩이 그렇게 만지작거리면!」")
			
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「후후, 뒤 만지작거릴 때에 큐는 꼭 죄여요? 사쿠야…")
			;하트를 표시해 괄호를 닫는다
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			
;OriginalString : (COLOR_K14("사쿠야"), "「いやっ！ 言わないで、そんな事！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아니! 말하지 말고, 그런 일!」")
			PRINTFORMW 「벌써 솔직하게 인정하면? 나에게 범해지면서, 엉덩이 만지작거려져 기쁨메스개도 일…」
			PRINTFORMW 「후후후…」
		;통상
		ELSE
			PRINTFORMDL 앨리스는 사쿠야를 격렬하게 찌르면서%ANUS_DESCRIPTION(MASTER)%(을)를 애무하기 시작했다…
			;일행 비운다
			PRINTL
			;진동 효과
			CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「やっ！ な、なに？ ひぁっ、あっ、あぁっ！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「얏! 뭐, 뭐? 히앗, 아, 아앗!」")
			PRINTFORMW 「후후, 그렇게 사랑스러운 소리 나오면(자) 흥분해버리는 원. 사쿠야♪」
			
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「조금 죄여 온 느낌으로, 꽤 좋네요, 이것")
			;하트를 표시해 괄호를 닫는다
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;연속실행
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;하반신복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;하반신겉옷가 요점 밀치고		하반신겉옷가 유로, 옷을 걷어 올려, 속옷 너머로애무 하는 상황
			IF LOWERJACKET(1) == 1
				PRINTFORMDW 앨리스는 사쿠야의 %OUTER_B(MASTER, 0, 1)%(을)를 걸은 상태로%ANUS_DESCRIPTION(MASTER)%(을)를 계속 애무하고 있다…
				PRINTFORMW 「후후, 피부 자리수옷이 매우 좋은 느낌이군요」
				PRINTFORMW 「마치 사쿠야를 강간하고 있는 것 같다♪」
			;하반신속옷가 요점 밀치고		하반신겉옷가 없음으로, 속옷 너머로애무 하는 상황
			ELSEIF LOWERUNDER(1)
				PRINTFORMDW 앨리스는 사쿠야의 %INNER_B(MASTER)%넘어로%ANUS_DESCRIPTION(MASTER)%(을)를 계속 애무하고 있다…
				PRINTFORMW 「속옷월 하기라도 기분 좋은 것 같구나? 후후」
			ENDIF
			;일행 비운다
			PRINTL
			;안면승마중
			IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0
;OriginalString : (COLOR_K14("사쿠야"), "「んんっ、んく…んんんっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응읏, 응쿠… 응응읏!」")
				PRINTFORMW 「그래요, 귀부인도 확실히 봉사해 주세요♪」
			;그 외
			ELSE
;OriginalString : (COLOR_K14("사쿠야"), "「んっ、くぅっ！ あっ、あぅっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏, 쿠웃! 아, 아웃!」")
				CALL PRINTCOLOR(COLOR_K14("엘리스"), "「엉덩이 만지작거려져, 그렇게 허덕여지면 나도 흥분해 버려요")
				;하트를 표시해 괄호를 닫는다
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
;OriginalString : (COLOR_K14("사쿠야"), "「やめてぇっ！ もう、おしり、弄らないでっ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그만두어엇! 이제(벌써), 엉덩이, 만지작거리지 맛!」")
		PRINTFORMW 「멈추기에는  가지 않네요. 사쿠야도 이렇게 기뻐해♪」
;OriginalString : (COLOR_K14("사쿠야"), "「わ、私は喜んでなんか…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「나, 나는 기뻐해 뭔가…」")
		PRINTFORMW 「그렇게? 그것이 진짜라면 멈추어도 괜찮은데?」
;OriginalString : (COLOR_K14("사쿠야"), "「…………」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…………」")
		PRINTFORMW 「대답이 없는 것이 대답은 일로 좋은 것일까? 후후, 좀 더 솔직해지세요, 사쿠야」
		RETURN 1
	ENDIF
	
	;=================================================
	;전회Ａ질내사정
	;=================================================
	;IF 구상기입 체크(0) && TFLAG:전회질싸 == 5
	;ENDIF
	
	;=================================================
	;하반신복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(100) && PANTSCHECK(1, 1)
		PRINTFORMW 「얌전하고 없음 있고, 사쿠야」
;OriginalString : (COLOR_K14("사쿠야"), "「な、なにをするのよ」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무, 무엇을 하는거야」")
		PRINTFORMW 「뭐는, 귀부인의 엉덩이를 만지작거려 주어. 후후」
		PRINTL
		;하반신겉옷가 요점 밀치고		하반신겉옷가 유로, 옷을 걷어 올려, 속옷 너머로애무 하는 상황
		IF LOWERJACKET(1) == 1
			PRINTFORMW 「응, 옷 입은 채로 걷어 올릴 수 있어 이런 일 되는 것, 흥분하겠지요?」
;OriginalString : (COLOR_K14("사쿠야"), "「別にどうって事ないわ、よ…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「별로 어떻게라는 일 없어요,…」")
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「사실일까? 조금 얼굴이 붉어요, 사쿠야")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		;하반신속옷가 요점 밀치고		하반신겉옷가 없음으로, 속옷 너머로애무 하는 상황
		ELSEIF LOWERUNDER(1)
			PRINTFORMW 「속옷월 하기라도 히크히크 하고 있는 것이 전해져 유곽이군요」
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「기대하고 있을까? 후후, 추잡하네요…")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
;OriginalString : (COLOR_K14("사쿠야"), "「し、知らないわよ！ そんな…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요! 그런…」")
			PRINTFORMW 「솔직하지 않네요? 키득」
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	;Ａ개발이 끝난 상태
	IF ABL:MASTER:Ａ감각 > 4
		PRINTFORMW 「응 사쿠야…」
;OriginalString : (COLOR_K14("사쿠야"), "「なによ」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야」")
