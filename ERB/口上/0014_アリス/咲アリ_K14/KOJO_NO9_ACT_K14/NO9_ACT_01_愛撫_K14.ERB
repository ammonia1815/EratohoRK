;========================================================================
;애무계 ACT10～19 & 27
;========================================================================
;·TFLAG:ACT(10=손으로 애무/11=가슴애무/12=입으로 애무/13=항문애무/14=항문펠라/15=키스/16=성기마찰한다/17=파이즈리한다/18=풋잡하기/19=손가락삽입한다/27=밀어 넘어트린다)
;·TFLAG:ACT분류 == 1에 해당
;·ACT18, 발코키는이상계로부터 파생하는 경우 있어
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "애무")로 줍는 것이 가능
;·죠교자가죠교대상를애무 한다. 이구상의 시점에서죠교자는 벌써손를 내고 있다
;·미약를 사용하는 케이스가 있다
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는구상측의 RESULT(종단의 RETURN 1 등)에 의해구상가 표시당했다의지 아닌지,
;·또 개소 마다의 특유의 거동(미약등구상를 스킵 따위)을 제어하고 있습니다
;·@구상기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는구상측에서 결정하고 쳐 하는필요가 있습니다
;·특별한 이유가 없는 한, 텐프레로서는 RETURN 1을 기술해 둡니다
;-------------------------------------------------
;관련 함수…
;FIRSTACT
;처음ACT 판정 함수
;TFLAG:ACT（TFLAG:90）를 자동으로 참조, 현재 실행되고 있는 ACT 마다번호가 독립해 보존된다
;-------------------------------------------------

;────────────────────────────────────
;NO9_ACT10, 손으로 애무(ACT10_0, 애무 하는/ACT10_1, 수음 하는/ACT10_2, 페니스밴드수음)
;────────────────────────────────────
;·Ｃ중심으로애무 한다
;·죠교대상에페니스가 있는 경우, 및페니스밴드장착시에는 파생 커맨드가 된다
;·죠교대상가자위중의 경우는 해제된다
;·죠교자의손에 더러움이 있는 경우불쾌SOURCE가 변동한다
;·죠교자기교, 애무, 손가락기술소질에 응해발기도상승
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_10
;-------------------------------------------------
;·TFLAG:ACT파생(0=애무/1=수음/2=페니스밴드수음)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT10_0, 애무 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	[SKIPSTART]
	;=================================================
	;처음
	;=================================================
	IF 구상기입 체크(0) && FIRSTACT(0)
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입 체크(0) && ONCEACT(0)
		SELECTCASE POLICY("금일") ;금일의죠교방침파생 처리
			CASE "통상"
				PRINTFORMW
			CASE "징계·"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "노닥노닥"
				PRINTFORMW
			CASEELSE ;그 외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;밀어넘어짐상태
	;=================================================
	IF 구상기입체크(100) && TEQUIP:밀어넘어지는중
		IF TFLAG:ACT == TFLAG:전ACT ;연속실행
			PRINTFORMW 「이대로 손으로 이나 키챠오 건더기등♪」
			PRINTFORMW 「이봐요, 좋을 것입니다? 좀 더 기분 좋아져…」
		ELSE ;통상
			PRINTFORMW 「, 나의 손가락으로 즐겨 주세요」
			PRINTFORMW 「민감한 곳도 만져 주어요… 후후…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가가버려
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:가버려
		;전부터
		IF TCVAR:MASTER:위치전후 == 1
			SELECTCASE TCVAR:MASTER:애태움도 ;애태움도파생 처리
				CASE IS > 1 ;애태움중
					PRINTFORMW 「간단하게 이라고 해도들은은 곤란해요? 사쿠야」
					PRINTFORM 「 좀 더 나를 즐기게 해 주세요
					CALL PRINT_SYMBOL_K14
				CASE 1 ;애태움종료
					PRINTFORMW 「후후, 이렇게 부풀려 버려서 이제 한계일까?」
					CALL PRINTCOLOR(COLOR_K14("엘리스"), "「슬슬 결정타를 찔러 주어요…")
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				CASEELSE ;애태움없음
					PRINTFORMW 「이제 때일까?」
					CALL PRINTCOLOR(COLOR_K14("엘리스"), "「그러면, 조금격렬하게 주어요…")
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDSELECT
		;뒤로부터
		ELSE
			SELECTCASE TCVAR:MASTER:애태움도 ;애태움도파생 처리
				CASE IS > 1
					PRINTFORMW 「후후, 이제 이 나무 그런? 얼굴이 안보여도 알아요」
					PRINTFORM 「 그렇지만 간단하게 이인가 다툴 생각은이야. 좀 더 초조하게 해 준다
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「좋아요, 이 오세요 사쿠야. 나의 팔 중(안)에서 이나 키라고 주기 때문에…」
					PRINTFORMDW 앨리스는 사쿠야의 목덜미에 키스를 하면서, 사쿠야를 절정으로 막상 되었다…
				CASEELSE
					PRINTFORMW 「후후, 이것으로는 도망가지 않아요?」
					PRINTFORMW 「이대로 이나 키라고 주기 때문에, 얌전하게 하고 있어…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;PALAM를 물게 해야 하는 것이라고 생각한다
	;=================================================
	;연속실행
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;하반신복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			PRINTFORMW 「응 사쿠야… 속옷 위로부터라도 아는 위 부풀어 올라 왔어요?」
			IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0 ;안면승마중
				PRINTFORMW 「어머나, 미안해요. 이 상태는 말할 수 없네요? 후후」
			ELSE ;그 이외
;OriginalString : (COLOR_K14("사쿠야"), "「しっ、知らないわよ！ そんなの…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요! 그런 것…」")
				CALL PRINTCOLOR(COLOR_K14("엘리스"), "「후후, 속여도 안돼")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　몸은 정직 하니까」")
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;입지 않았다
		;---------------------
		PRINTFORMW 「상당히 넌더리나고 넌더리나고 해 온 원이군요. 그렇게 기분 좋은 것일까, 응 사쿠야?」
		IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0 ;안면승마중
			PRINTFORMW 「이라고 해도, 이 상태는 대답은 할 수 없을까. 후후…」
		ELSE ;그 이외
;OriginalString : (COLOR_K14("사쿠야"), "「今そんな事聞かないで欲しいわね…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「지금 그런 일 (듣)묻지 않으면 좋네요…」")
			PRINTFORMW 「후후, 그렇구나. 생각이 잘 미치지 않아서 미안해요♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;전회Ｖ질내사정
	;=================================================
	;IF 구상기입 체크(0) && TFLAG:전회질싸 == 1
	;ENDIF
	
	;=================================================
	;눈가리개장착중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:눈가리개
		PRINTFORMDL 앨리스는 시야를 빼앗긴 사쿠야의 %CLITORIS_DESCRIPTION(MASTER)%음핵을 애무하기 시작했다…
		PRINTL
		PRINTFORMW 「호흡이 거치네요? 사쿠야. 눈 가림되어 흥분하고 있는 거나」
		PRINTFORMW 「후후, 몇시, 어디를 손대어지는지 모른다고 것도 좋은 것」
		RETURN 1
	ENDIF
	
	;=================================================
	;클리캡장착중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:클리캡
		PRINTFORMDL 앨리스는 구리 캡 넘어로사쿠야의 %CLITORIS_DESCRIPTION(MASTER)%클리토리스를 애무하기 시작했다…
		PRINTL
		PRINTFORMW 「이대로 만지작거리면, 귀부인 어떻게 되어버릴까 해들? 응 사쿠야」
		PRINTFORMW 「후후후…」
		RETURN 1
	ENDIF
	
	;eraWIZ K12 나딸구상에 인스파이어당했다개소
	;아무튼 Rev의 경우는 임의에 커맨드선택 할 수 있는 것이 아니고 이것이 성립하는 확률은 꽤 낮은일까w
	;=================================================
	;전ACT가스펭킹
	;=================================================
	IF 구상기입체크(100) && PREVACTNAME() == "스펭킹"
		PRINTFORMDW 앨리스는 보라색의 타척의 자국을 소프트에, 그리고 열심히 어루만지고 돌렸다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼파생
			CASE IS >= 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응읏, 앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				PRINTFORMW 「귀부인에게 있어서는 아픔도 쾌락인 것? 좋아요, 좀 더 상흔을 손대어 주기 때문에」
			CASEELSE
;OriginalString : (COLOR_K14("사쿠야"), "「あぁっ！ やっ、いやぁっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗! 얏, 싫어엇!」")
				PRINTFORMW 「아픈 것은 지금 가운데만…곧바로 좋아져요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전ACT가채찍
	;=================================================
	IF 구상기입체크(100) && PREVACTNAME() == "채찍"
		PRINTFORMDW 채찍에 의해 사쿠야의 신체에 새겨진 상흔을, 앨리스는 섬세한 손가락 값어치 살그머니 위를 덧쓰고 있다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼파생
			CASE IS >= 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응읏, 앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				PRINTFORMW 「귀부인에게 있어서는 아픔도 쾌락인 것? 좋아요, 좀 더 상흔을 손대어 주기 때문에」
			CASEELSE
;OriginalString : (COLOR_K14("사쿠야"), "「あぁっ！ やっ、いやぁっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗! 얏, 싫어엇!」")
				PRINTFORMW 「아픈 것은 지금 가운데만…곧바로 좋아져요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;가설
	;=================================================
	;전ACT가바늘
	;=================================================
	IF 구상기입 체크(0) && PREVACTNAME() == "바늘"
	ENDIF
	
	;가설
	;밧줄는 해제의 경우도 있기 때문에 뭐라고곳
	;=================================================
	;전ACT가밧줄
	;=================================================
	IF 구상기입 체크(0) && PREVACTNAME() == "밧줄"
	ENDIF
	[SKIPEND]
	
	;=================================================
	;하반신복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(100) && PANTSCHECK(1, 1)
		IF PALAM:MASTER:윤활 >= 500 ;윤활 충분히
			PRINTFORMW 「이제 이렇게 속옷을 더럽혀 버려서, 기다릴 수  없었던 것일까 해들?」
;OriginalString : (COLOR_K14("사쿠야"), "「違っ！ こ、これは…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「달랏! 이, 이것은…」")
			PRINTFORMW 「후후, 부끄러워하지 않아 좋아요. 젖고 있는 (분)편이 다양하게 적당한 것, 이군요♪」
		ELSE ;윤활불발
			PRINTFORMW 「그다지 젖지 않네요?」
			PRINTFORMW 「좋아요, 곧바로 자신으로부터 속옷을 벗고 싶어질 정도로 자주(잘) 해 주기 때문에♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;위치전후 == 2가 되는 확립, 및손간의 관점으로부터 이러한 모양에
	;RETURN 하지 않고 하층에 흘리면 상황에 따라서는 위화감이 나오므로 여기서 완결시킨다
	;=================================================
	;배후로부터
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:위치전후 == 2
		SELECTCASE TENSION(TARGET, 8) ;죠교자만족도 파생 처리
			CASE "높다"
				PRINTFORMW 「이봐요, 움직이지 마. 뒤로부터 귀여워해 주기 때문에」
				PRINTFORMDW 앨리스는 사쿠야의 목덜미에 키스를 하면서, 애무하기 시작했다…
;OriginalString : (COLOR_K14("사쿠야"), "「んっ！ やだ、엘리스…こ、こんなの…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏! 싫다, 엘리스… 이, 이런 건…」")
				PRINTFORMW 「그렇지만 싫지 않겠지요? 후후, 나에게는 알아요」
			CASE "일반"
				PRINTFORMW 「이봐요 어때? 뒤로부터 만지작거려지는 것은 좋을 것입니다?」
;OriginalString : (COLOR_K14("사쿠야"), "「し、知らないわよ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요!」")
				PRINTFORMW 「그렇게는 말하는데……반응이 조금 달라. 후후」
			CASE "낮다"
				PRINTFORMW 「뒤로부터 꼭 껴안을 수 있으면 얌전해져버린다 것?」
;OriginalString : (COLOR_K14("사쿠야"), "「なんの事か、私にはさっぱりね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무슨 일인가, 나에게는 형편없구나…」")
				PRINTFORM 「후후, 별로 좋아요. 다만, 좋은 소리로 울어 주기만 하면, 이군요
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자상태변화파생 처리
	;=================================================
	IF 구상기입체크(100)
		;-------------------------------------------------
		;정욕/울굴/몽롱
		;-------------------------------------------------
		IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(TARGET), "정욕", "울굴", "몽롱")
			SELECTCASE TENSION(TARGET, 8) ;죠교자만족도 파생 처리
				CASE "높다"
					IF PALAM:MASTER:윤활 >= 500 ;젖고 있다
						PRINTFORMW 「만지작거렸으면 좋았으면, 솔직하게 말하면 좋은데는 나는 생각하는데. 응 사쿠야?」
;OriginalString : (COLOR_K14("사쿠야"), "「なっ！？ 何を言ってるのか全然判らないわ…」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫!? 무슨 말을 하고 있는지 전혀 알지 않아요…」")
						PRINTFORM 「후후…귀부인은 알지 않아도, 여기는 갖고 싶고 이제 기다릴 수  없는 것 같지만, 이군요…
						CALL PRINT_SYMBOL_K14
					ELSE ;젖지 않았다
						PRINTFORM 「, 색정광의 변태 메이드에게 포상을 주어요! 마음껏 즐겨 주세요
						CALL PRINT_SYMBOL_K14
;OriginalString : (COLOR_K14("사쿠야"), "「その発言の内容は兎も角…なにその鬱陶しいハイテンションは…」")
