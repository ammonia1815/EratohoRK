		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「")
		CALL PRINTFONTSTYLE(COLOR2_K14("SAKUYA"), "그·러·니·까", 1)
;OriginalString : (COLOR_K14("사쿠야"), "、一々実況するのはやめなさいよ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), ", 하나 하나 실황하는 것은 그만두세요!」")
		PRINTFORMW 「후후, 미안해요♪」
	;미개발
	ELSE
		PRINTFORMW 「 아직 친숙해 졌다 라는 느낌이 아니네요…」
		CALL PRINTCOLOR(COLOR_K14("엘리스"), "「그렇지만 걱정하지 않아도 괜찮아요? 차분히 길들여 주기 때문에…")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
	ENDIF
	RETURN 1
ENDIF

