			CASE "통상"
				PRINTFORMW
			CASE "항문"
				PRINTFORMW
			CASE "징계·"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "노닥노닥"
				PRINTFORMW
			CASEELSE ;그 이외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;밀어넘어짐상태
	;=================================================
	IF 구상기입체크(100) && TEQUIP:밀어넘어지는중
