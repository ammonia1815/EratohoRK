;────────────────────────────────────
;NO9_ACT73, 방치플레이 FIRSTACT(0) RANDACT(0)
;────────────────────────────────────
;·죠교자가죠교대상를방치 한다
;·턴방침가"소프트", "노멀", "이상"라면 발생하지 않는다
;·(이상계 ACT로턴방침"이상"로 연주하고 있는 것은 미스라고 생각되고)
;·금일의방침가"노닥노닥"의 경우는 발생하지 않는다
;·밀어넘어지는중에는 발생하지 않는다
;·성교중/성교봉사중에는 발생하지 않는다
;·수치계로부터 발생하는 경우도 있다(v1. 010 현재, 수치계로부터 파생했다경우에게만 발생이라고 생각되고)
;·안면승마계는 해제된다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_73
;-------------------------------------------------
;·ACT의 파생 없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT73, 방치플레이
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
;OriginalString : (COLOR_K14("사쿠야"), "「何よ、これ。一体どういうつもりよ」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 이것. 도대체 어떤 생각이야」")
		PRINTFORML 「어떤 작정(생각)이 뭐고…, 단순한」
;OriginalString : (COLOR_K14("사쿠야"), "「ただの？」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「단순한?」")
		
		PRINTFORM 「
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "방치플레이", 1)
		PRINTFORMW 라는 녀석」
		
;OriginalString : (COLOR_K14("사쿠야"), "「は？」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「하?」")
		PRINTFORMW 「아니, 그러니까 방치 플레이라고 말하고 있지?」
;OriginalString : (COLOR_K14("사쿠야"), "「そうじゃなくって……、これに何の意味があるのかって聞いてるのよ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇지 않아서……, 이것에 무슨 의미가 있는지라고 (듣)묻고 있는거야!」")
		PRINTFORMW 「의미는…, 그런 것이 있다면 내가 (듣)묻고 싶을 정도 」
;OriginalString : (COLOR_K14("사쿠야"), "「……やれやれ。頭痛くなってきたわ」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「……아휴. 머리 아파졌어요」")
		RETURN 1
	ENDIF
	
	;성실하게 생각하면, 다양하게 가능성을 느낀다
	;-------------------------
	;그 외
	;-------------------------
	SELECTCASE RANDACT(3, 0)
		CASE 0
			PRINTFORMW 「소재내림인 사쿠야의 모습을 보는 것도 나쁘지 않네요」
;OriginalString : (COLOR_K14("사쿠야"), "「悪趣味ね、엘리스…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「악취미군요,엘리스…」")
			PRINTFORMW 「덕분에, 군요」
		CASE 1
			PRINTFORMW 「어머나, 아무것도 되지 않으면 오히려 초조한 것 같구나?」
;OriginalString : (COLOR_K14("사쿠야"), "「おかげさまで。貴女が何を企んでるのか、気が気じゃないだけよ」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「덕분에. 당신이 무엇을 기도하고 있는지, 기분이 기분이 아닌 것뿐」")
			PRINTFORMW 「「기도하고 있다」는…, 외문이 나쁜 일 말하지 않아 받을 수 있다?」
;OriginalString : (COLOR_K14("사쿠야"), "「そうね、貴女以外には言わないように気をつけるわ」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇구나, 당신 이외에는 말하지 않게 조심해요」")
			PRINTFORMW 「에 , 그것은 그것은. 기분사원는 나쁘네요」
;OriginalString : (COLOR_K14("사쿠야"), "「どういたしまして」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「천만에요」")
			PRINTFORMW 「…………」
;OriginalString : (COLOR_K14("사쿠야"), "「…………」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…………」")
		CASE 2
;OriginalString : (COLOR_K14("사쿠야"), "「あまりジロジロ見ないで頂けるかしら…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그다지 빤히 보지 않아 받을 수 있을까…」")
			PRINTFORMW 「어째서?」
;OriginalString : (COLOR_K14("사쿠야"), "「落ち着かないからよ」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「초조하기 때문에」")
			PRINTFORMW 「……알았다」
;OriginalString : (COLOR_K14("사쿠야"), "「え？」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「에?」")
			PRINTFORM 「 좀 더 초조한 기분에 시켜 주어요, 사쿠야
			CALL PRINT_SYMBOL_K14
;OriginalString : (COLOR_K14("사쿠야"), "「そんなことだろうと思ったわよ……」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 일일거라고 생각했어요……」")
			PRINTFORMW 「후후후…」
	ENDSELECT
	RETURN 1
ENDIF

;────────────────────────────────────
;NO9_ACT74, 공중육변기플레이 ※봉인가 끝난 상태
