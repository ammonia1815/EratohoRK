;
;		성교계 액션구상를 정리한 파일
;
;	EXTRA모드로 NO:9사쿠야를 MASTER에선택했다경우만 표시
;
;Version		Reason
;1. 70			@구상기입 체크 사양에. 그 외 섬세한 수정
;1. 61			일부구상의 함수화. TFLAG:ACT파생 4/5의 추가. 그 외 가필＆수정
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;성교계 ACT30～35
;========================================================================
;·TFLAG:ACT(30=정상위/31=후배위/32=기승위/33=대면좌위/34=배면좌위/35=항문섹스)
;·TFLAG:ACT분류 == 3에 해당
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "성교")로 줍는 것이 가능
;·원칙으로서죠교자→죠교대상의삽입가 된다
;·넣으려고 하고 있는 곳에 즈음한다
;·미약·로션를 사용하는 케이스가 있다
;·체위의 제휴에 의해 TFLAG:ACT파생로 파생한다
;· 각 ACT 에 있어서 주도권은@SEX_INITIATIVE에서 정의된다
;-------------------------------------------------
;·페니스에 의한성교와페니스밴드에 의한성교의 가능성이 있다
;·텐프레로서는 굳이 테두리를 마련하지 않는다
;·필요에 응해 PENIS(TARGET)/! PENIS(TARGET) 혹은 TEQUIP:죠교자페니스밴드/! TEQUIP:죠교자페니스밴드등으로 나누어 쓰면 좋을 것입니다
;·처녀, 동정 이외의 경우로 첫회 분기를 마련하고 싶은 경우는 FIRSTACT(0)로
;-------------------------------------------------
;관련플래그…
;TFLAG:전회질싸
;(1=Ｖ성교/2=Ｖ성교봉사/3=Ｖ성교(드라이사정)/4=Ｖ성교봉사(드라이사정)/5=Ａ성교/6=Ａ성교봉사/7=Ａ성교(드라이사정)/8=Ａ성교봉사(드라이사정))
;-------------------------------------------------
;· 각 ACT에 있어서 TFLAG:ACT파생 == 0은 테두리를 마련하지 않고 조건 없음으로서 취급한다
;※조건 결정하고 중 했다경우, 보완이 책임을 다하지 않기 때문에
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는구상측의 RESULT(종단의 RETURN 1 등)에 의해구상가 표시당했다의지 아닌지,
;·또 개소 마다의 특유의 거동(미약등구상를 스킵 따위)을 제어하고 있습니다
;·@구상기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는구상측에서 결정하고 쳐 하는필요가 있습니다
;-------------------------------------------------
;관련 함수…
;FIRSTACT
;처음ACT 판정 함수
;TFLAG:ACT（TFLAG:90）를 자동으로 참조, 현재 실행되고 있는 ACT 마다번호가 독립해 보존된다
;
;ONCEACT
;조교중미독판정 함수  죠교전에 초기화된다
;그 외의 사양은 거의 FIRSTACT와 같이
;
;RANDACT
;랜덤이벤트의 표시순서 정리 함수
;전종 볼 때까지 같은 숫자는 돌려주지 않는다. 모두 표시하면 초기화
;@KOJO_ACT_KX1_Y1 마다관리 된다
;-------------------------------------------------

;────────────────────────────────────
;ACT30, 정상위
;────────────────────────────────────
;·정상위·죠교자가죠교대상에삽입 한다
;·주도권은 TARGET(SEX_INITIATIVE(TARGET))
;·죠교대상이남자의 경우는 발생하지 않는다
;·죠교자의후타나리화, 혹은페니스밴드장착에 의해 발생한다
;·TEQUIP(삼각목마/페니스밴드/바이브)장착중에는 발생하지 않는다
;·TFLAG:징계·플래그가 서 있으면 발생하지 않는다
;·턴방침가"하드", "이상"라면 발생하지 않는다
;·TEQUIP:성교중 == 5(배면좌위)로부터는 발생하지 않는다
;·TFLAG:ACT파생는전턴로부터의상태, 및 체위의 참조에 충당된다
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_30
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속실행/2=다른Ｖ성교로부터/3=Ａ성교로부터/4=Ｖ성교봉사로부터/5=Ａ성교봉사로부터)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT30, 정상위
;━━━━━━━━━━━━━━━━━━━━━━━━
;·REACT분류가날뛴다였던 경우는취소 된다
;·받아들였을 경우, MASTER가처녀라면처녀상실, TARGET가동정라면동정상실가 일어난다
;·v1. 010 현재, 거절했다경우의 SOURCE 리셋트 처리가 없다
;-------------------------------------------------
IF 구상기입체크(100)
	[SKIPSTART]
	;=================================================
	;처음
	;=================================================
	IF 구상기입 체크(0) && FIRSTACT(0)
	ENDIF
	
	;=================================================
	;ONCEACT
	;=================================================
	IF 구상기입 체크(0) && ONCEACT(0)
		SELECTCASE POLICY("금일") ;금일의죠교방침파생 처리
			CASE "통상"
				PRINTFORMW
			CASE "항문"
				PRINTFORMW
			CASE "징계·"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "노닥노닥"
				PRINTFORMW
			CASEELSE ;그 이외
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;전회질싸
	;=================================================
	IF 구상기입체크(100) && TFLAG:전회질싸 && TFLAG:전턴성교
		;범용 처리
		CALL NO9_PRINTTEXT_CATEGORY_3_ACT_1_K14
		RETURN 1
	ENDIF
	
	;G스팟 고문은정상위한정으로 한다. 정직, 다른 체위로 할 수 있는 거야? 라는 느낌이니까
	;타인의 일은 모르지만, 스스로는정상위 이외로 할 수 있을 생각은 하지 않고, 했다일도 없다
	;라고 할까 아무튼, ACT95"정상위시킨다"에도 확률로 날조 해도 좋을지도 모른다. 정의는보정치만이라서 말야
	;=================================================
	;연속(정상위로부터)
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 1
		;-------------------------------------------------
		;죠교자가가버려
		;-------------------------------------------------
		IF 구상기입체크(100) && TCVAR:TARGET:가버려
			;범용 처리（한정 없음구상측에서 부른다）
			CALL PRINTTEXT_CATEGORY_3_ACT_2_K14
			RETURN 1
		ENDIF
		
		;G스팟 고문은 여기에 가져오는 것이 좋은 것 같은 생각도 든다…
		;-------------------------------------------------
		;죠교자가정욕
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("정욕")
			;범용 처리（한정 없음구상측에서 부른다）
			CALL PRINTTEXT_CATEGORY_3_ACT_3_K14
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;하반신복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1
				PRINTFORMDW 앨리스는 착의 인 채의 사쿠야를 계속 범하고 있다…
				CALL QUAKE(4)
				
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「아, 아읏! 핫, 아앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				
				PRINTFORMW 「흐트러진 옷이 굉장히 이야라시이원이군요…조금 흥분해버리는 원, 이것…」
			;속옷를 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1
				PRINTFORMDW 앨리스는 사쿠야의 %INNER_B(MASTER)%(을)를 벗게 한 두, 비켜 놓은 상태인 채 행위를 계속했다…
				CALL QUAKE(4)
				
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「응아앗! 핫, 아앗")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				
				PRINTFORMW 「사랑스러운 속옷이 완전히 더러워져 버렸다 원이군요?」
				PRINTFORMW 「 그렇지만, 후후……꽤 좋은 바라봐라, 사쿠야…」
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리
		;-----------------------------------------
		;2/3의확률로표시
		IF RAND:3
			CALL QUAKE(4)
			
			CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「얏, 아앗")
			CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  응응, 쿠웃!」")
			
			PRINTFORMW 「후후, 사랑스러워요, 사쿠야. 키스 하고 싶어져버린다…」
			PRINTFORMDW 앨리스는 사쿠야의 얼굴을 끌어 들여 키스를 하면서 사쿠야를 계속 범했다…
		;1/3의 확률로 G스팟 고문
		ELSE
			PRINTFORMW 「응 사쿠야, 이런 건은 어떻게든 해들?」
			PRINTFORMDL 앨리스는 사쿠야의 허리를 끌어 들이고 제대로 고정하면,
			PRINTFORMDW 사쿠야의Ｇ스포트를 소폭의 움직임으로 밀어 올리도록(듯이) 꾸짖기 시작했다…
			PRINTL
			CALL QUAKE(2)
;OriginalString : (COLOR_K14("사쿠야"), "「あ゛ッ！ やだソコっ！ ふぁぁっ、あぁあっ！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아˝! (이)나다 거기! 후앗, 아아앗!」")
			PRINTFORMW 「후후, 키모치이이겠지? 좋아요, 좀 더 흐트러져 버려서」
			
			PRINTFORM 「%CNAME(MASTER, TARGET)%가 바보가 되어 버릴 정도로, 차분히 꾸짖어 주어요
			CALL PRINT_SYMBOL_K14
			
			;쾌Ｖ보정를 날조 한다
			TIMES SOURCE:MASTER:쾌Ｖ, 2.50
			;REACT에서의 참조용으로 KFLAG에 대입한다
			KFLAG:(FLAG:주인님):250 = 1
			;후의 판정을 위해서(때문에) 인크리먼트. 최종적으로는사쿠야씨의Ｖ둔감를 해소하는 것 같은 방향으로
			KFLAG:(FLAG:주인님):251++
			;이 부분은 FIRSTACT에 자른 이후는 별도 기준에서의 처리가 바람직한 것인지도?
			;FIRSTACT에 확률로 떨어뜨리도록(듯이)하면 그만한 정합성으로는 될 것 같다
		ENDIF
		RETURN 1
	ENDIF
	
	;배면좌위로부터는 파생하지 않는다
	;=================================================
	;체위 변경(Ｖ성교로부터)
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 2
		PRINTFORM 「이것이라면 귀부인을 좀 더 잘 볼 수가 있어요
		CALL PRINT_SYMBOL_K14
		;-------------------------------------------------
		;하반신복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1
				PRINTFORMW 「이라고 말하는지, 입은 채로의 사쿠야로 하는 것은 굉장히 흥분해요…」
;OriginalString : (COLOR_K14("사쿠야"), "「な、なんの話よ…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무, 무슨 이야기야…」")
				
				PRINTFORM 「좋아, 신경쓰지 마. 여기의 이야기이니까
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			;속옷를 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1 && !TEQUIP:눈가리개
				PRINTFORMW 「사쿠야도 이 (분)편이 안심하지 않는다?」
				PRINTFORMW 「이봐요, 저희들이 연결되고 있다고(면) 개 좋게 봐. 드로드로의 속옷이 굉장히 음란한 느껴라」
;OriginalString : (COLOR_K14("사쿠야"), "「…ん。ホント、なんかイヤラシイ…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…응. 정말, 뭔가 추잡하다…」")
				
				PRINTFORM 「이지요? 그러면, 그런 (뜻)이유, 그리고…… 아직도 계속해요
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리
		;-----------------------------------------
		PRINTFORMW 「이봐요, 좀 더 다리를 열어……그래, 좋아요. 후후」
		PRINTFORM 「, 마구 마음껏 찔러 주기 때문에 각오 해…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;Ａ성교로부터
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT파생 == 3
		;-------------------------------------------------
		;하반신복장이 요점 밀치고
		;-------------------------------------------------
		IF 구상기입체크(100) && PANTSCHECK(1, 1)
			;옷을 걷고
			IF LOWERJACKET(1) == 1
				PRINTFORMW 「이봐요, 좀 더 옷을 넘겨 다리를 열어…그래, 좋아요. 후후」
				PRINTFORMW 「그러면, 조속히 살릴 수 있어 주어요」
				PRINTFORMDW 앨리스는 스스로 옷을 넘겨, 기다리는 사쿠야의 %VAGINA_DESCRIPTION2(MASTER)%(을)를 관철했다…
				CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「あぁぅっ！ あぁ、入って…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아아웃! 아아, 들어가…」")
			;속옷를 비켜 놓아
			ELSEIF LOWERUNDER(1) == 1
				PRINTFORMW 「응 사쿠야…」
;OriginalString : (COLOR_K14("사쿠야"), "「な、なによ」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐, 뭐야」")
				PRINTFORMW 「속옷이 방해야. 스스로 비켜 놓아 주세요」
;OriginalString : (COLOR_K14("사쿠야"), "「なんで私がそんな事！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「어째서 내가 그런 일!」")
				PRINTFORMW 「그러면, 이대로 하지 않아도 괜찮은거야?」
;OriginalString : (COLOR_K14("사쿠야"), "「そ、それはっ！ その…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그것은! 그…」")
				PRINTFORMW 「나는 어느 쪽이라도 좋아? 모두는 귀부인 나름. 어떻게 하고 싶은 것인지 분명히 하세요, 사쿠야…」
;OriginalString : (COLOR_K14("사쿠야"), "「わかったわよ、やればいいんでしょう、やれば！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「알았어요, 하면 되겠지요, 하면!」")
				PRINTL
				PRINTFORMDW 사쿠야는 스스로%INNER_B(MASTER)%(을)를 비켜 놓아, 숙이면서 삽입을 애타게 기다리고 있다…
				PRINTFORMW 「후후, 조르고 있는 것 같고 좋은 모습이군요?」
;OriginalString : (COLOR_K14("사쿠야"), "「う、ウルサイわね！ いいからさっさと…んぁぁっ！？」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우, 시끄럽네요! 좋으니까 빨리… 응앗!?」")
				CALL QUAKE(1)
				PRINTFORMW 「어머나, 갑자기 지났는지 해들? 후후…」
			ENDIF
			RETURN 1
		ENDIF
		
		;-----------------------------------------
		;샌 것은 여기서 처리 AUTORAND(130)
		;-----------------------------------------
		;범용 처리
		CALL NO9_PRINTTEXT_CATEGORY_3_ACT_2_K14
		RETURN 1
	ENDIF
	
	;기본적으로사정 할 때까지는 다른행동가 되는 일은 없기 때문에
	;페니스밴드에 의한성교봉사로부터의 파생, 이라고 하는 안배
	;TFLAG:ACT파생 == 4는 0616판 시점에서, 거의 무의미한상태라고 생각되고
	;=================================================
	;Ｖ성교봉사/Ａ성교봉사로부터
	;=================================================
