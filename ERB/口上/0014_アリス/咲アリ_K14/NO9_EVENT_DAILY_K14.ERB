;스킨십 0_4몸을 맡겨 젖가슴으로
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 4
;OriginalString : (COLOR_K14("사쿠야"), "「ちょ、ちょっと엘리스！ どこ触ってるのよ！」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「잠, 잠깐 앨리스! 어디를 손대고 있는거야!」")
	PRINTFORMW 「사쿠야가 좋다고 말했어요?」
	PRINTL
;OriginalString : (COLOR_K14("사쿠야"), "「いや、確かに、言いま했다けど…」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아니, 확실히, 말했지만…」")
	PRINTFORMW 「그렇지? 그러면, 얌전히 당해 주세요」
	PRINTL
;OriginalString : (COLOR_K14("사쿠야"), "「ま、まだ、するの…？」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「계속, 할꺼야…?」")
	PRINTFORM 「물론
	CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
	PRINTFORMW  사쿠야가 가버릴때 까지 그만두지 않아요? 후후」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;스킨십 0_5팬티갈아입게 해 했다
