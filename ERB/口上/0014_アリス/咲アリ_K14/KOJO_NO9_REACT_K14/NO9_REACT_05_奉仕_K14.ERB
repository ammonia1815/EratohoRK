;========================================================================
;봉사계 REACT50～57
;========================================================================
;· 각 REACT내의 나누는 방법으로서 TFLAG:REACT분류(IS_COMGRONAME)나 SELECTCOM(IS_NOWCOMNAME)가 생각됩니다
;·다만, 범위의 넓은 조건으로 주워 버리는 것은 기본적으로 추천하지 않습니다
;·ACT분류:봉사계의 경우, 봉사계 COM(COM40~COM44)를 중심으로, ACT에 따라서는애무계 COM가선택지가 됩니다만
;·상황에 따라서는 그 외의 것이선택지에 포함될 가능성도 있습니다
;·리액션(REACT)으로 처리되지 않았던 것은 범용 리액션(COM)으로 보완되므로
;·무리하게 모두 묻는필요는 없습니다
;·필요라고 생각하는 것을 좁혀, COM와의 제휴를 생각해 효율 좋게 구축해야 합니다
;-------------------------------------------------
;  REACT의 지문은 ACT_MESSAGE.ERB의 TRAIN_MESSAGE2_{TFLAG:ACT}로 처리됩니다
;  기본적으로 지문은 TFLAG:REACT분류(적극적으로 한다/태연하게 한다/용서를 빌다/소극적으로 한다)로 분기 해,
;  각 REACT분류내에서 TFLAG:REACT부하(자연스러운행동/무리가 없는행동/무리를했다행동)로 파생
;  고유의 TRAIN_MESSAGE2가 없는 경우는, REACTION_MESSAGE로 COM마다 범용 처리됩니다
;  ACT_MESSAGE.ERB REACTION_MESSAGE.ERB를 참조하면서의 작업을 추천합니다
;-------------------------------------------------

;=================================================
;관련 변수/함수등…
;
;TFLAG:REACT분류 (0=없음/1=소극적으로 한다/2=적극적으로 한다/3=용서를 빌다/4=날뛴다/5=요구에 응하지 않는, 거부/6=도망친다, 발버둥 친다)
;IS_COMGRONAME（"없음/소극적으로 한다/적극적으로 한다/용서를 빌다/날뛴다/거부/도망친다"）
;
;CONDITION (상태치로부터 캐릭터 라인을취득 한다  TFLAG:60, 61으로 TARGET와 MASTER를 별개에 참조)
;
;TENSION (BASE치를 대략적으로 나타낸다  0, 체력, 1, 기력, 2, 사정, 3, 모유, 4, 소변, 5, 이성, 6, 흥미, 7, 초조함, 8, 만족)
;50% 이상은 높은,25%이상은 일반,25% 미만은 낮다

;FIRSTREACT
;처음ACT 판정 함수
;TFLAG:ACT（TFLAG:90）를 자동으로 참조, 현재 실행되고 있는 REACT 마다번호가 독립해 보존된다
;
;ONCEREACT
;조교중미독판정 함수  죠교전에 초기화된다
;그 외의 사양은 거의 FIRSTREACT와 같이
;=================================================

;────────────────────────────────────
;NO9_REACT50, 손으로 애무를 강요·개별 리액션
;────────────────────────────────────
;·TFLAG:ACT파생(0=애무강제/1=수음강제/2=페니스밴드수음강제)
;-------------------------------------------------
;상정되는 COM…
;애무계 반응
;	COM:부끄러워한다
;봉사계 반응
;	COM:거부한다/소극적으로 한다/적극적으로 한다/애정을 담아 한다/태연하게 한다
;그 외 반응
;	COM:자위를 시작한다
;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_50
;-------------------------------------------------
;·지문은 REACT분류와 ACT파생로 분기
;·REACT분류:받아들이고는 REACTION_MESSAGE로 처리된다
;·REACT분류가소극적으로 한다/태연하게 한다/적극적으로 한다/받아들인다/용서를 빌다인 경우는 수용 취급
;·REACT분류가거부/날뛴다/도망치는 것 경우는 거절했다취급이 된다
;-------------------------------------------------
;·범용 처리는 ACT파생의 상층에서 정리해 처리한다
;-------------------------------------------------

;범용 처리. 부하 2라면 COM에 던진다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 요구에 응하지 않는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("거부/날뛴다/도망친다")
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K14
	RETURN 1
ENDIF

;범용 처리. REACT부하도 함수내에서 처리하므로 COM에 흘리지 않고 여기서 완결시킨다
;COM이루는대로라면 범용 리액션(COM)에
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && !IS_NOWCOMNAME("이루는대로")
	CALL PRINT_COMGRO_4_K14
	RETURN 1
ENDIF

;-----------------------------
;이하, ACT파생 마다의 처리
;-----------------------------

;일부에 수정을 더해, TFLAG:ACT파생 == 1으로 2를 같은 테두리로 처리해 본다
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT50_1 & 2, 수음/페니스밴드수음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && GROUPMATCH(TFLAG:ACT파생, 1, 2)
	;REACT파생 1, 「소극적인의가 마음에 들지 않는다」는 연주한다
	;=================================================
	;소극적으로 한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("소극적으로 한다") && IS_COMIMPNAME("약간좋은인상") && TFLAG:REACT부하 < 2
		;-------------------------------------------------
		;죠교자가 갈 것 같다
		;-------------------------------------------------=
		IF 구상기입체크(100) && TRUECHECK(TCVAR:TARGET:가버려, PALAM:쾌Ｖ > 8500) > 0
			;죠교자만족도 파생
			SELECTCASE TENSION(TARGET, 8)
				;50% 이상
				CASE "높다"
					PRINTFORMW 「, 어젯밤! 나, 이제 안 됨……」
;OriginalString : (COLOR_K14("사쿠야"), "「この程度でよくなっちゃうなら世話ないわね」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이 정도로 좋아져 버린다면 주선 없네요」")
					PRINTFORMW 「말해 주어요…뭣하면 그 입을 코레로 막아도 괜찮아…」
				;25% 이상
				CASE "일반"
					PRINTFORMW 「몇번  있고 조절인 느낌이 드는데…」
;OriginalString : (COLOR_K14("사쿠야"), "「そうは言ってもね？ 本気を出했다らすぐにイっちゃいそうだし」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇게는 말해도 말야? 진심을 출했다들 곧바로 가 버릴 것 같고」")
					PRINTFORMW 「…조금 화난다. 그렇지만 정말로 이제 나와버릴 그렇게…」
				;25% 이하
				CASE "낮다"
					PRINTFORMW 「…상당히 소극적이구나. 그렇지만무슨……이제\@ TFLAG:ACT파생 == 1 ? 출 # 이 \@좋을 것 같다…」
;OriginalString : (COLOR_K14("사쿠야"), "「あら、他愛無いわね？ 엘리스」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아라, 철없네요? 엘리스」")
					PRINTFORMW 「우르사이원이군요. 좋으니까 좀 더 성실하게 하세요!」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;이하, 죠교자상태변화를 처리
		;-------------------------------------------------
		;정욕/울굴/몽롱
		;-------------------------------------------------
		IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(TARGET), "정욕", "울굴", "몽롱") > 0
			IF TRUECHECK(PALAM:쾌Ｃ > 5000, PALAM:쾌Ｖ > 5000) > 0
				PRINTFORMW 「나쁘지 않은데……좀 더 분명하게 시라고 갖고 싶네요」
				PRINTFORMW 「그러면 어딘지 부족해, 사쿠야…」
			ELSE
				PRINTFORMW 「응, 좀 더 진지하게 해 주지 않을까」
;OriginalString : (COLOR_K14("사쿠야"), "「これでも真面目にやってるつもりだけど？」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이것이라도 성실하게 하고 있을 생각이지만?」")
				PRINTFORMW 「어떻게 봐도 그렇게는 생각되지 않아요…」
			ENDIF
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;분노
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("분노")
			PRINTFORM 「…완전히. 진짜 사용할 수 없다
			CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "개", 1)
			PRINTFORMW 그런데」
			PRINTFORMW 「좀 더 제대로 할 수 없는거야? 꾸물거리고 미련하고 어쩔 수 없는 느낌이예요」
;OriginalString : (COLOR_K14("사쿠야"), "「…言ってくれるわね。そんなに私を怒らせたいのかしら」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…말해 주어요. 그렇게 나를 화나게 하고 싶은 것일까」")
			PRINTFORMW 「화내고 있는 것은 여기야! 바보!」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;싫증
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("싫증")
			PRINTFORMW 「상당히 의지가 없을 것 같구나. 적당 그러면 싫증하지만?」
;OriginalString : (COLOR_K14("사쿠야"), "「悪かったわね、싫증で」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「나빴지요,싫증그래서」")
			PRINTFORMW 「정말로 나쁘다고 생각한다면 태도로 가리켰으면 좋아요. 말은 아니고, 군요」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;피폐/쇠약/무기력
		;-------------------------------------------------
		IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(TARGET), "피폐", "쇠약", "무기력") > 0
			PRINTFORMW 「이라고 할까……」
			PRINTFORMW 「사쿠야의 의지가 없음을 보고 있으면, 여기까지 꼭 좋아져 유곽이군요」
			RETURN 1
		ENDIF
		
		;이 개소만 확률로 범용 리액션(COM)에 날린다
		;-------------------------------------------------
		;통상
		;-------------------------------------------------
		IF 구상기입체크(60) && T_COND("통상")
			IF TRUECHECK(PALAM:쾌Ｃ > 4000, PALAM:쾌Ｖ > 4000) > 0
				PRINTFORMW 「응, 그 나름대로 나쁘지 않은데…」
				PRINTFORMW 「그 의지가 없는 느낌은 너무 좋은 기분은 하지 않네요…」
			ELSE
				PRINTFORMW 「이봐요, 그렇지 않아서…좀 더 이렇게」
				PRINTFORMW 「완전히, 안타까운도 참 짝이 없네요!」
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	
	;부하 2는 연주한다. REACT파생는 없기 때문에 고려하지 않는다
	;태연하게 한다의 경우는 확률을 짠다
	;=================================================
	;적극적으로 한다/태연하게 한다
	;=================================================
	IF 구상기입체크(100) && (IS_NOWCOMNAME("적극적으로 한다") || IS_NOWCOMNAME("태연하게 한다") && !RAND:2) && TFLAG:REACT부하 < 2
		;ACT파생 == 2, 페니스밴드수음때는 연주한다
		;여기 쓴 시점에서는페니스밴드수음는 존재하고 있지 않았다
		;-------------------------------------------------
		;죠교자가 갈 것 같다
		;-------------------------------------------------=
		IF 구상기입체크(100) && TFLAG:ACT파생 == 1 && TCVAR:TARGET:가버려
			;죠교자BASE:만족파생
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「그런데, 사쿠야……나, 이제 이크…」
					IF ABL:MASTER:정액중독 > 3
						CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「후후, 알았다. 조금 격렬하게 시라고 주어요")
						CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
;OriginalString : (COLOR_K14("사쿠야"), "「遠慮なくぶちまけていいわよ」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「사양말고 털어 놓아도 좋아요」")
					ELSE
						SELECTCASE RANDREACT(2, 0)
							CASE 0
;OriginalString : (COLOR_K14("사쿠야"), "「いいわよ、でも……」")
								CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋아요, 그렇지만……」")
;OriginalString : (COLOR_K14("사쿠야"), "「あまり汚さないようにして頂けるかしら」")
								CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그다지 더럽히지 않게 해 받을 수 있을까」")
								PRINTFORMW 「재미있는 일 말해요……, 뭣하면 마음껏 뿌려도 괜찮지만…」
							CASE 1
;OriginalString : (COLOR_K14("사쿠야"), "「珍しく可愛い事言うわね？」")
								CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「드물고 사랑스러운 일 말해요?」")
								PRINTFORMW 「얼버무리고 있다고(면) 화내요……」
;OriginalString : (COLOR_K14("사쿠야"), "「あら、ご免なさい♪ フフ」")
								CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아라, 미안해요♪ 후후」")
						ENDSELECT
					ENDIF
				CASE "일반"
					PRINTFORMW 「이나, 나다 사쿠야! 그렇게 시등…」
					IF ABL:MASTER:정액중독 > 3 && !TEQUIP:눈가리개
;OriginalString : (COLOR_K14("사쿠야"), "「そうは言ってもね？ 私もそろそろ貴女がイクとこ見たいんだけど」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇게는 말해도 말야? 나도 슬슬 당신이 가는 곳 보고 싶지만」")
;OriginalString : (COLOR_K14("사쿠야"), "「호라、少し強めにシてあげるから」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이봐요, 조금 강하게 해에 시라고 주기 때문에」")
					ELSE
;OriginalString : (COLOR_K14("사쿠야"), "「そう…もうイクのね」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그래…이제(벌써) 가군요」")
;OriginalString : (COLOR_K14("사쿠야"), "「それはいいけど、あまり飛ばさないで欲しいわ」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그것은 좋지만, 그다지 날리지 않으면 좋아요」")
						PRINTFORMW 「제멋대로인 일 말하고 있다고(면) 입에 돌진해요…」
					ENDIF
				CASE "낮다"
					PRINTFORMW 「, 어젯밤! 나, 이제……」
					IF ABL:MASTER:정액중독 > 3
;OriginalString : (COLOR_K14("사쿠야"), "「わかった、가게해줘あげるわ」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「안,가게해줘주어요」")
;OriginalString : (COLOR_K14("사쿠야"), "「どれだけ出るのか愉しみね？ フフ」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「얼마나 나오는지 기다려지구나? 후후」")
					ELSE
;OriginalString : (COLOR_K14("사쿠야"), "「そう、仕方無いわね」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그래, 방법 없네요」")
;OriginalString : (COLOR_K14("사쿠야"), "「でも私、あまり好きじゃないのよねコレ。손も汚れるし」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그렇지만 나, 너무 좋아하지 않아군요 이것.손도 더러워지고」")
						PRINTFORMW 「그런 일 말하고 있다고(면) 얼굴에 걸쳐요…」
					ENDIF
			ENDSELECT
			RETURN 1
		ENDIF
		
