					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  기다려! 앗, 안됏!」")
;OriginalString : (COLOR_K14("사쿠야"), "（そんな声聞か당했다ら待てる訳ないじゃない、후후……）")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "（그런 소리 문인가당했다들 기다릴 수 있을 이유 없지 않은, 후후……）")
				CASE "일반"
					PRINTFORMW 「아! 하……(이)나! (이)나다 사쿠야, 거기!」
;OriginalString : (COLOR_K14("사쿠야"), "「やだって事は、もっとシて欲しいって事よね？ 호라、こう？」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「싫다는 일은, 좀 더 시라고 갖고 싶다는 일이군요? 이봐요, 이렇게?」")
					;페이드 아웃
					CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("애무"), 2)%")
