;========================================================================
;수치계 REACT40～45
;========================================================================
;· 각 REACT내의 나누는 방법으로서 TFLAG:REACT분류(IS_COMGRONAME)나 SELECTCOM(IS_NOWCOMNAME)가 생각됩니다
;·다만, 범위의 넓은 조건으로 주워 버리는 것은 기본적으로 추천하지 않습니다
;·ACT분류:수치계의 경우, 기본적으로회화계 COM, 애무계 COM, 봉사계 COM가선택지가 됩니다만
;·상황에 따라서는 그 외의 것이선택지에 포함될 가능성도 있습니다
;·리액션(REACT)으로 처리되지 않았던 것은 범용 리액션(COM)으로 보완되므로
;·무리하게 모두 묻는필요는 없습니다
;·필요라고 생각하는 것을 좁혀, COM와의 제휴를 생각해 효율 좋게 구축해야 합니다
;-------------------------------------------------
;  REACT의 지문은 ACT_MESSAGE.ERB의 TRAIN_MESSAGE2_{TFLAG:ACT}로 처리됩니다
;  기본적으로 지문은 TFLAG:REACT분류(받아들인다/적극적으로 한다/태연하게 한다/용서를 빌다/소극적으로 한다)로 분기 해,
;  각 REACT분류내에서 TFLAG:REACT부하(자연스러운행동/무리가 없는행동/무리를했다행동)로 파생
;  고유의 TRAIN_MESSAGE2가 없는 경우는, REACTION_MESSAGE로 COM마다 범용 처리됩니다
;  ACT_MESSAGE.ERB REACTION_MESSAGE.ERB를 참조하면서의 작업을 추천합니다
;-------------------------------------------------
;=================================================
;관련 변수/함수등…
;
;TFLAG:REACT분류 (0=없음/1=소극적으로 한다/2=적극적으로 한다/3=용서를 빌다/4=날뛴다/5=요구에 응하지 않는, 거부/6=도망친다, 발버둥 친다)
;IS_COMGRONAME（"없음/소극적으로 한다/적극적으로 한다/용서를 빌다/날뛴다/거부/도망친다"）
;
;FIRSTREACT
;처음ACT 판정 함수
;TFLAG:ACT（TFLAG:90）를 자동으로 참조, 현재 실행되고 있는 REACT 마다번호가 독립해 보존된다
;
;ONCEREACT
;조교중미독판정 함수  죠교전에 초기화된다
;그 외의 사양은 거의 FIRSTREACT와 같이
;=================================================

;────────────────────────────────────
;NO9_REACT40, 자위·개별 리액션
;────────────────────────────────────
;·ACT파생없음. PENIS(MASTER)나 TEQUIP:페니스밴드로 나누는 일은 가능
;-------------------------------------------------
;상정되는 COM…
;회화계 반응
;	COM:용서를 빌다
;애무계 반응
;	COM:부끄러워한다/싫어한다/겁없이 웃는다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;봉사계 반응
;	COM:거부한다/소극적으로 한다/적극적으로 한다/태연하게 한다
;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_40
;-------------------------------------------------
;·지문은 REACT분류로 분기
;·REACT분류가소극적으로 한다/태연하게 한다/적극적으로 한다/받아들이는인 경우는 수용 취급
;·그 외의 반응의 경우는 REACT분류는 모두거부에 세트 된다(용서를 빌다도거부취급인 일에 주의)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상가 거절했다경우
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("거부")
	;=================================================
	;부끄러워서 명령 대로로 할 수 없었다（(치정선행）
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:치정 > PALAM:MASTER:반항
		;BASE:만족파생		초조함로 주워도 괜찮을지도이지만, BASE:7은변화가 적기 때문에
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「은? 뭐야. 부끄럽고 할 수 없다?」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 아직도 수치심은 것을 가지고 있었군요」
				PRINTFORMW 「약간의 놀라, 라는 곳일까. 응? 후후」
			CASE "일반"
				PRINTFORMW 「뭐, 확실히 부끄럽겠지요지만…」
				PRINTFORMW 「그러한 일 되면(자) 퇴색해. 트마라나이원이군요,%CNAME(MASTER, TARGET)%……」
			CASE "낮다"
				PRINTFORMW 「그렇게,%CNAME(MASTER, TARGET)%(을)를 부끄러워하게 하는 것도 나쁘지 않은데. 그렇지만, 이군요…」
				PRINTFORMW 「말해진 일을 하지 않으면%CNAME(MASTER, TARGET)%에 있어 바람직하지 않은 결과가 되어요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;싫어해 요구에 응하지 않는다（반항선행）
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:반항 > PALAM:MASTER:치정 
		;BASE:만족파생
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「그다지 칭찬할 수 있던 것이 아니네요, 그러한 태도…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 자신의 입장을 분별하는 것이 좋아요」
			CASE "일반"
				PRINTFORMW 「말하는 것이 (들)물을 수 없다면, 상응하는 처사를 하는 일이 되는데…」
				PRINTFORMW 「뭐, 방법 없어요?  %CNAME(MASTER, TARGET)%가 바란 결과야 것」
			CASE "낮다"
				PRINTFORMW 「 나에 따를 수 없다고 말한다면, 상응하는 보답을 받아 주어요」
				PRINTFORMW 「이유는 어때라고 해 원.%CNAME(MASTER, TARGET)%(은)는, 자신의 행동에 대해서 벌을 받아」
				CALL FADEIN(12, "「각오 하세요……」")
				WAIT
				TCVAR:행동예약 = 60
		ENDSELECT
		RETURN 1
	ENDIF
RETURN 0
ENDIF

;TEQUIP:눈가리개/거울/야외플레이시는 연주한다
;쾌락에 대해서부끄러워한다경우도 연주한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;부끄러워한다（상황에부끄러워한다）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && (IS_NOWCOMNAME("부끄러워한다") && !TFLAG:REACT파생) && !TRUECHECK(TEQUIP:눈가리개, TEQUIP:거울, TEQUIP:야외플레이)
	SELECTCASE RANDREACT(2, 0)
		CASE 0
			PRINTFORMW 「부끄러우면 그만두면 좋다고 생각하는데」
			PRINTFORMW 「그렇지만, 손은 멈추지 않아?  \@ AFFECTION("상사상애") ? 후후 # 후후 \@」
		CASE 1
			PRINTFORMW 「어머나, 그렇게 부끄러워하는 일 없어요」
			PRINTFORM 「지금의 %CNAME(MASTER, TARGET)%,\@ AFFECTION("상사상애") ? 매우 # 꽤 \@사랑스러워요
			CALL PRINT_SYMBOL_K14
	ENDSELECT
	RETURN 1
ENDIF

;소극적인의가 마음에 들지 않는다（징계·포인트가산）에 파생했다등 연주한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;소극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("소극적으로 한다") && TFLAG:REACT파생 < 1 && TFLAG:REACT부하 < 2
	IF AFFECTION("상사상애")
		PRINTFORMW 「이제 와서 부끄러워하는 일 없지요…?」
		PRINTFORM 「 그렇지만, 그런 곳도 사랑스럽네요
		CALL PRINT_SYMBOL_K14
	ELSEIF AFFECTION("익애")
		PRINTFORMW 「부끄러운 듯이  , 자신을 위로하고 있는 모습이 감등 없네요…」
		PRINTFORMW 「후후후 후후……」
	ELSEIF AFFECTION("권애")
		PRINTFORMW 「싫어해도 결국 하군요…」
		PRINTFORMW 「후후후……」
	ELSE
		SELECTCASE RANDREACT(2, 1)
			CASE 0
				PRINTFORMW 「이봐요, 좀 더 잘 보이도록 하세요」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 어떻게 자신을 위로하는지, 확실히 지켜봐 주어요」
			CASE 1
				PRINTFORMW 「남의 앞에서 자위라든지 부끄럽지 않은거야…?」
				PRINTFORM 「아무튼, 내가 시켜이지만…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;무리를했다행동라면 COM에
;━━━━━━━━━━━━━━━━━━━━━━━━
;적극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("적극적으로 한다") && TFLAG:REACT부하 < 2
	IF AFFECTION("상사상애")
		SELECTCASE RANDREACT(2, 2)
			CASE 0
				PRINTFORMW 「그렇게 격렬하게 해 괜찮아? 라고, 생각이 드는데…」
				PRINTFORMW 「나로 보여지는 것이 그렇게 흥분할까, 군요?」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RANDTEXT_K14("クスクス", "ふふ")%…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RANDTEXT_K14("킥킥", "후후")%…」")
				CLEARLINE 1
			CASE 1
				PRINTFORMW 「후후, 좋아요. 그래…, 좀 더」
				PRINTFORM 「%CNAME(MASTER, TARGET)%의 한심한 모습, 나에게 자주(잘) 보여 주세요
				CALL PRINT_SYMBOL_K14
			CASE 2
				PRINTFORMW 「좋아요? 좀 더 격렬하게 시라고도. 후후…, 그렇게 자주. 그 상태」
				PRINTFORMW 「다만 착각 하지 않고 갖고 싶지만…, 마음대로 이크의 것은 허락하지 않으니까」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RANDTEXT_K14("ふふっ", "フフ")%……」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RANDTEXT_K14("후후", "후후")%……」")
				CLEARLINE 1
		ENDSELECT
	ELSEIF AFFECTION("익애")
		SELECTCASE RANDREACT(2, 3)
			CASE 0
				PRINTFORM 「후후…멋져요
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 스스로 시 비치는 곳, 분명하게 보고 있어 주어요」
			CASE 1
				PRINTFORMW 「나로 보여져 기쁜 듯하구나? 진짜, 구제할 길 없어요…」
				PRINTFORM 「 그렇지만 방법 없어요.%CNAME(MASTER, TARGET)%…, 볼 수 있어 기쁨헨타이니까
				CALL PRINT_SYMBOL_K14
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후…」")
				CLEARLINE 1
		ENDSELECT
	ELSEIF AFFECTION("권애")
		SELECTCASE RANDREACT(2, 4)
			CASE 0
				PRINTFORM 「어머나, 적극적이군요
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「후후…, 좋아요.%CNAME(MASTER, TARGET)%의 정 없는 모습, 확실히 보고 있어 준다…」
			CASE 1
				PRINTFORMW 「좋은 상태군요? 후후…」
				PRINTFORMW 「그렇게,\@ !TEQUIP:눈가리개 ? 눈 # 얼굴 \@(을)를 돌리지 마. 확실히 여기\@ !TEQUIP:눈가리개 ? 를 봐주는거야 # 에 향하고 하는거야 \@」
				PRINTFORM 「 나는 차분히 감상시켜 받기 때문에…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ELSE
		SELECTCASE RANDREACT(2, 5)
			CASE 0
				PRINTFORMW 「수치않다 빌려주어 기분도 없고, 자주(잘) 해요?」
				PRINTFORMW 「아무튼, 내가 말할 수 있던 의리가 아닌데. 후후…」
			CASE 1
				PRINTFORMW 「에, 적극적인 것? 나로 보여져 흥분하고 있는 거나」
				PRINTFORM 「 그렇지만 아무튼…, 후후. 나쁘지 않은 경치군요
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
;NO9_REACT40, 자위

;────────────────────────────────────
;NO9_REACT41, 비패개장·개별 리액션
;────────────────────────────────────
;·ACT파생없음
;-------------------------------------------------
;상정되는 COM…
;회화계 반응
;	COM:용서를 빌다
;애무계 반응
;	COM:부끄러워한다/싫어한다/겁없이 웃는다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_41
;-------------------------------------------------
;·지문은 REACT분류(받아들인다, 적극적으로 한다, 태연하게 한다/용서를 빌다/소극적으로 한다)로 분기
;·ACTION_APLLY2에 고유의 처리 없음
;-------------------------------------------------

;REACT파생 1, 「의견을 내다니 좋은 번가슴군요!」는 연주한다
;REACT분류:용서를 빌다도 줍는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;용서를 빌다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && (IS_NOWCOMNAME("용서를 빌다") && !TFLAG:REACT파생) || IS_COMGRONAME("용서를 빌다")
	PRINTFORMW 「후후, 안되어요. 그런 일 말해도 허락해 주지 않아요」
	PRINTFORM 「이봐요, 좀 더 손가락으로 열려 준다
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;부끄러워한다（상황에부끄러워한다）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(50) && (IS_NOWCOMNAME("부끄러워한다") && !TFLAG:REACT파생) && !TRUECHECK(TEQUIP:눈가리개, TEQUIP:거울, TEQUIP:야외플레이)
	;윤활 충분히
	IF PALAM:MASTER:윤활 >= 500
		PRINTFORMW 「안쪽으로부터 넘쳐 나오고 해 멈추지 않는 것 같구나…」
		PRINTFORMW 「응…, 부끄럽지만, 나로 보여져 흥분하고 있는 지요」
		PRINTFORMW 「정직하게 말할 수 있으면, 좀 더 좋은 일 해 주어도 괜찮아?」
		PRINTFORMW 「\@ AFFECTION("상사상애") ? 후후후 # 우후후 \@…」
	;윤활불발
	ELSE
		PRINTFORMW 「후후, 조금씩 젖어 와」
		PRINTFORM 「부끄러워해 보였더니, 몸에 거짓말은 붙이지 않는 것 같구나
		CALL PRINT_SYMBOL_K14
	ENDIF
	RETURN 1
ENDIF

;용서를 빌다취급의 경우는 연주한다
;TFLAG:REACT부하 == 2의 경우는 연주한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;쾌감을 참는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && (IS_NOWCOMNAME("쾌감을 참는다") && TFLAG:REACT부하 < 2) && !IS_COMGRONAME("용서를 빌다")
	SELECTCASE RANDREACT(2, 0)
