;OriginalString : (COLOR_K14("사쿠야"), "「んんっ！ あ、엘리스…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응읏! 아, 엘리스…」")
			CALL FADE(20, COLOR2_K14("SAKUYA"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 2)%」")
			;행 삭제
			CLEARLINE 1
			CALL PRINT_REACT_15_01
