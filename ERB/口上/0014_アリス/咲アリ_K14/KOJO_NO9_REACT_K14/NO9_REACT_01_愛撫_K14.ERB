			CALL FADE(20, COLOR2_K14("SAKUYA"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 2)%」")
			;행 삭제
			CLEARLINE 1
			CALL PRINT_REACT_15_01
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;기승위
		;-------------------------------------------------
		IF 구상기입체크(100) && TRUECHECK(TEQUIP:성교중 == 3, TEQUIP:성교봉사중 == 3) > 0
