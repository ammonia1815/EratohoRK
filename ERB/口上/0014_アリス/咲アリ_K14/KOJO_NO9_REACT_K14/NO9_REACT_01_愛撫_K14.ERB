					PRINTFORM 「어머나, 상당히 솔직하지 않다? 라면, 좋은 아이에게는 포상을 주지 않으면…
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					;페이드 아웃
					CALL FADE(19, COLOR2_K14("ALICE"), COLOR_K14("black"), "츗…")
					;행 삭제
					CLEARLINE 1
;OriginalString : (COLOR_K14("사쿠야"), "「もうっ、馬鹿…子供じゃないんだから…」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「이제(벌써), 바보…아이가 아니기 때문에…」")
					PRINTFORMW 「…… 그렇지만 진짜는 싫지 않다. 다르다?」
;OriginalString : (COLOR_K14("사쿠야"), "「う、ウルサイわよ！」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우, 시끄러워요!」")
					PRINTFORMW 「네네 안, 여기까지로 해 주어요…♪」
					;키스를 날조
					CALL SET_KISS
			ENDSELECT
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;분노
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("분노")
;OriginalString : (COLOR_K14("사쿠야"), "「…っく！ もっと丁寧にして欲しいわね…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「… 읏쿠! 좀 더 정중하게해 주었으면 하네요…」")
			PRINTFORMW 「나를 화나게 해 둬, 평상시 대로가 기대되어도 곤란해요?」
			PRINTFORMW 「그만한 보답은 받아 주어요, 나쁜데…」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;싫증
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("싫증")
			;큰 다랑어가 환상 들어가고 있는지, 사쿠야가 왜 모르는 것인지, 엘리스가 왜 알고 있는지인가, 우선 내던진다
			;설명하면 쓸데없게 길어질 것 같고
			;아무튼 지금의 느낌이라면 비교적 뭐든지 있음이라고 해석해도 문제 없는 것 같은 것이긴 하구나w
			PRINTFORMW 「바뀐 보람의 하지 않는 싫증한 반응이군요? 이제 아주 싫증이 나라, 사쿠야」
;OriginalString : (COLOR_K14("사쿠야"), "「なっ！？ それは聞き捨てならないわね…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫!? 그것은 묵과할 수 없네요…」")
			PRINTFORMW 「그렇다면 좀 더 귀염성이 있는 반응하면 어떻게야? 이 마구로녀!」
			CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「그 『마구로』가 무엇으로 있을까는 모르지만…」")
;OriginalString : (COLOR_K14("사쿠야"), "「酷い侮辱というのは私にも良く判るわよ！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「심한 모욕이라고 하는 것은 나에게도 자주(잘) 알아요!」")
			PRINTFORMW 「유석소쇄인 종자는 총명해 계(오)시는 것 같아, 말에는 조심하지 않으면? 후후…」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;피폐/쇠약/무기력
		;-------------------------------------------------
		IF 구상기입체크(100) && GROUPMATCH(GET_CONDITION(TARGET), "피폐", "쇠약", "무기력") > 0
;OriginalString : (COLOR_K14("사쿠야"), "「なによ、随分いい加減ね…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 상당히 적당이네…」")
			PRINTFORMW 「지쳐있는 때에 그다지 당치 않음 말하지 않으면 좋네요?」
			PRINTFORMW 「그렇게 불만이라면 스스로 만지작거리면 좋다고 생각하는데」
;OriginalString : (COLOR_K14("사쿠야"), "「勝손な言い草ね、엘리스…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「승손말투군요,엘리스…」")
			PRINTFORMW 「그렇구나? 그렇지만 피차일반이야, 사쿠야」
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;통상
		;-------------------------------------------------
		IF 구상기입체크(100) && T_COND("통상")
			;죠교대상쾌Ｂ파생
			SELECTCASE PALAM:MASTER:쾌Ｂ
				CASE IS >= 8500
;OriginalString : (COLOR_K14("사쿠야"), "「ま、待って엘리스！ そんなに당했다ら…も、もうっ！」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「기, 기다려엘리스! 그렇게당했다들… 이, 이제(벌써)!」")
					PRINTFORMW 「어떻게 되어버린다의? 이봐요, 입다물고 있어 알지 않아요」
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「그런 일, 말할 수 있다! 야, 안돼엣! 아")
					CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  아!」")
					PRINTFORM 「후후, 말하지 않아도 알고 있었지만, 이군요…
					CALL PRINT_SYMBOL_K14
				CASE IS >= 4000
					CALL QUAKE(1)
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「앗, 얏")
					CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "  아…… 아아…… 아, 앗!」")
					PRINTFORMW 「상스러운 소리 내 버려서… 지금의 모습, 타인에게는 보여지지 않네요?」
;OriginalString : (COLOR_K14("사쿠야"), "「やめてよ、そういう事言うの…」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그만두어요, 그런 일 말해…」")
					;1/3의확률로표시
					IF !RAND:3
						PRINTFORMW 「걱정하지 않아 좋아요? 어차피 누구에게도 보일 생각은 없으니까」
						PRINTFORMW 「그러니까… 나의 앞에서 사양하는 일은 없어? 귀부인의 모두, 나에게 보이게 해 주세요」
;OriginalString : (COLOR_K14("사쿠야"), "「流石に恥ずかしいわ、そういうの…」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「과연 부끄러워요, 그러한 것…」")
						PRINTFORMW 「소쇄인 메이드장은 부끄럽게 여기기인 것?」
;OriginalString : (COLOR_K14("사쿠야"), "「いや、だから…もうホントにやめて頂戴」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아니, 그러니까…이제(벌써) 정말 그만두어 줘」")
						PRINTFORM 「후후후…거기까지 말한다면 방법 없네요…
						CALL PRINT_SYMBOL_K14
					ENDIF
				CASEELSE
					PRINTFORMW 「사양은 하지 않아 좋아요… 이봐요, 좀 더 목소리를 들을만해?」
					CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「んんっ！ やっ、あっ！ んあぁぁっ！」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응읏! 얏, 앗! 응아앗!」")
					PRINTFORMW 「후후, 그래서 있고 것. 좋은 아이로 하고 있으면 좀 더 귀여워해 주기 때문에…」
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
RETURN 0
ENDIF
