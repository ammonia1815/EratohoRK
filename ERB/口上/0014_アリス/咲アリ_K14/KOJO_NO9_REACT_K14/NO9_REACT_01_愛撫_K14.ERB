			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「하, 하…, 또 그런 일 말해…… 아, 아앗!」")
			PRINTFORMW 「불평은 나중에 분명하게 (들)물어 주기 때문에…, 지금은 불필요한 일은 생각하지 마…」
			;페이드 아웃
			CALL FADE(22, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
			CLEARLINE 1
			CALL PRINT_REACT_15_01
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;대면좌위
