;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상が恥ずかしがって着替えられない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG >= 1000 && (ARG & 10) == 2
	SELECTCASE ARG
		;반라だって恥ずかしい
		CASE 1012
			IF CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도
				PRINTFORMW 「그렇게, 어쩔 수 없네요…」
			ELSE
				PRINTFORMW 「부끄러워했더니, 이제 와서 어쩔 도리가 없는데？」
