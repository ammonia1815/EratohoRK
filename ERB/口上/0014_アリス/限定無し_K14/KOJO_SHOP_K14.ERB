			PRINTFORMW 「그것이１번%CNAME(MASTER, TARGET)%답네요」
			RETURN 1
		;반라되었다
		CASE 1011
			PRINTFORMW 「이런 것도 나쁘지 않은, 일지도…」
			RETURN 1
		;전라되었다
		CASE 1021
			PRINTFORM 「후후…, 이 변태
			CALL PRINT_SYMBOL_K14
			RETURN 1
		;……すーすーする
		;CASE 1031
		;	PRINTFORMW 「これはコレで有り、かも…」
		;	RETURN 1
		;その他
		CASEELSE
			RETURN 0
	ENDSELECT
