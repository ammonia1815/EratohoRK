;========================================================================
;성교봉사계
;========================================================================
;·COM20, 허리를 흔든다는 ACT에 의해 REACT분류가변화 한다
;·역강간의 경우는 REACT분류("받아들인다")
;·그 외성교봉사ACT에서는 REACT분류("적극적으로 한다") 취급이 된다
;·이변화는 함수@SEX_INITIATIVE에서 행해진다
;-------------------------------------------------

;────────────────────────────────────
;REACT95, 정상위시킨다·개별 리액션
;────────────────────────────────────
;·TFLAG:ACT파생(0=통상/1=연속/2=Ｖ성교봉사계로부터/3=Ａ성교계로부터/4=Ｖ성교계로부터/5=Ａ성교봉사계로부터)
;-------------------------------------------------
;상정되는 COM…
;애무계 반응
;	COM:부끄러워한다/싫어한다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;성교계 반응
;	COM:허리를 흔든다
;고통계 반응
;	COM:날뛴다
;봉사계 반응
;	COM:거부한다/소극적으로 한다/적극적으로 한다/애정을 담아 한다/태연하게 한다
;그 외 반응
;	COM:가게해줘!
;-------------------------------------------------
@KOJO_REACT_K14_95
;-------------------------------------------------
;·지문은 REAC 분류로 분기
;·REACT분류가날뛴다/거부의 경우는취소취급
;·ACT가취소가 되었을 경우는 TEQUIP:성교봉사중도 리셋트
;·그 외의 REACT분류(받아들인다/적극적으로 한다/태연하게 한다/용서를 빌다/소극적으로 한다)의 경우는 수용 취급이 된다
;·죠교대상가동정라면동정상실, 죠교자가처녀라면처녀상실가 일어난다
;·TEQUIP:성교봉사중가 서 있을 때에 REACT분류가날뛴다라면 거절 취급이 된다(즉, 성교봉사계 ACT 이외에도 거절 처리가 되는 경우가 있다)
;-------------------------------------------------
;오토모드용 처리
[IF_DEBUG]
	IF TCVAR:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W("처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가날뛴다/도망친다/거부
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("날뛴다/도망친다/거부") && FLAG:지문제어 == 2
	CALL PRINT_ACTGR_10_REJECT_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;애태움개시
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TCVAR:MASTER:가버려 && TCVAR:MASTER:애태움도 > 1 && IS_COMGRONAME("적극적으로 한다/태연하게 한다/받아들인다")
	CALL TEXTDISPLAY_CATEGORY_10_TEASE_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM, 쾌감을 참는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(55) && IS_NOWCOMNAME("쾌감을 참는다") && TCVAR:MASTER:참다
	CALL TEXTDISPLAY_CATEGORY_10_COM14_K14
	RETURN 1
ENDIF

;COM15, 쾌감을 받아들인다/COM16, 기분 좋다! 등을 상정. ACT103, 역강간의 경우, COM20, 허리를 흔든다도 REACT분류받아들이는이 된다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류, 받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다")
	;=================================================
	;죠교대상가가버려		자궁입에 닿을 만큼 깊게 운운…
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:가버려
		CALL PRINT_ACTGR_10_ACCEPT_0_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자가가버려
	;=================================================
	IF 구상기입체크(100) && PALAM:쾌Ｖ > 8500
		CALL PRINT_ACTGR_10_ACCEPT_1_K14
		RETURN 1
	ENDIF
	
	;죠교대상/죠교자가가버려인 경우 이외를 처리
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(70)
		CALL PRINT_ACTGR_10_ACCEPT_2_K14
		RETURN 1
	ENDIF
;조건으로부터 빠지면 범용 리액션（COM）에
RETURN 0
ENDIF

;소극적 반응을 정리한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류, 소극적으로 따랐다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적으로 한다/용서를 빌다")
	;=================================================
	;REACT부하 2(무리를했다행동)
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를했다행동"
		;실은적극
		IF COMCOR_POSI()
			PRINTFORMW 「…응. 부끄러워해 보였더니, 허리는 멈추지 않구나…」
			PRINTFORMW 「\@ AFFECTION("상사상애") ? 저기, # \@조금은 솔직해지면 어때\@ TENSION(TARGET, 7) == "높다" ? (이)야!  # ?  \@」
			SIF IS_COMGRONAME("소극적으로 한다")
				PRINTFORMW 「나머지%CNAME(MASTER, TARGET)%…, 나부터 시선을 일의,\@ AFFECTION("상사상애") ? 그만두세요!  # 그만두어 받을 수 있다?  \@」
		;실은 싫어
		ELSE
			PRINTFORMW 「…\@ IS_COMGRONAME("소극적으로 한다") ? 단조롭고 시시해요 # 나의 움직임에 맞추는 일도 할 수 없다든가\@. 싫증%RAND_SPLIT("(이)야/저기")%,%CNAME(MASTER, TARGET)%…」
			PRINTFORMW 「그러니까, 그렇지 않아서! 아 이제…, 정말로%RAND_SPLIT("속상하다/안타깝다/안절부절 한다")%\@ TENSION(TARGET, 7) == "높다" ? 의 것!  # 원이군요… \@」
		ENDIF
		RETURN 1
	ENDIF
	
	;REACT파생 1, 소극적인의가 마음에 들지 않는 것 경우는 COM로 처리
	;=================================================
	;COM41, 소극적으로 한다 「소극적이지만 좋은가」
	;=================================================
	IF 구상기입체크(55) && IS_NOWCOMNAME("소극적으로 한다") && TFLAG:REACT파생 == 0
		IF AFFECTION("상사상애")
			PRINTFORMW 「아…팥고물……아……」
			PRINTFORMW 「응… 초조하게 하지 마…… 좀 더 격렬하게 해……!」
		ELSEIF AFFECTION("익애")
			PRINTFORMW 「안타까워서……이상해질 것 같다…」
			PRINTFORMW 「응…… 좀 더, 좀 더……!」
		ELSEIF AFFECTION("권애")
			PRINTFORMW 「무슨 소극적이구나……안타까워요…」
		ELSE
			PRINTFORMW 「 좀 더 허리 거절하세요…」
			PRINTFORMW 「이봐요, 좀 더 마음껏…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;허리를 흔든다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("허리를 흔든다")
	;=================================================
	;가버려상태
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:가버려
		SELECTCASE TCVAR:MASTER:애태움도
			CASE 1 ;애태움종료
				;@AUOTHEART계를 사용해, 지문 날조 개소를 분기 시켜 주면 1개에 모을 수 있을 것 같지만 뒷전
				IF AFFECTION("상사상애")
					;RR 2
					SELECTCASE RANDREACT(2, 2)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「응! , 바보! 그렇게 격렬하게 찔리면!」
							CALL QUAKE(4)
							PRINTFORM 「이나, 격렬해져! 팥고물
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  아, 아앗!!」")
						CASE 1
							CALL QUAKE(4)
							PRINTFORM 「은! 아
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  아! 이, 이런 건… 은!」")
							CALL QUAKE(4)
							PRINTFORMW 「이나, 망가진다! 부서져버리는!」
					ENDSELECT
					PRINTFORMDW 자궁구까지 도려내지는 감각에, 앨리스는 머리카락을 흩뜨려 허덕여 괴로워하고 있다…
				ELSE
					;RR 3
					SELECTCASE RANDREACT(2, 3)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「응! , 바보! 그렇게 격렬하게 찔리면!」
							CALL QUAKE(4)
							PRINTFORMW 「이나, 격렬해져! 아, 아!!」
						CASE 1
							CALL QUAKE(4)
							PRINTFORMW 「은! 아, 아! 개, 이런 건… 라는!」
							CALL QUAKE(4)
							PRINTFORMW 「이나, 망가진다! 부서져버리는!」
					ENDSELECT
					PRINTFORMDW 질내의 최안쪽까지 켜 거론되는 감각에, 앨리스는 미친 것처럼 허덕여 괴로워하고 있다…
				ENDIF
			CASEELSE ;애태움없음
				IF AFFECTION("상사상애")
					;RR 4
					SELECTCASE RANDREACT(2, 4)
						CASE 0
							CALL QUAKE(4)
							PRINTFORM 「아! , 안쪽에 해당되어! 아, 아
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
							CALL QUAKE(4)
							PRINTFORMW 「이나, 거기! 때굴때굴 되면!」
						CASE 1
							CALL QUAKE(4)
							PRINTFORM 「아, 아
							CALL HEARTCB(COLOR2_K14("pink"), 1, "나눔고딕")
							PRINTFORM  팥고물, 후~! 응응
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
							CALL QUAKE(4)
							PRINTFORM 「개, 이런 건…! , 견딜 수 있는 것……
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					ENDSELECT
				ELSE
					;RR 5
					SELECTCASE RANDREACT(2, 5)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「아! , 안쪽에 해당되어! 아, 아!」
							CALL QUAKE(4)
							PRINTFORMW 「이나, 거기! 때굴때굴 되면!」
						CASE 1
							CALL QUAKE(4)
							PRINTFORMW 「아, 아! 팥고물, 후~! 응응!!」
							CALL QUAKE(4)
							PRINTFORMW 「개, 이런 건…! , 견딜 수 있는 것……!」
					ENDSELECT
				ENDIF
				PRINTFORMDW 자궁구에 닿을 정도의 깊고 격렬한 찌르기 포함에, 앨리스는 전신을 진동시키면서 허덕여 괴로워하고 있다…
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;서로정욕
	;=================================================
	IF 구상기입체크(100) && (M_COND("정욕") && T_COND("정욕"))
		CALL QUAKE(4)
		PRINTFORMW 「은, 팥고물! 아…… 아… 아, 아!!」
		CALL QUAKE(4)
		PRINTFORMW 「응! 도, 이제…… 정말로! , 이상해져버리는!」
		PRINTFORMDW 정욕에 고두 명은 서로 서로 미칠 듯하게 요구해 , 짐승과 같이 격렬하게 사귀고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가정욕
	;=================================================
	IF 구상기입체크(100) && M_COND("정욕")
		CALL QUAKE(4)
		PRINTFORMW 「아,! 개, 이런 건… 이제, 견딜 수 있고! 아… 아˝…」
		CALL QUAKE(4)
		PRINTFORMW 「아, 후~! (이)나… 도, 다멕! 아, 아!」
		PRINTFORMDW 정욕에 고%CALLNAME:MASTER%의 격렬한 찌르기 포함에, 앨리스는 머리카락을 흩뜨리면서 격렬하게 허덕이고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자가정욕
	;=================================================
	IF 구상기입체크(100) && T_COND("정욕")
		CALL QUAKE(4)
		PRINTFORMW 「! (이)나, 나다…… 요… 멈추지 않게 되어버리는!」
		CALL QUAKE(4)
		PRINTFORMW 「응,… 하, 팥고물! , 거기! 아, 아…」
		PRINTFORMDW 정욕에 고앨리스는 허리를 격렬하게 쳐 붙여 쾌락을 탐내고 있다…
		RETURN 1
	ENDIF
	
	;확률로 범용 리액션에
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(60)
		IF AFFECTION("상사상애")
			;RR 6
			SELECTCASE RANDREACT(2, 6)
				CASE 0
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)% 응 좀 더, 좀 더…!」
					CALL QUAKE(4)
					PRINTFORM 「, 아
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  그런, 격렬하고! 아읏, 아앗!」")
				CASE 1
					CALL QUAKE(4)
					PRINTFORMW 「하, 하…… 응… 응! 아…… 아!」
					CALL QUAKE(4)
					PRINTFORM 「아……아, 아
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "  응, 응앗!」")
					PRINTFORMDW 앨리스는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하면서, 격렬한 찌르기 포함을 받아 들이고 있다…
			ENDSELECT
		ELSE
			;RR 7
			SELECTCASE RANDREACT(2, 7)
				CASE 0
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)% 하면서의 키스도 나쁘지 않은, 원이군요…」
					CALL QUAKE(4)
					PRINTFORMW 「, 아! 그런, 격렬하고! 팥고물, 아!」
				CASE 1
					CALL QUAKE(4)
					PRINTFORMW 「하, 하…… 응… 응! 아…… 아!」
					CALL QUAKE(4)
					PRINTFORMW 「아…… 아, 아! 응, 응!」
					PRINTFORMDW 앨리스는 %CALLNAME:MASTER%(을)를 가만히 응시하면서, 격렬한 찌르기 포함을 받아 들이고 있다…
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF

