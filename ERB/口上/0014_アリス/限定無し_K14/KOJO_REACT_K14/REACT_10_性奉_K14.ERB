			CASE "무리를했다행동"
				PRINTFORM  %RAND_SPLIT("이봐요/자")%, 좀 더!  %CNAME(MASTER, TARGET)%의 %RAND_SPLIT("한심한/안쓰러운")%%RAND_SPLIT("소리…, 들려줘/얼굴…, 나에게 보이게 해 줘")%
		ENDSELECT
		;PALAM:쾌Ｖ가 일정 이상이라면 PS2를 CALL 한다
		IF PALAM:쾌Ｖ > 4000
			PRINT …
			CALL PRINT_SYMBOL2_K14
		;그 외라면 보통으로 끝난다
		ELSE
			PRINTW ……」
		ENDIF
		;죠교자가정욕나몽롱, 혹은만족도 50%이상 한편 1/2의 확률로 진
		IF GROUPMATCH(GET_CONDITION(TARGET), "정욕", "몽롱") > 0 || (TENSION(TARGET, 8) == "높다" && !RAND:2)
			;콘피그로 FADE가 ON의 경우 1/2의 확률로 진
			IF ANIME_CONFIG("FADE") && !RAND:2
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
