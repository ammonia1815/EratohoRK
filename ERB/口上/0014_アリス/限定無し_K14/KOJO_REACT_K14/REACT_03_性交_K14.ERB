;────────────────────────────────────
;REACT32, 기승위·개별 리액션
;────────────────────────────────────
;상정되는 COM…
;애무계 반응
;	COM:싫어한다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;성교계 반응
;	COM:허리를 흔든다/안에 내!/안에 내면 안돼!
;고통계 반응
;	COM:고통을 참는다/비명지른다/날뛴다
;봉사계 반응
;	COM:거부한다/소극적으로 한다/적극적으로 한다/애정을 담아 한다/태연하게 한다
;-------------------------------------------------
@KOJO_REACT_K14_32
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속실행/2=다른Ｖ성교로부터/3=Ａ성교로부터/4=Ｖ성교봉사로부터/5=Ａ성교봉사로부터)
;-------------------------------------------------
;·취소처리는 ACT_MESSAGE_OTHERS에서 범용 처리
;·다른성교계와는 달라, 봉사계 반응(COM40 접수대)도 나오는 일에 주의
;·취소가 성립하는 것은 다른성교계와 달리, REACT분류가날뛴다/거부의 경우이다
;·주도권은죠교대상(SEX_INITIATIVE(MASTER))
;-------------------------------------------------
;오토모드용 처리
[IF_DEBUG]
	IF TCVAR:MASTER:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W(@"%CALLNAME:MASTER%처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;COM이루는대로라면 범용 리액션에 날린다
IF IS_NOWCOMNAME("이루는대로")
	RETURN 0
ENDIF

;병렬처리이지만 COM에 의해 확률을 짠다. REACT부하의 처리는 여기서 완결시킨다
;ACT/REACT30기승위의 경우, COM20, 허리를 흔든다는적극적으로 한다취급이 된다
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM, 태연하게 한다/허리를 흔든다/적극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && (IS_NOWCOMNAME("태연하게 한다") || (IS_NOWCOMNAME("허리를 흔든다") && !RAND:2) || (IS_NOWCOMNAME("적극적으로 한다") && !RAND:3))
	;=================================================
	;자연스러운행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "자연스러운행동"
		;죠교자상태변화파생
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕"
				PRINTFORM 「%RANDTEXT_K14("팥고물", "응아")%, 아%RANDTEXT_K14("", "＿")%
				;AH
				CALL AUTOHEARTCB_K14
				;2/3로 표시
				IF RAND:3
					;RANDREACT(0)
					SELECTCASE RANDREACT(2, 0)
						CASE 0
							PRINTFORMW  나를 억누르다는 거만함%RANDTEXT_K14("군요", "인 것이야")%…」
							PRINTFORMW 「 그렇지만무슨…, 흥분해요. 이런 것……」
						CASE 1
							PRINTFORMW  나를 범하고서라도 있을 생각%RANDTEXT_K14("인 걸까…", "?", "인 이유?")%」
							PRINTFORMW 「 그렇지만,%RANDTEXT_K14("때로는", "＿")%이런 것도 나쁘지 않다. 지금%RANDTEXT_K14("그런 생각", "그런 느낌")%」
					ENDSELECT
					PRINTFORM 「이봐요, 좋아요. 이대로 좀 더,%RANDTEXT_K14("나를 즐겁게 해", "격렬하게 해")% 주세요…
					;PS로 닫는다
					CALL PRINT_SYMBOL_K14
				;1/3로 표시
				ELSE
					PRINTFORMW  조금 건방지구나. 그렇지만 싫지 않아요. 이런 것…」
					PRINTFORMDW %CALLNAME%(은)는 %CALLNAME:MASTER%의 손을 지불이 차면(자) 목덜미를 팔을 회 해, 억지로 끌어 들이면서 그 입술을 빼앗았다…
					PRINTL
					;RANDREACT(1)
					SELECTCASE RANDREACT(2, 1)
						CASE 0
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
							CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
							CLEARLINE 1
						CASE 1
							PRINTFORML 「%KOJO_RAND_K14(("키스"), 1)%」
					ENDSELECT
					PRINTFORMW 「이봐요, 격렬한 것이 기호이지요? 좋아요, 교제해 준다…」
					PRINTFORMDW 두 명은 서로의 입술을 탐내면서,\@ !TIME ? %RANDTEXT_K14("대낮의", "＿")%광 # %RANDTEXT_K14("소", "밤의", "＿")%암 \@중(안)에서\@ M_COND("정욕") ? 짐승과 같이 격렬하고 # 요염하고 \@사귀고 있다…
					;1/2로 표시
					SIF !RAND:2
						PRINTFORMDW 근처에는 추잡한 물소리가 끊어지는 일 없이 영향을 주어 건너고 있다…
					;키스를 날조
					CALL SET_KISS
				ENDIF
			CASE "피폐", "쇠약"
				PRINTFORMW 「%RANDTEXT_K14("후~, 하아", "하, 하")%…, 내가 지쳐 그다지 움직일 수 없기 때문에는,%RANDTEXT_K14("조금", "어쩐지") %본궤도에 오르지 않다?」
				PRINTFORM 「%RANDTEXT_K14("", "＿")%아
				;AH
				CALL AUTOHEARTCB_K14
				PRINTFORMW  \@ TENSION(TARGET, 8) == "높다" ? 이제(벌써) # 하 \@, 너무 격렬해요…\@ TENSION(TARGET, 8) == "높다" ? 읏!  # \@」
			CASEELSE
				PRINTFORM 「응응! 아
				;AH
				CALL AUTOHEARTCB_K14
				PRINTFORMW  %RANDTEXT_K14("조금 건방지네", "치열하네")%. 그렇지만…」
				PRINTFORMW 「이것은 이것대로, 나쁘지 않은, 일지도……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;무리가 없는행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리가 없는행동"
		;죠교자상태변화파생
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕"
				PRINTFORM 「%RANDTEXT_K14("응응", "아")%! 아%RANDTEXT_K14("", "＿")%
				CALL AUTOHEARTCB_K14
				PRINTFORMW  %RANDTEXT_K14("힘내", "열중하")%는……, 사랑스럽다%RANDTEXT_K14("원", "＿")%」
				PRINTFORMDW %CALLNAME%(은)는 %CALLNAME:MASTER%의 수를 잡으면, 게다가 몸을 끌어 들이고 정열적인 키스를 했다…
				PRINTL
				;RANDREACT(2)
				SELECTCASE RANDREACT(2, 2)
					CASE 0
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
						CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
						CLEARLINE 1
					CASE 1
						PRINTFORML 「%KOJO_RAND_K14(("키스"), 1)%」
				ENDSELECT
				PRINTFORMW 「\@ TENSION(TARGET, 8) == "높다" ? 저기 # 이봐요\@, 좀 더…이대로 키스 하고 있어 주기 때문에……」
				;키스를 날조
				CALL SET_KISS
			CASE "피폐", "쇠약"
				PRINTFORMW 「%RANDTEXT_K14("후~, 하아", "하, 하")%…, 그렇게%RANDTEXT_K14("힘내", "열심히 해")%주는 것은 좋지만…」
				PRINTFORMW 「유석에,%RANDTEXT_K14("조금", "약간")% 지쳐 왔어요%RANDTEXT_K14("군요", "＿")%」
			CASEELSE
				PRINTFORM 「\@ AFFECTION("익애") ? 후후 # 후후 \@…, 좋다%RANDTEXT_K14("느낌", "상태")%
				;AH2
				CALL AUTOHEARTCB2_K14
				PRINTFORM  지금의 %CNAME(MASTER, TARGET)%, 매우 사랑스럽다%RANDTEXT_K14("원", "＿")%
				CALL PRINT_SYMBOL_K14
				PRINTFORMW 「, 좀 더\@ TENSION(TARGET, 8) == "높다" ? !  #  \@ %CNAME(MASTER, TARGET)%의 %RANDTEXT_K14("왜곡된", "야한")%모습, 나에게 보이게 해 주세요…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;무엇이 COMCOR_POSI야 목표 있는은 똥이라든지 생각하지 않는 것도 없지만
	;상황을 세세하게 주울 수 있으므로 묘사의 폭은 넓어지고, 결정 집 할 수 있으면 오히려 쓰기 쉬운 면도 있다
	;라고 할까 뭐본래는, 쾌감을 참는다/소극적으로 한다등의 미묘한 뉘앙스의 COM는 산다
	;=================================================
	;무리를했다행동
	;=================================================
	IF 구상기입체크(100) && GET_COM_RANK() == "무리를했다행동"
		;당초, 상태변화를 별도 기준 할 생각이 귀찮음되었다의 것으로 통합해 보면 심하게 알기 어려운 기분이 들어 슬프다
		;실은 그렇지만도 않다
		IF COMCOR_POSI()
			;죠교자BASE:만족파생
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「쾌락에 흐르게 되어, 자신이 유지할 수 없게 될 것 같고 무섭다? 고집이 없구나」
					;죠교자가정욕가 아니다
					SIF !T_COND("정욕")
						PRINTFORMW 「아무튼에서도…, 그런%CNAME(MASTER, TARGET)%(을)를 보고 있으면, 철저하게 타로서 하고 싶어져요…」
				CASEELSE
					PRINTFORMW 「%RAND_SPLIT("재미없다/시시하다")%구애됨은 버리게 되면 어떻게야?」
					PRINTFORMW 「…시치미를 떼어도 타목. 얼굴을 보면, 무엇을 생각하고 있을까 라고 곧 알아요」
					;죠교자가정욕가 아니다
					SIF !T_COND("정욕")
						PRINTFORMW 「 그렇지만…, 그런%CNAME(MASTER, TARGET)%(을)를 녹여 버리는 것도, 그것은 그래서 일흥이군요」
			ENDSELECT
			;죠교자가정욕가 아니다
			IF !T_COND("정욕")
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「ウフフ…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「우후후…」")
				CLEARLINE 1
			ENDIF
			;COMCOR 분기를 빠지면상태변화로 분기
		;실은 싫어
		ELSE
			;죠교자BASE:만족파생
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「이런 일 하는데 저항이 있겠지요? 보고 있으면 알아요%RANDTEXT_K14("야", "＿")%, 그런 것」
					;죠교자가정욕가 아니다
					SIF !T_COND("정욕")
						PRINTFORMW 「 그렇지만 나로서는, 이런 기회를 살리지 않는 손은 없네요. 우후후…」
				CASEELSE
					PRINTFORMW 「그러면 기분 잡기도 안 돼요, 나쁜데.%CNAME(MASTER, TARGET)%가 무리하고 있는 것이 전해져 오는 것」
					;죠교자가정욕가 아니다
					SIF !T_COND("정욕")
						PRINTFORMW 「다만, 그렇구나? 이 때%CNAME(MASTER, TARGET)%(을)를 학째라고 주는 것도 일흥이군요…」
			ENDSELECT
			;COMCOR 분기를 빠지면상태변화로 분기
		ENDIF
		;죠교자가정욕			대사, 지문, 키스의 처리
		IF T_COND("정욕")
			PRINTFORMW 「 그렇지만…, 지금은 어때라고 해 원\@ TENSION(TARGET, 8) == "높다" ? !  #  \@ 그런\@ TENSION(TARGET) == "높다" ? 노 # 것… \@」
			PRINTFORMDL %CALLNAME%(은)는 %CALLNAME:MASTER%의 상체를 억지로 끌어 들이면(자), 정열적인 키스를 해 왔다…
			PRINTFORMDW 정욕에 물들었다%CALLNAME%의 눈동자는 요염한 빛을 띠고 있다…
			PRINTL
			;RANDREACT(3)
			SELECTCASE RANDREACT(2, 3)
				CASE 0
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
					CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
					CLEARLINE 1
				CASE 1
					PRINTFORML 「%KOJO_RAND_K14(("키스"), 1)%」
			ENDSELECT
			PRINTFORMW 「각오 해.%CNAME(MASTER, TARGET)%(을)를 엄청으로 해 주기 때문에…」
			;키스를 날조
			CALL SET_KISS
		;그 외					지문만
		ELSE
			PRINTFORMDL %CALLNAME%(은)는 \@ COMCOR_POSI() ? 도발적인 웃음을 띠면서 # 한마디 그렇게 고하면 \@, %CALLNAME:MASTER%(은)는 허리를 거절하는 리듬에 맞추어 밀어 올린다
			PRINTFORMDW 필사적으로 \@ COMCOR_POSI() ? 자신을 유지하자 # 쾌락에 저항하자 \@(으)로 하는 %CALLNAME:MASTER%의 노력도 허무하고, 그 %RANDTEXT_K14("이성", "마음")%은 녹여져 간다…
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && IS_NOWCOMNAME("쾌감을 받아들인다/기분 좋다!")
	;=================================================
	;COM15/16, 쾌감을 받아들인다/기분 좋다!
	;=================================================
	IF 구상기입체크(100) && (IS_NOWCOMNAME("쾌감을 받아들인다") && PERCENT(90)) || (IS_NOWCOMNAME("기분 좋다!") && PERCENT(65))
		;-------------------------------------------------
		;85%로 전용 처리
		;-------------------------------------------------
		IF 구상기입체크(85)
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORMW 「그러한 느낌에 허리를 사용하면 키모치 좋아, 알아요…」
					;죠교자가가버려 혹은 쾌PALAM가 일정 이상
					IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
						PRINTFORMW 「은, 나도 완전히 좋은 기분이 들어 버린다 이지만……」
					ELSE
						PRINTFORMW 「 그렇지만, 나로서는 이마이치이지만」
					ENDIF
				CASE "무리가 없는행동"
					PRINTFORMW 「자신이 키모치 좋은 트코, 찾아내 버렸다 같구나? 후후」
					;죠교자가가버려 혹은 쾌PALAM가 일정 이상
					IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
						PRINTFORMW 「은 나도 조금 코레, 위험할지도……」
					ELSE
						PRINTFORMW 「 그렇지만%CNAME(MASTER, TARGET)%, 자신이 기분 좋아질 수 밖에 머릿속에 없는 보고 싶다……」
					ENDIF
				CASE "무리를했다행동"
					PRINTFORMW 「\@ ALI(0) ? 후후 # 후후 \@, 스윗치가 들어가 버렸다 같구나」
					PRINTFORMW 「나쁘지 않은 경치예요……」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;---------------------
		;새어 오면 범용 처리			필요하지 않을지도 모르지만 일단. 있는 것은 사용하고 싶어지는 고
		;---------------------
		CALL PRINT_REACT_30_COM15_COM16_K14
		RETURN RESULT
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:용서를 빌다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("용서를 빌다") && IS_NOWCOMNAME("용서를 빌다/쾌감을 참는다")
	CALL PRINT_REACT_30_COM7_COM14_K14
	RETURN RESULT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:소극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적으로 한다") && IS_NOWCOMNAME("쾌감을 참는다")
	LOCALS = \@ ALI(0) ? 후후 # 후후 \@
	;2/3로 진
	IF RAND:3
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「조금 부끄러운 것 같구나? 이런 일 하고 있는 단계에서 이제 와서 지나는데」
				PRINTFORMW 「 그렇지만, 허리가 멈추지 않는다고 (일)것은…, 이러니 저러니로 좋아져 버린다 의 것인지 해들.%LOCALS%…」
			CASE "무리가 없는행동"
				PRINTFORMW 「단조로워 실로 트마라나이원이군요. 너무 싫증해 졸려져요\@ TENSION(TARGET, 7) == "높다" ? !  # \@」
				PRINTFORMW 「\@ ABL:MASTER:성교 > 4 ? %CNAME(MASTER, TARGET)%라면, 좀 더 할 수 있을 것이군요?  # 아무튼, 익숙해지지 않은 동안은 이런 것인가 해들 \@」
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「부끄러워서 적극적으로 될 수  없는, 이라는 느낌일까?」
					PRINTFORMW 「 그렇지만…, 허리의 움직임은 멈추지 않구나.%LOCALS%」
				;실은 싫어
				ELSE
					PRINTFORMW 「 「 좀 더 정열적으로」라고 말할 생각은 없지만, 너무 단조로운 것은…」
					PRINTFORMW 「\@ ABL:MASTER:성교 > 4 ? %CNAME(MASTER, TARGET)%라면 좀 더 할 수 있을 것이지만, , 지금은 그럴 기분이 들 수 없다는 (뜻)이유야…?  # 낯선 안은, 이런 것인지도 모르는데 \@」
				ENDIF
		ENDSELECT
		;분기내에서 RESULT을반환
		RETURN 1
	;1/3로 진
	ELSE
		;부하 0, 1이라면 RETURN 1
		IF GROUPMATCH(GET_COM_RANK(), "자연스러운행동", "무리가 없는행동") > 0
			IF AFFECTION("상사상애")
				PRINTFORMW 「이봐요,%CNAME(MASTER, TARGET)%모모와 즐겨…」
				PRINTFORM 「언제까지나 등지고 있다고(면)……, 마음껏 찔러 버리니까…
				CALL PRINT_SYMBOL_K14
			ELSEIF AFFECTION("익애")
				PRINTFORMW 「응, 좀 더 움직이세요…」
