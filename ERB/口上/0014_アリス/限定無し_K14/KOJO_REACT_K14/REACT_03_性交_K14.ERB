;────────────────────────────────────
;REACT31, 후배위·개별 리액션
;────────────────────────────────────
;상정되는 COM…
;회화계 반응
;	COM:용서를 빌다
;애무계 반응
;	COM:부끄러워한다/싫어한다/서투르다고 모욕한다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;성교계 반응
;	COM:허리를 흔든다/안에 내!/안에 내면 안돼!
;고통계 반응
;	COM:날뛴다
;-------------------------------------------------
@KOJO_REACT_K14_31
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속실행/2=다른Ｖ성교로부터/3=Ａ성교로부터/4=Ｖ성교봉사로부터/5=Ａ성교봉사로부터)
;-------------------------------------------------
;오토모드용 처리
[IF_DEBUG]
	IF TCVAR:MASTER:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W(@"%CALLNAME:MASTER%처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && IS_NOWCOMNAME("쾌감을 받아들인다/기분 좋다!/허리를 흔든다")
	;=================================================
	;COM20, 허리를 흔든다		80%
	;=================================================
	IF 구상기입체크(80) && IS_NOWCOMNAME("허리를 흔든다")
		LOCALS = %RAND_SPLIT("하, 후~/응읏, 구/아…, 응응/하, 아/쿠웃, 응/핫, 하아, 하아")%……
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORM 「%LOCALS%
				IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
					PRINTFORMW 좋아요, 매우 좋아요.%CNAME(MASTER, TARGET)%…」
					PRINTFORMW 「덕분에 나도,%RAND_SPLIT("완전히 실려져 버릴 것 같지만/무심코 진지해져 버려요")%……」
				ELSE
					PRINTFORMW 이 모습, 평소보다 흥분해버리는의 것인지 해들…?」
					PRINTFORM 「그 기분, 어딘지 모르게이지만……알지 않는 것도 아니어요
					CALL PRINT_SYMBOL_K14
				ENDIF
			CASE "무리가 없는행동"
				PRINTFORM 「%LOCALS%
				IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
					PRINTFORMW %RAND_SPLIT("하했다없다/추잡하다/좋다")%소리로 울어 주어 버려서…」
					PRINTFORMW 「%RAND_SPLIT("무심코 그런 기분이 들게 되어져 버려요/읏, 나도 여유 없네요")%……」
				ELSE
					PRINTFORMW 이봐요, 이것이 좋겠지요? 기분 좋겠지요?」
					PRINTFORMW 「 그렇지만 이런 건, 아직도 시초야……」
				ENDIF
			CASE "무리를했다행동"
				IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
					PRINTFORMW 「%LOCALS%후후, 야무지지 못해요. 좋은 경치군요」
					PRINTFORMW 「뭐, 그런 일 말하고 있는데……나도, 이제 안될지도…」
				ELSE
					PRINTFORMW 「완전히 흐르게 되어 버려서, 철없네요…」
					PRINTFORMW 「 그렇지만, 이런 것은 미안하다%RAND_SPLIT("로부터/원이야")%」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM15/16, 쾌감을 받아들인다/기분 좋다!
	;=================================================
	IF 구상기입체크(100) && ((IS_NOWCOMNAME("쾌감을 받아들인다") && PERCENT(90)) || (IS_NOWCOMNAME("기분 좋다!") && PERCENT(65)))
		CALL PRINT_REACT_30_COM15_COM16_K14
		RETURN RESULT
	ENDIF
RETURN 0
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:용서를 빌다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("용서를 빌다") && IS_NOWCOMNAME("용서를 빌다/쾌감을 참는다")
	CALL PRINT_REACT_30_COM7_COM14_K14
	RETURN RESULT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:소극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적으로 한다") && IS_NOWCOMNAME("부끄러워한다/싫어한다/서투르다고 모욕한다/쾌감을 참는다")
	;=================================================
	;COM10, 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「그렇게, 필사적으로 소리를 눌러 참아도 나쁘지 않아요」
				PRINTFORMW 「오히려, 불필요하게 흥분해버릴까도. 후후…」
			CASE "무리가 없는행동"
				PRINTFORMW 「우선, 거역할 생각은 없구나?」
				PRINTFORMW 「뭐, 그런 것이라면…, 정령들 좋아하는 대로 시켜 주어요」
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 이러니 저러니로 좋아져 버린다 의 것인지 해들 」
					PRINTFORMW 「이래, 응? 엉덩이를 이렇게 높게 올려 버려서. 후후……」
				;실은 싫어
				ELSE
					PRINTFORMW 「에, 저항할 생각은 없을 것 같구나?」
					PRINTFORMW 「뭐, 나에게 있어서는 형편상 좋지만……」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM11, 싫어한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("싫어한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「그렇게 싫으면, 보통은 저항하는 것이라고 생각하지만요?」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 말할 정도 싫어 해 (뜻)이유는 없을 것 같구나」
			CASE "무리가 없는행동"
				PRINTFORMW 「싫어한다%CNAME(MASTER, TARGET)%(을)를 억지로 범한다 라고 것도 나쁘지 않아요. 우도…」
				PRINTFORMW 「말하는만큼 싫 그렇게도 안보이지만. 응?」
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「정말로 싫으면, 이런… 마치 조르는 것 같은 모습은 하지 않을 것이군요?」
					PRINTFORMW 「이봐요, 정직하게 말해 보세요…」
				;실은 싫어
				ELSE
					PRINTFORMW 「혐이라든가 말하면서도 저항은 하지 않다든가. 도대체 어떤 심경인 이유?」
					PRINTFORMW 「아무튼우도…, 혐들 말하고 있다%CNAME(MASTER, TARGET)%(을)를 범하는 것도 나쁜 것이 아닌데. 후후…」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM12, 서투르다고 모욕한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("서투르다고 모욕한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「느끼지 않았어들, 좀 더 여유가 있어도 좋을 것 같은 것이지만?」
				PRINTFORMW 「거짓말쟁이인 아이에게는, 조금 징계나무가 필요할까……」
			CASE "무리가 없는행동"
				PRINTFORMW 「에 , 말해 주잖아. 뭐, 그것이 사실이라고 해…」
				PRINTFORMW 「그런 나에게 좋은 느낌에 명인가 되어 버린다%CNAME(MASTER, TARGET)%는, 도대체 뭐야?」
				IF !RAND:3
					PRINTFORMW 「이라고 하는 이야기가 되는데.%CNAME(MASTER, TARGET)%, 거기까지는 생각하지 않게」
					PRINTFORMW 「우도…, 그런 여유도 없는 것일까. 응?」
				ENDIF
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「유석에 그것은…, 조금 설득력이 부족하다고 생각하지만요?」
					PRINTFORMW 「지금의 자신이 어떤 상태인가, 자주(잘) 생각해 보는 일을 추천해요」
				;실은 싫어
				ELSE
					PRINTFORMW 「이 상황으로, 아직 그렇게 말할 수 있군요」
					PRINTFORMW 「우도…, 이제 와서 뭐 말했더니, 설득력이 부족한 것이 슬픈 곳이지만」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM14, 쾌감을 참는다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 참는다")
		LOCALS = \@ ALI(0) ? 후후 # 후후 \@
		;2/3로 진
		IF RAND:3
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORML 「대강,%RAND_SPLIT("고집부려 버려…,/고집/노력해요")%응?」
					PRINTFORMW 「사실은,\@ PALAM:MASTER:쾌Ｖ > 8500 ? 당장가버려(이)가 되어 있다 # 기분 좋아서 견딜 수 없다 \@버릇에……」
					SELECTCASE TENSION(TARGET, 8)
						CASE "높다"
							PRINTFORMW 「 그렇지만, 그런%CNAME(MASTER, TARGET)%(을)를 억지로 울릴 수 있어 보는 것도 일흥이군요」
							PRINTL
							PRINTFORMDL %CALLNAME%(은)는 필사적으로 쾌락에 참으려고 하는 %CALLNAME:MASTER%(을)를,
							PRINTFORMDW 숨도 쉴 수 없을 수록 격렬하고, 배후로부터 괴롭혔다…
							;SOURCE 날조
							TIMES SOURCE:MASTER:쾌Ｖ, 1.50
						CASE "일반"
							PRINTFORMW 「무심코, 조금 학째 싶어져버리는 원이군요.%LOCALS%…」
						CASE "낮다"
							PRINTFORMW 「무엇이라면, 좀 더 격렬하게 시라고 주어도 괜찮아」
					ENDSELECT
				CASE "무리가 없는행동"
					PRINTFORMW 「%LOCALS%, 이런 것도 나쁘지 않네요」
					PRINTFORMW 「울지 않는다면, 고집에서도 울릴 수 있어 주기 때문에 각오 해……」
				CASE "무리를했다행동"
					;실은 기분이 좋다
					IF COMCOR_POSI()
						PRINTFORMW 「소리를 내는 것은 참을 수 있어도, 신체는 반응해버린다 것이 슬픈 곳.%LOCALS%」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%의 모습…, 만으로 산등성이다 하고 있는 것 같이 되어있는 원이야」
					;실은 싫어
					ELSE
						PRINTFORMW 「정직, 소리를 내지 않는 것이 불만인데…」
						PRINTFORMW 「저항하지 않다면, 나의 좋아하는 대로 희롱할 수 있어요.%LOCALS%…」
					ENDIF
			ENDSELECT
			;분기내에서 RESULT을반환
			RETURN 1
		;1/3로 진
		ELSE
			;부하 0, 1이라면 RETURN 1
			IF GROUPMATCH(GET_COM_RANK(), "자연스러운행동", "무리가 없는행동") > 0
				IF AFFECTION("상사상애") && PERCENT(80)
					PRINTFORMW 「이봐요, 능욕되고 있는 것 같아 흥분하고 있는 지요…?」
					PRINTFORM 「정직하게 말할 수 있으면, 좀 더 즐기게 해 주어요…
					CALL PRINT_SYMBOL_K14
				ELSEIF AFFECTION("익애") && PERCENT(70)
					PRINTFORMW 「무슨 억지로 하고 있는 것 같아, 흥분해버리는 원이군요…」
					PRINTFORM 「지금의 %CNAME(MASTER, TARGET)%, 매우 사랑스러워요…
					CALL AUTOHEARTCB2_K14
					PRINTFORMW  이봐요, 좀 더 즐기게 해 주세요…」
				ELSE
					PRINTFORMW 「필사적으로 키모치 좋은 것 참고 있는 것 같지만…」
					PRINTFORMW 「사실은, 뒤로부터 찔리는 것을 좋아하는 것이겠죠?  %LOCALS%」
				ENDIF
				RETURN 1
			;부하 2라면 RETURN 0
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	ENDIF
RETURN 0
ENDIF
