;========================================================================
;성교계 REACT30～35
;========================================================================
;· 각 REACT내의 나누는 방법으로서 TFLAG:REACT분류(IS_COMGRONAME)나 SELECTCOM(IS_NOWCOMNAME)가 생각됩니다
;·ACT분류:성교계의 경우, 기본적으로애무계 COM(COM10~COM16), 성교계 COM(COM20~COM22), 고통계 COM(COM30~COM33)가선택지가 됩니다만
;·상황에 따라서는 그 외의 것이선택지에 포함될 가능성도 있습니다
;-------------------------------------------------
;=================================================
;관련 변수…
;
;TFLAG:REACT분류 (0=없음/1=소극적으로 한다/2=적극적으로 한다/3=용서를 빌다/4=날뛴다/5=요구에 응하지 않는, 거부/6=도망친다, 발버둥 친다)
;IS_COMGRONAME（"없음/소극적으로 한다/적극적으로 한다/용서를 빌다/날뛴다/거부/도망친다"）
;
;FIRSTREACT
;처음ACT 판정 함수
;TFLAG:ACT（TFLAG:90）를 자동으로 참조, 현재 실행되고 있는 REACT 마다번호가 독립해 보존된다
;
;ONCEREACT
;조교중미독판정 함수  죠교전에 초기화된다
;그 외의 사양은 거의 FIRSTREACT와 같이
;=================================================

;────────────────────────────────────
;REACT30, 정상위·개별 리액션
;────────────────────────────────────
;상정되는 COM…
;회화계 반응
;	COM:더 하드하게 해
;애무계 반응
;	COM:싫어한다/서투르다고 모욕한다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;성교계 반응
;	COM:허리를 흔든다/안에 내!/안에 내면 안돼!
;고통계 반응
;	COM:날뛴다
;-------------------------------------------------
@KOJO_REACT_K14_30
;-------------------------------------------------
;·TFLAG:ACT파생(0=통상/1=연속실행/2=다른Ｖ성교로부터/3=Ａ성교로부터/4=Ｖ성교봉사로부터/5=Ａ성교봉사로부터)
;·주도권은죠교자(SEX_INITIATIVE(TARGET))
;-------------------------------------------------
;오토모드용 처리
[IF_DEBUG]
	IF TCVAR:MASTER:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W(@"%CALLNAME:MASTER%처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:받아들인다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("받아들인다") && IS_NOWCOMNAME("쾌감을 받아들인다/기분 좋다!/허리를 흔든다")
	;=================================================
	;COM20, 허리를 흔든다		80%
	;=================================================
	IF 구상기입체크(80) && IS_NOWCOMNAME("허리를 흔든다")
		LOCALS = %RAND_SPLIT("하, 후~/응읏, 구/아…, 응응/하, 아/쿠웃, 응/핫, 하아, 하아")%……
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORM 「%LOCALS%
				;죠교자가가버려. 혹은 쾌PALAM가 일정 이상
				IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
					PRINTFORMW 사랑스러운 소리로 울어 주어 버려서…」
					PRINTFORMW 「 나%RAND_SPLIT(", 견딜 수 없게 되어 버려요/머, 멈추지 않게 될 것 같다")%……」
				;그 외
				ELSE
					PRINTFORMW 후후, 응석부린 소리다 해 버려서…」
					PRINTFORMW 「 좀 더, 좀 더 좋은 소리로 울릴 수 있었던 구 되어버리는 원……」
				ENDIF
			CASE "무리가 없는행동"
				PRINTFORM 「%LOCALS%
				;죠교자가가버려. 혹은 쾌PALAM가 일정 이상
				IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
					PRINTFORMW %RAND_SPLIT("엣치한/추잡하다/꽤의")%허리 사용해, 군요…」
					PRINTFORMW 「그런 일 당하면, 이제 %RAND_SPLIT("나……, 스윗치 들어가 버리기 때문에/……그만두어 줄 수 없으, 니까")%…」
				;그 외
				ELSE
					PRINTFORMW 후후, 좋은 느껴라…」
					PRINTFORMW 「이봐요, 좀 더 나를 느껴……」
				ENDIF
			CASE "무리를했다행동"
				;죠교자가가버려. 혹은 쾌PALAM가 일정 이상
				IF TCVAR:가버려 || PALAM:쾌Ｃ > 6000
					PRINTFORMW 「%LOCALS%얌전하고, 몸을 맡기기로 했다고 (뜻)이유?」
					PRINTFORMW 「뭐, 그러한 일이라면……,%RAND_SPLIT("소망 대로, 범해 주어요/마음껏 범해 주기 때문에")%…」
				;그 외
				ELSE
					PRINTFORMW 「그렇게 자주, 그것으로 좋아…」
					PRINTFORMW 「언제까지나 쾌락에 항우노는,%CNAME(MASTER, TARGET)%왜냐하면 괴로울 것입니다?」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;COM측의 충실도에 의해 확률을 치우치게 한다든가 그런 생각. REACT마다 나누어 쓰는 것이 귀찮음되었다의 것으로 범용 처리로 끝마친다
	;=================================================
	;COM15/16, 쾌감을 받아들인다/기분 좋다!
	;=================================================
	IF 구상기입체크(100) && ((IS_NOWCOMNAME("쾌감을 받아들인다") && PERCENT(90)) || (IS_NOWCOMNAME("기분 좋다!") && PERCENT(65)))
		;범용 처리를 부른다
		CALL PRINT_REACT_30_COM15_COM16_K14
		;RESULT치는 함수측에서 지정. 나중에 바꾸는 경우 이 (분)편이 락이라든지 그런 생각
		RETURN RESULT
	ENDIF
RETURN 0
ENDIF

;일단 COM 조건에용서를 빌다를 포함하고 있지만, 우선 없는 것이라고 생각하고 있다. 무서워한다는 너무 귀찮으므로 고려하지 않는다. 발생했다등 COM로 어떻게든 한다고 할 자세
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:용서를 빌다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("용서를 빌다") && IS_NOWCOMNAME("용서를 빌다/쾌감을 참는다")
	SELECTCASE GET_COM_RANK()
		CASE "자연스러운행동"
			;BASE:만족에 의한 파생（높다=50%이상/일반=25%이상/낮다=25% 미만）
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「나, 말하지 않았는지 해들? 그런 일 하는 것은 역효과래…」
					PRINTFORMW 「오히려%CNAME(MASTER, TARGET)%(을)를, 좀 더 엄청으로 하고 싶어져버리는 원……」
				CASE "일반"
					PRINTFORMW 「자주(잘) 말해요. 사실은 느껴 버린다 버릇에…」
					PRINTFORMW 「거짓말쟁이인 아이에게는, 징계 기사라고 주지 않으면. 응?」
				CASE "낮다"
					PRINTFORMW 「이제 와서 그런 일 말해져도, 제지당하는 것 없지 않다」
					PRINTFORMW 「얌전하게 단념하군요…」
			ENDSELECT
		CASE "무리가 없는행동"
			;BASE:만족에 의한 파생
			SELECTCASE TENSION(TARGET, 8)
				CASE "높다"
					PRINTFORMW 「그만두어라고 말해지면(자), 불필요하게 그만두고 싶지 않게 되어버린다 것이죠…」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%도…, 내가 그 정도는 그만두지 않는다고, 알아 하고 있는  것이 아닌거야?」
				CASE "일반"
					PRINTFORMW 「지금은 등, 괴로운 것은%CNAME(MASTER, TARGET)%의 (분)편이라고 생각하지만요?」
					PRINTFORMW 「정말로 그만두었으면 좋으면, 그런 소리는 나오지 않을 것이라고 생각하는데」
				CASE "낮다"
					PRINTFORMW 「그런 일 말하는 비교적, 상당히 좋은 소리가 나와 버린다 같지만?」
					PRINTFORMW 「 「이런 일 그만두며」는…, 그것이%CNAME(MASTER, TARGET)%의 본심인가, 이상한 곳」
			ENDSELECT
		CASE "무리를했다행동"
			;실은 기분이 좋다
			IF COMCOR_POSI()
				PRINTFORMW 「허락해라고 말하면서, 상당히 좋은 느낌의 허리 사용해?」
				PRINTFORMW 「사실은 싫지 않겠지요. 이봐요, 정직하게 말해 보세요…」
			;실은 싫어
			ELSE
				PRINTFORMW 「말해져 그만둘 정도로라면, 최초부터 하지 않아요」
				PRINTFORMW 「적당, 단념하면 어떻게야?」
			ENDIF
	ENDSELECT
	PRINTL
	PRINTFORMDW %CALLNAME%(은)는 몸을 비트는 %CALLNAME:MASTER%(을)를 억누르면서 계속 범했다…
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:소극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류로 단락짓고 있는 이상, REACT파생는 고려하는필요없음
;------------------------------------------------
IF 구상기입체크(100) && IS_COMGRONAME("소극적으로 한다") && IS_NOWCOMNAME("부끄러워한다/싫어한다/서투르다고 모욕한다/쾌감을 참는다")
	;=================================================
	;COM10, 부끄러워한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「솔직하지 않아.%RAND_SPLIT("나에게 소리를 들려/혹시, 소리를 내")%하지만 부끄럽다든가?」
				PRINTFORMW 「뭐, 그것은 그래서%RAND_SPLIT("나쁘지 않다/좋다")%(이)지만…」
				PRINTFORM 「억지로라도 울릴 수 있었던 구\@ TENSION(TARGET, 8) == "높다" ? 되어 버려요 # 되어요 \@……
				CALL PRINT_SYMBOL_K14
			CASE "무리가 없는행동"
				PRINTFORMW 「참았더니, 좋은 느낌의 소리가 나와 버린다 원이군요」
				PRINTFORM 「이제 와서 부끄러워해도, 이제 뒤늦음이라고 생각해요……
				CALL PRINT_SYMBOL_K14
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「새빨간 얼굴 해 부끄러워하고 있는 비교적은…, 나를 떼어 놓으려고 하지 않구나\@ TENSION(TARGET, 8) == "높다" ? ?  # \@」
					PRINTFORMW 「뭐, 그런 것이라면…. 사\@ TENSION(TARGET, 8) == "높다" ? 에 매우 형편상 좋다는 것은. 후후 # 도 정령들 즐겁게 해 받아요 \@……」
				;실은 싫어
				ELSE
					PRINTFORMW 「그렇게 부끄러워해 보이고 있지만, 사실은 키모치 좋겠지요」
					IF AFFECTION("익애")
						PRINTFORMW 「적당, 참는 것도 괴로운 듯이  보이지만요」
					ELSE
						PRINTFORMW 「솔직해질 생각이 없다면…, 좀 더 학째라고 하기 때문에……」
					ENDIF
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM11, 싫어한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("싫어한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「혐이라든가 말하고 있는 비교적은, 상당히 좋은 기분이 들어 버린다 같지만. 응?」
			CASE "무리가 없는행동"
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 그렇게 싫어해 보이고 있다\@ TENSION(TARGET, 8) == "높다" ? 버릇에 # 하지만 \@……」
				PRINTFORMW 「\@ TENSION(TARGET, 8) == "높다" ? 상당히 좋은 소리로 울군요? 후후… # 좋은 소리가 나와 버리고 있는 일을 알아차리지 않은 것일까. 저기?  \@」
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					CALL FADEIN(10, "「저기 사실은……」")
					PRINTFORMW 「반드시 싫지만은 않은, 나쁘지 않다고 생각하고 있는 지요」
					PRINTFORMW 「이래%CNAME(MASTER, TARGET)%……, 혐이라든가 말하면서 나를 떼어 놓으려고 하지 않고. 응?」
				;실은 싫어
				ELSE
					PRINTFORMW 「후후, 소리가 떨리고 있는 원이야. 말할 정도 싫어 해 (뜻)이유는 없을 것 같구나?」
					PRINTFORMW 「좋은 느낌의 소리도 나와 버린다 것에, 이제 와서 혐이라든지 조금 무리가 있다고 생각해요」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;COM12, 서투르다고 모욕한다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("서투르다고 모욕한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「느끼지 않았어들, 좀 더 여유가 있어도 좋을 것 같은 것이지만?」
				PRINTFORMW 「거짓말쟁이인 아이에게는, 조금 징계나무가 필요할까……」
			CASE "무리가 없는행동"
				PRINTFORMW 「에 , 말해 주잖아. 뭐, 그것이 사실이라고 해…」
				PRINTFORMW 「그런 나에게 좋은 느낌에 명인가 되어 버린다%CNAME(MASTER, TARGET)%는, 도대체 뭐야?」
				IF !RAND:3
					PRINTFORMW 「이라고 하는 이야기가 되는데.%CNAME(MASTER, TARGET)%, 거기까지는 생각하지 않게」
					PRINTFORMW 「우도…, 그런 여유도 없는 것일까. 응?」
				ENDIF
			CASE "무리를했다행동"
				;실은 기분이 좋다
				IF COMCOR_POSI()
					PRINTFORMW 「유석에 그것은…, 조금 설득력이 부족하다고 생각하지만요?」
					PRINTFORMW 「지금의 자신이 어떤 상태인가, 자주(잘) 생각해 보는 일을 추천해요」
				;실은 싫어
				ELSE
					PRINTFORMW 「이 상황으로, 아직 그렇게 말할 수 있군요」
					PRINTFORMW 「우도…, 이제 와서 뭐 말했더니, 설득력이 부족한 것이 슬픈 곳이지만」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;조건마다 RESULT을반환. 낡은 기술을 살려 통합하기 위한 고육지책이라든지 그런 응
	;=================================================
	;COM14, 쾌감을 참는다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 참는다")
		;2/3로 진
		IF RAND:3
			SELECTCASE GET_COM_RANK()
				CASE "자연스러운행동"
					PRINTFORMW 「그러한 것도 나쁘지는 않은데…」
					PRINTFORMW 「 나로서는, 좀 더%CNAME(MASTER, TARGET)%의 소리를 듣고 싶어……」
				CASE "무리가 없는행동"
					PRINTFORMW 「소리가 나와 버린다 원이야.%CNAME(MASTER, TARGET)%…, 참지 않았어?」
					PRINTFORMW 「 그렇지만…, 후후. 나쁘지 않아요. 이봐요, 좀 더 목소리를 들을만해……」
				CASE "무리를했다행동"
					;실은 기분이 좋다
					IF COMCOR_POSI()
						PRINTFORMW 「%RAND_SPLIT("아무리/그렇게,")%자신을 속이려고 했더니…」
						PRINTFORMW 「신체는 솔직하게 반응해버린다 것이죠. 슬픈 이야기이지만. 후후」
					;실은 싫어
					ELSE
						PRINTFORMW 「얼마나 견디려고 했더니, 이제 소리 나와버린다 것이 억제할 수 없는 것 같지만」
						PRINTFORMW 「적당, 단념해 편하게%RAND_SPLIT("되면 좋은데/되어 버리면")%?」
					ENDIF
			ENDSELECT
			RETURN 1
		;1/3로 진
		ELSE
			;부하 0, 1이라면 RETURN 1
			IF GROUPMATCH(GET_COM_RANK(), "자연스러운행동", "무리가 없는행동") > 0
				IF AFFECTION("상사상애")
					PRINTFORMW 「아무리 그런 태도 했더니…」
					PRINTFORM 「%CNAME(MASTER, TARGET)%의 질내는 벌컥벌컥 단단히 조여 오고 있는 원
					CALL PRINT_SYMBOL_K14
				ELSEIF AFFECTION("익애")
					PRINTFORMW 「그다지 내켜하는 마음이 아닌 것일까…?」
					PRINTFORM 「 그렇지만, 이렇게 되어 버렸다들 함께군요…
					CALL PRINT_SYMBOL_K14
				ELSE
					PRINTFORMW 「그다지 시 싶지 않은 것일까?」
					PRINTFORMW 「소극너무 적이어 시시하네요……」
				ENDIF
				RETURN 1
			;부하 2라면 RETURN 0
			ELSE
				RETURN 0
			ENDIF
		ENDIF
	ENDIF
RETURN 0
ENDIF
