IF 구상기입체크(70) && IS_NOWCOMNAME("용서를 빌다")
	;=================================================
	;파생:「나에게 의견하다는건방짐군요!」
	;=================================================
	IF 구상기입체크(100) && TFLAG:REACT파생
		SELECTCASE RANDREACT(2, 0)
			CASE 0
				PRINTFORMW 「우르사이원이군요.%CNAME(MASTER, TARGET)%의 의견은 듣지 않아요!」
				PRINTFORMW 「얌전하게 해 주세요」
			CASE 1
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 그렇게 말할 때에 허락하고 있었던 는, 이야기가 되지 않아」
				PRINTFORMW 「적당, 단념하면 어떻게야!?」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;파생:「흐음」
	;=================================================
	IF 구상기입체크(100) && !TFLAG:REACT파생
		SELECTCASE RANDREACT(2, 1)
			CASE 0
				PRINTFORMW 「후후, 안되어요. 그런 일 말해도 허락해 주지 않아요」
				PRINTFORM 「이봐요, 좀 더 손가락으로 열려 준다
				CALL PRINT_SYMBOL_K14
			CASE 1
				PRINTFORMW 「뭐, 마음대로 말하고 있으면 좋아요」
				PRINTFORMW 「 나는 하고 싶게 시켜 받기 때문에…후후」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

