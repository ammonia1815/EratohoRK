;========================================================================
;수치계 REACT40～45
;========================================================================
;·ACT분류:수치계의 경우, 기본적으로회화계 COM, 애무계 COM, 봉사계 COM가선택지가 됩니다만
;·상황에 따라서는 그 외의 것이선택지에 포함될 가능성도 있습니다
;-------------------------------------------------
;  REACT의 지문은 ACT_MESSAGE.ERB의 TRAIN_MESSAGE2_{TFLAG:ACT}로 처리됩니다
;  기본적으로 지문은 TFLAG:REACT분류(받아들인다/적극적으로 한다/태연하게 한다/용서를 빌다/소극적으로 한다)로 분기 해,
;  각 REACT분류내에서 TFLAG:REACT부하(자연스러운행동/무리가 없는행동/무리를했다행동)로 파생
;  고유의 TRAIN_MESSAGE2가 없는 경우는, REACTION_MESSAGE로 COM마다 범용 처리됩니다
;-------------------------------------------------

;────────────────────────────────────
;REACT40, 자위·개별 리액션
;────────────────────────────────────
;·ACT파생없음. PENIS(MASTER)나 TEQUIP:페니스밴드로 나누는 일은 가능
;-------------------------------------------------
;상정되는 COM…
;회화계 반응
;	COM:용서를 빌다
;애무계 반응
;	COM:부끄러워한다/싫어한다/겁없이 웃는다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;봉사계 반응
;	COM:거부한다/소극적으로 한다/적극적으로 한다/태연하게 한다
;-------------------------------------------------
@KOJO_REACT_K14_40
;-------------------------------------------------
;·지문은 REACT분류로 분기
;·REACT분류가소극적으로 한다/태연하게 한다/적극적으로 한다/받아들이는인 경우는 수용 취급
;·그 외의 반응의 경우는 REACT분류는 모두거부에 세트 된다(용서를 빌다등도거부취급인 일에 주의)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상가 거절했다경우
;━━━━━━━━━━━━━━━━━━━━━━━━
;잠정적으로대상한정구상측을 호출하고 있었지만 멈춘다@v1. 71
;------------------------------------------------
IF 구상기입체크(100) && IS_COMGRONAME("거부")
	;=================================================
	;부끄러워서 명령 대로로 할 수 없었다（(치정선행）
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:치정 > PALAM:MASTER:반항
		;BASE:만족파생		초조함로 주워도 괜찮을지도이지만, BASE:7은변화가 적기 때문에
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「은? 뭐야. 부끄럽고 할 수 없다?」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 아직도 수치심은 것을 가지고 있었군요」
				PRINTFORMW 「약간의 놀라, 라는 곳일까. 응? 후후」
			CASE "일반"
				PRINTFORMW 「뭐, 확실히 부끄럽겠지요지만…」
				PRINTFORMW 「그러한 일 되면(자) 퇴색해. 트마라나이원이군요,%CNAME(MASTER, TARGET)%……」
			CASE "낮다"
				PRINTFORMW 「그렇게,%CNAME(MASTER, TARGET)%(을)를 부끄러워하게 하는 것도 나쁘지 않은데. 그렇지만, 이군요…」
				PRINTFORMW 「말해진 일을 하지 않으면%CNAME(MASTER, TARGET)%에 있어 바람직하지 않은 결과가 되어요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;싫어해 요구에 응하지 않는다（반항선행）
	;=================================================
	IF 구상기입체크(100) && PALAM:MASTER:반항 > PALAM:MASTER:치정 
		;BASE:만족파생
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「그다지 칭찬할 수 있던 것이 아니네요, 그러한 태도…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 자신의 입장을 분별하는 것이 좋아요」
				;만족도가 높은 경우는 놓쳐 받을 수 있는적인
				;그 외의 경우는징계·!
			CASE "일반"
				PRINTFORMW 「말하는 것이 (들)물을 수 없다면, 상응하는 처사를 하는 일이 되는데…」
				PRINTFORMW 「뭐, 방법 없어요?  %CNAME(MASTER, TARGET)%가 바란 결과야 것」
				TCVAR:행동예약 = 60
			CASE "낮다"
				PRINTFORMW 「 나에 따를 수 없다고 말한다면, 상응하는 보답을 받아 주어요」
				PRINTFORMW 「이유는 어때라고 해 원.%CNAME(MASTER, TARGET)%(은)는, 자신의 행동에 대해서 벌을 받아」
				CALL FADEIN(12, @"「각오%RAND_SPLIT("하군요/하세요")%……」")
				WAIT
				TCVAR:행동예약 = 60
		ENDSELECT
		RETURN 1
	ENDIF
RETURN 0
ENDIF

;죠교대상가 수용했다경우만
;━━━━━━━━━━━━━━━━━━━━━━━━
;처음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("소극적으로 한다/적극적으로 한다/받아들인다/태연하게 한다") && FIRSTREACT(0)
	PRINTFORMW 「응, 과연.\@ PENIS(MASTER) ? 그렇게 단순하게 잡아당기는 것만으로 기분 좋은 것일까 # %CNAME(MASTER, TARGET)%(은)는 그러한 느낌으로 하는 것 \@?」
	PRINTFORMW 「좋아요, 그대로 계속해. 분명하게 보고 있어 주기 때문에…」
	PRINTFORMDL %CALLNAME:MASTER%(은)는 %CALLNAME%의 시선의 원이라고 하는 이상한 상황에\@ IS_COMGRONAME("소극적으로 한다") ? 부끄러움을 기억하면서도 # 흥분하면서 \@,
	PRINTFORMDW \@ IS_COMGRONAME("소극적으로 한다") ? 어쩔 수 없이 # 열중해 \@자신의 \@ PENIS(MASTER) ? 페니스를계속 훑어 냈다 # 비밀스런 장소를 만지작거렸다 \@…
	PRINTFORMDW %CALLNAME%(은)는 웃음을 띄워, 스스로를 위로하는 %CALLNAME:MASTER%의 모습을 응시하고 있다…
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;얼굴에 사정하기자위절정
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ME_PLAYER(1) == 9
	;BASE:만족파생
	SELECTCASE TENSION(TARGET, 8)
		CASE "높다"
			PRINTFORMW 「이봐요, 여기에 내도 좋아요. 나의 입에…」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%왜냐하면 그러한 있는이지요? 나의 얼굴을 정액으로 질퍽질퍽하게 해 보고 싶겠지요?」
			PRINTFORMW 「 나의 흥이 깨기 전에, 빨리 하는 것이 좋아요」
			CALL FADEIN(15, "「이런 기회, 좀처럼 없기 때문에……」")
			WAIT
		CASE "일반"
			PRINTFORMW 「뭐 멍청히 하고 있어. 말해 좋다고 말해. 안다?」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%왜냐하면, 이런 것 싫지 않을 것입니다」
			PRINTFORMW 「 나의 기분이 바뀌기 전에, 빨리 시(분)편이 좋다고 생각해요」
		CASE "낮다"
			PRINTFORMW 「이봐요, 여기에 내. 알았다?」
			PRINTFORMW 「스스로도 자주(잘) 알지 않지만, 왠지 그러한 있고 기분이야」
	ENDSELECT
	RETURN 1
ENDIF

;REACT분류로 줍는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류:적극적으로 한다/받아들일까 개가버려상태로애태움중은 아니다（즉애태움종료/없음）경우
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM70, 가게해줘! 그리고 파생:가게해줘 주지 않는은애태움도로 연주하고 있으므로, 여기의 조건에는 걸리지 않는다
IF 구상기입체크(100) && IS_COMGRONAME("적극적으로 한다/받아들인다") && TCVAR:MASTER:가버려 && TCVAR:MASTER:애태움도 < 2
	LOCALS = \@ ALI(0) ? 후후 # 후후 \@
	;=================================================
	;처음 한편, 확실히절정에 이르는 경우
	;=================================================
	IF 구상기입체크(100) && TRUECHECK(NOWEX:MASTER:사정, NOWEX:MASTER:Ｃ절정) && FIRSTREACT(1)
		PRINTFORMW 「그렇게, 이 나무 그래. 뭐, 그것은 좋지만……」
		PRINTFORMW 「마음대로\@ PENIS(MASTER) ? 사정한다 # 간다 \@의는 허락하지 않아요. 내가 좋다고 말할 때까지 인내 없음 있고」
		PRINTFORM 「내가 허가하기 전에 이도 참……
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "오·시·오·키·", 1)
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		RETURN 1
	ENDIF
	
	;남자와후타나리로 나누는 것은 과연 힘들기 때문에 그만두고. 뭐 기분이 내키면 생각하지 않는 것도 아니다w
	;=================================================
	;죠교대상에페니스 있어
	;=================================================
	IF 구상기입체크(100) && PENIS(MASTER)
		SELECTCASE RANDREACT(5, 1)
			CASE 0
				PRINTFORMW 「에 , 이제 나와버린다의. 그것은 대단하구나」
				PRINTFORMW 「그렇다면 뭔가 말하는 일이 있겠지요? 시치미를 떼어도 안되어요」
				PRINTFORMW 「이봐요, 말하세요. 「사정시켜 주세요」는…」
			CASE 1
				PRINTFORMW 「 나에게 허가를 요구한다든가, 꽤 기특한 배려군요.%LOCALS%」
				PRINTFORMW 「여기서 보고 있어 주기 때문에, 마음껏 하면 좋아요」
			CASE 2
				PRINTFORMW 「-응, 그렇게. 나로 보여지면서 정액 털어 놓고 싶구나. 이 변태…」
				PRINTFORM 「이라면, 그렇구나…「
				CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), @"\@ TALENT:MASTER:남자 ? 나 # 나 \@가사정 하는 곳 봐 주세요", 1)
				PRINTFORMW 」(이)라고도 말해 받을까.%LOCALS%…」
			CASE 3
				PRINTFORMW 「뭐야, 이제 나와버린다의? 나로 보여지면서, 시코시코 해 사정해버리는 (뜻)이유?」
				PRINTFORMW 「바보 같다 얼굴 하면서, 필사적으로 취급는… 그렇게 키모치 좋다? 이 변태!」
			CASE 4
				PRINTFORMW 「이제 내자? 상당히 진전된 것 같고 최상이구나」
				PRINTFORM 「 나는 여기서 보고 있으니, 기분이 내킬 때까지 시코시코 하면 좋아요. 변태\@ TALENT:MASTER:남자 ? 쿠 # 씨 \@…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가페니스밴드장착중
	;=================================================
	IF 구상기입체크(100) && TEQUIP:페니스밴드
		SELECTCASE RANDREACT(3, 2)
			CASE 0
				PRINTFORMW 「그렇게, 이 나무 그래. 그런 것을 몸에 대고 있어도 좋아져버린다 것?」
				PRINTFORMW 「오히려, 몸에 대고 있기 때문이야말로…인지도이지만.%LOCALS%…」
			CASE 1
				PRINTFORMW 「대강, 사양이 없는 사람. 남자에라도 된 것 같은 기분으로 흥분해 버렸다라든지?」
				PRINTFORMW 「뭐,%CNAME(MASTER, TARGET)%의 경우…볼 수 있어와 흥분해버린다, 는만의 이야기일까」
			CASE 2
				PRINTFORMW 「어머나, 좋네요. 여자 아이가 그런 것 몸에 대면서, 어떤 바람으로 이우노인가…」
				PRINTFORMW 「모처럼의 기회이니까, 차분히 감상시켜 주어요」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외（죠교대상가여자 또한페니스밴드장착중이 아닌 경우）
	;-------------------------
	SELECTCASE RANDREACT(5, 3)
		CASE 0
			PRINTFORMW 「방법 없네요. 이제 와서 안된다고 말해도, 이제 멈추지 않는 것 같고….%LOCALS%」
			PRINTFORM 「뭐, 손을 대지 않고…다만 보고 있다고 것도 이따금은 좋네요
			CALL PRINT_SYMBOL_K14
		CASE 1
			PRINTFORMW 「마음대로 이일까 산 일은 칭찬해 주어도 괜찮아요. 좋은 배려군요」
			PRINTFORMW 「 나는 여기서 보고 있으니, 좋아하는 대로 하면 좋아요」
		CASE 2
			PRINTFORMW 「에 , 그렇게. 나로 보여지면서 이(뜻)이유…」
			PRINTFORMW 「오히려 나에게…, 이크곳을 보았으면 좋은 것일까. 응?」
		CASE 3
			PRINTFORMW 「-알았어요. 자위하면서 이곳, 내가 보았으면 좋겠지요?」
			PRINTFORMW 「여기서 보고 있어 주기 때문에,%CNAME(MASTER, TARGET)%(이)가 기분이 내키도록(듯이) 하면 돼요」
			;1/3로 진
			IF !RAND:3
				PRINTFORM 「이런 것, 싫지 않다%RAND_SPLIT("오히려 좋아하고/해")%. 그런데…
				CALL PRINT_SYMBOL_K14
			ENDIF
		CASE 4
			PRINTFORMW 「\@ GET_KFLAG_K14("상사상애") ? 헤에, 가 버린다 # 흐음, 그렇게…가버려(이)야 \@.%RAND_SPLIT("나의 눈앞/남의 앞")%그리고 자위하면서. 응?  %LOCALS%」
			PRINTFORMW 「으로? 내가 보았으면 좋다는 (뜻)이유? 그런 것이겠죠」
			PRINTFORMW 「이봐요, 정직하게 말\@ GET_KFLAG_K14("상사상애") ? 라고 보세요 # 우노 \@……」
	ENDSELECT
	RETURN 1
ENDIF

;쾌락에 대해서부끄러워한다의 경우는 COM에 흘린다
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM10, 부끄러워한다（상황에 대해서） RANDREACT(10)~
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다") && TFLAG:REACT파생 == 0
	SELECTCASE RANDREACT(2, 10)
		CASE 0
			PRINTFORMW 「부끄러우면 그만두면 좋다고 생각하는데」
			PRINTFORMW 「그렇지만, 손은 멈추지 않아?  \@ AFFECTION("상사상애") ? 후후 # 후후 \@」
		CASE 1
			PRINTFORMW 「어머나, 그렇게 부끄러워하는 일 없어요」
			PRINTFORM 「지금의 %CNAME(MASTER, TARGET)%,\@ AFFECTION("상사상애") ? 매우 # 꽤 \@사랑스러워요
			CALL PRINT_SYMBOL_K14
	ENDSELECT
	RETURN 1
ENDIF

;소극적인의가 마음에 들지 않는 것 경우는 COM에 흘린다
