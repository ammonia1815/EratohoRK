;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM14, 쾌감을 참는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;부하 2, 파생:애원, 죠교대상에페니스없음의 경우는 연주한다
IF 구상기입체크(55) && ALLSAMES(1, PENIS(MASTER), IS_NOWCOMNAME("쾌감을 참는다"), TFLAG:REACT부하 < 2, !TFLAG:REACT파생)
	IF AFFECTION("상사상애")
		SELECTCASE RANDREACT(2, 0)
			CASE 0
				PRINTFORMW 「%KOJO_RAND_K14(("펠라"), 1)%」
				PRINTFORMW 「무슨 반응 얇아요…조금 화나요……」
			CASE 1
				PRINTFORMW 「참는다면 억지로라도, 녹여 준다…!」
				PRINTFORMW 「%KOJO_RAND_K14(("펠라"), 1)%」
		ENDSELECT
	ELSEIF AFFECTION("익애")
		SELECTCASE RANDREACT(2, 1)
			CASE 0
				PRINTFORMW 「%KOJO_RAND_K14(("펠라"), 1)%」
				PRINTFORMW 「그다지 즐겨 줄 수 있지 않은 것 같구나…」
