			CASE "무리가 없는행동"
				SELECTCASE RANDREACT(3, 141)
					CASE 0
						PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 정말로, 속상해」
					CASE 1
						PRINTFORMW 「%CNAME(MASTER, TARGET)%가 분명하게 하지 않으면, 나부터 빼앗을 때까지…」
					CASE 2
						CALL FADEIN(8, "「…………」")
						;WAIT 사이에 두지 않고 연결한다			과연 조금 괴로운 느낌이 들지 않는 것은 아니다. 눈가리개라든지 죽어w
						PRINTFORMDL %CALLNAME%(은)는 가만히%CALLNAME:MASTER%\@ !TEQUIP:눈가리개 ? 의 눈동자 # \@(을)를 응시해
						PRINTFORMDW 그\@ !TEQUIP:눈가리개 ? 시선 # 분위기 \@에 뒷걸음질쳤다%CALLNAME:MASTER%(은)는\@ !TEQUIP:눈가리개 ? 눈 # 얼굴 \@(을)를 덮으려고 한 순간, 입술을 빼앗았다…
				ENDSELECT
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			CASE "무리를했다행동"
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%KOJO_RAND_K14(("키스"), 1)%」")
				IF ANIME_CONFIG("FADE")
					CLEARLINE 1
				ENDIF
