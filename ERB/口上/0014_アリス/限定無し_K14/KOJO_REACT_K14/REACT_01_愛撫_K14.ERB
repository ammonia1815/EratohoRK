	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 받아들인다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				SELECTCASE RANDREACT(2, 150)
					CASE 0
						PRINTFORMW 「허리를 띄워 태연하게 산등성이다 는 (뜻)이유? 후후후」
						PRINTFORMW 「뭐,%CNAME(MASTER, TARGET)%의 경우…, 손가락 2 개는 어딘지 부족할지도」
					CASE 1
						PRINTFORMW 「\@ ABL:MASTER:Ｖ감각 > 4 ? 안쪽까지 손대어지는 것도, 상당히 익숙해졌다 # 안쪽을 손대어지는 것은, 아직 익숙해지지 않는다 \@까?」
						PRINTFORMW 「이라고 할까…. 후후, 허리 띄워 버려서. 내가 걱정할 것도 없었다 같구나」
				ENDSELECT
			CASE "무리가 없는행동"
				SELECTCASE RANDREACT(2, 151)
					CASE 0
						PRINTFORMW 「이봐요, 좀 더 다리를 열어. 그러면 안쪽까지 손가락이 닿아요」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%의 가장 안쪽의 코리코리 했다 곳, 가득 귀여워해 준다…」
					CASE 1
						PRINTFORMW 「안쪽을 손대어져도 아픔은 없을 것 같구나? 오히려, 상당히 좋은 느낌에 녹아 버린다의 것인지 해들. 후후」
						PRINTFORMW 「이봐요… 천천히 풀도록(듯이) 손대어지면 키모치 좋지요?」
						PRINTFORM 「담담하게, 그리고 상냥하게가 결정적 수단…라고  하면, 조금 이상할지도이지만
						CALL PRINT_SYMBOL_K14
				ENDSELECT
			CASE "무리를했다행동"
				PRINTFORMW 「후후, 상당히 얌전하다고 생각하면, 빚 녹아 버려서 의 것」
				PRINTFORMW 「좋아요, 그대로…, 나의 손가락끝의 움직임에 집중해……」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;REACT27, 밀어 넘어트린다·개별 리액션
;────────────────────────────────────
;상정되는 COM
;회화계 반응
;	COM:마음 약하게 응답한다/무뚝뚝하게 응답한다/상냥하게 응답한다/강하게 응답한다/용서를 빌다/기분좋게 해
;고통계 반응
;	COM:무서워한다
;-------------------------------------------------
@KOJO_REACT_K14_27
;-------------------------------------------------
;TFLAG:ACT파생（=0, 밀어 넘어트린다/=1, 밀어넘어짐종료）
;-------------------------------------------------
;애무계이지만 27이라고 한다
;애무계는 10~19까지 메워지고 있으므로 이제 와서 옮길 수도 없다
;-------------------------------------------------
SIF TFLAG:ACT파생
	RETURN 1

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT27, 밀어 넘어트린다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;Ａ섹스는 고려하지 않아도 괜찮은 것인지도 모른다
	;KOJO_TRAIN_MESSAGE2로 지문을 처리한다
	;=================================================
	;COM, 허리를 흔든다＆성교봉사중
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 받아들인다/허리를 흔든다/적극적으로 한다/태연하게 한다") && TEQUIP:성교봉사중 && FLAG:지문제어 == 2
		PRINTFORM 「%RAND_SPLIT("후아, 아앗/아웃/쿠, 앗/응앗/우아… 앗/하웃, 아… 아˝/아… 아˝! 야")%…
		CALL AUTOHEARTCB_K14
		;쾌PALAM로 파생
		SELECTCASE V_SEX(MASTER) ? PALAM:쾌Ｖ # PALAM:쾌Ａ
			CASE IS >= 8500
				SELECTCASE TRAINNAME:SELECTCOM
					CASE "쾌감을 받아들인다"
						PRINTFORMW  그런 얼굴 되면(자), 감등 돌아가셔버린다가 아니다」
						PRINTFORMW 「신체중이 뜨거워져, 나…이제 멈추지 않게 될 것 같다……」
					CASE "태연하게 한다"
						PRINTFORMW  (이)나다 코레, 내가 이인가 되어버릴 그렇게…」
						PRINTFORMW 「내가 약하면 나뭇 조각빌리고 꾸짖다니 간사하다%RAND_SPLIT("와/")%……」
					CASEELSE
						PRINTFORMW  지금, 그렇게 되면(자)…」
						PRINTFORMW 「조금 코레, 위험한, 일지도……」
				ENDSELECT
			CASE IS >= 5000
				SELECTCASE TRAINNAME:SELECTCOM
					CASE "쾌감을 받아들인다"
						PRINTFORMW  응 키모치 좋다? 키모치 좋지요?」
						PRINTFORMW 「이봐요…, 좀 더 나를 느껴……」
					CASE "태연하게 한다"
						PRINTFORMW  뭐야……, 아직도 여유는 (뜻)이유?」
						PRINTFORMW 「 나뿐 좋아져 버려서, 어쩐지 분해요…」
					CASEELSE
						PRINTFORMW  꽤, 하잖아」
						PRINTFORMW 「좋은 곳 탐색 기대는, 조금 적이군요…」
				ENDSELECT
			CASEELSE
				SELECTCASE TRAINNAME:SELECTCOM
					CASE "쾌감을 받아들인다"
						PRINTFORMW  얌전해져 버려서…」
						PRINTFORMW 「조금 사랑스럽네요……」
					CASE "태연하게 한다"
						PRINTFORMW  %CNAME(MASTER, TARGET)%, 상당히 여유 있어요…」
						PRINTFORMW 「조금 거만함……」
					CASEELSE
						PRINTFORMW  상당히 적극적이 아니다. 그렇지만…」
						PRINTFORMW 「나쁘지 않아요, 그것……」
				ENDSELECT
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT분류가 받아들인다
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("받아들인다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「후후, 그런 얼굴 되면(자) 기대에 응하지 않는 것에는 가지 않네요」
				PRINTFORM 「다만 그것이,%CNAME(MASTER, TARGET)%(이)가 바란것인지 어떤지는 뭐라고 말할 수 없지만. 그런데…
				CALL PRINT_SYMBOL_K14
			CASE "무리가 없는행동"
				PRINTFORMW 「말하지 않아도 알아요. 지금부터 나로 되는 일을 생각하고 있는 지요?」
				PRINTFORMW 「키모치 좋은 금년이라고 받을 수 있다고 생각해, 기대하고 있는 지요?」
				PRINTFORMW 「후후…,%CNAME(MASTER, TARGET)%라고 알기 쉬워」
			CASE "무리를했다행동"
				PRINTFORMW 「그렇구나, 다소는 저항해 주는 위가 즐길 수 있는 것」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 잘 알고 있잖아. 후후…」
		ENDSELECT
		RETURN 1
	ENDIF

	;=================================================
	;REACT분류가적극적으로 한다/태연하게 한다
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		;부하 0으로 1은 정리해 버린다
		IF GROUPMATCH(TFLAG:REACT부하, 0, 1)
			;죠교대상가 꼬마님
			IF TALENT:MASTER:작은체형
				PRINTFORM 「후후, 사랑스럽다…
				CALL AUTOHEARTCB2_K14
				PRINTFORMW  내가 마음대로 해도 좋다는 일일까?」
				PRINTFORMW 「뭐, 그러한 일이라면……충분히귀여워해 주기 때문에, 즐거움으로 하고 있어…」
			;죠교대상이남자
			ELSEIF TALENT:MASTER:남자
				PRINTFORMW 「여자에게 밀어 넘어뜨려져, 되는 대로의 남자는 조금 어떻게야? 라는 의문으로 생각하는데…」
				PRINTFORMW 「뭐, 이것은 이것대로 나쁘지 않네요. 후후」
			;그 외
			ELSE
				PRINTFORMW 「어머나, 상당히 솔직하지 않다. 사랑스러워요…」
				PRINTFORMW 「그런데, 지금부터 어째서 줄까, 응?」
				PRINTFORMW 「후후…」
			ENDIF
		;부하 2
		ELSE
			;실은 그렇지만도 않다
			IF COMCOR_POSI()
				PRINTFORMW 「에 , 상당히 복잡한 표정 해.%CNAME(MASTER, TARGET)%(은)는 지금, 무엇을 생각하고 있을까」
				PRINTFORMW 「아무튼에서도… 저항하지 않는다고 것은, 나의 자유에 해도 좋다는 일이군요?」
			;실은 싫어
			ELSE
				PRINTFORMW 「강한척 하면서도, 어딘가 무서워하고 있도록(듯이)?」
				PRINTFORMW 「 그렇지만, 저항하는 관심이 없다면…, 나머지는 좋아하게 시켜 주어요」
			ENDIF
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ ALI(0) ? ふふ # フフ \@…」")
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ ALI(0) ? 후후 # 후후 \@…」")
			SIF ANIME_CONFIG("FADE")
				CLEARLINE 1
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT분류가용서를 빌다
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("용서를 빌다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 나를 무엇이라고 생각하고 있을까?」
				PRINTFORMW 「아무튼에서도, 걱정하지 않아 좋아요. 별로 심한 일 할 생각이 아니니까」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 얌전하게 따라서 있는 한은, 이군요…」
			CASE "무리가 없는행동"
				PRINTFORMW 「정면에서 무서워할 수 있는 것은, 보통 좋은 기분은 하지 않는 것이지만…」
				PRINTFORMW 「지금은 나쁘지 않다는 생각이 들어요. 오히려, 두근두근 해버릴까도」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「ウフフ…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「우후후…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			CASE "무리를했다행동"
				;실은 그렇지만도 않다
				IF COMCOR_POSI()
					PRINTFORMW 「그러면무엇? 심한 일이 아니면 어떻게 해도 좋다는 일?」
					PRINTFORMW 「이봐요, 부끄러워하지 마. 분명하게 여기를 향해 대답하세요」
				;실은 싫어
				ELSE
					PRINTFORMW 「뭐, 그래요. 나의 기분을 해치지 않게 배려하는 것은 잘못하지 않아요」
					PRINTFORMW 「다만…, 심한 일 할지 어떨지는%CNAME(MASTER, TARGET)%나름이야」
					PRINTFORMW 「기억해 두세요……」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT분류가소극적으로 한다
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("소극적으로 한다")
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				PRINTFORMW 「어머나, 그렇게 무서워하는 일 없어요?」
				PRINTFORM 「얌전하게 하고 있으면, 귀여워해 주어요…
				CALL PRINT_SYMBOL_K14
			CASE "무리가 없는행동"
				PRINTFORMW 「,%RAND_SPLIT("체념해/단념해")%얌전하게 하고 있어 주세요」
				SIF !RAND:3
					PRINTFORMW 「우도…, 저항했더니 쓸데없다고 생각하는데. 그런데」
			CASE "무리를했다행동"
				;실은 그렇지만도 않다
				IF COMCOR_POSI()
					PRINTFORMW 「후후, 그렇게 부끄러워하는 일 없어요」
					PRINTFORMW 「사실은, 지금부터 되는 것에 기대하고 있는 지요?」
					PRINTFORMW 「이봐요, 정직하게 말해 보세요…」
				;실은 싫어
				ELSE
					PRINTFORMW 「이 상황은 본의가 아니지만, 나를 화나게 하는 것은 꺼려진다 라는 곳인가 해들?」
					PRINTFORMW 「아무튼에서도…, 그것은 현명한 판단이라고 생각해요」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
