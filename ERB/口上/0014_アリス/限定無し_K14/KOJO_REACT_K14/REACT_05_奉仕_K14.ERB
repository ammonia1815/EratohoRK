;=================================================
@KOJO_REACT_K14_52
;=================================================

;범용 처리
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가 요구에 응하지 않는다(거절)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_COMGRONAME("거부/날뛴다/도망친다") && TFLAG:REACT부하 < 2
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K14
