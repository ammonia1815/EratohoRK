;========================================================================
;회화계 ACT0～9
;========================================================================
;·TFLAG:ACT(0=지금 기분을 묻다/1=자신에게의 마음을 묻다/2=성에 관한 이야기를 하다/3=상냥하게 위로한다/4=엄하게 위협한다/5=의상 변경을 요구한다/6=합의를 요구한다/7=징계한다고 위협한다/8=고함치다/9=기분나쁘게 웃다)
;·TFLAG:ACT분류 == 0에 해당
;·ACT9, 기분나쁘게 웃다는이상계 ACT분류로부터 파생하는 경우가 있다
;·범용 리액션에 대해서는 MENUMATCH(TFLAG:ACT, "회화")로 줍는 것이 가능
;-------------------------------------------------
;·반환값(RESULT)에 대해
;·시스템측에서는구상측의 RESULT(종단의 RETURN 1 등)에 의해구상가 표시당했다의지 아닌지,
;·또 개소 마다의 특유의 거동(미약등구상를 스킵 따위)을 제어하고 있습니다
;·@구상기입 체크의 경우 대입 처리를 생략 할 수 있습니다만, RESULT치는구상측에서 결정하고 쳐 하는필요가 있습니다
;·특별한 이유가 없는 한, 텐프레로서는 RETURN 1을 기술해 둡니다
;-------------------------------------------------
;────────────────────────────────────
;ACT0, 지금 기분을 묻다
;────────────────────────────────────
;·지금의 기분에 대해 들어 온다
;·죠교대상의 반응에 응해 REACT 이후에 ACT파생에의 대입을 한다
;·턴방침가"이상"라면 발생하지 않는다
;-------------------------------------------------
@KOJO_ACT_K14_0
;-------------------------------------------------
;APPLY2로 대입을 하는 것이 ACT파생라고 하는 것도 뭔가감
;머지않아 정리되어야 할 종류의 대용품
;-------------------------------------------------
;회화계 ACT와 저것 하지 않으면이다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT0, 첫회
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && FIRSTACT(0)
	PRINTFORMW 「어머나, 기분 나쁜 것 같구나」
	PRINTFORMW 「응… 당연? 뭐, 그래요. 후후후……」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;그죠교의 최초의 액션
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:경과시간 == 1
	IF AFFECTION("상사상애")
		PRINTFORMW 「응, 기분은 어때? 라고 말할까…」
		PRINTFORMW 「내가 말을 건네고 있기 때문에, 좀 더 기쁜 듯한 얼굴 해 주세요!」
	ELSE
		PRINTFORMW 「정직,%CNAME(MASTER, TARGET)%의 지금의 기분이라든지에 흥미없는거야%RAND_SPLIT("(이)군요/…")%」
		PRINTFORMW 「 나의 하고 싶게 해요. 우후후…」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상가자위중
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && GETBIT(TEQUIP:자위중, 0)
	LOCALS:0 = \@ GET_KFLAG_K14("상사상애") ? %RAND_SPLIT("후후후/후후/후후/킥킥")% # %RAND_SPLIT("후후후/후후/후훅")% \@
	SELECTCASE RANDACT(20)
		CASE 0
			PRINTFORMW 「어때? 지금의 기분은. 라고……」
			PRINTFORMW 「즐거움중에 이런 일 (듣)묻는 것은 멋없었던 까. 응?」
		CASE 1
			PRINTFORMW 「대강, 힘써요? 라고……」
			PRINTFORMW 「이 경우의 말의 의미,%CNAME(MASTER, TARGET)%(은)는 제대로 이해할 수 있는 것일까」
		CASE 2
			PRINTFORMW 「\@ GET_KFLAG_K14("상사상애") ? 후후 # 아라아라 \@, 즐거운 듯 하고 최상이구나\@ !GET_KFLAG_K14("상사상애") ? 에?  # \@」
			PRINTFORMW 「뭐, 정직……, 지금 나에게 참견 나오는 것은 폐가 되는 응이지요지만」
			PRINTFORMW 「 그렇지만, 입다물고 봐도 아레이고……, 그만 만지작거리고 싶어져버린다 것이군요」
	ENDSELECT
	CALL FADEOUT(10, @"「%LOCALS:0%……」")
	CALL FADE_CLINE(1)
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;성교봉사계로질내사정의 후
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:전회질싸 == 2
	PRINTFORMW 「뭐야, 완수한 것 같은 얼굴 해 버려서…」
	PRINTFORMW 「 그렇지만…, 후후. 나의 질내는 좋았다 같구나」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;성교계로질내사정의 후
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:전회질싸 == 1
	PRINTFORMW 「%CNAME(MASTER, TARGET)%에 질내사정…최고군요♪」
	PRINTFORMW 「응, 나에게 정복 된 기분은 어떻게 까?」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;갈 것 같다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TCVAR:MASTER:가버려
	;=================================================
	;갈 것 같은 것을참다 하고 있다
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:참다
		PRINTFORMW 「어머나, 노력해요?」
		PRINTFORM 「%CNAME(MASTER, TARGET)%가 언제까지 견딜 수 있는지, 시험해 보는 것도 나쁘지 않네요
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;애태움중
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:애태움도 > 1
		PRINTFORMW 「응, 초조하게 해지고 있는 기분은 어때?」
		PRINTFORMW 「간단하게는 이나 등 있고 원이야, 나쁜데. 후후…」
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「\@ GET_KFLAG_K14("상사상애") ? 후후 # 후후 \@, 이키 그런?」
	PRINTFORM 「 그렇지만, 간단하게 이나 키라고는 재미있지 않아
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;볼개그장착중
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TEQUIP:볼개그
	PRINTFORMW 「후후, 좋은 모습이군요. 지금의 기분은 어떻게 까?」
	PRINTFORMW 「이라고 말해도, 지금의 %CNAME(MASTER, TARGET)%에는 대답할 수 없겠지만, 이군요♪」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;눈가리개장착중
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TEQUIP:눈가리개
	PRINTFORMW 「시야를 빼앗긴 기분은 어떻게든 해들? 후후…」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;안면승마중
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0
	PRINTFORMW 「응……, 나에게 이런 일 되고 있는 기분은 어떻게든 해들?」
	PRINTFORMW 「그렇지만 이 상태에서는 대답할 수 없을까, 군요? 후후후…」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;삼각목마사용중
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TEQUIP:삼각목마
	PRINTFORMW 「나라도 생각해? 이런 상황으로 기분은 (들)물어서는 안된다고」
	PRINTFORMW 「후후후…」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;발기
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && PENIS(MASTER) && TCVAR:MASTER:발기도 >= 1000
	IF AFFECTION("상사상애")
		PRINTFORMW 「 이제 긴긴이 되어…」
		PRINTFORMDW 앨리스는 %CALLNAME:MASTER%의 페니스를 손가락으로 쿡쿡 찌르기 시작했다…
		PRINTFORMW 「비쿤비쿤 해 버려서… 후후후…」
	ELSE
		PRINTFORMW 「다소는 사마가 되어도 곳일까」
		PRINTFORMW 「쇠약해지고 자른 정도, 흥 깨고 하는 것은 없고. 응?」
		PRINTFORMW 「후후후…」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;쇠약해졌다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && PENIS(MASTER) && (TCVAR:MASTER:발기도 < TCVAR:MASTER:전발기도)
	IF AFFECTION("상사상애")
		PRINTFORMW 「 나의 앞에서 쇠약해진다든가, 허락하지 않아요…」
	ELSE
		SELECTCASE RANDACT(2, 110)
			CASE 0
				PRINTFORMW 「그렇지 않아도 궁상스러운데, 그런에 위네 시켜 버려서…」
				PRINTFORMW 「진짜, 볼품이 하지 않네요」
			CASE 1
				PRINTFORMW 「… 완전히. 너무 허술해 슬퍼져요?」
				PRINTFORMW 「사실, 사용할 수 없어요」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT0, 기타·조건없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	PRINTFORMW 「응,%RAND_SPLIT("상태는 어떨까/기분은 어떠세요?/지금의 기분은 어때?")%」
	RETURN 1
ENDIF

;────────────────────────────────────
;ACT1, 자신에게의 마음을 묻다
;────────────────────────────────────
;·자신의 일을 어떻게 생각할까 얘기한다
;·죠교대상의 반응에 응해 REACT 이후에 ACT파생에의 대입을 한다
;·턴방침가"이상"라면 발생하지 않는다
;·화제 『호의』가 기출이 되어, 이후, 동일조교중에서는 억제된다
;-------------------------------------------------
@KOJO_ACT_K14_1
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT1, 첫회
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 구상기입 체크(0) && FIRSTACT(0)
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;그죠교의 최초의 액션
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:경과시간 == 1 && !GET_KFLAG_K14("상사상애")
	PRINTFORMW 「응…, 조금 들려주었으면 좋지만」
	PRINTFORMW 「%CNAME(MASTER, TARGET)%, 나의 일 어떻게 생각하고 있는 거나?」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;상사상애 또한이벤트로플래그가 서 있다
;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스씨는 이런 것이 너무 어울려 곤란함
;여기는 상황 묘사를 날조 해도 될지도 시련
;-------------------------------------------------
IF 구상기입체크(100) && GET_KFLAG_K14("상사상애")
	LOCALS = \@ TENSION(TARGET, 8) == "높다" ?!  # \@
	;kawaii
	SELECTCASE RANDACT(4, 0)
		CASE 0
			PRINTFORMW 「이제 와서 대답은 (들)물을 것도 없지만, 일단 (들)물어 두어요」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 나의 일 어떻게 생각하고 있다?」
		CASE 1
			PRINTFORMW 「다 안 일에서도, 가끔 말로 하는 것은 중요해」
			PRINTFORMW 「이봐요, (들)물어 주기 때문에 빨리 좋다 차이야%LOCALS%」
		CASE 2
			PRINTFORMW 「무엇으로 지금 쯤 그런 일이라든지, 시시한 것을 말%RAND_SPLIT("우노는 그만두군요/도 참 때려요")%…」
			PRINTFORMW 「이런 것은, 한 번 말하면 끝난다 라는 이야기는 아닌거예요. 안다?」
		CASE 3
			PRINTFORMW 「이봐요! 멍청히 하지 않고, 삿사라고 대답하세요」
			PRINTFORMW 「어차피…, 대답은 다 알고 있지만%LOCALS%」
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;기타·조건없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	PRINTFORMW 「%CNAME(MASTER, TARGET)%는 나의 일 어떻게 생각하고 있는 거나?」
	PRINTFORMW 「응, 들려주어 주세요…」
	RETURN 1
ENDIF

;────────────────────────────────────
;ACT2, 성에 관한 이야기를 함
;────────────────────────────────────
;·죠교자가 성에 대한지식를 말하기 시작한다
;·TFLAG:징계·플래그가 서 있으면 발생하지 않는다
;·턴방침가"이상"라면 발생하지 않는다
;·죠교자에성지식가 없으면 발생하지 않는다
;·화제 『지식』이 기출이 되어, 이후, 동일조교중에서는 억제된다
;-------------------------------------------------
@KOJO_ACT_K14_2
;-------------------------------------------------
;정직, 미묘한 커맨드다라고 생각한다w
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;처음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && FIRSTTIME(0)
	PRINTFORMW 「응,%CNAME(MASTER, TARGET)%는 이런 이야기에 대해 들은 일 있는 것일까?」
	PRINTFORMW 「 나도 자주(잘) 알고 있는 (뜻)이유가 아니지만――」
	RETURN 1
