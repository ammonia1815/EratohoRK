;/////////////////////////////
;ACT11, 가슴애무
;/////////////////////////////
[SKIPSTART]
・가슴애무/乳首吸い/乳の揉み合い/乳首責め
・죠교대상の性別によって派生の動作は異なる

#죠교대상이남자の場合
	1.^방침[11] -かわいがっての場合服で妨害されなければ乳首吸い
	2.^죠교자の호의2000若しくは죠교자が素質：혀기술持ちの場合は2/3の確率で乳首吸い
	3.^上記の条件から漏れたら乳首責め
	それぞれに派生する

#죠교대상이남자でない(여자の)場合
	1.^방침[11] -かわいがっての場合服で妨害されなければ乳首吸い
	2.^전회ACTが가슴애무の場合、죠교자に素質：유치若しくは혀기술がある場合3/4、なければ1/4で乳首吸い(これらは服による妨害を受ける)
		それ以外は乳首責め。
	3.^上記の条件から漏れた場合は揉み系
		죠교자の욕망+Ｂ감각３以上なら、1/2で가슴の揉み合い
		揉み合いに派生しなければ가슴애무(派生0)のまま
[SKIPEND]
@KOJO_ACT_K14_11 ;TFLAG:ACT파생(0=가슴애무/1=乳首吸い/2=揉み合い/3=乳首責め)
#DIMS DYNAMIC STRING
#DIMS DYNAMIC FIRSTPERSON
#DIMS DYNAMIC RAND_HEART
#DIMS DYNAMIC MSTR
#DIMS DYNAMIC TGT
STRING '= AFFECTION("익애", TARGET) ? "ふふ" # "フフ" 
FIRSTPERSON '= TALENT:MASTER:작은체형 ? "お姉さん" # "私" 
RAND_HEART  '= RAND:3 ? HTML_HEARTB(, COLOR("PINK")) # HTML_HEART(, COLOR("PINK"))
MSTR '= CALLNAME:MASTER
TGT  '= CALLNAME:TARGET

;ACT11_0, 가슴애무
;================
IF !TFLAG:ACT파생
	;//後ろから
	IF TCVAR:MASTER:위치전후 == 2
		SELECTCASE RANDACT(3, 0)
			CASE 0
				PRINTFORMW 「뒤로부터라면 신체가 밀착해 좋은 느껴라.%CNAME(MASTER, TARGET)%도 그렇게 생각하지 않는다？」
			CASE 1
				PRINTFORMW 「역시 뒤로부터(분)편이 좋아요. 젖가슴 이외의 트코도 귀여워해 줄 수 있고」
				CALL FADEOUT(15, @"「%STRING%…」")
				CALL FADE_CLINE(1)
			CASE 2
				{
				PRINTFORMW 「%CNAME(MASTER, TARGET)%의 표정을 볼 수  없는 것은 조금\@ 
				AFFECTION("익애", TARGET) ? 残念だ # 物발りない 
				\@けど、これはこれで悪くないわね…」
				}
		ENDSELECT
		PRINTFORMDL %TGT%은(는) %MSTR%의 귀나 목덜미를 애무하면서, 접할까 접하지 않는가의 절묘한 터치로
		PRINTFORMDW \@ BRACHECK(1, 1) && BRACHECK(1) ? 천 너머로 # \@%MSTR%의 %OPPAI_DESCRIPTION(MASTER)%전체를 애무했다…
	;//前から
	ELSE
		SELECTCASE RANDACT(3, 1)
			CASE 0
				PRINTFORMW 「긴장하지 않아 좋아요. 후는%FIRSTPERSON%에 맡겨…」
			CASE 1
				PRINTFORMW 「이제 와서일지도 모르지만, 이렇게 해 보면(자) 꽤 사랑스럽네요,%CNAME(MASTER, TARGET)%…」
				CALL FADEOUT(15, @"「%STRING%…」")
				CALL FADE_CLINE(1)
			CASE 2
				PRINTFORMW 「이렇게%CNAME(MASTER, TARGET)%의 표정을 볼 수 있는 것도 좋은 것」
				PRINTFORMW 「이봐요, 신체가 힘을 빼.%FIRSTPERSON%하지만 귀여워해 주어요…」
		ENDSELECT
		PRINTFORMDL %TGT%하\@ BRACHECK(1, 1) && BRACHECK(1) ? 천 너머로 # 노출되었다 \@%MSTR%의 %OPPAI_DESCRIPTION(MASTER)%전체를
		PRINTFORMDW 소프트인 터치로 애무했다…
	ENDIF
	[SKIPSTART]
	PRINTFORMW 「민감하고 귀여운 가슴이군요…」
	PRINTFORMW 「손에 달라붙는 느낌으로, 매우 멋져요」
	[SKIPEND]
	RETURN 1
ENDIF


;ACT11_1, 乳首吸い
;==================
IF TFLAG:ACT파생 == 1
	;//성교중・성교봉사중(事実上성교중:3の대면좌위の時にしか発生しないはず)
	IF TEQUIP:성교중 || TEQUIP:성교봉사중
		SELECTCASE RANDACT(3, 11)
			CASE 0
				CALL HTMLPRINTW(@"「응…%RAND_HEART% 좋은 느낌이야. 이봐요, 여기도 귀여워해 준다…」")
			CASE 1
				CALL HTMLPRINTW(@"「후후, 지금의 %CNAME(MASTER, TARGET)%매우 사랑스러워요…%RAND_HEART% 」")
				{
				CALL HTMLPRINTW(@"「이봐요 여기…츄%RAND_SPLIT("읏/푸/풋")%%RAND_HEART% " + 
				@"이렇게 되어…%RAND_SPLIT("엣취/츄웃")%%RAND_HEART% %RAND_SPLIT("어떨까/기분 좋을 것입니다")%？」")
				}
			CASE 2
				CALL HTMLPRINTW(@"「조금 여기가 외로운 것 같구나.%FIRSTPERSON%하지만 귀여워해 준다…%RAND_HEART%」")
		ENDSELECT
		CALL FADEOUT(20, @"  %KOJO_RAND_K14(("乳首吸い"), 1)%")
		CALL FADE_CLINE(1)
		RETURN 1
	ENDIF

	;//수음・애무継続中
	IF TEQUIP:죠교대상Ｃ사용 == 1 ;PENIS(MASTER)で分けたいが面倒なので統合
		SELECTCASE RANDACT(3, 12)
			CASE 0
				{
				CALL HTMLPRINTW(@"「이봐요, 어때？ \@ 
				PENIS(MASTER) ? シコシコ # クチュクチュ 
				\@されながら乳首を\@ 
				TALENT:MASTER:남자 ? 舌で転がされる # 吸われる 
				\@の %STRING%…%RAND_HEART%」")
				}
				{
				PRINTFORMDL %TGT%(은)는 느긋한 stroke로\@ 
				TCVAR:MASTER:애태움도 > 1 ? 焦らすように # 優しく 
				\@%MSTR%の%Ｃ描写(MASTER)%を\@ 
				PENIS(MASTER) ? しごき # 指先で撫で \@하면서、
				}
				PRINTFORMDW %NIPPLE_DESCRIPTION(MASTER)%유두를 애무하고 있다…
			CASE 1
;OriginalString : (@"「あ～～%RAND_HEART%」")
				CALL HTMLPRINTL(@"「아～～%RAND_HEART%」")
				PRINTFORMDW %TGT%(은)는 과시하는것 같이 입을 크게 열어 혀끝을 펴%MSTR%의 %NIPPLE_DESCRIPTION(MASTER)%유두를 애무하기 시작했다…
;OriginalString : (@"「%RAND_SPLIT("ん/ちゅ/くちゅ/ちゅぷ")%…%RAND_HEART% ねぇ…、きもちいい？ キモチイイでしょう？」")
				CALL HTMLPRINTL(@"「%RAND_SPLIT("응/츄/쿠츄/츄푸")%…%RAND_HEART% ねぇ…、きもちいい？ キモチイイでしょう？」")
				PRINTFORMDW %TGT%(은)는 그렇게 귓전으로 속삭이면서, 군요 새로 한 손놀림으로%MSTR%의 %Ｃ描写(MASTER)%(을)를\@ PENIS(MASTER) ? 해 어휘 # 애무해 \@(이)라고 있다…
			CASE 2
;OriginalString : (@"「%RAND_SPLIT("ちゅっ/ん")%…%RAND_HEART% %RAND_SPLIT("くちゅ…くちゅ…/クチュ…クチュ")%%RAND_HEART%」")
				CALL HTMLPRINTL(@"「%RAND_SPLIT("츗/응")%…%RAND_HEART% %RAND_SPLIT("くちゅ…くちゅ…/クチュ…クチュ")%%RAND_HEART%」")
				PRINTFORMDW %MSTR%의 입술을 한 바탕 맛본 후,%TGT%은(는) %MSTR%의 %NIPPLE_DESCRIPTION(MASTER)%유두를 혀끝에서 애무하기 시작했다…
				PRINTFORMDL %TGT%은(는) %MSTR%의 %Ｃ描写(MASTER)%(을)를 어루만지도록(듯이) 상냥하고\@ PENIS(MASTER) ? 호된 훈련 # 애무해 \@(이)면서,
				PRINTFORMDW %MSTR%의 유두에 말이야 새와 혀를 기게 했다…
				PRINTL
				{
				PRINTFORMW 「응～이것, 조금 강하게 해에\@ 
				PENIS(MASTER) ? シコシコ # クチュクチュ 
				\@했다ら%CNAME(MASTER, TARGET)%、すぐにイっちゃいそうね%RAND_SPLIT("ぇ。/。/？ ")%%STRING%…」
				}
				;금일의방침が애만들기
				SIF IS_POLICY("금일", "애만들기") && PENIS(MASTER)
					CALL HTMLPRINTW(@"「그렇지만애만들기의 날인 것이니까…낸다면%FIRSTPERSON%의 안으로 해 받지 않으면. 저기%HTML_PS_K14()%」")
		ENDSELECT
		RETURN 1
	ENDIF

	;@NIPPLE_DESCRIPTIONに対応して죠교대상のPALAMで派生
