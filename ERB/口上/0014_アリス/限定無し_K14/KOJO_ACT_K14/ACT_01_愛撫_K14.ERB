		ENDSELECT
		;フェードアウト
		CALL FADEOUT(20, @"「\@ AFFECTION("상사상애") ? %RAND_SPLIT("후후후/킥킥/킥킥")% # %RAND_SPLIT("フフフ/ウフフ/フフ")% \@……」")
		CALL FADE_CLINE(1)
		RETURN 1
	ENDIF

