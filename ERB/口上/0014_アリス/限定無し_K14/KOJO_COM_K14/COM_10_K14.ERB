@KOJO_COM_K14_13
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM13, 겁없이 웃는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:REACT파생
	;=================================================
	;無理を했다행동
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理を했다행동"
		SELECTCASE RANDCOM(2, 0)
			CASE 0
				PRINTFORMW 「에 , 재미있지 않다. 그렇지만…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 상당 무리하고 있는 것 같지만. 그근처 어떤가 해들 , 이군요」
			CASE 1
				PRINTFORMW 「그런 얼굴 했더니, 여유가 없는 것은 들키고 들키고지만？」
				PRINTFORMW 「지금의 자신이, 단순한 익살꾼도 일이 알지 않는 것일까」
				PRINTFORMW 「우둔, 이군요…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が애무
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "애무")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울굴", "몽롱"
				PRINTFORMW 「지금의 %CNAME(MASTER, TARGET)%(으)로 할 수 있는 힘껏의 저항, 이라는 곳일까？ 기특한 일이군요」
				PRINTFORMW 「아무튼 무엇으로 저것, 지금의 나에게는 꼭 좋은 일이야. 충분히 귀여워해 주어요…」
			CASE "분노"
				PRINTFORMW 「조금은 상황을 생각했으면 좋은 것」
				PRINTFORMW 「 나, 지금은 관대한 기분이 될 수 없어, 나쁜데…」
			CASE "싫증"
				PRINTFORMW 「…보고 싶고도 없는 얼굴이군요. 싫증하고 있는 때에는 특히」
				PRINTFORMW 「진짜, 진절머리 나요」
			CASEELSE
				PRINTFORMW 「재미있는 얼굴이군요. 진짜는 좋아져 버린다 것에, 속이자고 일일까」
				PRINTFORMW 「…어쨌든. 거역하지 않는다는 것은, 받아들이고 있다고 일이죠？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が도구
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "도구")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울굴", "몽롱"
				PRINTFORMW 「……건방지구나. (이)라도 좋아요, 지금은 놓쳐 준다」
				PRINTFORMW 「그 만큼 마음껏 귀여워해 주기 때문에, 각오 해…」
			CASE "분노"
				PRINTFORMW 「몇번이나 말한 것 같은 생각이 드는데…기분이 나쁠 때에 그렇게 말하는 것 그만두어 주지 않을까？」
				PRINTFORMW 「너무 나의 선의에 기대되어도 곤란하기 때문에…」
			CASE "싫증"
				PRINTFORMW 「어차피라면 좀 더 싫증을 감출 수 있는 것 같은 일해 주었으면 한 것」
				PRINTFORMW 「거기에…그 모양으로 그런 얼굴 해도, 자신이 비참하게 될 뿐(만큼)이 아닐까？」
			CASEELSE
				PRINTFORMW 「그런 모양으로 그런 얼굴 되어도, 응？」
				PRINTFORMW 「아무튼에서도……그 나머지히로시가 언제까지 계속되는 것일까. 이것은 기다려지구나…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が성교
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "성교")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울굴", "몽롱"
				PRINTFORMW 「……건방지구나. 아무튼 좋아요」
				PRINTFORMW 「그러한 아이에게 적당한 귀여워하는 방법을 해 주기 때문에. 각오 해 주세요…」
			CASE "분노"
				PRINTFORMW 「강한척 해 버려서 사랑스럽다…정말, 지금은 말하는 기분은 아닌거예요. 미안한데」
				PRINTFORMW 「대개에 해 두지 않으면 아픈 눈에 맞추어요……」
			CASE "싫증"
				PRINTFORMW 「…아휴, 군요. 싫증한 마구로\@ TALENT:MASTER:남자 ? 남자 # 여자 \@의 생각은 어째서 이렇게 경박한가 해들 」
				PRINTFORMW 「그런 얼굴을 보여주었는데로, 작은 프라이드가 그저 일순간 채워질 뿐」
				PRINTFORMW 「최종적으로는 오히려…,%CNAME(MASTER, TARGET)%에 있어 불이익하게 되어」
			CASEELSE
				PRINTFORMW 「그러한 얼굴 하는 것은, 너무 유리한 계책이라고는 할 수 없다고 생각해요」
				PRINTFORMW 「지금 상태로 귀부인을 어떻게 하는지, 모두 나나름이야」
				PRINTFORMW 「적어도 그 정도의 일은 이해하고 있어 주지 않으면 여기의 흥이 꺾여. 그다지 실망시키지 말고 주세요…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が가학
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "가학")
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울굴", "몽롱"
				PRINTFORMW 「그것이 지금의 %CNAME(MASTER, TARGET)%에 할 수 있는 힘껏의 저항이라는 곳일까？」
				PRINTFORMW 「기특해, 애처로워서, 좀 더 학째 싶어져버리는 원…」
			CASE "분노"
				PRINTFORMW 「그러한 얼굴이 생긴다 라는 일은, 아직도 여유가 있을 듯 하구나？」
				PRINTFORMW 「좋아요…%CNAME(MASTER, TARGET)%가 어디까지 견딜 수 있는지, 정령들 시험하게 해 받기 때문에…」
			CASE "싫증"
				PRINTFORMW 「네네, 좋아하게 아무쪼록. 뭐라고 생각하지 않아요, 그런 것」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%가 무엇을 했더니 싫증하고 밖에 없어요, 나쁜데」
			CASEELSE
				PRINTFORMW 「이런 상황에서도 그러한 얼굴이 생기군요？ 조금 다시 보았어요」
				PRINTFORMW 「 그렇지만 본래라면, 좀 더 의연히 한 태도를 해야 한다고 생각하는데…」
				PRINTFORMW 「무엇인가 응이다 말해%CNAME(MASTER, TARGET)%…, 조금 흐르게 되고 있는  것이 아닌거야？ 후후후」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	IF RAND:3
		IF 0
		ELSEIF AFFECTION("상사상애") && !T_COND("분노")
			PRINTFORMW 「그게 뭐야… 재미있는 얼굴 해 버려서」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%라는 가끔 진짜 차눈이군요？ 킥킥…」
		ELSEIF AFFECTION("익애") && !T_COND("분노")
			PRINTFORMW 「뭐야, 그것…」
			PRINTFORMW 「%RANDTEXT_K14("そんな顔しないでよ", "可愛くないわね")%…」
		ELSEIF AFFECTION("권애") && !T_COND("분노")
			PRINTFORMW 「어머나, 조금 건방지구나…」
		ELSEIF AFFECTION("嫌悪") || T_COND("분노")
			PRINTFORMW 「어머나, 아직 그런 얼굴을 할 여유가 있군요…？」
			PRINTFORMW 「아무튼, 그것은 그것으로 좋은 것이지만…」
			PRINTFORMW 「그 결과가 어떻게 될까 정도의 일은, 상상해 주었으면 한 것…」
		ELSE
			PRINTFORMW 「후후… 좋은 담력이군요…」
			PRINTFORMDW 앨리스는 겁없게 웃어 돌려주어 왔다
		ENDIF
	ELSE
		SELECTCASE GET_CONDITION(TARGET)
			CASE "정욕", "울굴", "몽롱"
				PRINTFORMW 「후후, 재미있네요. 그런데, 그런 얼굴 되면(자)…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%(을)를 학째 싶어져버린다 것이죠…」
			CASE "분노"
				PRINTFORMW 「응, 가르쳐 주지 않을까？」
				PRINTFORMW 「 나를 화나게 해,%CNAME(MASTER, TARGET)%에 도대체 무슨 이익이 있는 것인가…」
				PRINTFORMW 「그렇지 않으면, 자신의 프라이드를 지키고 싶은 것뿐인가 해들 , 이군요」
			CASE "싫증"
				PRINTFORMW 「…트마라나이원이군요. 그 정도는 심심풀이도 안 돼요」
			CASEELSE
				PRINTFORMW 「싫지 않아요, 그러한 것.%CNAME(MASTER, TARGET)%나름대로 프라이드를 지키려고 필사적이야？」
				PRINTFORMW 「우도….%CNAME(MASTER, TARGET)%에 지켜야 할 프라이드는 것이 있는지 어떤지…」
				CALL FADEIN(8, "「나에게는 의문이지만…, 저기」")
				WAIT
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

