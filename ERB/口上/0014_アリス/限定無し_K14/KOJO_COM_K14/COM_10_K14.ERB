@KOJO_COM_K14_11
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM11, 싫어한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:REACT파생
	;=================================================
	;無理を했다행동
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理を했다행동"
		SELECTCASE RANDCOM(2, 1)
			CASE 0
				PRINTFORMW 「그렇게, 무리하게 싫어해 보이는 것은 무엇인가 의미가 있는 것？」
				PRINTFORM 「아무튼 확실히…, 이런 것도 나쁘지 않다고, 생각하지 않는 것도 없지만
			CASE 1
				PRINTFORMW 「후후…, 그러한 솔직하지 않은 태도도, 지금의 %CNAME(MASTER, TARGET)%에는 괴로운 것 같구나」
				PRINTFORM 「뭐, 나는 그래서 상관없어요？ 저항되면(자), 조금 흥분해버리고, 군요
		ENDSELECT
		;PSで閉じる
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;TCVAR:상태변화で下位に流すのもありかもと思ったけど保留
	;================================================
	;ACT분류が회화
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "회화") ;&& GROUPMATCH(GET_CONDITION(TARGET), "정욕", "울굴", "몽롱")
		LOCALS = \@ TENSION(TARGET, 7) == "高い" ? ！ # \@
		SELECTCASE GET_COM_RANK()
			CASE "自然な행동"
				SELECTCASE RANDCOM(4, 1)
					CASE 0
						PRINTFORMW 「그런 것…, 내가 안 일이 아니어요%RAND_SPLIT("/저기/")%%LOCALS%」
					CASE 1
						PRINTFORMW 「몰라요, 그런 것. 무엇이야？」
					CASE 2
						PRINTFORMW 「%CNAME(MASTER, TARGET)%가 무슨 말을 하려고, 흥미없네요%LOCALS%」
					CASE 3
						PRINTFORMW 「응, 그래서？ 싫기 때문에, 뭐든지 말해%LOCALS%」
				ENDSELECT
			CASE "無理のない행동"
				PRINTFORMW 「응, 아무래도 좋네요」
				PRINTFORMW 「마음대로 혼자서 싫어하고 있으면 좋아요%LOCALS%」
			CASE "無理を했다행동"
				IF COMCOR_POSI()
					PRINTFORMW 「진심으로 싫어 해 느낌에서도 없을 것 같지만」
					PRINTFORMW 「정말로, 어떤가 해들. 응？」
				ELSE
					PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 소리가 떨리고 있는 원이야」
					PRINTFORMW 「무리해 버려서. 응」
				ENDIF
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が애무
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "애무")
		;BASE:만족派生処理
		SELECTCASE TENSION(TARGET, 8)
			;50%以上
			CASE "高い"
				PRINTFORML 「때에는 그 정도의 일, 허락해 주어도 괜찮아요」
				PRINTFORMW 「지금의 나, 매우 너그로운 기분이야. 그렇지만, 그렇다고 해서…」
				PRINTFORMW 「몇시라도 그렇다라고는 한정되지 않으니까. 자주(잘) 생각해 주세요」
			;25%以上
			CASE "一般"
				PRINTFORMW 「…후~, 완전히. 귀찮음 거는 것도 적당히 해 주지 않는다？」
				PRINTFORMW 「 나,%CNAME(MASTER, TARGET)%의 부엌을 인정할 생각은 없는거야. 미안하지만, 이군요」
			;その他
			CASE "低い"
				PRINTFORMW 「뭐야, 싫어？ 남아 보살펴 주게 한 지 말았으면 좋겠다 응이지만」
				PRINTFORMW 「거역하고만 있으면, 그 중 아픈 눈에 맞추기 때문에……」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が도구
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "도구")
		;만족度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「어머나？ 우선, 형태만으로도 싫어하는 헛 스윙이라는 느낌인가 해들 」
				PRINTFORMW 「 그렇지만 정직…,%CNAME(MASTER, TARGET)%(이)가 생각하고 있는 일은, 진짜 좋게 알지 않아요」
			CASE "一般"
				PRINTFORMW 「뭐야…, 그렇게 싫어하는 일 없다고 생각하는데」
				PRINTFORMW 「 나에게 직접적으로 접했으면 좋다고 말하는, 완곡 더다 리카 무엇이지의？」
			CASE "低い"
				PRINTFORMW 「응,%CNAME(MASTER, TARGET)%, 이것이 싫어 해 일은……」
				PRINTFORMW 「 나에게 직접시라고 갖고 싶다고 말하는, 완곡 표현이나 무엇이지(뜻)이유？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;================================================
	;ACT분류が성교
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "성교")
		;만족度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「뭐, 그래요？ 형태만으로도 싫어하는 헛 스윙을 보이지 않으면 모습이 붙지 않아요？」
				PRINTFORMW 「아무튼 좋아요……지금은 즐기게 해 주기 때문에」
			CASE "一般"
				PRINTFORMW 「싫어했더니 멈출 생각은 없어요」
				PRINTFORMW 「얌전하게 하고 있으면 귀여워해 준다. 그렇지만, 그렇지 않으면…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%에 있어, 반드시 형편의 나쁜 일이 된다고 생각해요」
			CASE "低い"
				PRINTFORMW 「응, 나 말하지 않았는지 해들？」
				PRINTFORMW 「그러한 태도 되면(자) 불필요하게 흥분해버린다 것」
				PRINTFORMW 「저항하고 싶으면 하면 좋은데. 나, 그만둘 생각은 없으니까…」
		ENDSELECT
		RETURN 1
	ENDIF

	;================================================
	;ACT분류が수치
	;================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "수치")
		SELECTCASE GET_ACTNAME(TFLAG:ACT)
			CASE "자위"
				;快ｘが規定以上
				IF TCVAR:MASTER:가버려 || PALAM:MASTER:쾌Ｃ > 4000 || PALAM:MASTER:쾌Ａ > 4000 || PALAM:MASTER:쾌Ｂ > 2000
					;快쾌Ｃ条件
					IF PALAM:MASTER:쾌Ｃ > 4000
						PRINTFORMW 「싫어하고 있는 비교적은, 이제 상당히 좋아질 것 같지만？」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%가 어떤 상태일까 라고, 보면 알아」
					;쾌Ａ条件
					ELSEIF PALAM:MASTER:쾌Ａ > 4000 && !TALENT:MASTER:남자
						PRINTFORMW 「그런 태도 했더니, 나의 눈은 속일 수 없어요」
						PRINTFORMW 「엉덩이……기분이 좋겠지요？ 거짓말은 좋지 않아요…」
					;쾌Ｂ条件
					ELSEIF !TALENT:MASTER:남자
						PRINTFORMW 「싫어하는 위라면, 젖가슴 만지작거리는 것 그만두면 좋은데？」
						PRINTFORMW 「후후, 그런 상태로 싫어해 보여도 설득력이 없어요」
					ENDIF
				;その他
				ELSE
					PRINTFORMW 「1개(살) 말하는데……싫어해 보였더니 아무것도 변함없어요？」
					PRINTFORMW 「따르지 않으면 무리하게라도 따르게 할 때까지」
					PRINTFORMW 「 그렇지만, 할 수 있으면 그러한 일은 하고 싶지 않아. 그것을 알아 받을 수 없을까？」
				ENDIF
			CASE "비패개장"
				;윤활派生処理
				SELECTCASE PALAM:MASTER:윤활
					CASE IS >= 1000
						PRINTFORML 「대강, 이것은 어떤 일일까？」
						PRINTFORMW 「싫어하고 있는 버릇에, 이렇게 비쇼비쇼로 해 버려서」
						PRINTFORMW 「어차피 (듣)묻는다면, 정직한 신체(분)편에 (듣)묻는 것이 좋았는지 해들. 응？」
					CASE IS >= 500
						PRINTFORMW 「…심한 것. 이야라시이자견의 냄새로 코가 구부러져 그래요」
						PRINTFORMW 「볼 수 있는 것을 싫어하고 있었던 버릇에, 상스럽게 적셔 버려서…」
						PRINTFORMW 「뭐, 지금 상태로 무슨 말했더니, 모습은 붙지 않아요」
						;フェードアウト
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후후……」")
						CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후후……」")
						SIF ANIME_CONFIG("FADE")
							CLEARLINE 1
					CASEELSE
						PRINTFORMW 「싫어했더니 내가 그만두는 것이 아니고, 그런 것 무의미하다는 것은 일을 기억해 받고 싶네요」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%그렇지만, 그 정도는 이해할 수 있겠지요？ 만약, 언제까지나 어리석은 흉내를 할 생각이라면…」
						PRINTFORMW 「그 대상은 반드시 비싸게 들기 때문에, 자주(잘) 기억해 두어…」
				ENDSELECT
			CASEELSE
				;만족度派生処理
				SELECTCASE TENSION(TARGET, 8)
					CASE "高い"
						PRINTFORMW 「응, 그렇구나. 지금의 기분적으로는 그 정도 놓쳐 주어도 괜찮은데…」
						PRINTFORMW 「언제나 그렇다라고는 한정되지 않기 때문에, 잘 기억해 두며」
					CASE "一般"
