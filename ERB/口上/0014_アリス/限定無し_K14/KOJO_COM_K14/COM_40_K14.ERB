;━━━━━━━━━━━━━━━━━━━━━━━━
;COM44, 태연하게 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:REACT파생
	;역강간의 경우 해당 COM는 트리거되지 않는다. SEX_INITIATIVE(MASTER)로 실로 되는 상황만
	;=================================================
	;성교봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "성봉")
		LOCALS = %RAND_SPLIT("아/응응/응/아/아/하우")%
		;-------------------------------------------------
		;가버려상태
		;-------------------------------------------------
		IF 구상기입체크(100) && TCVAR:MASTER:가버려
			;애태움도파생
			SELECTCASE TCVAR:MASTER:애태움도
				;애태움중
				CASE IS > 1
					PRINTFORMW 「%RAND_SPLIT("하, 하/응읏! 쿳/하아, 하아")%…안되어요. 간단하게는 이나 등 있고 원」
					PRINTFORMW 「내가 만족할 때까지, 좀 더 교제해 주기 때문에…」
					;실은죠교자도가버려
					SIF TCVAR:가버려 || PALAM:쾌Ｖ > 8500 || PALAM:쾌Ａ > 8500 || PALAM:쾌Ｂ > 8500
						PRINTFORMW （뭐라고 말하면서, 실은 내가 이인가 되어버릴 것 같은 것은 비밀…）
				;그 외（애태움없음）
				CASEELSE
					PRINTFORM 「아%RANDTEXT_K14("", "응", "＿")%
					CALL AUTOHEARTCB_K14
					PRINTFORM  아…
					CALL AUTOHEARTCB_K14
					PRINTFORMW  격렬하다\@ TENSION(TARGET, 8) == "높다" ? 잇!  # 원이군요… \@」
					PRINTFORM 「아
					CALL AUTOHEARTCB_K14
					PRINTFORM  아…,%RAND_SPLIT("후//응")%아!
					PRINTFORMW \@ TCVAR:가버려 || PALAM:쾌Ｖ > 8500 || PALAM:쾌Ａ > 8500 ?  싫다, 이래서야 나도… 가 버리는, 가게 되어 버린다!  # \@」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;그러나 뭐라고도 읽기 어렵다
		;-------------------------------
		;가버려 이외는 REACT부하로 처리
		;-------------------------------
		;REACT부하파생
		SELECTCASE GET_COM_RANK()
			CASE "자연스러운행동"
				SELECTCASE GET_ACTNAME(TFLAG:ACT)
					;역강간라고 차진다고 생각하지만, 일단 넣어 둔다
					CASE "역강간", "정상위시킨다", "후배위시킨다", "배면좌위시킨다"
						PRINTFORM 「%LOCALS%…
						CALL AUTOHEARTCB_K14
						PRINTFORMW  %RAND_SPLIT("(이)나다 이것! 오슬오슬 햇…!/거, 거기… 약한데…!")%」
						PRINTFORM 「\@ T_COND("정욕") ? 나…, 머릿속…… 새하얗게! 아앗# 아, 아아… 읏! 후아앗 \@
						CALL PRINT_SYMBOL2_K14
					CASE "대면좌위시킨다"
						IF RAND:2
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
							CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
						ELSE
							PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
						ENDIF
						PRINTFORMW 「응 좀 더\@ TENSION(TARGET, 8) == "높다" ? !  # … \@ %RAND_SPLIT("좀 더,키스해/키스하면서찔러")%…!」
						;키스를 날조
						CALL SET_KISS
					CASE "항문섹스시킨다"
						PRINTFORM 「%LOCALS%
						CALL AUTOHEARTCB_K14
						PRINTFORMW  뭐야…, 상당히 상냥하다\@ TENSION(TARGET, 8) == "높다" ? (이)가 아니다 # 의 \@……」
						PRINTFORMW 「 그렇지만 코레…\@ ABL:Ａ감각 < 4 ? 아직 위화감 있어요…… # 어째서 엉덩이로, 이렇게…!  \@」
				ENDSELECT
			CASE "무리가 없는행동"
				SELECTCASE GET_ACTNAME(TFLAG:ACT)
