@KOJO_COM_K14_42
;=================================================
;회화계는 REACT로 완결시키고 있으므로 테두리를 마련하지 않는다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM42, 적극적으로 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	;시험 운용중…
	;=================================================
	;무리를했다행동
	;=================================================
	IF 구상기입체크(100) && (GET_COM_RANK() == "무리를했다행동" && (!TCVAR:가버려 || PALAM:쾌Ｖ < 8500 || PALAM:쾌Ａ < 8500 || PALAM:쾌Ｂ < 8500))
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「무리하고 있는 것이 보이고 보여라. 아무튼에서도…」
				PRINTFORM 「그 자세는 평가해야할 것인가 해들 , 이군요
				CALL PRINT_SYMBOL_K14
			CASE "일반"
				PRINTFORMW 「대강, 노력해 버려서 기특한 일. 아무튼 기분은 나쁘지 않는데…」
				PRINTFORMW 「\@ AFFECTION("익애") ? 포상을 갖고 싶으면 # 그만한 취급을 받고 싶으면 \@, 좀 더 노력하는 일이군요」
				SIF AFFECTION("익애")
					PRINTFORMW 「기대하고 있어요. 후후…」
			CASE "낮다"
				PRINTFORMW 「필사적으로 꼬리 흔들어 버려서……,%CNAME(MASTER, TARGET)%에는 프라이드가 없는 것일까」
				PRINTFORMW 「뭐, 나는 별로 상관없어요? 그만한,%CNAME(MASTER, TARGET)%에 적당한 취급을 해 주기 때문에」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「フフフ…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후후…」")
				CLEARLINE 1
		ENDSELECT
		RETURN 1
	ENDIF
	
	;기승위는 MENUMATCH로 주울 가능성이 있으므로, 조건 추가해 연주한다
	;똥 장황한 조건은 바람직하지 않지만, 테두리를 신설하는 것도 귀찮은 것으로 타협한다
	;=================================================
	;봉사
	;=================================================
	IF 구상기입체크(100) && MENUMATCH(TFLAG:ACT, "봉사") && (!IS_NOWACTNAME("기승위") || TEQUIP:성교중 != 3) && (SOURCE:쾌Ｃ + SOURCE:쾌Ｖ + SOURCE:쾌Ａ + SOURCE:쾌Ｂ > 10)
		;쓴 것은 끝까지 기능해 받을 자세
		IF AFFECTION("상사상애") && PERCENT(80)
			SELECTCASE RANDCOM(2, 0)
				CASE 0
					PRINTFORM 「
					CALL AUTOHEARTCB_K14
					PRINTFORMW  (이)나, 나다! 소코, 약한데……」
					PRINTFORMW 「응…, 아… 아! 이렇게, 되면…!」
				CASE 1
					PRINTFORM 「팥고물
					CALL AUTOHEARTCB_K14
					PRINTFORMW  응아, 후~응…, 응구…!」 
					PRINTFORM 「좋아요……, 좀 더, 좀 더…아…
					CALL PRINT_SYMBOL2_K14
			ENDSELECT
		ELSEIF AFFECTION("익애") && PERCENT(70)
			PRINTFORMW 「아, 응응! 꽤, 좋은 느껴라……」
			PRINTFORMW 「… 응! (이)나…, 그런, 곳까지……」
		ELSE
			SELECTCASE RANDCOM(2, 1)
				CASE 0
					PRINTFORMW 「응응…! 아무튼 아무튼이라는 느낌…」
					PRINTFORMW 「언제나 이 상태로 부탁하고 싶은 것이예요……」
				CASE 1
					PRINTFORMW 「응…, 응응! 나쁘고, 없네요…」
					PRINTFORMW 「이봐요, 좀 더 부탁…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;성교의기승위는봉사계 반응이 나온다. 테두리 신설이 귀찮아서 여기에 떨어져 받는다
	;=================================================
	;성교봉사＆기승위（성교）
	;=================================================
	IF 구상기입체크(100) && ((MENUMATCH(TFLAG:ACT, "성봉") || IS_NOWACTNAME("기승위") || TEQUIP:성교봉사중 || TEQUIP:성교중 == 3) && (SOURCE:쾌Ｃ + SOURCE:쾌Ｖ + SOURCE:쾌Ａ + SOURCE:쾌Ｂ > 10))
		IF TEQUIP:성교중 == 3 || IS_NOWACTNAME("기승위")
			PRINTFORM 「후후, 좋은 느껴라. 지금의 %CNAME(MASTER, TARGET)%, 매우 사랑스러워요…
			CALL PRINT_SYMBOL_K14
			PRINTFORMW 「이봐요, 좀 더 좋은 소리를 듣게 해 주세요…」
		ELSE
			SELECTCASE RANDCOM(2, 2)
				CASE 0
					PRINTFORM 「…, 아
					CALL AUTOHEARTCB_K14
					PRINTFORMW  (이)다, 안돼! 좀 더, 천천히……」
					PRINTFORMW 「히……, 아! 아! 응…, 응구!」
				CASE 1
					PRINTFORM 「팥고물
					CALL AUTOHEARTCB_K14
					PRINTFORMW  응아! 후~응……, 응!」
					PRINTFORM 「좋아요……, 좀 더, 좀 더! 아…
					CALL PRINT_SYMBOL2_K14
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;범용 처리
	;-------------------------
	IF AFFECTION("상사상애")
		SELECTCASE RANDCOM(2, 3)
			CASE 0
				PRINTFORMW 「뭐, 그래요. 그 정도는 해 받지 않으면」
				PRINTFORMW 「따로 칭찬하고 있는 (뜻)이유가 아니에요? 이 정도의 일, 당연하다고 생각하고… 응?」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「쿠쿡…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「킥킥…」")
				CLEARLINE 1
			CASE 1
				PRINTFORMW 「완전히 길러라면 되어 버려서……, 라고 말하는 것은 금구일까? 후후」
				PRINTFORMW 「 그렇지만 그렇구나.%CNAME(MASTER, TARGET)%가 솔직하면, 나도 기뻐?」
				PRINTFORM 「이렇게 보여도, 군요…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ELSE
		SELECTCASE RANDCOM(2, 4)
			CASE 0
				PRINTFORMW 「어머나, 상당히 적극적이 아니다?」
				PRINTFORMW 「뭐, 이것은 이것대로…, 나쁘지 않아요.%CNAME(MASTER, TARGET)%의 본심은 모르지만, 이군요」
			CASE 1
				PRINTFORMW 「우선, 솔직하게 나에 따르는 것은 좋은 경향이라고 말할 수 있는 것일까?」
				PRINTFORM 「언제나 이런 느낌이라고, 나도 락 할 수 있는데…, (이)군요
				CALL PRINT_SYMBOL_K14
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

