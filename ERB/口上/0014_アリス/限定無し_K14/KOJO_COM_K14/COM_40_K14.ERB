@KOJO_COM_K14_44
;=================================================
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM44, 태연하게 한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:REACT파생
	;역강간の場合当該COMはトリガーされない。SEX_INITIATIVE(MASTER)で真になる状況のみ
	;=================================================
	;성교봉사
	;=================================================
	IF 구상記入チェック(100) && MENUMATCH(TFLAG:ACT, "성봉")
		LOCALS = %RAND_SPLIT("ふあ/んん/んぁ/あ/あぁ/はぅ")%
		;-------------------------------------------------
		;가버려상태
		;-------------------------------------------------
		IF 구상記入チェック(100) && TCVAR:MASTER:가버려
			;애태움도派生
			SELECTCASE TCVAR:MASTER:애태움도
				;애태움中
				CASE IS > 1
					PRINTFORMW 「%RAND_SPLIT("하, 하/응읏！ 쿳/하아, 하아")%…안되어요. 간단하게는 이나 등 있고 원」
					PRINTFORMW 「내가 만족할 때까지, 좀 더 교제해 주기 때문에…」
					;実は죠교자も가버려
					SIF TCVAR:가버려 || PALAM:쾌Ｖ > 8500 || PALAM:쾌Ａ > 8500 || PALAM:쾌Ｂ > 8500
						PRINTFORMW （뭐라고 말하면서, 실은 내가 이인가 되어버릴 것 같은 것은 비밀…）
				;その他（애태움無し）
				CASEELSE
					PRINTFORM 「아%RANDTEXT_K14("ぅ", "ん", "＿")%
					CALL AUTOHEARTCB_K14
					PRINTFORM  아…
					CALL AUTOHEARTCB_K14
					PRINTFORMW  격렬하다\@ TENSION(TARGET, 8) == "高い" ? 잇！ # 원이군요… \@」
					PRINTFORM 「아
					CALL AUTOHEARTCB_K14
					PRINTFORM  아…,%RAND_SPLIT("후//응")%아！
					PRINTFORMW \@ TCVAR:가버려 || PALAM:쾌Ｖ > 8500 || PALAM:쾌Ａ > 8500 ?  싫다, 이래서야 나도…가 버리는, 가게 되어 버린다！ # \@」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;しかしなんとも読み難い
		;-------------------------------
		;가버려以外はREACT부하で処理
		;-------------------------------
		;REACT부하派生
		SELECTCASE GET_COM_RANK()
			CASE "自然な행동"
				SELECTCASE GET_ACTNAME(TFLAG:ACT)
					;역강간だと蹴られると思うけど、一応入れておく
					CASE "역강간", "정상위시킨다", "후배위시킨다", "배면좌위시킨다"
						PRINTFORM 「%LOCALS%…
						CALL AUTOHEARTCB_K14
						PRINTFORMW  %RAND_SPLIT("(이)나다 이것！ 오슬오슬 햇…！/거, 거기…약한데…！")%」
						PRINTFORM 「\@ T_COND("정욕") ? 나…, 머릿속……새하얗게！ 아앗# 아, 아아…읏！ 후아앗 \@
						CALL PRINT_SYMBOL2_K14
					CASE "대면좌위시킨다"
						IF RAND:2
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
							CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
						ELSE
							PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
						ENDIF
						PRINTFORMW 「응 좀 더\@ TENSION(TARGET, 8) == "高い" ? ！ # … \@ %RAND_SPLIT("좀 더,키스해/키스하면서찔러")%…！」
						;키스を捏造
						CALL SET_KISS
					CASE "항문섹스시킨다"
						PRINTFORM 「%LOCALS%
						CALL AUTOHEARTCB_K14
						PRINTFORMW  뭐야…, 상당히 상냥하다\@ TENSION(TARGET, 8) == "高い" ? (이)가 아니다 # 의 \@……」
						PRINTFORMW 「 그렇지만 코레…\@ ABL:Ａ감각 < 4 ? 아직 위화감 있어요…… # 어째서 엉덩이로, 이렇게…！ \@」
				ENDSELECT
			CASE "無理のない행동"
				SELECTCASE GET_ACTNAME(TFLAG:ACT)
					CASE "정상위시킨다", "후배위시킨다"
						PRINTFORMW 「응응！ 삐걱삐걱 떠 주어 버려서…\@ TENSION(TARGET, 8) == "高い" ? 날것 삶… # 건방짐 \@」
						PRINTFORM 「 그렇지만 이것, 안쪽까지 와……%RAND_SPLIT("나/굉장히")%, 키모치 좋아서…！%RAND_SPLIT("あん/や")%,%RAND_SPLIT("후/")%아
						CALL PRINT_SYMBOL2_K14
					CASE "대면좌위시킨다"
						PRINTFORMW 「응！？ %KOJO_RAND_K14(("키스"), 1)%」
						PRINTFORMW 「%CNAME(MASTER, TARGET)%(으)로부터 키스 해 오다니 조금 거만함……그렇지만」
						PRINTFORMW 「이런 것도, 나쁘지 않은, 원이군요……」
						;키스を捏造
						CALL SET_KISS
					CASE "배면좌위시킨다"
						PRINTFORM 「%LOCALS%
						CALL AUTOHEARTCB_K14
						PRINTFORMW  \@ TENSION(TARGET, 8) == "高い" ? 싫다…거기, 만지작거려！ # 응…거기, 기분 좋다…… \@」
						PRINTFORM 「\@ TENSION(TARGET, 8) == "高い" ? 응하앗, 아앗！ 그것…, 안돼엣 # 응아…하아……아, 아앗 \@
						CALL PRINT_SYMBOL2_K14
					CASE "항문섹스시킨다"
						PRINTFORMW 「%LOCALS%！ \@ ABL:Ａ감각 < 4 ? 너무 격렬해요 # 겨, 격렬하닷 \@！」
						PRINTFORM 「이렇게 되면(자), 원, 나%RAND_SPLIT("읏/")%！ 응아, 아
						CALL PRINT_SYMBOL_K14
					CASE "역강간"
						PRINTFORM 「아
						CALL AUTOHEARTCB2_K14
						PRINTFORMW  좋아요, 그것.%CNAME(MASTER, TARGET)%, 자주(잘) 알고 있잖아…」
						PRINTFORMW 「이봐요, 좀 더\@ TENSION(TARGET, 8) == "高い" ? ！ #  \@ 번화가가 도착한, 개같이…！」
				ENDSELECT
			CASE "無理を했다행동"
				SELECTCASE GET_ACTNAME(TFLAG:ACT)
					CASE "정상위시킨다", "후배위시킨다", "배면좌위시킨다"
						;実はまんざらでもない
						IF COMCOR_POSI()
							PRINTFORMW 「뭐그것…, 페이스%RAND_SPLIT("다운/를 떨어뜨리자")%라는 (뜻)이유？」
							PRINTFORMW 「응응！ 그렇지만 그 나누기에……\@ PALAM:쾌Ｖ > 5000 ? 기분 좋은 곳 더듬어 찾을 수 있어 버려, 조금 적이군요 # 좋은 곳에 해당되어요 \@…」
						;実は嫌
						ELSE
							PRINTFORMW 「조금 저항은 있어도, 결국은 따르군요？」
							PRINTFORMW 「그렇게 기특하게 봉사하는 모습, 나쁘지 않아요. 사랑스러우면조차 말할 수 있는 것일까. 응？」
						ENDIF
					CASE "대면좌위시킨다"
						;実はまんざらでもない
						IF COMCOR_POSI()
							PRINTFORMW 「…응. 키스 하면서 시라고 주다니 마음이 후하구나？ 후후…」
							PRINTFORMW 「이봐요, 이번은 나부터 키스 해 주어요……」
							;1/2でフェードアウト
							IF RAND:2
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"　　　　　%KOJO_RAND_K14(("키스"), 1)%")
								CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("키스"), 1)%")
								CLEARLINE 1
							;1/2でPRINT
							ELSE
								PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)%」
							ENDIF
							;키스を捏造
							CALL SET_KISS
						;実は嫌
						ELSE
							PRINTFORMW 「%CNAME(MASTER, TARGET)%가 무리하고 있는 것이 자주(잘) 알아요. 다만, 그렇구나？」
							PRINTFORM 「그렇게 기특하게 노력하는 모습은, 조금 사랑스러울지도이지만
							CALL PRINT_SYMBOL_K14
						ENDIF
					CASE "배면좌위시킨다"
						;実はまんざらでもない
						IF COMCOR_POSI()
							PRINTFORMW 「%LOCALS%…！ \@ ABL:MASTER:기교 > 4 ? 손다하고가 미덥지 않지만, 나쁘지 않아요… # 사실, 추잡하다손다하고… \@」
							PRINTFORMW 「\@ TCVAR:가버려 || PALAM:쾌Ｖ > 8500 || PALAM:쾌Ｂ > 8500 ? 이래서야…,손만으로 가게 되어 버릴 것 같다…… # 다만…, 허리의 움직임이 멈추어 있는 것이 속상하다%RAND_SPLIT("노/와")%… \@」
						;実は嫌
						ELSE
							PRINTFORMW 「강제당하는 것은 싫은소리 도미이지만…」
							PRINTFORMW 「그런데도 말하는 일은 (듣)묻군요？ 뭐, 이런 것도 나쁘지 않아요…」
						ENDIF
					CASE "항문섹스시킨다", "역강간"
						;実はまんざらでもない
						IF COMCOR_POSI()
							PRINTFORMW 「%LOCALS%…！ %CNAME(MASTER, TARGET)%나름대로 신경을 써 주고 있군요」
							IF IS_NOWACTNAME("항문섹스시킨다")
								PRINTFORMW 「 그렇지만\@ ABL:Ａ감각 < 4 ? 이 느껴 아직 익숙해지지 않아요 # , 조금 안타깝네요. 이래서야 \@…」
							ELSE
								PRINTFORMW 「 그렇지만\@ ABL:Ｖ감각 < 4 ? 이 느껴 아직 익숙해지지 않아요 # , 조금 안타깝네요. 이래서야 \@…」
							ENDIF
						;実は嫌
						ELSE
							PRINTFORMW 「그래요,%CNAME(MASTER, TARGET)%(은)는 나의 하라는 대로 되어 있으면 좋은거야」
							PRINTFORMW 「그것이 얼마나 이상한 일이든지, 군요.%LOCALS%…！」
						ENDIF
					CASEELSE
				ENDSELECT ;			書いたあとから刻みたくなったけど、後回しにする構え
		ENDSELECT
		RETURN 1
	ENDIF
	
	;봉사系、もしくはその他の場合に落ちてくるのを想定。
	;ただ、この構造なら봉사系の枠を埋めておけばその他に落ちる可能性は無いはず？
	;MENUMATCH(TFLAG:ACT, "봉사")でやらなかったのは、汎用処理を別個につくるのが面倒だったから
	;何かを表示する為の最後の箇所なんで、まぁ一般分岐はあった方が望ましいよね、という話
	;色々な可能性を完全に把握するのは非常に困難
	;-------------------------
	;その他を処理
	;-------------------------
	SELECTCASE GET_COM_RANK()
		CASE "自然な행동"
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					PRINTFORMW 「…응, 나쁘지 않아요.%CNAME(MASTER, TARGET)%으로서는%RAND_SPLIT("좋은 결과//뭐 그래")%까？」
					PRINTFORMW 「 그렇지만 그렇구나…, 그 나머지히로시의 표정이%RAND_SPLIT("적에 방해되어요/조금건방짐저기")%」
					PRINTFORMW 「뭐, 제멋대로 하는 모양 이 이상 없는 이야기인 것은 알고 있을 생각이야. 후후…」
				CASE "一般"
					PRINTFORMW 「아무튼, 이 정도는 되어있고 당연해요%RAND_SPLIT("？/")%」
					PRINTFORMW 「이라고 할까 정직…, 지금의 %CNAME(MASTER, TARGET)%(이)라면 좀 더 할 수 있을 것이라고 생각하는데」
					SIF !RAND:3
						PRINTFORMW 「그근처 어떤가 해들. 응？」
				CASE "低い"
					PRINTFORMW 「%RAND_SPLIT("헤에/아라/흐음")%, 상당히 여유%RAND_SPLIT("がある/")%(이)가 아니다」
					PRINTFORMW 「이것이라면,%CNAME(MASTER, TARGET)%에는 좀 더 여러가지 요구해도 좋은 것 같구나%RAND_SPLIT("？/")%」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? クスクス # ウフフ \@…」")
					CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? 킥킥 # 우후후 \@…」")
					CLEARLINE 1
			ENDSELECT
		CASE "無理のない행동"
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					PRINTFORM 「그러한%CNAME(MASTER, TARGET)%의 모습을 보는 것은, 나쁘지 않은 기분이군요
					CALL PRINT_SYMBOL_K14
					PRINTFORMW 「 그렇지만 지금의 %CNAME(MASTER, TARGET)%(이)라면, 좀 더 할 수 있지 않을까. 응？」
				CASE "一般"
					PRINTFORMW 「\@ AFFECTION("익애") ? 후후 # 후후 \@…, 나의 환심을 사려고 필사같구나%RAND_SPLIT("？/")%」
					PRINTFORM 「뭐 그렇지만……, 그 자세는 평가 해 주어요
					CALL PRINT_SYMBOL_K14
				CASE "低い"
					PRINTFORMW 「뭐, 이 정도의 일은 해 받지 않으면」
					PRINTFORMW 「\@ AFFECTION("익애") ? 지금의 %CNAME(MASTER, TARGET)%(이)라면 가능하는 것이 당연하고, 군요 # 나에게 마음에 들고 싶다면 더욱 더. 응？ \@」
			ENDSELECT
		CASE "無理を했다행동"
			;実はまんざらでもない
			IF COMCOR_POSI()
				PRINTFORMW 「아무것도 아닌 것 같은 얼굴 하고 있지만, 사실은 다르겠지요%RAND_SPLIT("？/")%」
				PRINTFORM 「그러한 것은 전원%RAND_SPLIT("루/라고 끝낸다")%의군요.\@ TENSION(TARGET, 8) == "高い" ? ザ・ン・ネ・ン # 残念 \@그렇지만
				CALL PRINT_SYMBOL_K14
				;의존도변화
				CALL ADD_DEPEND(-1, +2)
			;実は嫌
			ELSE
				PRINTFORMW 「평정을 가장해도, 무리를 하고 있는 것은 보면 알아요」
				PRINTFORMW 「그 정도%RAND_SPLIT("그러면 나의 환심은 살 수 없어요/그리고 나의 비위를 맞추려고 해도 쓸데없어요")%, 나쁜데」
				;의존도변화
				CALL ADD_DEPEND(-2, +3)
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF
