;*************************************************
;자세변경
;*************************************************
;우선 테두리만
;쓸지 어떨지는 뭐라고도 말할 수 없는 곳
;-------------------------------------------------
@KOJO_POSE_K14(ARG, ARG:1, ARG:2)
;ARG = 자세
;ARG:1 = 위치전후
;ARG:2 = 위치상하
;-------------------------------------------------
[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(자유)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && ARG:2 == 0
	;=================================================
	;자세변화 있어
	;=================================================
	IF 구상기입 체크(0) && ARG >= 0
		;=================================================
		;자유
		;=================================================
		IF 구상기입 체크(0) && ARG == 0
		ENDIF
		
		;=================================================
		;누움
		;=================================================
		IF 구상기입 체크(0) && ARG == 1
		ENDIF
		
		;=================================================
		;안정도
		;=================================================
		IF 구상기입 체크(0) && ARG == 2
		ENDIF
	
		;=================================================
		;네발로 엎드림
		;=================================================
		IF 구상기입 체크(0) && ARG == 3
		ENDIF
		
		;=================================================
		;기립
		;=================================================
		IF 구상기입 체크(0) && ARG == 4
		ENDIF
		
		;=================================================
		;무릎으로 서있다
		;=================================================
		IF 구상기입 체크(0) && ARG == 10
		ENDIF
	ENDIF
	
	;=================================================
	;자세변화없음
	;=================================================
	IF 구상기입 체크(0) && ! ARG
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(위를 탔다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && ARG:2 == 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(위에 실었다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && ARG:2 == 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(상하변화없음)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && ! ARG:2
	;=================================================
	;누움
	;=================================================
	IF 구상기입 체크(0) && ARG == 1
;OriginalString : 
		PRINTFORMW
		IF ARG:1 >= 0
			SELECTCASE ARG:1
				CASE 0
				CASE 1
				CASE 2
			ENDSELECT
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;안정도
	;=================================================
	IF 구상기입 체크(0) && ARG == 2
	ENDIF
	
	;=================================================
	;네발로 엎드림
	;=================================================
	IF 구상기입 체크(0) && ARG == 3
	ENDIF
	
	;=================================================
	;기립
	;=================================================
	IF 구상기입 체크(0) && ARG == 4
	ENDIF
	
	;=================================================
	;무릎으로 서있다
	;=================================================
	IF 구상기입 체크(0) && ARG == 10
	ENDIF
	
	;=================================================
	;자유
	;=================================================
	IF 구상기입 체크(0) && ARG == 0
	ENDIF
ENDIF
[SKIPEND]
