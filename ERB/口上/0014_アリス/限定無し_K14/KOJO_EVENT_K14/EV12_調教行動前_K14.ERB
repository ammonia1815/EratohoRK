;━━━━━━━━━━━━━━━━━━━━━━━━
;장내사정直後(죠교자→죠교대상)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(25) && GROUPMATCH(TFLAG:전회질싸, 5, 7) > 0
	;=================================================
