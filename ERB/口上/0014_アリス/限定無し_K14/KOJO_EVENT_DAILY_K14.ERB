	;상사상애
	IF TALENT:DAILY_TARGET:주인님 && AFFECTION("상사상애", (FLAG:주인님))
		PRINTFORMW 「어머나, 기우군요? 꼭 좋아요. 세 명으로 함께 들어옵시다?」
		CALL HTMLPRINTW(@"（사실은 알았지만…%HTML_PS_K14()%）")
	;그 이외
	ELSE
		PRINTFORMW 「우후후…」
		CALL PRINT_KOJO_COLORW(DAILY_SUBTARGET, "『후후…』", DAILY_TARGET)
	ENDIF
	RETURN 1
ENDIF

