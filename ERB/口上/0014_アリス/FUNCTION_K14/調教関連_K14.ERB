	CASE "T_CB절정"
		RETURNF LOCAL:7 > 0 && LOCAL:10 > 0
	CASE "T_VA절정"
		RETURNF LOCAL:8 > 0 && LOCAL:9 > 0
	CASE "T_VB절정"
		RETURNF LOCAL:8 > 0 && LOCAL:10 > 0
	CASE "T_AB절정"
		RETURNF LOCAL:9 > 0 && LOCAL:10 > 0
	CASEELSE
		CALLF PRINT_ERRORMESSAGE(@"@GET_EX_K14에 무효인 정의가 도 사라질 수 있는 했다")
		RETURNF -1
ENDSELECT
RETURNF 0

