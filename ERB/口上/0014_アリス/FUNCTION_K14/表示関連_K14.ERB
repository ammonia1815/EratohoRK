;-------------------------------------------------
;SETFONT_K14
;폰트를나눔명조, 폰트 스타일을 굵은 글씨, 이탤릭으로 한다
;현상, 용도가 한정되기 때문에 1대의 비행기노우에 사무치는 일에. 확장했다구 되면 생각한다
;-------------------------------------------------
@SETFONT_K14
SIF CHKFONT("나눔명조")
	SETFONT "나눔명조"
FONTSTYLE 3


;-------------------------------------------------
;CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K14
;인  수:ARG :0 = FADE의 속도
;인  수:ARG :1 = 1으로 TARGET, 2로 MASTER의상태변화를 참조한다
;인  수:ARG :2 = -2이외라면 FADE가 OFF인 경우에 RESETCOLOR로 PRINT를 출력했다 후, 구상주의 문자색에 되돌린다
;인  수:ARGS:0 = @KOJO_RAND_K14의 행위의 분류를 지정한다. 캐릭터 라인 인수
;
;ARG:0의 속도로 SETCOLOR당했다문자색으로부터 키 경치에 ARGS:0의 분류의 KOJO_RAND_K14를 페이드 아웃
;-------------------------------------------------
@CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K14(ARG:0, ARGS:0, ARG:1 = 1, ARG:2 = -2)
IF ANIME_CONFIG("FADE")
	CALL FADE(ARG:0, GETCOLOR(), GETBGCOLOR(), @"     %KOJO_RAND_K14((ARGS:0), ARG:1)%")
ELSE
	SIF ARG:2 != -2
		RESETCOLOR
	PRINTFORMW \@ ARG:2 == -2 ? 「 # （ \@%KOJO_RAND_K14((ARGS:0), ARG:1)%\@ ARG:2 == -2 ? 」 # ） \@
	SIF ARG:2 != -2
		CALL KOJO_COLOR(TARGET)
ENDIF
CALL FADE_CLINE(1)


;PRINT_SYMBOL의 HTMLFONT판
;========================
@HTML_PS_K14(ARG:0 = -2) ;//인수를 생략 하면 TARGET
#FUNCTIONS
#LOCALSSIZE 1
VARSET LOCALS
ARG:0 = ARG:0 != -2 ? ARG:0 # TARGET
;//기본적으로 행말에서의 사용을 상정

[SKIPSTART]
호의, 호감도로 파생한다
하트계의 출력은 2/3으로 HEARTB, 1/3그리고 HEART를 출력한다
음표계의 출력은 2/3으로 8분 음표, 1/3그리고 련 8분 음표를 출력한다
[SKIPEND]
IF GET_KFLAG_K14("상사상애", ARG:0)
	;BASE:만족로 파생한다
	SELECTCASE TENSION(ARG:0, 8)
		;BASE:만족가 50%이상
		CASE "높다"
			;1/4로 하트 2개를 출력
			IF PERCENT(25)
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K14("PINK"), 2) # HTML_HEART(, COLOR2_K14("PINK"), 2)
			;그 외라면 하트 1개
			ELSE
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K14("PINK")) # HTML_HEART(, COLOR2_K14("PINK"))
			ENDIF
		;BASE:만족가 25%이상
		CASE "일반"
			LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
		;BASE:만족가 25% 미만
		CASE "낮다"
			;아무것도 출력하지 않는다
			LOCALS:0 '= ""
	ENDSELECT
ELSEIF AFFECTION("익애", ARG:0)
	SELECTCASE TENSION(ARG:0, 8)
		CASE "높다"
			;1/4로 하트계를 출력
			IF PERCENT(25)
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K14("PINK")) # HTML_HEART(, COLOR2_K14("PINK"))
			;그 외라면 음표계를 출력
			ELSE
				LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
			ENDIF
		CASE "일반"
				LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
		CASE "낮다"
			LOCALS:0 '= ""
	ENDSELECT
ELSE
	SELECTCASE TENSION(ARG:0, 8)
		CASE "높다"
			LOCALS:0 '= HTML_EIGHTHNOTE()
		CASEELSE
			LOCALS:0 '= ""
	ENDSELECT
ENDIF
RETURNF LOCALS:0


