						CASE 17
							LOCALS = %LOCALS%%RANDTEXT_K14("페로", "")%, 츗%ELLIPSIS2_K14()%구
						CASE 18
							LOCALS = %LOCALS%%ELLIPSIS2_K14()%%RANDTEXT_K14("할짝", "")%、%RANDTEXT_K14("", "")%
						CASE 19
