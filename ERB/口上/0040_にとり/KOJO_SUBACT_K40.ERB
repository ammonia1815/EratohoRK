	IF 구상기입 체크(0) && TFLAG:자주탈의 && ZENRACHECK(0)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·만p FIRSTDATUI(30)
;------------------------------------------------
;·이번탈의로성기가 보이도록(듯이)되었다경우에 표시
;·팬티 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;처음
	;=================================================
	IF 구상기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(3)
	ENDIF
	
	;=================================================
	;벌써가슴가 보이고 있다
	;=================================================
	IF 구상기입 체크(0) && B_DATUICHECK(0) == 1
	ENDIF
	
	;=================================================
	;응 이렇게 p
	;=================================================
	IF 구상기입 체크(0) && PENIS(TARGET)
	ENDIF
	
	;그 외
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·말하는 p FIRSTDATUI(40)
;------------------------------------------------
;·이번탈의로 젖가슴이 보이도록(듯이)되었다경우에 표시
;·브라 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;처음
	;=================================================
	IF 구상기입 체크(0) && TALENT:TARGET:주인님 && ! TALENT:거유 && FIRSTDATUI(4)
	ENDIF
	
	;=================================================
	;벌써성기가 보이고 있다
	;=================================================
	IF 구상기입 체크(0) && P_DATUICHECK(0) == 1
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입 체크(0) && ONCEDATUI(4)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·팬티 보이고 FIRSTDATUI(50)
;------------------------------------------------
;·이번탈의로팬티（캐릭터에 의해드로와즈·트렁크）가 보이도록(듯이)되었다경우에 표시
;·하반신속옷가 없는 캐릭터는 쓰는필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;처음
	;=================================================
	IF 구상기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(5)
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입 체크(0) && ONCEDATUI(5)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·브라 보이고 FIRSTDATUI(60)
;------------------------------------------------
;·이번탈의로 브라（캐릭터에 의해스포츠브라·사라시）가 보이도록(듯이)되었다경우에 표시
;·상반신속옷가 없는 캐릭터는 쓰는필요없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입 체크(0) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && ! IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;처음
	;=================================================
	IF 구상기입 체크(0) && TALENT:TARGET:주인님 && FIRSTDATUI(6)
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입 체크(0) && ONCEDATUI(6)
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·전라에 FIRSTDATUI(70)
;------------------------------------------------
;·「전라까지 벗었다」만이 아니고, 「지금 벗었으므로전라되었다」라고 하는 의미
;·「겉옷 밖에 남지 않은상태로겉옷를 벗었다」등도 포함된다
