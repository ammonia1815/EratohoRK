;────────────────────────────────────
;미약등사용구상
;────────────────────────────────────
;·죠교자가미약나로션를 사용했다때에 범용적으로 표시
;·「액션구상」로 기입 체크를=2로했다경우는 표시되지 않는다
;·쓰는 경우, 액션구상의 뒤로 이어 위화감이 없는 형태로 쓰여지는 것이 바람직함
;-------------------------------------------------
@KOJO_USE_K40
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;로션/미약동시 사용
;------------------------------------------------
;·주로성교계 커맨드로부터 발생한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && GETBIT(TFLAG:미약등사용구상, 0) && GETBIT(TFLAG:미약등사용구상, 1)
	;아라이멘도정
	IF ALI(0)
		SELECTCASE RANDUSE(3, 0)
			CASE 0
;OriginalString : 「これ、楽しみ？」
				PRINTFORMW 「이것, 즐거움?」
			CASE 1
;OriginalString : 「ぬるぬるで、凄いよ？」
				PRINTFORMW 「미끈미끈으로, 굉장해?」
			CASE 2
;OriginalString : 「薬でどうなっちゃうんだろうねえ？」
				PRINTFORMW 「약으로 어떻게 되어 버리겠지 응?」
		ENDSELECT
	;아라이멘도부
	ELSE
		SELECTCASE RANDUSE(3, 1)
			CASE 0
;OriginalString : 「面倒だから、使っちゃうね」
				PRINTFORMW 「귀찮기 때문에, 사용해버린다」
			CASE 1
;OriginalString : 「乱れる顔、見せて？」
				PRINTFORMW 「흐트러지는 얼굴, 보여 줘?」
			CASE 2
;OriginalString : 「あ……凄い、音……」
				PRINTFORMW 「아……굉장한, 소리……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;로션사용
;------------------------------------------------
;·완구·성교계 커맨드등으로부터 발생한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && GETBIT(TFLAG:미약등사용구상, 0)
	;아라이멘도정
	IF ALI(0)
		SELECTCASE RANDUSE(3, 10)
			CASE 0
;OriginalString : 「すごい、ぬるぬるだあ……」
				PRINTFORMW 「대단히, 미끈미끈이다……」
			CASE 1
;OriginalString : 「これで、もっと気持ちいいよ……」
				PRINTFORMW 「이것으로, 좀 더 기분이 좋아……」
			CASE 2
;OriginalString : 「ほら、力、抜いて……」
				PRINTFORMW 「이봐요, 힘, 뽑아……」
		ENDSELECT
	;아라이멘도부
	ELSE
		SELECTCASE RANDUSE(3, 11)
			CASE 0
;OriginalString : 「これで、無茶できるね？」
				PRINTFORMW 「이것으로, 당치 않음할 수 있는군?」
			CASE 1
;OriginalString : 「凄く、広がってるよ？」
				PRINTFORMW 「굉장히, 퍼지고 있어?」
			CASE 2
;OriginalString : 「ぬるぬるで、気持ち良い？」
				PRINTFORMW 「미끈미끈으로, 기분 좋다?」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;미약사용
;------------------------------------------------
;·애무계 커맨드등으로부터 발생한다
;·키스로 발생했다경우는입옮기기가 되기 (위해)때문에, 그 케이스는키스의 액션구상내에서 처리하는 편이 좋다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && GETBIT(TFLAG:미약등사용구상, 1)
	;아라이멘도정
	IF ALI(0)
		SELECTCASE RANDUSE(3, 20)
			CASE 0
;OriginalString : 「そろそろ、熱くなってきた？」
				PRINTFORMW 「이제, 뜨거워졌다?」
			CASE 1
;OriginalString : 「蕩ける顔、見せて？」
				PRINTFORMW 「녹는 얼굴, 보여 줘?」
			CASE 2
;OriginalString : 「良い声、聞きたいなあ」
				PRINTFORMW 「좋은 소리, (듣)묻고 싶다」
		ENDSELECT
	;아라이멘도부
	ELSE
		SELECTCASE RANDUSE(3, 21)
			CASE 0
;OriginalString : 「悔しい？　薬なんか使われて？」
				PRINTFORMW 「분하다? 약은 사용되어?」
			CASE 1
;OriginalString : 「どう？　そろそろ効いてきた？」
				PRINTFORMW 「어때? 이제 효과가 있어 왔다?」
			CASE 2
;OriginalString : 「乱れる所、見たいなあ」
				PRINTFORMW 「흐트러지는 곳, 보고 싶다」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
