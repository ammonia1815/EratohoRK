;────────────────────────────────────
;EV21, 대만족구상
;────────────────────────────────────
;·죠교자의대만족보너스를취득했다경우에 표시
;-------------------------------------------------
@KOJO_EVENT_K40_21(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;니토리님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	;=================================================
	;중에이고라고!
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("안에 내!")
		;아라이멘도정
		IF ALI(0)
			SELECTCASE RANDEVENT(3, 0)
				CASE 0
;OriginalString : 「凄く、嬉しいよ……っ」
					PRINTFORMW 「굉장히, 기뻐……」
;OriginalString : 「私も、만족させてあげるから……」
					PRINTFORMW 「 나도, 만족시켜 주기 때문에……」
				CASE 1
;OriginalString : 「何か、凄く……」
					PRINTFORMW 「무엇인가, 굉장히……」
;OriginalString : 「ううん、何でも無いよっ！」
					PRINTFORMW 「으응, 뭐든지 없어!」
				CASE 2
;OriginalString : 「%CNAME(MASTER, TARGET)%で、良かったよ……」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%그리고, 좋았어요……」
;OriginalString : 「私、本当にそう思ってるんだからね……」
					PRINTFORMW 「 나, 정말로 그렇게 생각하고 있기 때문에……」
			ENDSELECT
		;아라이멘도부
		ELSE
			SELECTCASE RANDEVENT(3, 11)
				CASE 0
;OriginalString : 「いいなあ、この感じ……」
					PRINTFORMW 「좋다, 이 느낌……」
;OriginalString : 「お裾分け、してあげたいくらいだよ……」
					PRINTFORMW 「이익 분배, 해 주고 싶을 정도 야……」
				CASE 1
;OriginalString : 「何て言うか、あれだねえ……」
					PRINTFORMW 「뭐라고 말하는지, 저것이구나……」
;OriginalString : 「호기심をそそられるよ、%CNAME(MASTER, TARGET)%には……」
					PRINTFORMW 「호기심을 돋워져,%CNAME(MASTER, TARGET)%에는……」
				CASE 2
;OriginalString : 「今、ぞくって来たよ……」
					PRINTFORMW 「지금, 속편은 왔어……」
;OriginalString : 「もっと、言って？」
					PRINTFORMW 「좀 더, 말해?」
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;=================================================
	;비명지른다
	;=================================================
	IF 구상기입체크(100) && IS_NOWCOMNAME("비명지른다")
		;아라이멘도정
		IF ALI(0)
			SELECTCASE RANDEVENT(3, 10)
				CASE 0
;OriginalString : 「ちょっと、今……」
					PRINTFORMW 「조금, 지금……」
;OriginalString : 「何か妙な感じだったねえ……」
					PRINTFORMW 「무엇인가 이상한 느낌이었다 응……」
				CASE 1
;OriginalString : 「今の声、凄く……」
					PRINTFORMW 「지금의 소리, 굉장히……」
;OriginalString : 「ううん、何でもないんだけどさあ……」
					PRINTFORMW 「으응, 아무것도 아니지만 자……」
				CASE 2
;OriginalString : 「うあっ……」
					PRINTFORMW 「아……」
;OriginalString : 「びっくり했다……のと、何か……」
					PRINTFORMW 「놀랐다……의와 무엇인가……」
			ENDSELECT
		;아라이멘도부
		ELSE
			SELECTCASE RANDEVENT(3, 11)
				CASE 0
;OriginalString : 「ああ……良いねえ」
					PRINTFORMW 「아……좋구나」
;OriginalString : 「%CNAME(MASTER, TARGET)%の反応……最高だよ」
					PRINTFORMW 「%CNAME(MASTER, TARGET)%의 반응……최고야」
				CASE 1
;OriginalString : 「수치心とか、無いのかなあ」
					PRINTFORMW 「수치심이라든지, 없는 것인지」
;OriginalString : 「私は凄く、嬉しいんだけどさあ」
					PRINTFORMW 「 나는 굉장히, 기쁘지만 자」
				CASE 2
;OriginalString : 「私なんかにそんなことしちゃって……」
					PRINTFORMW 「 나같은 것에(이) 그런 일 해 버려서……」
;OriginalString : 「惨めだとは思わない……？」
					PRINTFORMW 「비참하다고는 생각하지 않는다……?」
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;아라이멘도정
	IF ALI(0)
		SELECTCASE RANDEVENT(3, 20)
			CASE 0
;OriginalString : 「何か당했다いこととか、あるかなあ」
				PRINTFORMW 「무엇인가 되고 싶은 것이라든지, 걷고」
;OriginalString : 「自分で言うのもなんだけど、今凄く機嫌良いんだよねえ」
				PRINTFORMW 「스스로 말하는 것도이지만, 지금 굉장히 기분 좋아」
			CASE 1
;OriginalString : 「じゃあ次は、私が何か봉사してあげようかな？」
				PRINTFORMW 「자 다음은, 내가 뭔가 봉사 해 줄까?」
;OriginalString : 「どう당했다いか、ちゃんと考えておくこと。いいね？」
				PRINTFORMW 「어떻게 하시고 싶은지, 제대로 생각해 두는 것. 좋다?」
			CASE 2
;OriginalString : 「これは、ご褒美あげないとなあ……」
				PRINTFORMW 「이것은, 포상 주지 않으면……」
;OriginalString : 「何がいい？　多少の無理は聞くよ？」
				PRINTFORMW 「무엇이 좋다? 다소의 무리는 (들)물어?」
		ENDSELECT
	;아라이멘도부
	ELSE
		SELECTCASE RANDEVENT(3, 21)
			CASE 0
;OriginalString : 「大分、わかってきたみたいだねえ……」
				PRINTFORMW 「상당히, 알게 된 것 같구나……」
;OriginalString : 「私は、嬉しいよ……っ」
				PRINTFORMW 「 나는, 기뻐……」
			CASE 1
;OriginalString : 「今の%CNAME(MASTER, TARGET)%、良かったよ？」
				PRINTFORMW 「지금의 %CNAME(MASTER, TARGET)%, 좋았어요?」
;OriginalString : 「惚れちゃいそうな程、可哀想でさあ……」
				PRINTFORMW 「반해버릴 것 같을 수록, 불쌍해 자……」
			CASE 2
;OriginalString : 「いやー、만족しちゃったなあ」
				PRINTFORMW 「아니―, 만족해 버렸다」
;OriginalString : 「次は、どんなのが良い？」
				PRINTFORMW 「다음은, 어떤 것이 좋다?」
		ENDSELECT
	ENDIF
	RETURN 2
ENDIF

