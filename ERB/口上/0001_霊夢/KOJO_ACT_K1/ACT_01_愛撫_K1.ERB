	;		;전부터애무
	;		IF TCVAR:MASTER:위치전후 == 1
	;			PRINTFORMW 
	;		;뒤로부터애무
	;		ELSE
	;			PRINTFORMW 
	;		ENDIF
	;		RETURN 1
	;	ENDIF
	;	
	;	;-------------------------------------------------
