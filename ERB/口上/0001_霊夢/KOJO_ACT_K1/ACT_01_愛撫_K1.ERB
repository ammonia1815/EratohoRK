					CALL FADEOUT(8, "（내, 내）")
					[SKIPSTART]
					;말하게 하지 않는 것이 좋은 것 같다
					PRINTFORM 「키-해 먹여
					CALL HEARTBW
					[SKIPEND]
				ELSE
;OriginalString : 아웃12, "じゅうっ…ちゅぱ…ぢゅっ…ぢゅーっ…")
					CALL FADEOUT(12, "쥬웃…츄파…쥿…쥿…")
					PRINTFORMW 「마셔 줄테니까, 빨리 사정해버려」
;OriginalString : 아웃6, "「あむっ！」")
					CALL FADEOUT(6, "「아뭇!」")
					PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%의 음경을 입보지로 깊고 강하게 들이마셔 먹고 있다…
					IF !RAND:3
						PRINTFORMDW 가지고 갈 곳이 곤란하는 %CALLNAME:MASTER%의 시선은 %CALLNAME:TARGET%의 가는 어깨에 빨려 들여갔다
						PRINTFORMDW 그 가는 선은 %CALLNAME:MASTER%안의 수컷에게 %CALLNAME:TARGET%의 암컷을 의식시켰다…
					ELSEIF !RAND:2
						PRINTFORMDW %CALLNAME:TARGET%의 요염한 흑발이 %CALLNAME:MASTER%의 시선을 파악한다…
					ELSE
						PRINTFORMDW 일심불란하게  페니스를 빨아먹는 %CALLNAME:TARGET%의 표정은 긴 머리카락에 덮여 보이지 않는다…
					ENDIF
				ENDIF
			;애태움없음
			CASEELSE
				PRINTFORMDW %CALLNAME:TARGET%는 리본을 흔들면서 %CALLNAME:MASTER%의 음경을 후오까지 사용해 봉사하고 있다…
		ENDSELECT
		RETURN 1
	ENDIF
	
	;가버려는 상류에 있는+3회연속는 우선 나오지 않기 때문에, 다른플레이로 이행해사정시킨다일을 의식했다 (분)편이 좋은 것인지도 모른다
	;가게 할 생각이 아닌 형태도 성격이 잘 맞을지도 모른다
	;=================================================
	;펠라연속실행
	;=================================================
	IF 구상기입체크(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;안면승마중
		;-------------------------------------------------
		IF 구상기입체크(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;연속실행펠라 %CALLNAME:MASTER%정욕
		;-------------------------------------------------
		IF 구상기입체크(50) && IS_CONDITION(MASTER, "정욕")
;OriginalString : 아웃8, "（変な声出しちゃって）")
			CALL FADEOUT(8, "（이상한 소리 내 버려）")
;OriginalString : 인8, "（可愛いんだから）")
			CALL FADEIN(8, "（사랑스럽기 때문에）")
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;연속실행펠라 %CALLNAME:TARGET%정욕
		;-------------------------------------------------
		IF 구상기입체크(50) && IS_CONDITION(TARGET, "정욕")
			[SKIPSTART]
			;병콜라가 존재하므로 구형 패키지라고 강변 하면 캇빠 새우 선도 환상향에 존재할 수 있다. 플레이야씨가 곤란하므로 없음
			PRINTFORMW （아─… 그만두고들 응 없어요 이것…）
			PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%의 캇빠 새우 선사이즈의 페니스달라붙어서 놓지 않고 있다…
			;비명과의 제휴를 생각하면 없이는 없지만, 아프기 때문에 없음
			PRINTFORMW （별로 맛있지 않네요… 씹으면 육즙 나오는 것일까…? ）
			[SKIPEND]
			PRINTFORMDW %CALLNAME:TARGET%는 맛있을 것 같게 %CALLNAME:MASTER%의 음경을 맛보고 있다
			RETURN 1
		ENDIF
		
		;연속실행의 네스트를 개방한다면 여기는 반드시 써 둡시다
		;-------------------------------------------------
		;그 외
		;-------------------------------------------------
		IF 구상기입체크(100)
			PRINTFORML 「계속해요? 짠다…」
			PRINTFORMDW %CALLNAME:TARGET%는 헌신적으로 %CALLNAME:MASTER%의 일물을 입으로 봉사하고 있다…
			RETURN 1
		ENDIF
	ENDIF
	
	;-------------------------------------------------
	;안면승마중
	;-------------------------------------------------
	IF 구상기입체크(100) && TRUECHECK(TEQUIP:안면승마) > 0
		PRINTFORMW 「 나의 %RAND_SPLIT("/보지/소중한 곳/저기")%그리고 자지 이렇게 해 버려서…」
;OriginalString : 아웃4, "「んふふ」")
		CALL FADEOUT(4, "「응후후」")
;OriginalString : 인12, "「しょうがないわね」")
		CALL FADEIN(12, "「어쩔 수 없네요」")
		PRINTFORMW 「 나로 이렇게 했으니까…책임과 간장은 배달시켜 주지 않으면…%RAND_SPLIT("/안 돼요")%그런데」
		;앵글적으로 %CALLNAME:MASTER%에는 안보이기 때문에 없음
		;PRINTFORMDW %CALLNAME:TARGET%는 기쁜듯이 %CALLNAME:MASTER%의 음경을 물었다
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;LOWERJACKET(1) == 1 「」LOWERUNDER(1) == 1 「비켜 놓아」등으로 확장하는 것도 좋을 것입니다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(100)
		;지문
		;%CALLNAME:TARGET%는 %CALLNAME:MASTER%의 페니스를 잡아 천천히 상하시키고 있다…
		;%CALLNAME:TARGET%는 %CALLNAME:MASTER%의 사타구니에 얼굴을 묻어 터질듯이 노장했다남성기를입으로 애무하기 시작했다…
		;구상여기
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%의 가랑이에 얼굴을 묻으면(자), 늘어지는 흑발이%CALLNAME:TARGET%의 표정을 숨긴다
				PRINTFORMDW 안보이는 앵글이, 오히려%CALLNAME:MASTER%(을)를 어지럽혔다
			CASEELSE
				PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%(을)를 높이려고 열심히 머리를 움직일 때마다, 리본이 팔랑팔랑춤추었다…
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT13, 항문애무
;────────────────────────────────────
;·항문애무/전립선자극/항문바이브출납
;·항문바이브장비중은, 항문바이브출납에 파생
;·연속실행의 경우, 전립선자극에 파생
;·가버려or쾌Ｃ가 5000이상의 경우, 죠교자의애무스킬에 응해전립선자극에 파생
;·발기도가 낮은 경우, 죠교자의애무스킬에 응해전립선자극에 파생
;·요강부디기의 경우, 전립선자극에 파생
;·TFLAG:징계·플래그가 서 있으면 발생하지 않는다
;·턴방침가"소프트", "이상"라면 발생하지 않는다
;·TEQUIP(삼각목마/페니스밴드/항문비즈)장착중에는 발생하지 않는다
;·성교봉사중에는 발생하지 않는다
;-------------------------------------------------
@KOJO_ACT_K1_13
;-------------------------------------------------
;·TFLAG:ACT파생（0=항문애무/1=전립선자극/2=항문바이브출납）
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT13_0, 항문애무
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORML 「응…아마 여기서 맞고 있는 원이군요」
		PRINTFORMDL %CALLNAME:TARGET%는 되어가는 대로 하게 %CALLNAME:MASTER%의 국문을 꾸짖어 왔다
		RETURN 1
	ENDIF

	;=================================================
	;탄 떨어짐 또한 고추 서 있지 않다
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:발기도 <= 1000 && BASE:MASTER:사정 == 0
		PRINTFORML 「총알 끊어지고 하면 허락받을 수 있다고 생각했는지 해들?」
		PRINTFORML 「%RAND_SPLIT("나는 아직만족하고 있지 않아요/좀 더 교제하세요/허락하지 않아요/허없·어·요♪")%」
		RETURN 1
	ENDIF

	;=================================================
	;안면승마중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;하반신복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(0) && PANTSCHECK(1, 1)
		;하반신겉옷를 걷고
		IF LOWERJACKET(1) == 1
			PRINTFORMW 
		;속옷 너머로
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(100)
		SELECTCASE ABL:MASTER:Ａ감각
			CASE 0 TO 2
				PRINTFORML 「아파? 괜찮아, 곧 좋아져」
;OriginalString : 아웃4, "「…多分」")
				CALL FADEOUT(4, "「…아마」")
			CASEELSE
				PRINTFORMW 「엉덩이로 기분 좋아져버린다니, 너는 %RAND_SPLIT("/사실")%」
				PRINTFORMW 「어쩔 수 없%RAND_SPLIT("는 변태/")%구나?」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT13_1, 전립선자극(항문애무)
;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상이남자의 경우에만 파생
;후타나리의 전립선은 요도, 즉 질 위에 있다고 해, Ｇ스팟 자극에
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 1
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORMW 「아 여기인가? 대충 알 것 같아」
		RETURN 1
	ENDIF
	;=================================================
	;발기강제
	;=================================================
	IF 구상기입체크(100) && TCVAR:MASTER:발기도 <= 1000
		PRINTFORMW 「자! 좀 더 자지를 세워!」
		PRINTFORMW 「… %RAND_SPLIT("너의 엉덩이는 편리하구나/너의 자지는 편리하구나/편리하구나 이것")%」
		RETURN 1
	ENDIF
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(100)
		PRINTFORMW 「응, 편리한건지 단순한건지… 이것은 어떤 의미로는, 남자다운 것일까?」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT13_2, 항문바이브출납
;━━━━━━━━━━━━━━━━━━━━━━━━
;·항문바이브장비중은, 항문바이브출납이 된다
;-------------------------------------------------
IF 구상기입체크(100) && TFLAG:ACT파생 == 2
	;죠교대상이남자
	IF TALENT:MASTER:남자
		PRINTFORMW 「엉덩이라고%RAND_SPLIT("는 히는 히 말한다/응 몸부림친다")%의는 좋은데…」
		PRINTFORMW 「헛됨 공격하면, 징계나무 해요」
		RETURN 1
	;그 외
	ELSE
		;PRINTFORMW 
		RETURN 0
	ENDIF
	;RETURN 1
ENDIF

;────────────────────────────────────
;ACT14, 항문펠라
;────────────────────────────────────
;·항문펠라
;·TFLAG:징계·플래그가 서 있으면 발생하지 않는다
;·턴방침가"소프트", "이상"라면 발생하지 않는다
;·TEQUIP(항문바이브/항문비즈/관장그릇＋플러그/페니스밴드/삼각목마)장착중에는 발생하지 않는다
;·성교중/성교봉사중에는 발생하지 않는다
;·TCVAR:MASTER:애태움도가 실로 되어 있는 경우는 발생하지 않는다
;·혀에 의한전립선자극에의 파생은 「그런 곳까지 혀가 닿을 이유 없지 않은가www」라고 하는 일로 숙청되고 있습니다
;·현시점에서 ACT가 파생하는 일은 없지만, ACT 개변의 가능성에 대비해, TFLAG:ACT파생 == 0으로 지정
;-------------------------------------------------
@KOJO_ACT_K1_14
;-------------------------------------------------
;·ACT파생없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT14_0, 항문펠라
;━━━━━━━━━━━━━━━━━━━━━━━━
;구음는 물소리에 두어 그다지 말하게 하지 않는다
IF 구상기입체크(0) && !TFLAG:ACT파생
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자가자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:자위중, 1)
		;죠교자가페니스밴드 몸에 걸치고 있다
		IF TEQUIP:죠교자페니스밴드
			PRINTFORMW
		;그 외
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;안면승마중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가자위중
	;=================================================
	IF 구상기입체크(0) && GETBIT(TEQUIP:자위중, 0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;하반신복장이 요점 밀치고
	;=================================================
	IF 구상기입체크(0) && PANTSCHECK(1, 1)
		;하반신겉옷를 걷고
		IF LOWERJACKET(1) == 1
			PRINTFORMW 
		;속옷 너머로
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT15, 키스
;────────────────────────────────────
;·키스/미약키스
;·TFLAG:징계·플래그가 서 있으면 발생하지 않는다
;·턴방침가"하드", "이상"라면 발생하지 않는다
;·죠교자에 TALENT:벽통과가 없는 경우, TEQUIP:볼개그장착중은 튕겨진다
;-------------------------------------------------
@KOJO_ACT_K1_15
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT15, 키스 첫회
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && FIRSTACT(0)
	;=================================================
	;서로 처음
	;=================================================
	IF 구상기입체크(0) && EXP:MASTER:키스경험 == 0 && EXP:TARGET:키스경험 == 0
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상가 처음
	;=================================================
	IF 구상기입체크(0) && EXP:MASTER:키스경험 == 0
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자가 처음
	;=================================================
	IF 구상기입체크(100) && EXP:TARGET:키스경험 == 0
		PRINTFORMW 「…」
		;입맞춤은 히라가나가 분위기가 나온다
		PRINTFORMDW 강제적인 행위와는 너무 다른, 접하는 것 같은 상냥한 입맞춤
		PRINTFORMW 「이것으로 일선, 넘어 버렸네」
		PRINTFORMDL 뺨을 붉히고 입가를 누르는 행동이 기특하다…
		RETURN 1
	ENDIF

	;"첫회"의 네스트를 개방해, 각 조건의 분기를방치 한다면 여기서 보완합시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
