	;=================================================
	;뒤로부터
	;=================================================
	IF 구상기입체크(0) && TCVAR:MASTER:위치전후 == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;전부터
	;=================================================
	IF 구상기입체크(0) && TCVAR:MASTER:위치전후 == 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;여기를 최초로 씁시다
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
