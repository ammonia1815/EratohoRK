	;=================================================
	;액량 2
	;=================================================
	IF 구상기입체크(0) && TEQUIP:관장그릇＋플러그 == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;액량의 분기를 모두 묻었을 경우는 TEQUIP:관장그릇＋플러그 == 1에 상당
	;개별적으로 나누지 않고 SELECTCASE TEQUIP:관장그릇＋플러그로 정리해 버리는 일도 가능
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT68_1, 관장그릇＋플러그해제
;━━━━━━━━━━━━━━━━━━━━━━━━
;여기에 파생하는 시점에서 액량은 5가 되어 있다
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:ACT파생 == 1
	
	;지문↓
	;%CALLNAME:TARGET%는%TARGET_DESCRIPTION("하면서, ")% %CALLNAME:MASTER%로부터
	;항문마개를 뽑아, 장내의 오물을 항문으로부터 따돌려 산등시켰다…
	
	;=================================================
	;처음 「배출」
	;=================================================
	IF 구상기입체크(0) && FIRSTACT(1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT68_2, 관장액추가
;━━━━━━━━━━━━━━━━━━━━━━━━
;이미 주입 끝난 경우에 이 개소에 파생
;-------------------------------------------------
IF 구상기입체크(0) && TFLAG:ACT파생 == 2
	
	;지문↓
	;%CALLNAME:TARGET%는%TARGET_DESCRIPTION("하면서, ")% %CALLNAME:MASTER%의 항문에
	;한층 더관장액을 주입하려고했다…
	
	[SKIPSTART]
	액량으로 나누어 쓴다면 comment out를 해제
	하층의 PRINTFORMW를 삭제해 주세요
	;관장액분량 파생
	SELECTCASE TEQUIP:관장그릇＋플러그
		;액량 5
		CASE 5
			PRINTFORMW 
		;액량 4
		CASE 4
			PRINTFORMW 
		;액량 3
		CASE 3
			PRINTFORMW 
		;그 외
		CASEELSE
			PRINTFORMW 
	ENDSELECT
	[SKIPEND]
	;나누어 쓰지 않는 것이면 여기서
	PRINTFORMW 
	RETURN 1
ENDIF
