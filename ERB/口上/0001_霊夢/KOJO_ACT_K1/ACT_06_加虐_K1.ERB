@KOJO_ACT_K1_66
;-------------------------------------------------
;·ACT의 파생 없음
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT66, 매도
;━━━━━━━━━━━━━━━━━━━━━━━━
;·지문에 분기 없음@TARGET_DESCRIPTION에 의한 상황 묘사가 들어갈 가능성 있어
;-------------------------------------------------
IF 구상기입체크(100) && !TFLAG:ACT파생
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && FIRSTACT(0)
		PRINTFORMW 「바보」
		;지문과의 맞대고 비빔이기 때문에 궤도수정
		PRINTFORMDW %CALLNAME:TARGET%의 매도는 요령을 얻지 않았다
		RETURN 1
	ENDIF
	
	;=================================================
	;성교중/성교봉사중
	;=================================================
	IF 구상기입체크(0) && TRUECHECK(TEQUIP:성교중, TEQUIP:성교봉사중) > 0
		;성교중
		IF TEQUIP:성교중
			PRINTFORMW 
		;성교봉사중
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;자위중
	;=================================================
	IF 구상기입체크(100) && TRUECHECK(GETBIT(TEQUIP:자위중, 0), GETBIT(TEQUIP:자위중, 1)) > 0
		;죠교대상가자위중
		IF GETBIT(TEQUIP:자위중, 0)
			;심플한 (분)편이 박힌다
			PRINTFORMW 「………」
;OriginalString : 아웃8, "「くすっ」")
			CALL FADEOUT(8, "「키득」")
			PRINTFORMW 「」
		;죠교자가자위중
		ELSE
			RETURN 0
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;Ｃ애무계속중
	;=================================================
	IF 구상기입체크(0) && GROUPMATCH(TEQUIP:죠교대상Ｃ사용, 1, 2)
		;수음·애무계속중
		IF TEQUIP:죠교대상Ｃ사용 == 1
			PRINTFORMW 
		;펠라·쿤닐링구스 계속중
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;그 외·조건 없음
	;=================================================
	IF 구상기입체크(100)
		;길기 때문에 분할
		SELECTCASE RAND:7
			CASE 0
				PRINTFORML 「변태」
				PRINTFORML 「짐승」
				PRINTFORML 「색이상」
			CASE 1
				PRINTFORML 「바람둥이」
				PRINTFORML 「색골」
				PRINTFORMW 「로리콘」
			CASE 2
				PRINTFORML 「겨드랑이 페티쉬」
				PRINTFORML 「배꼽페티쉬」
				PRINTFORMW 「다리페티쉬」
			CASE 3
				PRINTFORML 「늅」
				PRINTFORML 「쫄폭충」
				PRINTFORMW 「이지 컨티뉴충」
			CASE 4
				PRINTFORML 「우유부단」
				PRINTFORML 「항문 페티쉬」
				PRINTFORMW 「발 페티쉬」
			CASE 5
				PRINTFORML 「바지런한 기질 없음」
				PRINTFORML 「둔감」
				PRINTFORMW 「바보 남편」
			CASEELSE
				PRINTFORML 「조루」
				PRINTFORML 「포경」
				PRINTFORML 「6.9」
				PRINTFORMW 「환남충」
		ENDSELECT
		PRINTFORML
		PRINTFORMW 「음…그 밖에 뭐가 있었더라…」
		RETURN 1
	ENDIF
ENDIF

