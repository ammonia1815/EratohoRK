;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(0) && ARG >= 1000 && (ARG & 10) == 3
	SELECTCASE ARG
		;반라는 싫어
		CASE 1013
			PRINTFORML 
