;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[Ａ감각]업
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(25) && ARG == 5
	;굳이 개발 도상에서 1회만 감상
	SELECTCASE ABL:MASTER:Ａ감각
		CASE 3
			PRINTFORML 「…완전히…변태니까」
		CASE IS >= 5
			PRINTFORML 「……이제 애널의 노예가 되어버렸구나」
			PRINTFORM 「몹쓸 남편
			CALL HEARTBW
		CASEELSE
			PRINTFORMW
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[Ｂ감각]업
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(25) && ARG == 6
	IF TALENT:MASTER:남자
		SELECTCASE ABL:MASTER:Ｂ감각
		CASE 1 TO 3
			PRINTFORMW 「반응이 꽤 재밌어♪」
		CASEELSE
			PRINTFORMW 「남자인데도 여기로 느낀다니… 변태」
		ENDSELECT
	ELSE
		PRINTFORMW
	ENDIF
	RETURN 1
ENDIF

