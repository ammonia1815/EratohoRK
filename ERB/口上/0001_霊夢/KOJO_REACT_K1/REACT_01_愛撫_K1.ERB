	;머리카락을 흩뜨려 몸부림쳤다…
	;=================================================
	;죠교대상에페니스없음
	;=================================================
	IF 구상기입체크(0) && !PENIS(MASTER)
		;-------------------------------------------------
		;쾌감을 받아들인다
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을 받아들인다")
			PRINTFORMW
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;쾌감을 참는다
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을 참는다")
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
;조건에 히트 하지 않으면 범용 리액션에 흘린다
RETURN 0
ENDIF

