		ENDIF
		
		;-------------------------------------------------
		;쾌감을 참는다
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을 참는다")
			PRINTFORMW
			RETURN 1
		ENDIF
