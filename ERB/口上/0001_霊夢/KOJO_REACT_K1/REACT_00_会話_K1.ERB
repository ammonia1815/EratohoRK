IF 구상기입체크(0) && IS_COMGRONAME("소극적으로 한다")
	;자연스러운행동
	;=================================================
	;REACT부하 0, 각오를 결정한 것처럼, 작은 소리로 승낙했다
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "자연스러운행동"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;무리가 없는행동
	;=================================================
	;REACT부하 0, 작은 소리로 승낙했다	
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "무리가 없는행동"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;무리를했다행동
	;=================================================
	;REACT부하 2
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "무리를했다행동"
		;실은적극			부끄러움에 스러질 것 같은 소리로 승낙했다
		IF COMCOR_POSI()
			PRINTFORMW 
		;실은 혐				분함에 얼굴을 비뚤어지게 하면서도 승낙했다
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT6_1, 마음대로 하면 좋다고 대답했다
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류가태연하게 한다의 경우
;-------------------------------------------------
IF 구상기입체크(0) && IS_COMGRONAME("태연하게 한다")
	;자연스러운행동
	;=================================================
	;REACT부하 0, 별로 싫지 않다고 말했다
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "자연스러운행동"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;무리가 없는행동
	;=================================================
	;REACT부하 1, 마음대로 하면 좋다고 대답했다
	;=================================================
	IF 구상기입체크(0) && GET_COM_RANK() == "무리가 없는행동"
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;무리를했다행동
	;=================================================
