;이하, COM 마다의 분기
;-----------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM10, 부끄러워한다(REACT분류:소극적으로 한다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("부끄러워한다")
	;=================================================
	;쾌감에부끄러워한다
	;=================================================
	IF 구상기입체크(0) && TFLAG:REACT파생 == 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;파생 1을 썼을 경우는 REACT파생 0, 「지금의 상황에부끄러워한다」취급
	;쓰지 않았던 경우는 REACT파생에 관련되지 않고, 이 개소를 보완합니다
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(100)
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「전부터(분)편이 좋아?」
				PRINTFORMW 「전부터 하면, 너에게%RAND_SPLIT("표정/느끼고 있는 곳/군침 나와 있는 곳")%볼 수 있어버리니까…」
				;기어들어가는 소리를 표현했다겉껍데기, fadeout는 뭔가 틀리다는 느낌이 든다. 겨우 짜 출했다 소리로서 FADEIN로 변경
;OriginalString : 인12, "「私が恥ずかしいのよ…」")
				CALL FADEIN(12, "「내가 부끄러운거야…」")
			CASEELSE
				PRINTFORMW 「짐승의 교미하고 싶고 부끄러울까?」
				PRINTFORMW 「그렇게 부끄러워하지 않아도, 모처럼 둘이서니까, 군요?\@ IS_CONDITION(MASTER, "정욕") ? 번식합시다 # 즐깁시다 \@」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;파생 1은 REACT분류:날뛴다취급인 것으로 고려하지 않는다
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM11, 싫어한다(REACT분류:소극적으로 한다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("싫어한다")
	PRINTFORMW 「교미하고 싶고 혐?」
	PRINTFORMW 「케다모노인 너에게는 이것으로 좋아」
	RETURN 1
ENDIF

;파생 1의 경우는 REACT분류:용서를 빌다취급
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM14, 쾌감을 참는다(소극적으로 한다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 참는다")
	;=================================================
	;애원(REACT분류:용서를 빌다)
	;=================================================
	IF 구상기입체크(0) && TFLAG:REACT파생 == 1
		RETURN 1
	ENDIF
	
	;파생 1을 썼을 경우는 REACT파생 0, 「내구」취급
	;쓰지 않았던 경우는 REACT파생에 관련되지 않고, 이 개소를 보완합니다
	;=================================================
	;그 외
	;=================================================
	IF 구상기입체크(0)
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM15, 쾌감을 받아들인다(REACT분류:받아들인다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("쾌감을 받아들인다")
	[SKIPSTART]
	;개만들기를 너무 전면에 내세우면 붉은 방석 운반을 연상하기 때문에(위해) comment out
	;이제(벌써) 취하고 있다. 이미 낳게 하고 있을 때에 모순되기 때문에(위해) comment out
	;개선의 여지가 남는다
	PRINTFORMDW %CALLNAME:MASTER%는 책임을 진다%RAND_SPLIT("각오를 하면/결의를 굳히면")% %CALLNAME:TARGET%의 허리를 잡아 근원까지 음경을 념 글자 붐비었다
	;%CALLNAME:MASTER%의 자지는 과연 안쪽까지 닿는 것인가
	PRINTFORMW 「그렇게… 안쪽까지 확실히 삽입하세요」
	
	;뒤손에 양손붙잡기는 지문과 모순된다
	
	;접수당신가 공격을 받아들이는플레이
	[SKIPEND]
	;허리를 흔든다보다 소극적인당신주도 
	IF IS_CONDITION(MASTER, "정욕")
		;레이무씨가 기분이 좋다!
		PRINTFORML 「너로 되어버린다의, 기분이 좋다」
	ELSE
		;레이무씨가더 하드하게 해
		PRINTFORML 「 좀 더 격렬하고%RAND_SPLIT("라고도 괜찮기 때문에/해도 되니까")%」
	ENDIF
	
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM16, 기분 좋다! (REACT분류:받아들인다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("기분 좋다!")
	PRINTFORMW 「기분 좋다? 보지 기분 있고?」
	RETURN 1
ENDIF

;주도권에 의해 REACT분류가변화 하는 일에 주의
;━━━━━━━━━━━━━━━━━━━━━━━━
;COM20, 허리를 흔든다(REACT분류:적극적으로 한다)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_NOWCOMNAME("허리를 흔든다")
	[SKIPSTART]
	IF ! RAND:2
		;탈의분기가필요
		PRINTFORMDW 흰 등에 퍼지는 흑발이 요염함
		
		;일본식 방에 소리의 반향은 맞지 않는 생각이 들어 왔다
		PRINTFORMDW %CALLNAME:TARGET%의 허리를 단단히잡아, 엉덩이에 허리를 부딪힐 때,
		PRINTFORMDW 리본이 나비와 같이 춤추어 놀아, 짐승과 같이 허덕여 소리가 방에 울렸다
		;조교중의 언동과 맞대고 비비고 격렬한 이전 (위해)때문에 comment out
		;PRINTFORMW 「응읏! 격렬해져 왔군요」
		;PRINTFORMW 「열심히 허리 흔들어 버려, 좀 더격렬하게도 좋아요」
		PRINTFORMW 「응응! 열심히 허리 털어 버려서」
		;사정시와의 맞대고 비빔가필요
		PRINTFORMW 「너의 성욕, 전부 부딪쳐도 좋아요」
		
		;너의 %CALLNAME:TARGET%（소유）를 사용하고 싶지만, 「노」라고 궁합이 나쁜 조건의 조정도필요
		;PRINTFORMW 「좀 더%RAND_SPLIT("너/")%의\@ TALENT:TARGET:임신? 잉 보고 구멍 # 보지\@유용해」
		;엉덩이에욕정했다짐승풍
		PRINTFORMDW %CALLNAME:MASTER%는 %CALLNAME:TARGET%의 엉덩이를 덥석으로 하면서 필사적으로 허리를 부딪혔다
		PRINTFORMDW %CALLNAME:MASTER%는 %CALLNAME:TARGET%의 다리를 움켜 쥐고 필사적으로 허리를 부딪혔다
		RETURN 1
	ENDIF
	[SKIPEND]
	IF IS_CONDITION(MASTER, "정욕")
		PRINTFORMDW %CALLNAME:MASTER%(은)는 %CALLNAME:TARGET%의 허리를 잡아 고정하면서 허리를 부딪혔다
		PRINTFORML 
		PRINTFORMW 「이거 참, 너무 격렬해」
		RETURN 1
	ENDIF
	
	IF IS_CONDITION(TARGET, "정욕")
