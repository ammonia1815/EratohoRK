;========================================================================
;EV13, 죠교종료구상
;========================================================================
;·@EVENTCOMEND로부터 불려 간다
;·구상주가죠교종료하기 직전에 표시
;·ARG:1, ARG:2의 경우는 지문 표시 후에 PRINTL로 공행. 그 뒤로구상를 표시
;·ARG:0의 경우는 고유의 지문 없음. 구상표시전에 PRINTL로 공행이 들어간다
;·RESULT 0이상을반환와구상표시 후에 PRINTL로 공행이 들어간다
;-------------------------------------------------
;관련 변수…
;FLAG:일상제어	죠교종료시 상황(1=통상/2=정력 0/3=일회휴게/4=실신했다)
;-------------------------------------------------
@KOJO_EVENT_K1_13(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(0, 보통으로 종료, 1, 죠교대상가 피곤한, 2, 죠교대상가실신했다)
;-------------------------------------------------
IF TALENT:Ｖ민감 == 1
	PRINTFORML
	PRINTFORMDW 아이 만들기 섹스에 반응하고 있던 %CALLNAME:TARGET%의 질은, 원래의 감도에 돌아왔다
	TALENT:Ｖ민감 = 0
ENDIF
IF TALENT:매혹 == 1
	PRINTFORML
	PRINTFORMDW %CALLNAME:TARGET%의 유노에상태와 함께 매혹은 해제되었습니다
	TALENT:매혹 = 0
ENDIF

;────────────────────────────────────
;ARG:1, ARG:2, 피로·실신에 의한죠교종료 FIRSTEVENT(0)～
;────────────────────────────────────
;·죠교대상소모도에 응해 피로/실신로 나누어진다
;·호출 타이밍은 지문의 표시→PRINTL→구상의 표시
;·EVENT201, 간호(일상이벤트)의 개소를 쓰는 경우는, 상손구제휴할 수 있도록(듯이)하면 좋을 것입니다
;------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상가 피곤함
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 1
	PRINTFORMW 「슬슬 끝맺을까요」
	PRINTFORMW 「넘어질 때까지해 주었으면 했다? 그렇게 초조해 하지 않아도, 나는」
;OriginalString : 인20, "「逃がさないわよ♪」")
	CALL FADEIN(20, "「놓치지 않아요♪」")
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상가실신했다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && ARG == 2
	PRINTFORMDW %CALLNAME:TARGET%는 의식을 잃은 %CALLNAME:MASTER%(을)를 메었다
	RETURN 1
ENDIF

;────────────────────────────────────
;굴복종료  이번에취득했다인가? 를 참조하지 않으면 의미가 없다
;────────────────────────────────────
;【굴복:25】질내사정취득첫회
IF 구상기입체크(100) && GETBIT(CFLAG:각인, 25) && FIRSTEVENT(125)
	PRINTFORML 「이군요. 어땠어요? 무엇은 그」
;OriginalString : 아웃6, "  もじもじ")
	CALL FADEOUT(6, "  머뭇머뭇")
	PRINTFORML 「아기만들기.」
	PRINTFORML 「아기만들기 어, 좋아하게 될 수 있을 것 같다?」
	RETURN 1
ENDIF
;【굴복:11】키스취득첫회
IF 구상기입체크(100) && GETBIT(CFLAG:각인, 11) && FIRSTEVENT(111)
;OriginalString : 아웃4, "  ちら")
	CALL FADEOUT(4, "  등")
	PRINTFORMDL %CALLNAME:TARGET%는 행때문을 끝맺으려고 하면서도,
;OriginalString : 아웃4, "  ちら")
	CALL FADEOUT(4, "  등")
	PRINTFORMDL 끊임없이 %CALLNAME:MASTER%의 입가에 시선을 향하여 왔다…
	PRINTFORML 
	PRINTFORMDL 스스로의 입술에 손가락을 대는 %CALLNAME:TARGET%
	PRINTFORML 「입맞췄으니까, 이것으로 우리들 [애인]이군요」
	PRINTFORMW 「좋구나, 부부 겸 애인이야.」
;OriginalString : 인6, "「んふふ♪」")
	CALL FADEIN(6, "「응후후♪」")
	PRINTFORMW 「이봐, 우리들은 이런 경험이 없었잖아」
	PRINTFORML 
	PRINTFORMDW %CALLNAME:TARGET%는 조교 후 흘린 땀을 씻으러 욕실로 향했다…
	;안보이는 뒷모습으로 들리는 콧노래 튀는발잡기에 맞추어소녀기상곡
;OriginalString : 아웃12, "「らんらんらーらーらー♪ららんらーらーら♪」")
	CALL FADEOUT(12, "「들 응등응등-들 -들―♪라라응등-들 -들♪」")
	RETURN 1
ENDIF
;────────────────────────────────────
;ARG:0보통으로 종료 FIRSTEVENT(10)～
;────────────────────────────────────
;·통상의죠교종료시의구상
;·지문 없음. PRINTL에 의한 공행의 뒤로구상표시
;·테두리마다이벤트번호를 털어 있습니다만, 임의로 설정하는 일은 가능합니다
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;대만족보너스를 얻고 있다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:금회대만족
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTEVENT(10)
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상의 정력 0
	;=================================================
	IF 구상기입체크(100) && FLAG:일상제어 == 2
		PRINTFORML 「오늘도 즐거웠어요」
		PRINTFORMDL %CALLNAME:TARGET%는 윤기 윤기로 하면서 %CALLNAME:MASTER%에 생긋 한 웃는 얼굴을 향했다
		RETURN 1
	ENDIF

	;=================================================
	;가능하게 되었다  임신하게 하고 나서 낳을 때까지 물건발리나 있고 때문 전개 강화
	;=================================================
	IF 구상기입체크(0) && CFLAG:임신 == 10
		PRINTFORMDL 
		RETURN 1
	ENDIF

	IF 구상기입체크(100) && CFLAG:임신 == 4
		PRINTFORMDW %CALLNAME:TARGET%는 체온계와 같은 것을 전해 왔다
		PRINTFORMDL 체온계와 같은 것에는 환에 종선이 2개 줄표시되고 있다…
		PRINTFORML 
		PRINTFORMDL 아무래도 내일과 모레는 개이는 것 같다…
		PRINTFORMDL %CALLNAME:TARGET%에는 햇빛이 잘 어울린다
		RETURN 1
	ENDIF
	
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		RETURN 1
	ENDIF
	;무슨이야기였는지 망실
	;여기서 리턴 시키고 하부를 대만족 하고 있지 않는 %CALLNAME:TARGET%씨로 만들어 낸다
	RETURN 1
ENDIF

;*************************************************
;이하, 호감도에 의한 파생
;*************************************************

;EVENT11 근처에서 임의의 KFLAG를 세워 조건으로 해 준다고 이야기의 이치는 맞추기 쉽습니다
;━━━━━━━━━━━━━━━━━━━━━━━━
;상사상애（호의 3000 이상·호감도 3000 이상）
;-------------------------------------------------
;·서로 서로 좋아하고 있다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && AFFECTION("상사상애")
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && FIRSTEVENT(20)
		PRINTFORMDW %CALLNAME:MASTER%의 방을 나오려고 해, 한 번 뒤돌아 봐 입가를 피기 시작하게 한다
		PRINTFORML 「행복한 표정 해 버려서.」
		RETURN 1
	ENDIF
	[SKIPSTART]
	;=================================================
	;후타나리%CALLNAME:TARGET%씨가 대만족 하고 있지 않는가 개성교굴복획득이 끝난【굴복:22】질내사정당했다
	;=================================================
		;=================================================
		;%CALLNAME:MASTER%의 처녀를 받아 기분
		;=================================================
		
		;=================================================
		;%CALLNAME:MASTER%가남자 또한항문개발 끝난 경우 엉덩이를 사용된다
		;=================================================
		
	[SKIPEND]
	;=================================================
	;대만족 하고 있지 않는가 개성교봉사가 끝난;대 만족하지 이길 성교 봉사 요청
	;=================================================
	;%CALLNAME:TARGET%과 엣치했다일 있는 경우의 조건 설정이 생각해내지 못하기 때문에 임신하게 한 후의 포상에 자리 매김을 한다
	;대만족는 발생하지 않아도 매일밤만족는시킨다. 사티스파크션!
	IF 구상기입체크(100)
		PRINTFORML 「응. 여기로부터는 너가 마음대로 해도 좋아요」
		PRINTFORMDW %CALLNAME:TARGET%는 아직 만족하지 않는 것 같다…
		PRINTFORML 
		;%CALLNAME:MASTER%가 %CALLNAME:TARGET%에 반하고 있는 일을 암시
		PRINTFORMDL %CALLNAME:MASTER%(은)는 %CALLNAME:TARGET%는 만족할 때까지 천천히 후희를 즐겼다
		
		;실험용 코드
		EXP:MASTER:Ｃ경험   += 10
		PRINTL Ｃ경험 +10
		EXP:MASTER:성교경험 += 3
		PRINTL 성교경험 +3
		EXP:MASTER:절정경험 += 3
		PRINTL 절정경험 +3
		EXP:MASTER:사정경험 += 3
		PRINTL 사정경험 +3
		EXP:ARG:Ｖ경험   += 10
		PRINTFORML （%CALLNAME:ARG%）Ｖ경험 +10
		EXP:ARG:성교경험 += 3
		PRINTFORML （%CALLNAME:ARG%）성교경험 +3
		EXP:ARG:절정경험   += 3
		PRINTFORML （%CALLNAME:ARG%）절정경험 +3
		EXP:ARG:정액경험   += 3
		PRINTFORML （%CALLNAME:ARG%）정액경험 +3
		;MASTER의 BASE:사정가 감소
		BASE:MASTER:사정 -= 600
		CFLAG:ARG:무라무라 = 0
		
		RETURN 1
	ENDIF
	
	;=================================================
	;기타·조건없음
	;=================================================
	IF 구상기입체크(0)
		RETURN 1
	ENDIF
ENDIF

;호감도의 성장적에익애의 뒤가 된다고 생각됩니다
;━━━━━━━━━━━━━━━━━━━━━━━━
