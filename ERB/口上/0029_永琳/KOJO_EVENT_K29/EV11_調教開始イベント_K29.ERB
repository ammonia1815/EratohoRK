	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(0) && FIRSTEVENT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;처음 이외
	;=================================================
	IF 구상기입체크(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

