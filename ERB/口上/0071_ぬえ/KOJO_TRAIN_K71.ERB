;=================================================
;EV20, 절정구상 FIRSTTIME(500～519)
;=================================================
;·죠교자나죠교대상가절정에 이를 때 표시
;·소분기를 모두 묻으려고 하면 괴로워지므로 주의
;-------------------------------------------------
@KOJO_EVENT_K71_20(ARG)
;-------------------------------------------------
;특수 상황
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && KFLAG:1
	SELECTCASE KFLAG:1
		;날개 풀 콤보
		CASE IS == 1 
			PRINTFORMW 「구구법, 고치소사마♪」
			PRINTFORMDW 체내를 세정할 것 같은 쾌감으로부터 개방된 것은, %CALLNAME:MASTER%의 숨도 끊어지고 끊어지가 되었을 무렵이었다…
			PRINTFORMDW 슬슬 날개가 돌아온 후에는, 마음 탓인지 말라 붙었다%CALLNAME:MASTER%(은)는 널려 있다.
		;나는 애완동물
		CASE IS == 2
			PRINTFORMW 「아- 이제, 속상하다!」
			PRINTFORMDW 그렇게 말하자 마자, 한층 더 큰 이물감과 가랑이가 찢어지는 것 같은 아픔이%CALLNAME:MASTER%(을)를 덮친다.
			PRINTFORMDW 초조해 할 수 있던 꿰맬 수 있는이  힘껏 어널 비즈를 밀어넣었던 것이다라고 안 것은,
			PRINTFORMDW 아픔과 쾌감에 의식을 날려, 지면에 넘어지고 따른 다음에 있었다….
		;목 4의 글자
		CASE IS == 3
			CALL FLASH
			PRINTFORMDW 슥과 의식이 멀어져 가는 순간, 다가와 나오도록(듯이) 복받친 정액이
			PRINTFORMDW %CALLNAME:MASTER%의 페니스를 흔들어 분출한다.
			PRINTFORMW 「아는 , 굉장히…자손 남기려고 필사적이다♪」
			PRINTFORMW 「어디에서도 좋은, 맞았으면 좋기 때문에는 비쿤비쿤 너무 필사적이고♪」
			PRINTFORMDW 게호게호와 기침 넣으면서, %CALLNAME:MASTER%(은)는 페니스의 하늘 치는 것을 계속했다…
	ENDSELECT
	KFLAG:1 = 0
	RETURN 2
ENDIF
;-------------------------------------------------
;질내사정(죠교대상→죠교자)
;-------------------------------------------------
;·Ｖ성교봉사계로질내사정 하는 경우에 표시
;·TFLAG:106은 뽑지 않는 판정 마다+1된다
;·사정량에 의한 분기는필요에 응하고 추가해 사용하는 것이 바람직함
;-------------------------------------------------
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && (TFLAG:전회질싸 == 2 || TFLAG:전회질싸 == 4)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;안빼고에 속행
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:안빼고
		;동시절정(죠교자가 동시에Ｖ절정)
		IF NOWEX:Ｖ절정
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 「응…후~, 나도……이구♪」
					CALL QUAKE(4)
					PRINTFORMW 「이제 나오지 않는다? 그런 일, 좋으니까 허리 거절해…♪」
				;그 외
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「응…아, 후~……자궁구 열어버린다…」
							CALL QUAKE(4)
							PRINTFORMW 「배안에 주입…♪」
						CASEELSE
							PRINTFORMW 「…, 지금 내지면(자)…」
							CALL QUAKE(4)
							PRINTFORMW 「아……아……♪」
					ENDSELECT
					RETURN LOCAL
			ENDSELECT
		;통상
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 「텅 비기 때문에는, 놓치거나 하지 않아?」
					PRINTFORMW 「얌전하고 식 깨지는 것이군, 구구법♪」
				;그 외
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「나의 질내가 너무 좋아 인내 할 수 없다?」
							PRINTFORMW 「…아직 딱딱하다…어디까지 짜 잡힐까…」
						CASEELSE
							PRINTFORMW 「안으로 벌벌은 하는 것이……아는 」
							PRINTFORMW 「굴복 시키고 있는 것을 알 수 있어…좋은 것」
					ENDSELECT
					RETURN LOCAL
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;동시절정(죠교자가 동시에Ｖ절정)
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:Ｖ절정
			;그 외
			PRINTFORMW 「아, 응…나도……」
			CALL QUAKE(4)
			PRINTFORMW 「이구… 이 먹는다…!」
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;애태움로부터
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:MASTER:애태움도
		;애태움중
		IF TCVAR:MASTER:애태움도 > 1
			SELECTCASE NOWEX:MASTER:사정
				;Ａ책사정
				CASE 5
					PRINTFORMW 「무엇, 어널꾸짖을 수 있고 흘려 버렸어?」
					PRINTFORMW 「아무도 이라고 해라고 말해 없지만…」
				;그 외(조루)
				CASEELSE
					PRINTFORMW 「아―, 이라고 해라고 말하지 않았는데…」
					PRINTFORMW 「이건, 조루가 나을 때까지 철저 조교군요」
			ENDSELECT
		;결정타
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 「모처럼 이는 좋다고 말했는데, 하늘 치는 것으로 좋은거야? 구구법」
				;그 외
				CASEELSE
					PRINTFORMW 「, 내버릴…나카에 충분히」
					PRINTFORMDW 꿰맬 수 있는은 킥킥 웃으면서 허리를 요 들이마시고 있다…
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;사정의파생
	SELECTCASE NOWEX:MASTER:사정
		;드라이사정
		CASE 1
			PRINTFORMW 「아는, 무엇이다, 이제가 아닌거야…?」
		;대량사정
		CASE 3
			PRINTFORMW 「아…처, 이런…응응…」
			PRINTFORMW 「……후~, 충분히…너무 내고…」
			PRINTFORMDW 와 질내의 정액을 빨아 올리고 있다…
			RETURN LOCAL
		;조루
		CASE 4
			PRINTFORMW 「네? 아, 조!」
			PRINTFORMW 「뭐야…, 이제 낼 수 없다고 말하면 용납하지 않아?」
		;Ａ책사정
		CASE 5
			PRINTFORMW 「어널에 일본의 고대 탁구 세울 수 있어, 나의 만 와 --해버렸어…」
			PRINTFORMW 「구구법, 이 쾌감, 이제 잊을 수 없지요…?」
		;그 외(사정)
		CASEELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「… 뭐, 나를 잉세대는의…?」
					PRINTFORMW 「100해 빨라, 에 히히♪」
				CASEELSE
					PRINTFORMW 「아는…배중열있고……」
					PRINTFORMW 「마력이 고는 온다…♪」
			ENDSELECT
			RETURN LOCAL
	ENDSELECT
	RETURN 2
ENDIF

;-------------------------------------------------
;항문내사정(죠교대상→죠교자)
;-------------------------------------------------
;·Ａ성교봉사계로장내사정 하는 경우에 표시
;·TFLAG:106은 뽑지 않는 판정 마다+1된다
;·사정량에 의한 분기는필요에 응하고 추가해 사용하는 것이 바람직함
;-------------------------------------------------
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && (TFLAG:전회질싸 == 6 || TFLAG:전회질싸 == 8)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;안빼고에 속행
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TFLAG:안빼고
		;동시절정(죠교자가 동시에Ａ절정)
		IF NOWEX:Ａ절정
				;그 외
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「엉덩이, 엉덩이에 뜨거운 것 나오고 있는……」
							CALL QUAKE(4)
							PRINTFORMW 「아,… 이구, 말해버린다…!!」
						CASEELSE
							PRINTFORMW 「남편님이 이 먹어 하고 있어…」
							CALL QUAKE(4)
							PRINTFORMW 「새하얗게 염색해 내지 않음……응응♪」
					ENDSELECT
					RETURN LOCAL
		;통상
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 「아는, 어널에 다 내 버렸다?」
					PRINTFORMW 「아무튼, 아직도 멈추지 않지만요!」
					PRINTFORMDW 꿰맬 수 있는은 웃으면서 신체를 튀게 하고 있다…
				;조루
				CASE 4
					PRINTFORMW 「무엇, 어널 기분이야 지나자마자 내 버렸다?」
					PRINTFORMW 「에 히히… 오-들, 는, 너의 자식, 소화되어 구야…?」
				;그 외
				CASEELSE
					PRINTFORMW 「아는…엉덩이, 열…어널에 이런…」
					PRINTFORMW 「에 히히… 오-들, 는, 너의 자식, 소화되어 구야…?」
			ENDSELECT
			RETURN LOCAL
		ENDIF
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;동시절정(죠교자가 동시에Ａ절정)
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:Ａ절정
		SELECTCASE NOWEX:MASTER:사정
			;드라이사정
			CASE 1
				PRINTFORMW 
				CALL QUAKE(4)
				PRINTFORMW 
			;그 외
			CASEELSE
				PRINTFORMW 
				CALL QUAKE(4)
				PRINTFORMW 
		ENDSELECT
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;애태움로부터
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:MASTER:애태움도
		;애태움중
		IF TCVAR:MASTER:애태움도 > 1
			SELECTCASE NOWEX:MASTER:사정
				;Ａ책사정
				CASE 5
					PRINTFORMW 
				;그 외(조루)
				CASEELSE
					PRINTFORMW 
			ENDSELECT
		;결정타
		ELSE
			SELECTCASE NOWEX:MASTER:사정
				;드라이사정
				CASE 1
					PRINTFORMW 
				;그 외
				CASEELSE
					PRINTFORMW 
			ENDSELECT
		ENDIF
		RETURN 2
	ENDIF
	
	;사정의파생
	SELECTCASE NOWEX:MASTER:사정
		;드라이사정
		CASE 1
			PRINTFORMW 「아는 , 엉덩이로 비쿤비쿤 뛰어…」
			PRINTFORMW 「 그렇지만, 아무것도 나오지 않겠는가…재미없네요」
		;대량사정
		CASE 3
			PRINTFORMW 「아는…어널에 감색인 내…」
			PRINTFORMW 「응응♪…엉덩이, 열있고……」
			PRINTFORMW 「질퍽질퍽하게 녹고 있는 것 같다? 구구법♪」
		;조루
		CASE 4
			PRINTFORMW 「응? 무엇, 빠르구나……」
			PRINTFORMW 「그렇게 나의 어널이 좋았어? 에 히」
		;Ａ책사정
		CASE 5
			PRINTFORMW 「엉덩이면서 엉덩이에 사정은…」
			PRINTFORMW 「매우 배덕적…좋았다야, 귀중한 체험할 수 있어♪」
		;그 외(사정)
		CASEELSE
			PRINTFORMW 「아는…엉덩이, 열…어널에 이런…」
			PRINTFORMW 「오-들… 뽑을 때도 칼리에 걸려, 조와조와 하겠죠?」
	ENDSELECT
	RETURN 2
ENDIF

;-------------------------------------------------
;질내사정(죠교자→죠교대상)
;-------------------------------------------------
;·Ｖ성교계로질내사정 하는 경우에 표시
;·사정량에 의한 분기는필요에 응하고 추가해 사용하는 것이 바람직함
;·현재의 사양에서는죠교자가드라이사정 하는 것은 매우 희(거의 없어?)
;-------------------------------------------------
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && (TFLAG:전회질싸 == 1 || TFLAG:전회질싸 == 3)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;동시절정(죠교대상가 동시에Ｖ절정)
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:MASTER:Ｖ절정
		PRINTFORMW 「아는… 단단히 조일 수 있어…!」
		PRINTFORMW 「 나도, 이크…♪」
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;안에 내!
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && SELECTCOM == 21
		PRINTFORMW 
		RETURN 2
	ENDIF
	
	;-------------------------------------------------
	;안에 내면 안돼!
