;-------------------------------------------------
;COM0, 침묵한다
;-------------------------------------------------
;·따랐던 것에는 안 된다
;·질문에 대해서 묵비했다경우는 가볍고반항했다취급이 된다
;-------------------------------------------------
@KOJO_COM_K71_0
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;상사상애
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(3000, 3000)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;익애
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 2000)
;OriginalString : 「ちょっと…何黙ってんのよ…」
		PRINTFORMW 「조금…무엇 입다물고 있어…」
		RETURN LOCAL
	ENDIF 
	
	;-------------------------------------------------
	;연모
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 1000)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;불구대천
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && !FAVOR(-300)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;일반
;OriginalString : 「何、だんまり？」
	PRINTFORMW 「무엇, 침묵?」
	RETURN LOCAL
ENDIF

