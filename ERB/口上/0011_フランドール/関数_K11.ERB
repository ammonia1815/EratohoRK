		CALL K11_PRINT_HEARTCBW
	ENDIF
	IF RAND:2
		PRINTFORMDW 순진한 사랑스러움을 가지는 프랑에, 어른의 색과 향기가 추가되고%CALLNAME:MASTER%(을)를 유혹한다
	ENDIF
	
	LOCAL = GETCOLOR()
	SETCOLOR 0xCCCCCC
	DRAWLINE
	SETCOLOR LOCAL

ENDIF
RETURN RESULT

