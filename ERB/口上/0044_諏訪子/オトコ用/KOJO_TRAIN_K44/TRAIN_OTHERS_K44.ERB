;────────────────────────────────────
;EV21, 대만족구상
;────────────────────────────────────
;·죠교자의대만족보너스를취득했다경우에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_21(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;스와코님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
;OriginalString : 「ん…%HTML_HEARTB()%」")
	CALL HTMLPRINTW(@"「응…%HTML_HEARTB()%」")
;OriginalString : 「今の良かったわよ…%HTML_HEARTB()%」")
	CALL HTMLPRINTW(@"「지금의 좋았어요…%HTML_HEARTB()%」")
	RETURN 2
ENDIF

;────────────────────────────────────
;EV22, 상태변화구상
;────────────────────────────────────
;·상태이상가 붙었을 때에 표시
;·TCVAR:상태변화가죠교자, TCVAR:MASTER:상태변화가죠교대상
;·상태변화가 일어나면 3 턴지속 한다
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_22(ARG)
;-------------------------------------------------

;EV22_0, 죠교자상태변화
;**********************
;━━━━━━━━━━━━━━━━━━━━━━━━
;스와코님[피폐]화  소모시:피로를 위해 소극적행동가우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(50) && IS_CONDITION(TARGET, "피폐")
	;=================================================
	;조교중첫회
	;=================================================
	IF ONCEEVENT(0)
		PRINTFORMW 「―, 조금 탄마…」
		PRINTFORMW 「유석에는 사례품 지났는지 해들…」
		RETURN 2
	ENDIF

	;조건 없음
	PRINTFORMW 「어쩐지 지쳐 왔어요…」
	PRINTFORMW 「조금은 노 깨세요…」
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;스와코님[쇠약]화  강소모시:피로를 위해 소극적행동가우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && IS_CONDITION(TARGET, "쇠약")
	;=================================================
	;조교중첫회
	;=================================================
	IF ONCEEVENT(1)
		PRINTFORMW 「--… 아직!」
		PRINTFORMW 「아직도 이것인가… 오로로로」
		RETURN 2
	ENDIF

	;조건 없음
	PRINTFORMW 「 어째서 이렇게 지쳤는지 해들…」
	PRINTFORMW 「어제 카나자와 도철 너무 한 탓…?」
	RETURN 2
ENDIF

;EV22_1, 죠교대상상태변화
;************************
;기입 개소 없음

;────────────────────────────────────
;EV23, 죠교대상능력치 업구상
;────────────────────────────────────
;·조교중에죠교대상의 능력치가 상승했다때에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K44_23(ARG)
;-------------------------------------------------
;인수 내역…
;ARG=(이번 상승했다ABL번호/각 ARG번호는 ABL의 거기에 상당)

;ARG:0 순종/1 욕망/2 기교/3 Ｃ감각/4 Ｖ감각/5 Ａ감각/6 Ｂ감각/7 봉사정신/8 노출증
;    9 레즈끼/10 ＢＬ끼/11 마조끼/12 자위중독/13 정액중독/14 레즈중독/15 고통중독/16 구속중독

;ARG:20 회화/21 애무/22 도구/23 성교/24 수치/25 봉사/26 가학/27 이상
;-------------------------------------------------
IF 구상기입체크(100)
	;일행 비운다
	PRINTL
	;이번 상승했다ABL의 파생을 본다
	SELECTCASE ARG
		;죠교대상[순종]업
		CASE 0
			PRINTFORMW 「순수에…순수에…」
			RETURN 2
		;죠교대상[욕망]업
		CASE 1
			PRINTFORMW 「거기, 하하 하지 않는다」
			RETURN 2
		;죠교대상[기교]업
		CASE 2
			PRINTFORMW 「조금 능숙해졌지 않은거야…♪」
			RETURN 2
		;죠교대상[Ｃ감각]업
		CASE 3
			PRINTFORMW 「너무 민감해도 큰 일이지 않다?」
			RETURN 2
		[SKIPSTART]
		//남자용인 것으로 comment out
		;죠교대상[Ｖ감각]업
		CASE 4
			PRINTFORMW 「어디에 보지 붙고 있어」
			RETURN 2
		[SKIPEND]
		;죠교대상[Ａ감각]업
		CASE 5
			PRINTFORMW 「눈을 떠라…!」
			RETURN 2
		;죠교대상[Ｂ감각]업
		CASE 6
			PRINTFORMW 「좋았어요, 스스로 빨거나 하면 좋아요」
			RETURN 2
		;그 외
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

