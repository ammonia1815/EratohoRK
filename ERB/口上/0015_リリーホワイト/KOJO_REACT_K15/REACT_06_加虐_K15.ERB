	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("고통을 참는다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM31, 비명지른다(REACT분류:받아들인다)
	;=================================================
	IF 구상기입체크(0) && IS_NOWCOMNAME("비명지른다")
		;-------------------------------------------------
		;공포에 의한 비명
		;-------------------------------------------------
