			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
	
	;%CALLNAME:MASTER%는Ｇ스팟을 손가락으로 후벼파지는감각에
	;머리카락을 흩뜨려 몸부림쳤다…
	;=================================================
	;죠교대상에페니스없음
	;=================================================
	IF 구상기입체크(0) && !PENIS(MASTER)
		;-------------------------------------------------
		;쾌감을 받아들인다
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을 받아들인다")
			PRINTFORMW
			RETURN 1
		ENDIF
		
		;-------------------------------------------------
		;쾌감을 참는다
		;-------------------------------------------------
		IF 구상기입체크(0) && IS_NOWCOMNAME("쾌감을 참는다")
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
