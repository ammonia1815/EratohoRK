IF 구상기입 체크(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && ! IS_NOWASSIACTNAME(, "탈의")
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
