	$INPUT_LOOP
	INPUT
	SELECTCASE RESULT
		;이름의 변경
		CASE 0
			PRINTL 이름을 입력해 주세요.
			INPUTS
			CALLNAME:MASTER = %RESULTS%
;-----------------------------------------------------------
		;여자→후타나리/남자/조루/싫증를 OFF
		CASE 1
			TALENT:MASTER:후타나리 = 0
			TALENT:MASTER:남자 = 0
			TALENT:MASTER:조루 = 0
			TALENT:MASTER:싫증 = 0
		;후타나리→후타나리를 ON. 남자를 OFF
		CASE 2
			TALENT:MASTER:후타나리 = 1
			TALENT:MASTER:남자 = 0
		;남자→남자를 ONＶ민감/Ｖ둔감/빈유/거유/후타나리를 OFF
		CASE 3
			TALENT:MASTER:Ｖ민감 = 0
			TALENT:MASTER:Ｖ둔감 = 0
			TALENT:MASTER:빈유 = 0
			TALENT:MASTER:거유 = 0
			TALENT:MASTER:후타나리 = 0
			TALENT:MASTER:남자 = 1
;-----------------------------------------------------------
		;겁쟁이→겁쟁이를 반전. 굳셈를 OFF
		CASE 4
			TALENT:MASTER:겁쟁이 = !TALENT:MASTER:겁쟁이
			TALENT:MASTER:굳셈 = 0
		;반항적→반항적를 반전. 솔직를 OFF
		CASE 5
			TALENT:MASTER:반항적 = !TALENT:MASTER:반항적
			TALENT:MASTER:솔직 = 0
		;굳셈→굳셈를 반전. 겁쟁이를 OFF
