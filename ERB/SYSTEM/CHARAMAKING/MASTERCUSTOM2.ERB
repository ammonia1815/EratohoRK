			;공백
;OriginalString : 　
			PRINT 　
		ELSE
			;개행
			PRINTL 
;OriginalString : 　　　 
			PRINT 　　　 
			LOCAL:3 = 4
		ENDIF
		LOCAL:3--
		SETCOLOR TALENT:MASTER:LOCAL ? 0x00FF00 # 0xA0A0A0
		PRINTFORM [{LOCAL:1++,2}] %TALENTNAME:LOCAL,12,LEFT%
		;성격로 분류되지 않는 소질을 날린다
		SELECTCASE LOCAL
			CASE 17
				LOCAL += 2
			CASE 37
				LOCAL += 25
			CASE 63, 88, 90
				LOCAL += 1
			CASE 65
				LOCAL += 14
		ENDSELECT
	NEXT
	PRINTL 
	SETCOLOR LOCAL:2

	LOCAL:3 = 4
	LOCAL:4 = GETSTYLE()
;OriginalString : 体質： 
	PRINT 체질: 
	FOR LOCAL, 40, 127
		IF LOCAL == 40
			LOCAL:3--
			SETCOLOR TALENT:MASTER:LOCAL ? 0x00FF00 # 0xA0A0A0
			PRINTBUTTON @"[{LOCAL:1,2}] %TALENTNAME:LOCAL,12,LEFT%", LOCAL:1++
			CONTINUE
		ELSEIF LOCAL:3
;OriginalString : 　
			PRINT 　
		ELSE
			PRINTL 
;OriginalString : 　　　 
			PRINT 　　　 
			LOCAL:3 = 4
		ENDIF
		LOCAL:3--
		;여자/남자로선택 할 수 없는 소질에 취소해 선을 넣어 본다
		SELECTCASE LOCAL
			CASE 102, 103, 108, 109
				SIF TALENT:MASTER:남자
					FONTSTYLE 4
			CASE 123, 124
				SIF !TALENT:MASTER:후타나리 && !TALENT:MASTER:남자
					FONTSTYLE 4
		ENDSELECT
		SETCOLOR TALENT:MASTER:LOCAL ? 0x00FF00 # 0xA0A0A0
		PRINTFORM [{LOCAL:1++,2}] %TALENTNAME:LOCAL,12,LEFT%
		FONTSTYLE LOCAL:4
		SELECTCASE LOCAL
			CASE 43
				LOCAL += 16
			CASE 62
				LOCAL += 7
			CASE 71
				LOCAL += 28
			CASE 113
				LOCAL += 2
			CASE 116
				LOCAL += 6
		ENDSELECT
	NEXT
	PRINTL 
	SETCOLOR LOCAL:2

