@SHOW_USERCOM
#DIM SELECTCOM_RANK
#DIM LINE
#DIM L_FIRST
#DIM SELECTUSERCOM
L_FIRST = 0
SELECTCOM_RANK = 0
LINE = LINECOUNT
$START
SIF FLAG:죠교UI패드화
	REDRAW 0
CLEARLINE LINECOUNT - LINE
SELECTCOM = COM_VITALITY("소비저", , SELECTCOM_RANK)
LOCAL:9 = GETCOLOR()
FLAG:COM표기법 = 1
IF FLAG:COM표기법 == 0
	FOR LOCAL, 0, 100
		;CALL SHOW_USERCOM_COM(LOCAL, LOCAL:9)
		CALL SHOW_USERCOM_COM2(LOCAL, LOCAL:9)		;HTML_PRINT판으로 변경@revkoishi(14/07/07)
	NEXT
ELSE
	FOR LOCAL, 0, TFLAG:가능한COM수
		LOCAL:1 = COM_VITALITY("소비저", , LOCAL)
		SIF LOCAL:1 == -1
			CONTINUE
		SIF LOCAL:1 == SELECTCOM
			SELECTCOM_RANK = LOCAL
		;CALL SHOW_USERCOM_COM(LOCAL:1, LOCAL:9)
		CALL SHOW_USERCOM_COM2(LOCAL:1, LOCAL:9)	;HTML_PRINT판으로 변경@revkoishi(14/07/07)
	NEXT
ENDIF
IF FLAG:죠교UI패드화
	PRINTL 
	FOR LOCAL, 0, 4
		SIF SELECTCOM_RANK == TFLAG:가능한COM수 && LOCAL == SELECTUSERCOM
			SETCOLOR 0x7070C0
		PRINTBUTTON @"%"[" + AUTO_SPLIT("이력 표시_더러운 표시_능력 표시_파라미터 표시", "_", LOCAL)+ "]",22%", UNICODE(11000 + LOCAL)
		RESETCOLOR
	NEXT
	PRINTL 
ELSE
	PRINTL 
;OriginalString : C 履歴表示[800]
	PRINTC 이력 표시[800]
;OriginalString : C 汚れ表示[801]
	PRINTC 더러운 표시[801]
;OriginalString : C 能力表示[802]
	PRINTC 능력 표시[802]
;OriginalString : C パラメータ表示[803]
	PRINTC 파라미터 표시[803]
	PRINTL 
ENDIF
IF FLAG:디버그
;OriginalString : C 행동예약[997]
	PRINTC 행동 예약[997]
;OriginalString : C ヒート値表示[998]
	PRINTC 히트치 표시[998]
;OriginalString : C 죠교強制終了[999]
	PRINTC 조교 강제 종료[999]
	;디버그이기 때문에 999는죠교강제 종료인 채로
ENDIF
PRINTL 
DRAWLINE
;수정 후의자세표시는 일단 생략
;CALL POSE_PRINT(L_FIRST == 0)
L_FIRST = 1
IF FLAG:죠교UI패드화
	ONEINPUTS
	CLEARLINE 1
	SELECTCASE RESULTS
		CASE KEY_A
			IF SELECTCOM_RANK < TFLAG:가능한COM수
				REDRAW 1
				DOTRAIN SELECTCOM
				RETURN 1
			ELSE
				SELECTCASE SELECTUSERCOM
					CASE 0
						CALL SHOW_EQUIP
						;TFLAG:가능한COM수 = 0
						GOTO START
					CASE 1
						CALL STAIN_INFO
						;TFLAG:가능한COM수 = 0
						GOTO START
					CASE 2
						CALL SHOW_CHARADATA_PAD
						;TFLAG:가능한COM수 = 0
						GOTO START
					CASE 3
						CALL UPCHECK_REV
						WAIT
						;TFLAG:가능한COM수 = 0
						GOTO START
				ENDSELECT
			ENDIF
		CASE KEY_UP
			SIF --SELECTCOM_RANK == -1
				SELECTCOM_RANK = TFLAG:가능한COM수
			SIF SELECTCOM_RANK < TFLAG:가능한COM수
				SELECTUSERCOM = 0
		CASE KEY_DOWN
			SIF ++SELECTCOM_RANK == TFLAG:가능한COM수 + 1
				SELECTCOM_RANK = 0
			SIF SELECTCOM_RANK < TFLAG:가능한COM수
				SELECTUSERCOM = 0
			CASE KEY_LEFT
				SIF --SELECTUSERCOM == -1
					SELECTUSERCOM = 3
			CASE KEY_RIGHT
				SIF ++SELECTUSERCOM == 4
					SELECTUSERCOM = 0
				
		CASEELSE
			ENCODETOUNI %RESULTS%
			IF INRANGE(RESULT:1, 10000, 10999)
				DOTRAIN RESULT:1 - 10000
				RETURN 1
			ELSEIF INRANGE(RESULT:1, 11000, 11999)
				SELECTCASE RESULT:1 - 11000
					CASE 0
						CALL SHOW_EQUIP
						;TFLAG:가능한COM수 = 0
						GOTO START
					CASE 1
						CALL STAIN_INFO
						;TFLAG:가능한COM수 = 0
						GOTO START
					CASE 2
						CALL SHOW_CHARADATA
						;TFLAG:가능한COM수 = 0
						GOTO START
					CASE 3
						CALL UPCHECK_REV
						WAIT
						;TFLAG:가능한COM수 = 0
						GOTO START
				ENDSELECT
			ENDIF
	ENDSELECT
ELSE
	REDRAW 1
	INPUT
	SELECTCASE RESULT
		CASE 0 TO 100
			SIF TRAINNAME:RESULT == ""
				GOTO START
			SELECTCOM = RESULT
			RESULT = 0
			CALL COMABLE(SELECTCOM)
			SIF RESULT == 0
				GOTO START
			DOTRAIN SELECTCOM
			RETURN 1
		CASE 800
			CALL SHOW_EQUIP
			;TFLAG:가능한COM수 = 0
			GOTO START
		CASE 801
			CALL STAIN_INFO
			;TFLAG:가능한COM수 = 0
			GOTO START
		CASE 802
			CALL SHOW_CHARADATA
			;TFLAG:가능한COM수 = 0
			GOTO START
		CASE 803
			CALL UPCHECK_REV
			WAIT
			;TFLAG:가능한COM수 = 0
			GOTO START
		CASE 899
			IF FLAG:디버그
				PRINTFORML TFLAG:가능한COM수 = {TFLAG:92}
				;TFLAG:가능한COM수 = 0
				GOTO START
			ENDIF
		CASE 997
			IF FLAG:디버그
				CALL ACTION_RESERVATION
				;TFLAG:가능한COM수 = 0
				GOTO START
			ENDIF
		CASE 998
			IF FLAG:디버그
				CALL SHOW_HEAT
				;TFLAG:가능한COM수 = 0
				GOTO START
			ENDIF
		CASE 999
			;강제 종료시킨다때에 SHOW_USERCOM내에서 BEGIN 할 수 없기 때문에 EVENCOM에 떨어뜨려 끝낸다
			IF FLAG:디버그
				DOTRAIN 999
				RETURN 1
			ENDIF
	ENDSELECT
ENDIF
GOTO START


@SHOW_USERCOM_COM(ARG, ARG:1)
TSTR:실행COM명 = %TRAINNAME:ARG%
SIF ARG == 71 && PENIS(TARGET)
	TSTR:실행COM명 = 펠라티오
;동일 COM색변화를 한 번 멈춘다
;SETCOLOR PREVCOM == ARG ? 0xC0C070 # ARG:1
;부하에 의해 색을 바꾼다
IF SELECTCOM == ARG && FLAG:죠교UI패드화
	SETCOLOR 0x7070C0
	IF GET_COMEFFORT(TRAINNAME:ARG) == 1
		CALL BRIGHTCOLOR, 70
	ELSEIF GET_COMEFFORT(TRAINNAME:ARG) >= 2
		CALL BRIGHTCOLOR, 40
	ENDIF
ELSE
	SELECTCASE GET_COMEFFORT(TRAINNAME:ARG)
		CASE 0
			;데포 0xC0C0C0
			SETCOLOR ARG:1
		CASE 1
			SETCOLOR 0x909090
		CASEELSE
			SETCOLOR 0x606060
	ENDSELECT
ENDIF
IF FLAG:죠교UI패드화
	PRINTBUTTON @"%TSTR:3, 27 + 1%", UNICODE(10000 + ARG)
ELSE
	PRINTFORMC %TSTR:3%[{ARG,3}] 
ENDIF
LOCAL:1 = COM_VITALITY(, TRAINNAME:ARG)
LOCAL:2 = MIN(MAX(COM_VITALITY(, TRAINNAME:ARG), MAXBASE:MASTER:기력), COM_LIM_INS())
LOCAL:3 = COM_VITALITY(, TRAINNAME:ARG) > MAXBASE:MASTER:기력 ? 32 * COM_VITALITY(, TRAINNAME:ARG) / MAXBASE:MASTER:기력 # 32
LOCAL:4 = TFLAG:저항불능 || COM_VITALITY(, TRAINNAME:ARG) > BASE:MASTER:기력 ? 0xC07070 # 0x70C070
LOCAL:5 = 0x205020
PRINTFORM ({LOCAL:1, 4}) 
SETCOLOR ARG:1
CALL PRINT_COLORBAR(LOCAL:1, LOCAL:2, LOCAL:3, UNICODE(0x2584), UNICODE(0x2584), LOCAL:4, LOCAL:5)
PRINTL 


;-------------------------------------------------
;함수명:SHOW_USERCOM_COM2
;개  요점:COM 표시 함수(HTML_PRINT 대응판)
;인  수:ARG:0…COM번호
;　　　 ARG:1… 문자 표시색
;반환값:없음(RESULT 보호)
;@SHOW_USERCOM_COM 함수의 HTML_PRINT 대응판. 뭔가 무질서인 듯해 버렸다
;신규 작성@revkoishi(14/07/07)
;-------------------------------------------------
@SHOW_USERCOM_COM2(ARG:0, ARG:1)
TSTR:실행COM명 '= \@ ARG:0 == 71 && PENIS(TARGET) ? 펠라티오 # %TRAINNAME:(ARG:0)% \@
{
HTML_PRINT HTMLFONT(\@ FLAG:죠교UI패드화 ? # %" " * MAX(PRINTCLENGTH() - STRLENS(TSTR:실행COM명) - 6, 0)% \@
         + HTMLBUTTON(\@ FLAG:죠교UI패드화 ? %TSTR:실행COM명, 28% # %TSTR:실행COM명%[{ARG:0, 3}]%" "% \@,
           \@ FLAG:죠교UI패드화 ? %UNICODE(10000 + ARG:0)% # %TOSTR(ARG:0)% \@, COM_RANK(GET_COMEFFORT(TRAINNAME:(ARG:0))))
         + @"({COM_VITALITY(, TRAINNAME:(ARG:0)), 4}) ", , (SELECTCOM == ARG:0 && FLAG:죠교UI패드화)
           ? SIGNV(GET_COMEFFORT(TRAINNAME:(ARG:0)) - 1, 0x7070C0, BRIGHTCOLORF(70, 0x7070C0), BRIGHTCOLORF(40, 0x7070C0))
           # SIGNV(GET_COMEFFORT(TRAINNAME:(ARG:0)) - 1, ARG:1, 0x909090, 0x606060))
         + HTML_COLORBAR(COM_VITALITY(, TRAINNAME:(ARG:0)),
           LIMIT(COM_VITALITY(, TRAINNAME:(ARG:0)), COM_LIM_INS(), MAXBASE:MASTER:기력),
           COM_VITALITY(, TRAINNAME:(ARG:0)) > MAXBASE:MASTER:기력 ? 32 * COM_VITALITY(, TRAINNAME:(ARG:0)) / MAXBASE:MASTER:기력 # 32,
           UNICODE(0x2584), UNICODE(0x2584),
           TFLAG:저항불능 || COM_VITALITY(, TRAINNAME:(ARG:0)) > BASE:MASTER:기력 ? 0xC07070 # 0x70C070, 0x205020)
}
RETURN RESULT


