@ASSI_TRAIN_VARIATION, ARG, ARG:1
TCVAR:ARG:조수ACT파생 = 0
SELECTCASE GET_ASSIACTNAME(ARG:1)
	;애무→수음
	CASE "손으로 애무"
		SIF PENIS(MASTER)
			TCVAR:ARG:조수ACT파생 = 1
	;쿤닐링구스→펠라
	CASE "입으로 애무"
		SIF PENIS(MASTER)
			TCVAR:ARG:조수ACT파생 = 1
	;조개 우려라→스마타
	CASE "스마타"
		SIF PENIS(MASTER)
			TCVAR:ARG:조수ACT파생 = 1
	;로터, 오나홀
	CASE "Ｃ도구"
		SIF PENIS(MASTER) && ITEM:오나홀 > 0
			TCVAR:ARG:조수ACT파생 = 1
	;연속교합
	CASE "역강간"
		SIF TFLAG:조수연속교합 == 1
			TCVAR:ARG:조수ACT파생 = 1
	;가슴애무
	CASE "가슴애무"
		SIF TALENT:MASTER:남자
			TCVAR:ARG:조수ACT파생 = 1
	;유두핥기→유두 살짝 깨물기		우선 테두리만 마련한다. 조건은 어떻게 하지@/L
	;CASE "유두핥기"
	;	SIF 0
	;		TCVAR:ARG:조수ACT파생 = 1
ENDSELECT


;조수행동가 정해지면 여기로부터행동의 메세지를 호출합니다.
@ASSI_TRAIN_MESSAGE, ARG
SELECTCASE NOWASSIACTNAME(ARG)
	;애무→수음
	CASE "손으로 애무"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%(은)는 %PENIS_DESCRIPTION()% %CALLNAME:MASTER%의 남성기를 애무하기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 몸을 애무하기 시작했다…
		ENDIF
	;쿤닐링구스→펠라
	CASE "입으로 애무"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%(은)는 %PENIS_DESCRIPTION()% %CALLNAME:MASTER%의 남성기를 입으로 애무하기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 비순을 입으로 애무하기 시작했다…
		ENDIF
	;조개 우려라→스마타
	CASE "스마타"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%(은)는 %PENIS_DESCRIPTION()% %CALLNAME:MASTER%의 남성기를 자신의 고간에 맞추고 켜기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%(은)는 두 명의 성기를 거듭해 켜기 시작했다…
		ENDIF
	;파이즈리
	CASE "파이즈리한다"
		PRINTFORML %CALLNAME:ARG%(은)는 %PENIS_DESCRIPTION()% %CALLNAME:MASTER%의 남성기를 자신의 가슴으로 끼워 켜기 시작했다…
	;발코키
	CASE "풋잡하기"
		PRINTFORML %CALLNAME:ARG%(은)는 %PENIS_DESCRIPTION()% %CALLNAME:MASTER%의 남성기를 자신의 다리로 켜기 시작했다…
	;로터, 오나홀
	CASE "Ｃ도구"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 남성기에 오나홀를 씌우고 상하시켰다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 비순에 떨리는 로터를 꽉 눌렀다…
		ENDIF
	;역강간
	CASE "역강간"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 위에서 허덕이고 있다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 페니스를 억지로, 보지의 안쪽까지 질불공평 보고 허리를 털었다…
		ENDIF
	;항문애무
	CASE "항문애무"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 어널을 애무하기 시작했다…
	;항문펠라
	CASE "항문펠라"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 애널를 정중하게 빨기 시작했다…
	;항문로터
	CASE "항문로터"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 어널에 떨리는 로터를 꽉 눌렀다…
	;항문바이브
	CASE "항문바이브"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%에 어널 바이브래이터를 빼고 꽂기 했다…
	;항문섹스
	CASE "항문섹스"
	PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 어널에 삽입해 허리를 털었다…
	;가슴애무
	CASE "가슴애무"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 유두를 손가락으로 만지기 시작했다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 가슴을 애무하기 시작했다…
		ENDIF
	;유두핥기
	CASE "유두핥기"
		IF TFLAG:조수ACT파생 == 1
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 유두를 감 씹어 해 왔다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 유두를 들이마시기 시작했다…
		ENDIF
	;유두로터
	CASE "유두로터"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 유두에 떨리는 로터를 꽉 눌렀다…
	;손가락삽입
	CASE "손가락삽입"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 비순에 손가락을 삽입해, 안을 휘저었다…
	;바이브
	CASE "바이브"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%에 바이브래이터를 삽입 해내 가려 했다…
	;성교
	CASE "성교"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 비순을 관철해 허리를 털었다…
	;키스
	CASE "키스"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 얼굴에 가까워져 입술에 입술을 거듭했다…
	;안면승마
	CASE "안면승마"
		IF TEQUIP:성교봉사중 == 3
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 안면에 걸쳐 음순을 문지르면서 %CALLNAME:TARGET%(와)과 혀를 서로 걸었다…
		ELSE
			PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 안면에 걸쳐 음순을 문질렀다…
		ENDIF
	;발을핥게한다
	CASE "발을핥게한다"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%에 자신의 다리를 빨라고 명령했다…
	;자위를과시한다
	CASE "자위를과시한다"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%에 과시하듯이, 눈앞에서 자위를 시작했다…
	;매도
	CASE "매도"
		PRINTFORML %CALLNAME:ARG%(은)는 도발하듯이 %CALLNAME:MASTER%의 치태를 모욕했다…
	;사진촬영
	CASE "사진촬영"
		PRINTFORM %CALLNAME:ARG%(은)는 카메라를 지어, %CALLNAME:MASTER%의
		IF PALAM:MASTER:쾌Ｃ + PALAM:MASTER:쾌Ｖ + PALAM:MASTER:쾌Ａ + PALAM:MASTER:쾌Ｂ + PALAM:MASTER:욕정 > 1500
;OriginalString : 乱れる
			PRINT 흐트러진다
		ELSEIF PALAM:MASTER:공포 + PALAM:MASTER:고통 + PALAM:MASTER:불쾌 + PALAM:MASTER:억울 > 1000
;OriginalString : 惨めな
			PRINT 비참한
		ELSEIF !(TEQUIP:MASTER:하반신겉옷 || TEQUIP:MASTER:상반신겉옷 || TEQUIP:MASTER:전신겉옷)
;OriginalString : は했다ない
			PRINT 상스럽다
		ENDIF
		PRINTL 모습을 짭짤하게 기록했다…
	;조교자와키스
	CASE "조교자와키스"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%에 과시하듯이 %CALLNAME:TARGET%(와)과 혀를 걸기 시작했다…
	;치태를응시
	CASE "치태를응시"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 치태를응시하고 있다…
	;머리를짓밟다
	CASE "머리를짓밟다"
		PRINTFORML %CALLNAME:ARG%은(는) %CALLNAME:MASTER%의 머리를 짓밟았다…
	;------------------------------
	;SP액션
	;------------------------------
	CASE "신문 기사로 한다"
		PRINTFORML %CALLNAME:ARG%(은)는 카메라로%CALLNAME:MASTER%의 모습을 찍어,
		PRINTFORML 그것을 재료에 기사를 쓰기 시작했다...
ENDSELECT
WAIT
RESULT = 0
CALL KOJO_ACT_ASSI
RETURN RESULT
