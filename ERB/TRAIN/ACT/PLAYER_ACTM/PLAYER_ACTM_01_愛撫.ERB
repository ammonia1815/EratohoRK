		CASE 2 ;손가락, 혀, 애널성교 허용
			
		CASE 3 ;손가락, 혀, 도구 허용
			
		CASE 4 ;손가락, 혀만 허용
			
		CASE 5 ;혀만 허용
			CALL ADD_CRI_VAR("항문애무", -999)			
		CASE 6 ;손가락만 허용
			CALL ADD_CRI_VAR("항문펠라", -999)			
		CASE 7 ;항문조교 불가
			CALL ADD_CRI_VAR("항문애무", -999)
			CALL ADD_CRI_VAR("항문펠라", -999)
	ENDSELECT
	
	SELECTCASE FLAG:항문봉사옵션
		CASE 0 ;애널성교까지 허용
			
		CASE 1 ;손가락, 혀만 허용
			
		CASE 2 ;혀만 허용
			
		CASE 3 ;손가락만 허용
			
		CASE 4 ;항문봉사 불가
			
	ENDSELECT
	
	
