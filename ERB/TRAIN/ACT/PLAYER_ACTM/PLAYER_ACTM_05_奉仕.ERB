	CALL ADD_CRI_VAR("이라마치오", -3)
ELSEIF TALENT:굳셈
	CALL ADD_CRI_VAR("이라마치오", , 4)
ENDIF

;죠교자가얌전한/건방짐
IF TALENT:얌전한
	CALL ADD_CRI_VAR("발핥기강제", -2, -2)
ELSEIF TALENT:건방짐
	CALL ADD_CRI_VAR("발핥기강제", 2, 2)
ENDIF

;죠교자가프라이드높음/낮다
IF TALENT:프라이드높음
	CALL ADD_CRI_VAR("발핥기강제", 1, 6)
	CALL ADD_CRI_VAR("기승위"    , , -2)
ELSEIF TALENT:프라이드낮음
	CALL ADD_CRI_VAR("발핥기강제", -5)
	CALL ADD_CRI_VAR("기승위", , 2)
ENDIF

;죠교자가일선을넘지않음
SIF TALENT:일선을넘지않음
	CALL ADD_CRI_VAR("발핥기강제", -3)

