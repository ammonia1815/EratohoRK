@ACT_ACTM5_69

IF TEQUIP:안면승마 || TEQUIP:안면승마항문
	CALL ADD_CRI_VAR("스마타", -5)
	CALL ADD_CRI_VAR("발핥기강제", -5)
	CALL ADD_CRI_VAR("기승위", -5)
ENDIF
IF TEQUIP:죠교대상Ｃ사용 == 2
	CALL ADD_CRI_VAR("입으로 애무를 강요", 5)
